  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select Distinct'
      'S.clientID as ClientID, '
      'S.InvoiceDocNumber as Saleno,  '
      'S.SaleId as SaleId, '
      'S.Customername as CustomerName,'
      'S.ClientPrintName as PrintName, '
      'S.totalAmountinc As TotalAmountinc, '
      'S.totalAmount As TotalAmount, '
      'S.Payment As Payment, '
      'S.balance  as Balance, '
      'S.SaleDate as SaleDate, '
      
        'If(S.IsVoucher='#39'T'#39' and isinvoice="T", "Invoice Voucher" , if(S.I' +
        'sVoucher='#39'T'#39' ,'#39'Voucher'#39' , If(S.IsCashSale='#39'T'#39' And S.IsPOS='#39'F'#39' ,'#39 +
        'Cash Sale'#39', If(S.IsRefund='#39'T'#39','#39'Refund'#39',If(S.IsInvoice='#39'T'#39','#39'Invoi' +
        'ce'#39',If(S.IsCashSale='#39'T'#39' And S.IsPOS='#39'T'#39',if(S.IsLaybyPayment='#39'T'#39' ' +
        ',if( S.TotalAmountInc <0 , '#39'Layby Reverse Transaction '#39','#39'Layby P' +
        'ayment'#39' ), '#39'POS'#39'),'#39'Unknown'#39')))))) as Type,'
      'S.QuoteStatus as Status, '
      'S.ShipDate as shipdate, '
      'S.HoldSale as held,'
      'S.PONumber as PONumber, '
      'S.terms as terms,'
      'S.Comments as Comments,'
      'S.IsPOS as IsPOS ,'
      'S.IsCashSale as IsCashSale,'
      'S.IsInvoice as IsInvoice '
      'From tblSales S'
      'inner join tblsaleslines SL on S.saleID = SL.SaleId'
      'Where '
      'SaleDate between :DateFrom and :Dateto '
      'AND  SL.Invoiced = '#39'T'#39' '
      'ANd (S.IsPOS = '#39'T'#39'  or S.IsCashSale = '#39'T'#39' or S.IsInvoice = '#39'T'#39') '
      
        'and S.islayby ='#39'F'#39' and isrefund ="F" and S.Deleted ="F" and S.Ca' +
        'ncelled="F" and SL.Shipped >0  '
      'Order by SaleID Desc')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'Dateto'
      end>
