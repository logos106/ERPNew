  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT distinct '
      'M.GlobalRef,'
      'M.ManifestID,'
      'M.ManifestID as ManifestNo , '
      'M.DespatchDate, '
      'M.OwnerName, '
      'M.Driver, '
      'M.Phone, '
      'SUBSTRING(M.Notes,1,255) as Notes, '
      'M.VehicleName as Vehicle, '
      'M.Trailer, '
      'M.FromCity, '
      'M.FromState, '
      'M.FromPostcode, '
      'M.ToCity, '
      'M.ToState, '
      'M.ToPostcode,'
      'MR.RouteDesc as Route,'
      'MR.Distance as '#39'DistanceKM'#39','
      'M.Completed  ,'
      'SOSL.SaleID as SOID, '
      'INVSL.SaleID as INVID,'
      'ifnull(SOSLP.Partname , INVSLP.Partname) as Partname,'
      'ifnull(SOSLP.PREFEREDSUPP , INVSLP.PREFEREDSUPP) as PREFEREDSUPP'
      'FROM tblmanifest M'
      'Inner join tblmanifestlines ML on M.ManifestID = ML.ManifestID'
      
        'Left join (tblsaleslines SOSL  inner join tblparts SOSLP  on SOS' +
        'LP.PARTSID  = SOSL.productID ) on SOSL.saleLineid = ML.saleLineI' +
        'D'
      
        'Left join (tblsaleslines INVSL inner join tblparts INVSLP on INV' +
        'SLP.partsId = INVSL.productID) on INVSL.SaleLineID = ML.InvoiceL' +
        'ineID'
      
        'Left Join tblmanifestroutes MR ON MR.ManifestRouteID = M.Manifes' +
        'tRouteID'
      'Where M.DespatchDate Between  :txtfrom AND :txtto'
      'Order By ManifestID Desc;')
    Left = 49
    ParamData = <
      item
        DataType = ftString
        Name = 'txtfrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end>
