      'SOL.UnitofMeasureBackorder sobo, '
      'SOL.TotalLineAmount soamtEx, '
      'SOL.TotalLineAmountInc soAmtinc, '
      
        'concat(invlines.UnitofMeasureSaleLines, '#39'('#39' , invlines.UnitofMea' +
        'sureMultiplier,'#39')'#39') as InvUOM, '
      
        'invlines.UnitofMeasureQtySold invQtySold, invlines.UnitofMeasure' +
        'Shipped invQtyShipped, invlines.UnitofMeasureBackorder invBO, '
      
        'invlines.TotalLineAmount invAmtEx, invlines.TotalLineAmountInc i' +
        'nvAmtinc,'
      'inv.saleID as InvID'
      'from '
      'tblsales SO'
      'inner join tblsaleslines SOL on SO.saleId = SOL.saleID'
      
        'inner join tblProctree PT on SOl.SaleLineId'#9'= PT.masterID and PT' +
        '.ParentID=0'#9'and '#9'PT.Mastertype <> '#39'mtProduct'#39' '#9#9
      'inner join (tblsalesOrderInvoices SOI '
      
        'inner join tblsalesOrderInvoiceLines SOIL on SOIL.SOInvoiceID = ' +
