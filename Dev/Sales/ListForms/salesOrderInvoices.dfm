  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select  '
      '1 as Details,'
      'SO.IsInternalOrder as IsInternalOrder,'
      'SO.SaleId as SaleId, '
      'So.converted  as converted,'
      'SO.CustomerName as CustomerName, '
      'SO.TotalAmount as TotalAmount, '
      'SO.TotalAmountInc as TotalAmountInc, '
      'NULL as invoicedon,  '
      'NULL as Employeename, '
      '/*SUM(Inv.TotalAmount) as InvTotalAmount, '
      'sUM(Inv.TotalAmountInc) as InvTotalAmountInc,'
      'sUM(InvLines.UnitofMeasureShipped) as InvShipped,*/'
      
        'SUM(if(SO.isinternalOrder , SOIL.Qty * SOL.LinePrice ,Inv.TotalA' +
        'mount)) as InvTotalAmount, '
      
        'sUM(if(SO.isinternalOrder , SOIL.qty * SOL.LinePriceinc ,Inv.Tot' +
        'alAmountInc)) as InvTotalAmountInc,'
      
        'sUM(if(SO.isinternalOrder , SOIL.qty , InvLines.UnitofMeasureShi' +
        'pped)) as InvShipped,'
      'Null as SOShipped, '
      'NULL as SOInvoicedAmt,'
      'null as SaleLineId, '
      'null ProductName , '
      'null as Product_Description, '
      'null as  SOLUOM, '
      'null as soQtysold, '
      'null as soQtyShipped, '
      'null as sobo, '
      'null as soamtEx, '
      'null as soAmtinc, '
      'null as InvUOM, '
      'null as invQtySold, '
      'null as invQtyShipped, '
      'null as invBO, '
      'null as invAmtEx, '
      'null as invAmtinc,'
      'null as InvID'
      'from '
      'tblsales SO'
      'inner join tblsaleslines SOL on SO.saleId = SOL.saleID'
      
        'inner join tblProctree PT on SOl.SaleLineId'#9'= PT.masterID and PT' +
        '.ParentID=0'#9'and '#9'PT.Mastertype <> '#39'mtProduct'#39' '#9#9
      'Left  join (tblsalesOrderInvoices SOI '
      
        'inner join tblsalesOrderInvoiceLines SOIL on SOIL.SOInvoiceID = ' +
        'SOI.SOINvoiceID   '
      'Left join (tblsales Inv '
      
        'inner join tblsaleslines InvLines on Inv.saleId = invlines.SaleI' +
        'D ) on  Inv.SOInvoiceRef = SOI.globalref  and invlines.SOInvoice' +
        'LineRef = SOIL.globalref   ) on SO.saleId = SOI.salesorderID  an' +
        'd SOIL.SalesorderLineID = SOL.SaleLineID'
      '/*INNER join tblsales Inv on Inv.SOInvoiceRef = SOI.globalref  '
      
        'inner join tblsaleslines InvLines on Inv.saleId = invlines.SaleI' +
        'D and invlines.SOInvoiceLineRef = SOIL.globalref   ) on SO.saleI' +
        'd = SOI.salesorderID  and SOIL.SalesorderLineID = SOL.SaleLineID' +
        '*/'
      
        'where SOL.shipped<>0 and  SO.IsSalesOrder="T" and (SO.converted ' +
        '="F" or :Showconverted ="T") and  (So.saleId = :sALEid or (:sALE' +
        'id=0 and So.SaleDate between :DateFrom and :dateTo))'
      ''
      'GROUP BY So.SALEid'
      'union all '
      'Select '
      '2 as Details,'
      'SO.IsInternalOrder as IsInternalOrder,'
      'SO.SaleId as SaleId, '
      'So.converted  as converted,'
      'SO.CustomerName as CustomerName, '
      'SO.TotalAmount as TotalAmount, '
      'SO.TotalAmountInc as TotalAmountInc, '
      'Null as invoicedon,  '
      'Null as Employeename, '
      'SUM(InvLines.TotalLINEAmount) as InvTotalAmount, '
      'sUM(InvLines.TotalLINEAmountInc) as InvTotalAmountInc,'
      'sUM(InvLines.UnitofMeasureShipped) as InvShipped,'
      'ifnull(sUM(SOIL.UOMQty),0) as SOShipped, '
      'ifnull(sUM(SOIL.Qty *SOL.linepriceinc ),0) as SOInvoicedAmt,'
      'SOL.SaleLineID as SaleLineId,'
      'SOL.ProductName as ProductName, '
      'SOL.Product_Description as Product_Description,'
      
        'concat(SOL.UnitofMeasureSaleLines, '#39'('#39' , SOL.UnitofMeasureMultip' +
        'lier,'#39')'#39') as SOLUOM, '
      'SOL.UnitofMeasureQtySold soQtysold, '
      'SOL.UnitofMeasureShipped soQtyShipped, '
      'SOL.UnitofMeasureBackorder sobo, '
      'SOL.TotalLineAmount soamtEx, '
      'SOL.TotalLineAmountInc soAmtinc, '
      'null as InvUOM, '
      'null as invQtySold, '
