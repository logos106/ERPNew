  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      '0 as DetailType,'
      'C.ClientID as CustomerID,'
      'C.Company as CustomerName,'
      'Concat(C.FirstName," ",C.LastName) as ContactName, '
      'null as JobCustomerName,'
      'C.ClientNo as ClientNo,'
      'CT.TypeName as CustomerType,'
      'C.Street as CustomerStreet,'
      'C.Street2 as CustomerStreet2,'
      'C.Street3 as CustomerStreet3,'
      'C.Suburb as CustomerSuburb,'
      'C.State as CustomerState,'
      'C.Postcode as CustomerPostcode,'
      'null as JobID,'
      'null as JobClientNo,'
      'null as JobName,'
      'null as JobRegistration,'
      'null as JobNumber,'
      'null as JobWarrantyPeriod,'
      'null as JobNotes,'
      'null as SaleCustomerName,'
      'null as SaleDate,'
      'null as SaleDateTime,'
      'sum(S.TotalAmount) as SaleTotalEx,'
      'sum(S.TotalAmountInc) as SaleAmountInc,'
      'sum(S.TotalTax) as SaleTax,'
      'null as SaleDepartment,'
      'null as SaleComments,'
      'null as SaleCustField1,'
      'null as SaleCustField2,'
      'null as SaleCustField3,'
      'null as SaleCustField4,'
      'null as SaleCustField5,'
      'null as SaleCustField6,'
      'null as SaleCustField7,'
      'null as SaleCustField8,'
      'null as SaleCustField9,'
      'null as SaleCustField10,'
      'null as SaleTerms,'
      'null as ProductName,'
      'null as DocketNumber,'
      'null as MemoLine,'
      'null as UomQtySold,'
      'null as UomQtyShipped,'
      'null as UomQtyBackorder,'
      'null as UomName,'
      'null as AmountEx,'
      'null as AmountInc,'
      'null as AmountTax,'
      'null as TaxCode,'
      'sum(SL.Discounts) as AmountDiscount, '
      'null as DiscountPerUnit '
      'from tblclients C'
      'inner join tblclients CJ on C.ClientID = CJ.ParentClientID'
      'left join tblclienttype CT on C.ClientTypeID = CT.ClientTypeID'
      
        'inner join tblsales S on CJ.ClientID = S.ClientID and S.IsInvoic' +
        'e = "T" and S.Deleted = "F"'
      'inner join tblsaleslines SL on S.SaleID = SL.SaleID '
      'where (S.SaleDateTime between :DateFrom and :DateTo)'
      'and ((:ClientId = 0) or (C.ClientID = :ClientId))'
      'group by C.ClientID'
      'union all'
      'select '
      '1 as DetailType,'
      'C.ClientID as CustomerID,'
      'C.Company as CustomerName,'
      'Concat(C.FirstName," ",C.LastName) as ContactName, '
      'CJ.Company as JobCustomerName,'
      'C.ClientNo as ClientNo,'
      'CT.TypeName as CustomerType,'
      'C.Street as CustomerStreet,'
      'C.Street2 as CustomerStreet2,'
      'C.Street3 as CustomerStreet3,'
      'C.Suburb as CustomerSuburb,'
      'C.State as CustomerState,'
      'C.Postcode as CustomerPostcode,'
      'CJ.ClientID as JobID,'
      'CJ.ClientNo as JobClientNo,'
      'CJ.JobName as JobName,'
      'CJ.JobRegistration as JobRegistration,'
      'CJ.JobNumber as JobNumber,'
      'CJ.WarrantyPeriod as JobWarrantyPeriod,'
      'CJ.Notes as JobNotes,'
      'S.CustomerName as SaleCustomerName,'
      'null as SaleDate,'
      'null as SaleDateTime,'
      'sum(S.TotalAmount) as SaleTotalEx,'
      'sum(S.TotalAmountInc) as SaleAmountInc,'
      'sum(S.TotalTax) as SaleTax,'
      'null as SaleDepartment,'
      'null as SaleComments,'
      'null as SaleCustField1,'
      'null as SaleCustField2,'
      'null as SaleCustField3,'
      'null as SaleCustField4,'
      'null as SaleCustField5,'
      'null as SaleCustField6,'
      'null as SaleCustField7,'
      'null as SaleCustField8,'
      'null as SaleCustField9,'
      'null as SaleCustField10,'
      'null as SaleTerms,'
      'null as ProductName,'
      'null as DocketNumber,'
      'null as MemoLine,'
      'null as UomQtySold,'
      'null as UomQtyShipped,'
      'null as UomQtyBackorder,'
      'null as UomName,'
      'null as AmountEx,'
      'null as AmountInc,'
      'null as AmountTax,'
      'null as TaxCode,'
      'sum(SL.Discounts) as AmountDiscount, '
      'null as DiscountPerUnit '
      'from tblclients C'
      'inner join tblclients CJ on C.ClientID = CJ.ParentClientID'
      'left join tblclienttype CT on C.ClientTypeID = CT.ClientTypeID'
      
        'inner join tblsales S on CJ.ClientID = S.ClientID and S.IsInvoic' +
        'e = "T" and S.Deleted = "F"'
      'inner join tblsaleslines SL on S.SaleID = SL.SaleID '
      'where (S.SaleDateTime between :DateFrom and :DateTo)'
      'and ((:ClientId = 0) or (C.ClientID = :ClientId))'
      'group by CJ.JobName'
      'union all'
      'select '
      '2 as DetailType,'
      'C.ClientID as CustomerID,'
      'C.Company as CustomerName,'
      'Concat(C.FirstName," ",C.LastName) as ContactName, '
      'CJ.Company as JobCustomerName,'
