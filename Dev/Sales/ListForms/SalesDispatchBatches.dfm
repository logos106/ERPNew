  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'distinct 1 as Details, '
      'S.SaleID as SaleID, '
      'SL.saleLineId as saleLineId, '
      'S.saleDate as saleDate, '
      'SL.productname as productname, '
      'SL.UnitofMeasureQtySold as UnitofMeasureQtySold, '
      'SL.UnitofMeasureShipped as UnitofMeasureShipped, '
      'SL.UnitofMeasureBackorder  as UnitofMeasureBackorder, '
      
        'concat(SL.UnitofMeasureSaleLines , '#39'('#39' , SL.UnitofMeasureMultipl' +
        'ier , '#39')'#39') as UOM ,'
      'NULL as Batchno , '
      'NULL as BatchQty, '
      'NULL as TotalDispatched,'
      'NULL  as Tobedispatched'
      'From tblSales S'
      
        'inner join tblsaleslines SL on S.saleId = SL.saleID and ifnull(S' +
        'L.shipped,0)<> 0'
      
        'inner join tblpqa PQA on PQA.transID = S.saleId and PQA.TransLin' +
        'eID = SL.saleLineID and PQA.transType  in('#39'TInvoiceLine'#39' ) and P' +
        'QA.alloctype = '#39'OUT'#39
      
        'inner join tblpqaDetails PQABatch on PQABatch.pqaid = PQA.PQAID ' +
        'and PQABatch.PQAType ='#39'Batch'#39' and ifnull(PQABatch.Qty,0)<> 0'
      'Where S.SaleDate between :DateFrom and :DateTo'
      'union all '
      'Select '
      '2 as Details,'
      'S.SaleID as SaleID, '
      'SL.saleLineId as saleLineId, '
      'S.saleDate as saleDate, '
      'SL.productname as productname, '
      'SL.UnitofMeasureQtySold as UnitofMeasureQtySold, '
      'SL.UnitofMeasureShipped as UnitofMeasureShipped, '
      'SL.UnitofMeasureBackorder  as UnitofMeasureBackorder, '
      
        'concat(SL.UnitofMeasureSaleLines , '#39'('#39' , SL.UnitofMeasureMultipl' +
        'ier , '#39')'#39') as UOM ,'
      'PQABatch.Value as Batchno , '
      'Sum(PQABatch.uomQty) as BatchQty, '
      
        'ifnull((Select Sum(UOMQTY) from tblsalesdispatchlines SDL where ' +
        ' SDL.saleID = SL.saleID and SDL.salelineId = SL.salelineID and S' +
        'DL.BatchNo = PQABatch.Value ),0) as TotalDispatched,'
      
        'PQABatch.uomQty - ifnull((Select Sum(UOMQTY) from tblsalesdispat' +
        'chlines SDL where  SDL.saleID = SL.saleID and SDL.salelineId = S' +
        'L.salelineID and SDL.BatchNo = PQABatch.Value ),0) as Tobedispat' +
        'ched'
      'From tblSales S'
      
        'inner join tblsaleslines SL on S.saleId = SL.saleID and ifnull(S' +
        'L.shipped,0)<> 0'
      
        'inner join tblpqa PQA on PQA.transID = S.saleId and PQA.TransLin' +
        'eID = SL.saleLineID and PQA.transType  in('#39'TInvoiceLine'#39' ) and P' +
        'QA.alloctype = '#39'OUT'#39
      
        'inner join tblpqaDetails PQABatch on PQABatch.pqaid = PQA.PQAID ' +
        'and PQABatch.PQAType ='#39'Batch'#39' and ifnull(PQABatch.Qty,0)<> 0'
      'Where S.SaleDate between :DateFrom and :DateTo'
      'Group by SL.saleID ,SL.salelineID ,PQABatch.Value'
      
        'Order by saleID desc , saleDate desc , ProductName, SaleLineId ,' +
        ' BAtchno')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end>
