  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select Distinct'
      'S.clientId as ClientId,'
      'S.InvoiceDocNumber as Saleno,  '
      'S.SaleId as SaleId, '
      'S.BOID as BORef, '
      'S.Customername as CustomerName, '
      'S.totalAmountinc As TotalAmountinc, '
      'S.totalAmount As TotalAmount, '
      'S.Payment As Payment, '
      'S.balance  as Balance, '
      'S.SaleDate as SaleDate, '
      'S.Converted as Converted,'
      'S.futureSo as futureSo,'
      'S.Deleted as Done,'
      
        'If(S.IsRefund='#39'T'#39','#39'Refund'#39',If(S.IsInvoice='#39'T'#39','#39'Invoice'#39',If(S.IsS' +
        'alesOrder='#39'T'#39' , '#39'Sales Order'#39' ,'#39'Layby'#39'))) as Type,'
      'S.QuoteStatus as Status, '
      'S.ShipDate as shipdate, '
      'S.HoldSale as held,'
      'S.PONumber, '
      'S.terms,'
      'S.Comments,'
      'S.noofboxes, '
      'Sum(SL.shipped) as shipped,'
      'S.IsRefund as IsRefund,'
      'S.IsSalesOrder as IsSalesOrder,'
      'S.IsInvoice as IsInvoice,'
      'S.islayby as islayby'
      'From tblSales S'
      'inner join tblsaleslines SL on S.saleID = SL.SaleId'
      'Where '
      '(S.clientID= :CustomerId or :CustomerId=0) '
      'and SaleDate between :DateFrom and :Dateto '
      
        'AND (S.IsRefund = '#39'T'#39' or S.IsSalesOrder= '#39'T'#39' or S.IsInvoice = '#39'T' +
        #39' or  S.islayby ='#39'T'#39') '
      'Group by s.saleID'
      'Having :IncludeBos ="T" or Shipped >0'
      'Order by SaleID Desc')
    OnCalcFields = qryMainCalcFields
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'CustomerId'
      end
      item
        DataType = ftUnknown
        Name = 'CustomerId'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'Dateto'
      end
      item
        DataType = ftUnknown
        Name = 'IncludeBos'
      end>
