  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      '1 as details, '
      'S.SaleID as SaleId,'
      'S.InvoiceDocNumber as InvoiceDocNumber,'
      'S.CustomerName as CustomerName,'
      'S.TotalAmountInc as TotalAmountInc,'
      'S.IsInvoice as IsInvoice,'
      'S.IsQuote as IsQuote,'
      'S.IsSalesOrder as IsSalesOrder,'
      'Max(PP.PrePaymentDate) as PrePaymentDate,'
      'sum(PP.PayAmount) as PayAmount, '
      'S.TotalAmountInc - sum(PP.PayAmount) as Balance,'
      '0 as PrePaymentID'
      'from tblSales S '
      'Left join (tblSalesPrepayments  SP'
      'inner join tblprepayments PP on PP.GlobalRef = SP.PrepayRef'
      'and PP.Deleted = '#39'F'#39' )  on SP.salesref = S.Globalref'
      'where  s.OriginalNo =:SalesOriginalRef'
      'group by SaleID'
      'union all '
      'Select '
      '2 as details,'
      'S.SaleID as SaleId,'
      'S.InvoiceDocNumber as InvoiceDocNumber,'
      'S.CustomerName as CustomerName,'
      'S.TotalAmountInc as TotalAmountInc,'
      'S.IsInvoice as IsInvoice,'
      'S.IsQuote as IsQuote,'
      'S.IsSalesOrder as IsSalesOrder,'
      'PP.PrePaymentDate as PrePaymentDate,'
      'PP.PayAmount as PayAmount, '
      '0.0 as Balance,'
      'PP.PrePaymentID as PrePaymentID'
      'from tblSales S '
      'inner join (tblSalesPrepayments  SP'
      'inner join tblprepayments PP on PP.GlobalRef = SP.PrepayRef'
      'and PP.Deleted = '#39'F'#39' )  on SP.salesref = S.Globalref'
      'where  s.OriginalNo =:SalesOriginalRef'
      'order by sAleId desc , details , PrePaymentDate desc')
