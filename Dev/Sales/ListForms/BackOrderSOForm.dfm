  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'tblSales.SaleID as SaleID,'
      'tblClients.ClientID as ClientID,'
      'tblEmployees.EmployeeID as EmployeeID,'
      'tblSalesLines.ProductID as ProductID,'
      'tblSales.SaleDate as SaleDate,'
      '" " as Firstcolumn, '
      '" " as Secondcolumn, '
      '" " as Thirdcolumn,'
      
        'CONCAT_WS(" ", If(InStr(tblClients.Company,'#39'^'#39') -1 >0,Left(tblCl' +
        'ients.Company,InStr(tblClients.Company,'#39'^'#39')-1),tblClients.Compan' +
        'y),IF(char_length(tblClients.JobName)>0, Concat("(",tblClients.J' +
        'obName,")"),'#39#39')) as CustomerName,'
      'tblSales.ClientPrintName as PrintName,'
      'TypeName as CustomerType,'
      'tblSales.GlobalRef as GlobalRef,'
      'tblSales.OriginalNo as OriginalNo,'
      'tblSales.InvoiceDocNumber as SalesOrderNumber,'
      'tblSales.PONumber as PONumber,'
      'tblSalesLines.BackOrder as BackOrder,'
      'tblSalesLines.ProductName as ProductName,'
      'tblSalesLines.Product_Description as Product_Description,'
      'tblSalesLines.LinePrice  as LinePrice ,'
      'tblSalesLines.LinePriceInc as LinePriceInc,'
      'tblSalesLines.LineWholesalePrice as LineWholesalePrice,'
      'tblSalesLines.LineCost as LineCost,'
      'tblSalesLines.LineCostInc as LineCostInc,'
      'tblSalesLines.BackOrder*tblSalesLines.LineCost as TotalLineCost,'
      
        'tblSalesLines.BackOrder*tblSalesLines.LineCostInc as TotalLineCo' +
        'stinc,'
      
        'tblSalesLines.BackOrder*tblSalesLines.LinePrice as TotalLinePric' +
        'e,'
      
        'tblSalesLines.BackOrder*tblSalesLines.LinePriceInc as TotalLineP' +
        'riceInc,'
      'tblSalesLines.WarrantyEndsOn as WarrantyEndsOn,'
      'tblSalesLines.WarrantyPeriod as WarrantyPeriod,'
      'tblSalesLines.ShipDate as LineShipDate,'
      'tblSales.Deleted as Deleted,'
      'tblSales.Converted as Converted,'
      'tblSales.QuoteGlobalRef as ConvertFromQuote,'
      'tblSales.DueDate as DueDate,'
      'tblSales.ShipDate as ShipDate,'
      'tblSales.FutureSO as FutureSO,'
      'tblSales.Employeename as Rep, '
      'tblSales.EnteredBy as EnteredBy, '
      'tblsales.Class as class, '
      'tblSales.OriginalCreationDate as '#39'Date Entered'#39','
      
        'SUBSTRING_INDEX(tblSales.ShipTo,char(13),1) as '#39'Shipping Address' +
        #39','
      'tblSalesLines.SalesLinesCustField1,'
      'tblSalesLines.SalesLinesCustField2,'
      'tblSalesLines.SalesLinesCustField3,'
      'tblSalesLines.SalesLinesCustField4,'
      'tblSalesLines.SalesLinesCustField5,'
      'tblSalesLines.SalesLinesCustField6,'
      'tblSalesLines.SalesLinesCustField7,'
      'tblSalesLines.SalesLinesCustField8,'
      'tblSalesLines.SalesLinesCustField9,'
      'tblSalesLines.SalesLinesCustField10, '
      
        'tblsales.Balance - Sum(IfNull(tblprepayments.PayAmount,0)) - Sum' +
        '(IfNull(PP.TotalAmountInc,0)) as Balance, '
      '0.0 as AvailableQty,'
      '0.0 as instockQty,'
      '0.0 as AllocatedSOQty,'
      '0.0 as AllocatedBOQty,'
      '0.0 as OnorderQty,'
      '0.0 as onBuildQty,'
      '0.0 as buildingQty'
      'FROM tblSales'
      'INNER JOIN tblSalesLines Using(SaleID)'
      'LEFT join tblpqa PQA on tblSalesLines.productId = PQa.productID'
      'INNER JOIN tblParts ON tblSalesLines.ProductID=tblParts.PARTSID'
      
        'INNER JOIN tblClients ON  (tblSales.ClientID = tblClients.Client' +
        'ID)'
      'LEFT JOIN tblclienttype Using(ClientTypeID)'
      
        'LEFT JOIN tblEmployees ON  tblSales.EmployeeID = tblEmployees.Em' +
        'ployeeID'
      
        'left join tblsalesprepayments on tblsalesprepayments.SalesRef = ' +
        'tblsales.GlobalRef'
      
        'left join tblprepayments on tblprepayments.GlobalRef = tblsalesp' +
        'repayments.PrepayRef'
      
        'left join tblsales as PP on PP.SOProgressPaymentGlobalRef = tbls' +
        'ales.GlobalRef'
      
        'WHERE tblSales.IsRefund ='#39'F'#39' AND tblSales.IsCashSale='#39'F'#39' AND tbl' +
        'Sales.IsInvoice='#39'F'#39' AND tblSalesLines.Invoiced='#39'F'#39
      
        'AND tblSales.IsSalesOrder ='#39'T'#39' And tblSales.IsQuote<>'#39'T'#39' AND  tb' +
        'lSales.Deleted='#39'F'#39
      'AND tblSales.SaleDate Between :txtfrom AND :txtto'
      
        'AND tblSalesLines.BackOrder <> 0 AND tblSalesLines.Invoiced = '#39'F' +
        #39
      'AND   ifnull(tblSales.BOID , '#39#39') = '#39#39
      'GROUP BY tblSalesLines.SaleLineID'
      'order by SaleLineID desc')
    Top = 385
    ParamData = <
      item
        DataType = ftString
        Name = 'txtfrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end>
