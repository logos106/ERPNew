  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      
        'if(s.IsPOS = "T", "POS", if(s.IsCashSale = "T" ,  "Cash Sale", i' +
        'f(s.IsInvoice = "T" , "Invoice" ,"")))  as "SaleType",'
      's.InvoiceDocNumber   as DocNumber,  '
      's.Class            as Department,'
      'sl.Product_Description  as Product_Description, '
      'sl.TotalLineAmountInc as TotalLineAmountInc,'
      'SL.TotalLineAmount as TotalLineAmount, '
      'S.TotalAmount as TotalAmount,'
      'CT.TypeName as CustomerType,'
      'S.SaleDate as SaleDate,'
      'SL.SaleLineId as SaleLineId,'
      'S.TimeOfSale as TimeOfSale,'
      'S.CustomerName as CustomerName,'
      'S.ShipTo as ShipTo,'
      'S.GlobalRef as GlobalRef,'
      'Memo as Memo,'
      'Comments as Comments,'
      'S.OriginalNo as OriginalNo,'
      'S.InvoiceDocNumber as InvoiceNumber ,'
      'S.PONumber as PONumber,'
      'S.TotalAmount as TotalAmountEx,'
      'S.TotalTax as TotalTax,'
      'S.TotalAmountInc as TotalAmountInc,'
      'S.EmployeeName AS EmployeeName,'
      'S.ConNote as ConsignmentNote,'
      'S.Class as ClassName,'
      'SL.ProductName as ProductName,'
      'SL.Product_Description as ProductDescription,'
      'SL.LinePrice as ItemPriceEx,'
      'SL.LinePriceInc as ItemPriceInc,'
      'SL.LineCost as ItemCostEx,'
      'Round(SL.LineCostInc*SL.Shipped,5) as LineCostInc,'
      'Round(SL.LineCost*SL.Shipped,5) as LineCostEx,'
      'SL.LineTaxCode as TaxCode,'
      'SL.LineTax as LineTax,'
      'SL.Shipped as Shipped,'
      'SL.TotalLineAmount as LineTotalEx,'
      'SL.TotalLineAmountInc as LineTotalInc,'
      'S.Deleted as Deleted,'
      'S.EmployeeID as EmployeeID,'
      'S.ClientID as ClientID,'
      'S.SaleID as SaleID,'
      'S.RunName as RunName,'
      'SL.ProductID as ProductID,'
      'SL.Attrib1Sale as Attrib1Sale,'
      'SL.Attrib2Sale as Attrib2Sale,'
      'SL.Attrib1SaleRate as Attrib1SaleRate,'
      'SL.UnitOfMeasureSaleLines as UnitOfMeasureSaleLines,'
      'SL.UnitOfMeasureShipped as UnitOfMeasureShipped,'
      'S.ClassID as ClassID,'
      'tblPosTills.TillName as Till,'
      'SL.Area as Area,'
      'S.ShipDate as ShipDate,'
      'S.Medtype as POSSource,'
      'SL.WarrantyEndsOn as WarrantyEndsOn ,'
      'SL.WarrantyPeriod as WarrantyPeriod, S.POSPostCode,'
      'SL.ShipDate as LineShipDate,'
      'SL.DiscountPercent,SL.Discounts,'
      'SL.Markup,SL.MarkupPercent ,'
      'p.CUSTFLD1 as CUSTFLD1,'
      'p.CUSTFLD2 as CUSTFLD2,'
      'p.CUSTFLD3 as CUSTFLD3,'
      'p.CUSTFLD4 as CUSTFLD4,'
      'p.CUSTFLD5 as CUSTFLD5,'
      'p.CUSTFLD6 as CUSTFLD6,'
      'p.CUSTFLD7 as CUSTFLD7,'
      'p.CUSTFLD8 as CUSTFLD8,'
      'p.CUSTFLD9 as CUSTFLD9,'
      'p.CUSTFLD10 as CUSTFLD10,'
      'p.CUSTFLD11 as CUSTFLD11,'
      'p.CUSTFLD12 as CUSTFLD12,'
      'p.CUSTFLD13 as CUSTFLD13,'
      'p.CUSTFLD14 as CUSTFLD14,'
      'p.CUSTFLD15 as CUSTFLD15,'
      'p.CUSTDATE1 as CUSTDATE1,'
      'p.CUSTDATE2 as CUSTDATE2,'
      'p.CUSTDATE3 as CUSTDATE3'
      'FROM tblSales S '
      'inner join tblclients C on s.clientID = C.clientID '
      'left join tblclienttype CT on C.ClientTypeID = CT.ClientTypeID'
      'INNER JOIN tblSalesLines sl USING(SaleId) '
      'left join tblPosTills on tblPosTills.TillId = S.TillId '
      'inner join tblparts P on P.partsID = SL.productID'
      'WHERE S.Cancelled = "F" and S.Deleted ="F"  '
      
        'AND ((:ISOnlyPOS ="T" and  s.IsPos = "T") or ((:ISOnlyPOS ="F" a' +
        'nd (s.IsPos = "T" OR s.IsCashSAle = "T" OR s.IsInvoice = "T"))))'
      'AND s.SaleDateTime >= :DateFrom'
      'AND s.SaleDateTime < :Dateto'
      'ORDER BY s.SaleDateTime, SAleID, Productname')
    object qryMainCUSTFLD1: TWideStringField
      DisplayWidth = 255
      FieldName = 'CUSTFLD1'
