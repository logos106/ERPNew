  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select'
      's.sALeId as sALeId,'
      'S.customerName as customerName ,'
      'S.totalamountinc  as totalamountinc,'
      'S.Payment as Payment,'
      's.balance as balance,'
      's.ForeignExchangeCode as ForeignExchangeCode,'
      's.ForeignTotalAmount as ForeignTotalAmount,'
      's.ForeignBalanceAmount as ForeignBalanceAmount,'
      's.DueDate as DueDate,'
      's.ShipDate as ShipDate,'
      's.SaleDate as SaleDate,'
      
        'if( ifnull(S.duedate,0)=0 or  S.duedate >=      "2015-12-17",S.B' +
        'alance,0.00) as withinTerms, '
      
        'if(S.duedate between "2015-11-17" and "2015-12-17",S.Balance,0.0' +
        '0) as 1to30days, '
      
        'if(S.duedate between "2015-10-17" and "2015-11-17",S.Balance,0.0' +
        '0) as 30to60days, '
      
        'if(S.duedate between "2015-09-17" and "2015-10-17",S.Balance,0.0' +
        '0) as 60to90days, '
      
        'if(S.duedate <       "2015-09-17",S.Balance,0.00) as Over90days ' +
        ','
      'SL.linepriceinc as SurchargePaid,'
      'SL.Product_Description as SurchargeDescription'
      'From tblsales s  '
      'Left JOIN (tblsaleslines SL  '
      
        'INNER JOIN tblparts P on p.partsid = SL.productId and P.partname' +
        ' = '#39'Surcharge for Overdue'#39') on S.saleId = SL.SaleId  '
      'where s.IsInvoice ="T" and ifnull(balance,0) <> 0'
      'Order by Duedate Desc')
    OnCalcFields = qryMainCalcFields
    Active = True
