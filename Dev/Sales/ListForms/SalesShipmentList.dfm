  inherited qryMain: TERPQuery
    SQL.Strings = (
      
        'Select distinct 1 as details, saleId, ShipAddressId, shipAddress' +
        'name, null as uomQty , null as saleLineId, null as shipdate, nul' +
        'l as Productname, null as shipId, null as ShipmentStatus, isSale' +
        'sOrder , isinvoice, null as DetailDesc , null as ctn'
      
        'from tmp_salesshipment T where GroupShipmentsforSameAddress ="T"' +
        '  and (Select count(ID) from tmp_salesshipment t1 where t.saleId' +
        ' = t1.saleId and t.ShipAddressId = t1.shipaddressID) > 1'
      'union all '
      
        'Select distinct 2 as details, SaleId, ShipAddressId, shipAddress' +
        'name, uomQty as uomQty   ,saleLineId,  shipdate, Productname, sh' +
        'ipId, ShipmentStatus, isSalesOrder , isinvoice, DetailDesc , (Se' +
        'lect count(ID) from tmp_salesshipment t1 where t.saleId = t1.sal' +
        'eId and t.ShipAddressId = t1.shipaddressID) as cnt'
      'from tmp_salesshipment t'
      
        'where (Select count(ID) from tmp_salesshipment t1 where t.saleId' +
        ' = t1.saleId and t.ShipAddressId = t1.shipaddressID) >1 and Grou' +
        'pShipmentsforSameAddress ="T"  '
      'union '
      
        'Select distinct 3 as details, SaleId, ShipAddressId, shipAddress' +
        'name, uomQty as uomQty   ,saleLineId,  shipdate ,Productname, sh' +
        'ipId, ShipmentStatus, isSalesOrder , isinvoice, DetailDesc, (Sel' +
        'ect count(ID) from tmp_salesshipment t1 where t.saleId = t1.sale' +
        'Id and t.ShipAddressId = t1.shipaddressID) as cnt '
      'from tmp_salesshipment t'
      
        'where (Select count(ID) from tmp_salesshipment t1 where t.saleId' +
        ' = t1.saleId and t.ShipAddressId = t1.shipaddressID) =1 or Group' +
        'ShipmentsforSameAddress ="F"  '
      ''
      
        'order by SaleId, ShipAddressId, details, shipAddressname, Produc' +
        'tName')
    Active = True
