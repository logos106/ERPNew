  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'M.GlobalRef,'
      'M.ManifestID,'
      'M.ManifestID as ManifestNo ,'
      'M.DespatchDate,'
      'M.OwnerName,'
      'M.Driver,'
      'M.Phone,'
      'SUBSTRING(M.Notes,1,255) as Notes,'
      'M.VehicleName as Vehicle,'
      'M.Trailer,'
      'M.FromCity,'
      'M.FromState,'
      'M.FromPostcode,'
      'M.ToCity,'
      'M.ToState,'
      'M.ToPostcode,'
      'MR.RouteDesc as Route,'
      'MR.Distance as Distancekm,'
      'M.Completed,'
      'SL.ProductName,'
      'Left(SUBSTRING_INDEX(P.Productgroup,"^",1),255) as FirstColumn,'
      
        'Left(substring(substring_index(P.Productgroup,"^",2),char_length' +
        '(substring_index(P.Productgroup,"^",1))+1),255) as SecondColumn,'
      
        'Left(substring(substring_index(P.Productgroup,"^",2),char_length' +
        '(substring_index(P.Productgroup,"^",1))+2),255) as ThirdColumn '
      'from tblmanifest M'
      'inner join  tblmanifestlines ML on M.ManifestID = ML.ManifestID'
      
        'Left Join tblmanifestroutes MR ON MR.ManifestRouteID = M.Manifes' +
        'tRouteID'
      
        'inner join tblsaleslines SL on (ML.SaleLineID = SL.SaleLineID) o' +
        'r (ML.InvoiceLineID = SL.SaleLineID)'
      'inner join tblparts P on SL.ProductID = P.PARTSID'
      'Where M.DespatchDate Between  :DateFrom AND :DateTo'
      'Order By ManifestID Desc;')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end>
