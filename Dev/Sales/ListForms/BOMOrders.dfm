  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select'
      'Distinct '
      'S.saleID as SaleID,'
      'S.customername as  customername ,'
      'S.converted as converted,'
      'S.IsInternalOrder as IsInternalOrder'
      'from tblSales S'
      'inner join tblSaleslines SL on S.saleId = Sl.saleId '
      
        'inner join tblproctree PT on SL.saleLineID = PT.masterID and pt.' +
        'Mastertype <> '#39'mtProduct'#39' and ifnull(PT.parentID,0)=0'
      
        'Where S.isSalesorder ="T" and  S.SaleDate between :DateFrom and ' +
        ':DateTo'
      'Order by Saledate DEsc')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end>
    object qryMainSaleID: TIntegerField
      AutoGenerateValue = arAutoInc
      DisplayLabel = 'Sale #'
      DisplayWidth = 10
      FieldName = 'SaleID'
      Origin = 'S.SaleID'
    end
    object qryMaincustomername: TWideStringField
      DisplayLabel = 'Customer'
      DisplayWidth = 55
      FieldName = 'customername'
      Origin = 'S.CustomerName'
      Size = 255
    end
    object qryMainconverted: TWideStringField
      DisplayLabel = 'Converted?'
      DisplayWidth = 10
      FieldName = 'converted'
      Origin = 'S.Converted'
      FixedChar = True
      Size = 1
    end
    object qryMainIsInternalOrder: TWideStringField
      DisplayLabel = 'Is Internal~Order?'
      DisplayWidth = 10
      FieldName = 'IsInternalOrder'
      Origin = 'S.IsInternalOrder'
      FixedChar = True
      Size = 1
    end
  end
