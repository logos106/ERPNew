  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'S.SaleDate as "SaleDate",'
      's.ClassId as ClassId,'
      'S.TimeOfSale as TimeOfSale,'
      
        'If(S.IsCashSale="T" And S.IsPOS="F", "Cash Sale",If(S.IsRefund="' +
        'T"     , "Refund",If(S.ISSalesOrder="T" , "Sales Order",If(S.IsQ' +
        'uote="T"      , "Quote",If(S.IsCashSale="T" And S.IsPOS="T", if(' +
        ' S.IsLaybyPayment="T" ,if(  S.TotalAmountInc <0 , "Layby Reverse' +
        ' Transaction ","Layby Payment" ), "POS"),If(S.Islayby="T"    And' +
        ' S.IsPOS="T", "Layby",If(S.IsInvoice="T"    , "Invoice", "Unknow' +
        'n")))))))  as "TransactionType",'
      'S.CustomerName as `CustomerName` ,'
      'S.GlobalRef as `Globalref` ,'
      'S.OriginalNo as `OriginalRef`,'
      'S.InvoiceDocNumber as `InvoiceNumber` ,'
      'S.PONumber as `PONumber`,'
      'S.EmployeeName  AS `EmployeeName`,'
      'S.ConNote as `ConsignmentNote`,'
      'S.Class as `ClassName`,'
      'SL.PARTTYPE as `ProductType`,'
      'Left(SUBSTRING_INDEX(SL.ProductGroup,'#39'^'#39',1),255) as FirstColumn,'
      
        'Left(substring(substring_index(SL.ProductGroup,'#39'^'#39',2),char_lengt' +
        'h(substring_index(SL.ProductGroup,'#39'^'#39',1))+2),255) as SecondColum' +
        'n,'
      
        'Left(substring(substring_index(SL.ProductGroup,'#39'^'#39',3),char_lengt' +
        'h(substring_index(SL.ProductGroup,'#39'^'#39',2))+2),255) as ThirdColumn' +
        ','
      'SL.ProductName as `ProductName`,'
      'SL.Product_Description as `ProductDescription` ,'
      'SL.LinePrice as `LinePriceEx`,'
      'SL.LinePriceInc as `LinePriceInc`,'
      
        'If(SL.Shipped<>0.00,SL.TotalLineAmount,SL.BackOrder * SL.LinePri' +
        'ce) as `LineTotalEx`,'
      
        'If(SL.Shipped<>0.00,SL.TotalLineAmountInc,SL.BackOrder * SL.Line' +
        'PriceInc) as `LineTotalInc`,'
      'SL.LineTaxCode as `TaxCode`,'
      'SL.LineTax as `LineTax`,'
      'SL.LineCost as `LineCostEx`,'
      'SL.LineCostInc as `LineCostInc`,'
      'SL.UnitofMeasureQtySold as UnitofMeasureQtySold,'
      'SL.QtySold as QtySold,'
      'SL.UnitofMeasureShipped as UnitofMeasureShipped,'
      
        'if(SL.UnitofMeasureShipped<> 0.00 , 0.00 , SL.UnitofMeasureBacko' +
        'rder) UnitofMeasureBackorder,'
      'SL.UnitofMeasureSaleLines as UnitofMeasureSaleLines,'
      'SL.UnitofMeasureMultiplier as UnitofMeasureMultiplier,'
      
        'concat(SL.UnitofMeasureSaleLines," (" , SL.UnitofMeasureMultipli' +
        'er, ")") as UOM,'
      'SL.Shipped as Shipped,'
      'If(SL.Shipped<>0.00,0.00,SL.BackOrder) as BackOrder,'
      'SL.Attrib1Sale as Attrib1Sale,'
      'SL.Attrib2Sale as Attrib2Sale,'
      'SL.Attrib1SaleRate as Attrib1SaleRate,'
      'SL.Attrib1SaleRate* SL.Shipped as Attrib1SaleRateShipped,'
      'SL.Attrib1SaleRate* SL.QtySold as Attrib1SaleRateOrdered,'
      'SL.WarrantyEndsOn as WarrantyEndsOn,'
      'SL.WarrantyPeriod as WarrantyPeriod,'
      'SL.ShipDate as LineShipDate,'
      'S.Deleted as Deleted,'
      'S.EmployeeID as EmployeeID,'
      's.IsSalesOrder as IsSalesOrder,'
      'S.IsInvoice as IsInvoice,'
      'SL.SaleLineId as SaleLineId,'
      'replace(S.ShipTo, "\r\n" , ",") as ShipTo,'
      'replace(S.InvoiceTo, "\r\n" , ",") as InvoiceTo,'
      'S.Quotestatus as SalesCategory,'
      'C.ClientID as ClientID,'
      'S.SaleID as SaleID,'
      'S.SalesCategory as SalesCategory,'
      'C.CustFld1 as CustFld1,'
      'C.CustFld2 as CustFld2,'
      'C.CustFld3 as CustFld3,'
      'C.CustFld4 as CustFld4,'
      'C.CustFld5 as CustFld5,'
      'C.CustFld6 as CustFld6,'
      'C.CustFld7 as CustFld7,'
      'C.CustFld8 as CustFld8,'
      'C.CustFld9 as CustFld9,'
      'C.CustFld10 as CustFld10,'
      'C.CustFld11 as CustFld11,'
      'C.CustFld12 as CustFld12,'
      'C.CustFld13 as CustFld13,'
      'C.CustFld14 as CustFld14,'
      'C.CustFld15 as CustFld15,'
      'SL.SalesLinesCustField1 as SalesLinesCustField1,'
      'SL.SalesLinesCustField2 as SalesLinesCustField2,'
      'SL.SalesLinesCustField3 as SalesLinesCustField3,'
      'SL.SalesLinesCustField4 as SalesLinesCustField4,'
      'SL.SalesLinesCustField5 as SalesLinesCustField5,'
      'SL.SalesLinesCustField6 as SalesLinesCustField6,'
      'SL.SalesLinesCustField7 as SalesLinesCustField7,'
      'SL.SalesLinesCustField8 as SalesLinesCustField8,'
      'SL.SalesLinesCustField9 as SalesLinesCustField9,'
      'SL.SalesLinesCustField10 as SalesLinesCustField10,'
      'SL.ShipDate as ShipDate,'
      'SL.Discounts as Discounts,'
      'SL.DiscountPercent as DiscountPercent,'
      'SL.Markup as Markup,'
      'SL.MarkupPercent as MarkupPercent,'
      'SL.SmartOrderCreated as SmartOrderCreated,'
      'CT.TypeName as ClientType,'
      'null as BinLocation,'
      'null as BinNumber,'
      'null as ProductFld7,'
      '0.0 as TotalProductFld7'
      'FROM tblSalesLines SL'
      
        'INNER JOIN tblSales S ON  S.SaleID = SL.SaleID and ((S.IsRefund ' +
        '="T" OR S.IsCashSale="T" OR S.IsInvoice="T" OR (S.Converted<>"T"' +
        ' AND S.IsSalesOrder="T") OR S.IsPOS="T")'
      
        'AND S.IsQuote="F"  AND S.IsLayby="F" ) AND S.SaleDate Between '#39'1' +
        '8/07/2016'#39'  AND '#39'22/08/2016'#39
      'INNER JOIN tblClients C ON S.ClientID = C.ClientID'
      'LEFT JOIN tblclienttype CT on CT.ClientTypeID = C.ClientTypeID'
      
        'Where (ifnull(Qtysold,0)<>0 and ifnull(SL.Shipped,0)=0 and ifnul' +
        'l(S.BOID,"")="") or (S.IsSalesOrder="T")'
      'Order BY SL.SaleID Desc')
    Left = 64
