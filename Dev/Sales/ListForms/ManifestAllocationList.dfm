  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select * from tmp_ManifestAllocation'
      '/*'
      'SELECT S.clientID, '
      'S.GlobalRef as GlobalRef,'
      'S.SaleID as SaleID,'
      'InvoiceDocNumber as InvoiceNo, '
      'S.PickupFrom as "From",'
      'ShipTo as "To", '
      'S.IsSalesOrder as IsSalesOrder,'
      'ShipToAddress.City_sub as ToCity,'
      'ShipToAddress.state as ToState,'
      'ShipToAddress.PostCode as ToPostcode, '
      'AddToManifest as AddToManifest'
      'FROM tblSales S'
      
        'Left join tblLocations as ShipToAddress on  S.Shipto like concat' +
        '('#39'%'#39' , ifnull(ShipToAddress.City_sub,"") , '#39'%'#39') and  S.Shipto li' +
        'ke concat('#39'%'#39' , ifnull(ShipToAddress.State,"") , '#39'%'#39') and  S.Shi' +
        'pto like concat('#39'%'#39' , ifnull(ShipToAddress.Postcode,"") , '#39'%'#39') '
      'WHERE IsManifest="T" AND UsedOnManifest = "F"'
      'AND S.SaleDate BETWEEN :txtfrom AND :txtto'
      '*/')
    Left = 283
    Top = 378
