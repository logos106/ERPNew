  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'BOID, '
      'baseno,'
      'S.SaleDate as '#39'Sale Date'#39','
      'S.TimeOfSale as TimeOfSale,'
      'S.Payment as Payment, '
      'S.balance as balance,'
      'S.BOID as BOREF,'
      'Time(S.SaleDateTime) as TransTime,'
      'Date(S.SaleDateTime) as TransDate,'
      'S.Shipping as ShipVia,'
      'S.SalesCategory as SalesCategory,'
      
        'if(S.IsVoucher='#39'T'#39' and S.ISinvoice ="T" , '#39'Invoice Voucher'#39' , if' +
        '(S.ISVoucher = "T" , '#39'Voucher'#39' , If(S.IsCashSale='#39'T'#39' And S.IsPOS' +
        '='#39'F'#39' ,'#39'Cash Sale'#39', If(S.IsRefund='#39'T'#39','#39'Refund'#39',if(S.ISVoucher='#39'T'#39 +
        ' , '#39'Voucher'#39', If(S.IsInvoice='#39'T'#39','#39'Invoice'#39',If(S.IsCashSale='#39'T'#39' A' +
        'nd S.IsPOS='#39'T'#39',if(S.IsLaybyPayment='#39'T'#39' ,if( S.TotalAmountInc <0 ' +
        ', if(S.IsPOS='#39'F'#39' , '#39'Layby Reverse Transaction '#39' , '#39'POS Layby Rev' +
        'erse Transaction '#39') ,if(S.IsPOS='#39'F'#39' ,'#39'Layby Payment'#39' , '#39'POS Layb' +
        'y Payment'#39') ), '#39'POS'#39'),'#39'Unknown'#39'))))))) as Type,'
      ' SL.LaybyID ,'
      
        'CONCAT_WS(" ", If(InStr(C.Company,'#39'^'#39') -1 >0,Left(C.Company,InSt' +
        'r(C.Company,'#39'^'#39')-1),C.Company),IF(char_length(C.JobName)>0, Conc' +
        'at("(",C.JobName,")"),'#39#39')) as '#39'Customer Name'#39','
      'TypeName as CustomerType,'
      'C.JobName as JobName,'
      'C.Printname as Printname, '
      'C.JobNumber as JobNumber,'
      'S.GlobalRef as '#39'Global #'#39','
      'S.OriginalNo as '#39'Original #'#39','
      'S.ChequeNo as Refno,'
      'S.InvoiceDocNumber as '#39'Invoice Number'#39','
      'S.PONumber as '#39'PO Number'#39','
      'S.TotalAmount as '#39'Total Amount (Ex)'#39','
      'S.TotalTax as '#39'Total Tax'#39','
      'S.TotalAmountInc as '#39'Total Amount (Inc)'#39','
      'S.EmployeeName AS '#39'Employee Name'#39','
      'S.ConNote as '#39'Consignment Note'#39','
      'LEFT(Memo,255) as '#39'POS Memo'#39','
      'PickMemo as PickingMemo,'
      'S.Class as '#39'Class Name'#39','
      'S.Terms as Terms,'
      'S.HoldSale as Held,'
      'C.CardNumber as  CardNumber, '
      'S.QuoteGlobalRef as ConvertFromQuote,'
      'S.SalesOrderGlobalRef as ConvertFromSO,'
      'S.RepairGlobalRef as ConvertFromRepair,'
      'LEFT(S.Comments, 255) as Comments,'
      'S.Deleted as Deleted,'
      'S.EmployeeID as EmployeeID,'
      'C.ClientID as ClientID,'
      'S.SaleID as SaleID,'
      'ReplaceNewLine(S.ShipTo,"") as '#39'Ship To'#39','
      'S.ShipDate as '#39'Ship Date'#39','
      'S.Paid as '#39'Paid'#39','
      'S.DueDate as DueDate,'
      'C.CUSTFLD1 as CUSTFLD1,'
      'C.CUSTFLD2 as CUSTFLD2,'
      'C.CUSTFLD3 as CUSTFLD3,'
      'C.CUSTFLD4 as CUSTFLD4,'
      'C.CUSTFLD5 as CUSTFLD5,'
      'C.CUSTFLD6 as CUSTFLD6,'
      'C.CUSTFLD7 as CUSTFLD7,'
      'C.CUSTFLD8 as CUSTFLD8,'
      'C.CUSTFLD9 as CUSTFLD9,'
      'C.CUSTFLD10 as CUSTFLD10,'
      'C.CUSTFLD11 as CUSTFLD11,'
      'C.CUSTFLD12 as CUSTFLD12,'
      'C.CUSTFLD13 as CUSTFLD13,'
      'C.CUSTFLD14 as CUSTFLD14,'
      'C.CUSTFLD15 as CUSTFLD15, '
      'C.CUSTdate1 as CUSTdate1, '
      'C.CUSTdate2 as CUSTdate2, '
      'C.CUSTdate3 as CUSTdate3, '
      'S.Employeename as Rep, '
      'S.EnteredBy as EnteredBy, '
      'S.Class as class, '
      'S.POSPostCode,'
      'S.SaleConfirmed as SaleConfirmed,'
      'S.Medtype, '
      'S.QuoteStatus as SalesStatus,'
      'ReplaceNewLine(S.ShipTo,"") as ShipTo, '
      'S.PayMethod as PaymentMethod,'
      'RPC.Company as ParentRelatedCustomerName , '
      'SDD.PlannedDelFrom as PlannedDelFrom, '
      'SDD.PlannedDelto as PlannedDelto , '
      'SDD.PromisedDel as PromisedDel, '
      'SDD.CauseofDelay as CauseofDelay , '
      'SDD.ActualDel as ActualDel , '
      'SDD.FeedBack as DeliveryFeedBack,'
      'c.ClientNo as ClientAccountno'
      'FROM tblSales S'
      'INNER JOIN tblSalesLines SL on S.SaleId = SL.SaleId'
      'INNER JOIN tblClients C ON S.ClientID = C.ClientID'
      
        'LEFT JOIN tblClients RPC ON C.ParentRelatedClientID = RPC.Client' +
        'ID'
      'LEFT JOIN tblclienttype  CT ON CT.ClientTypeID = C.ClientTypeID'
      'LEFT JOIN tblsalesdeliverydetails SDD on SDD.SaleId = s.SAleID'
      'WHERE S.SaleDate Between :txtfrom AND :txtto AND'
      ' (:IgnoreClassfilter ="T" or S.ClassID = :ClassID ) AND'
      'S.IsLayby = '#39'F'#39' AND S.IsSalesOrder = '#39'F'#39' And S.IsQuote = '#39'F'#39' AND'
      
        '(S.IsPOS = '#39'T'#39' or S.IsRefund = '#39'T'#39' or S.IsCashSale = '#39'T'#39' or S.Is' +
        'Invoice = '#39'T'#39')'
      'AND SL.Invoiced = '#39'T'#39
      'and S.isinternalOrder ="F"'
      'Group By S.SaleID;')
    AutoCalcFields = False
    ParamData = <
      item
        DataType = ftString
        Name = 'txtfrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end
      item
        DataType = ftUnknown
        Name = 'IgnoreClassfilter'
      end
      item
        DataType = ftUnknown
        Name = 'ClassID'
      end>
