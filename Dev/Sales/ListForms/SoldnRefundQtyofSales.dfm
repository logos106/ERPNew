  object qryRefund: TERPQuery
    Connection = MyConnection1
    SQL.Strings = (
      'SELECT '
      '2 as DetailID, '
      'Sl.saleId as SaleId,'
      
        'concat(SL.UnitofMeasureSaleLines , '#39' ('#39' , SL.UnitofMeasureMultip' +
        'lier , '#39')'#39' ) as uom, '
      '0- Shipped as refundedqty,'
      'SL.UnitofMeasureMultiplier as UnitofMeasureMultiplier,'
      
        'if(S.IsCustomerReturn="T", "Customer Return" , "Refunded") as In' +
        'fo'
      'FROM tblSalesLines  SL '
      'inner join tblsales S on s.saleId = Sl.saleId'
      'WHERE RefundedSalesLineID = :SaleLineId  and S.Converted ="F"'
      'union all '
      'SELECT '
      '3 as DetailID, '
      'Sl.sAleId,'
      
        'concat(SL.UnitofMeasureSaleLines , '#39' ('#39' , SL.UnitofMeasureMultip' +
        'lier , '#39')'#39' ) as uom, '
      '0- Shipped as refundedqty,'
      'SL.UnitofMeasureMultiplier as UnitofMeasureMultiplier,'
      #39'POS Refund - EOP is not performed'#39' as Info'
      'FROM tblsaleslines_pos  SL '
      'WHERE RefundedSalesLineID = :SaleLineId '
      'order by  DetailID')
    FetchAll = False
    Active = True
    Left = 280
    Top = 409
