  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'Sl.saleId as SaleId,'
      'SL.SAleLineID as SAleLineID, '
      'SL.Productname  as Productname , '
      'SL.ProductID  as ProductID, '
      'SL.Product_Description as Product_Description, '
      'SL.LinePrice as LinePrice,'
      
        'concat(SL.UnitofMeasureSaleLines , '#39' ('#39' , SL.UnitofMeasureMultip' +
        'lier , '#39')'#39' ) as uom, '
      'SL.UnitofMeasureQtySold  as UnitofMeasureQtySold,'
      'SL.UnitofMeasureShipped as UnitofMeasureShipped,'
      'SL.UnitofMeasureBackorder as UnitofMeasureBackorder,'
      'SL.UnitOfMeasureID as UnitOfMeasureID,'
      'SL.shipped as shipped, '
      
        '(Select 0 - Sum(Shipped) refunded from tblsaleslines inner join ' +
        'tblsales using (SaleID) where RefundedSalesLineID= SL.saleLineID' +
        ' and converted ="F") as refundedqty,'
      'SL.UnitofMeasureMultiplier as UnitofMeasureMultiplier,'
      'S.clientID as ClientID'
      'FROM tblSalesLines  SL '
      'inner join tblsales S on s.saleId = SL.saleID'
      'WHERE SL.SaleID = :SaleID and SL.shipped >0 '
      'order by saleLineID')
    AfterOpen = qryMainAfterOpen
    OnCalcFields = qryMainCalcFields
    Active = True
    ParamData = <
      item
        DataType = ftInteger
        Name = 'SAleID'
        Value = 8017
      end>
