  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      
        '  1 as DetailType, SaleId, SaleDate, InvoiceDocNumber, CustomerN' +
        'ame, '
      #9'ProductName, QtySold, QtyShipped, QtyBackOrder,'
      #9'LinePrice, LinePriceInc, LinePriceForeign,'
      #9'LineTotal, LineTotalInc, CurrentLineTotal,'
      #9'ForeignExchangeRate, CurrentExchangeRate, ForeignExchangeCode,'
      
        #9'LineTotalForeign, LineTotalForeignHedged, LineTotalCurrencyVari' +
        'ation,'
      
        #9'IF(1,null,TotalAmount) as "TotalAmount", IF(1,null,TotalTax) as' +
        ' "TotalTax", IF(1,null,TotalAmountInc) as "TotalAmountInc",'
      
        #9'IF(1,null,LineTotalCurrencyVariation) as "TotalCurrencyVariatio' +
        'n"'
      #9'FROM tmp_ForeignSales'
      #9'UNION ALL'
      'SELECT'
      '  0, SaleId, SaleDate, InvoiceDocNumber, CustomerName, '
      #9'SPACE(30), null, null, null,'
      #9'null, null, null,'
      #9'null, null, null,'
      #9'null, null, null,'
      #9'null, null, null,'
      #9'TotalAmount, TotalTax, TotalAmountInc,'
      #9'SUM(LineTotalCurrencyVariation) '
      #9'FROM tmp_ForeignSales'
      #9'GROUP BY SaleId'
      #9'ORDER BY SaleId, DetailType'
      '')
