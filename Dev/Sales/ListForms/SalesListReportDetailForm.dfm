      'FROM tblSalesLines SL '
      
        'INNER JOIN tblSales S ON  S.SaleID = SL.SaleID and ((S.IsRefund ' +
        '='#39'T'#39' OR S.IsCashSale='#39'T'#39' OR S.IsInvoice='#39'T'#39' OR (S.Converted<>'#39'T'#39 +
