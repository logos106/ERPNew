  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select'
      'SL.LineTaxCode, '
      
        'if(ifnull(SA.ShipCountry,'#39#39')='#39#39', C.Country ,SA.ShipCountry ) as ' +
        'country, '
      
        'if(ifnull(SA.ShipState,'#39#39')='#39#39', C.State ,SA.ShipState ) as State,' +
        ' '
      'S.sALeId , '
      '"Invoice" as Saletype,'
      'S.SaleDate, '
      'sum(SL.TotalLineAmountInc) as totalAmountinc , '
      
        'Sum( if( SL.LineTax<>0, SL.TotalLineAmountInc , 0)) as Taxablesa' +
        'les, '
      
        'Sum( if (SL.LineTax=0, SL.TotalLineAmountInc , 0)) as nonTaxable' +
        'sales , '
      
        'Sum( if( SL.LineTax<>0 and Productname like '#39'%Freight%'#39', SL.Tota' +
        'lLineAmountInc , 0)) as FreightTaxablesales, '
      
        'Sum( if( SL.LineTax=0 and Productname like '#39'%Freight%'#39', SL.Total' +
        'LineAmountInc , 0)) as FreightnonTaxablesales, '
      
        'Sum( if( TC.description like '#39'%Exempt%'#39' , SL.TotalLineAmountInc ' +
        ', 0)) as ExcemptSales, '
      'Sum(SL.LineTax) as totalTax'
      'from tblsales S'
      'inner join tblsaleslines SL on S.SAleId = SL.saleId'
      'Left join tbltaxcodes TC on TC.Name = SL.LineTaxCode'
      'inner join tblclients C on S.ClientID = C.ClientID'
      
        'Left join tblshippingaddress SA on SA.ShipAddressID =  S.ShipToI' +
        'D'
      'Where S.SAleDate between :DateFrom and :DateTo '
      
        'and   (S.IsInvoice ="T" or S.ISCashSale ="T" or S.IsRefund ="T" ' +
        'or S.ISPOS ="T")'
      'and (:TaxCode="" or SL.LineTaxCode =:TaxCode)'
      'group by SaleDate , saleID , SL.LineTaxCode'
      'Order by LineTaxCode, State , SaleDate, saleID')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'TaxCode'
      end
      item
        DataType = ftUnknown
        Name = 'TaxCode'
      end>
