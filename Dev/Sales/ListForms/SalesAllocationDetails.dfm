  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'S.saleID, SL.saleLineID,'
      'S.saleDAte , '
      'SL.productname , '
      'SL.UnitofMeasureShipped , '
      
        'concat(SL.UnitofMeasureSaleLines , "(" , SL.UnitofMeasureMultipl' +
        'ier , ")") as uom, '
      '0 as Sequencedown, '
      'PQa.productname as Caption,'
      'TT.Description as Transtype,'
      'pqa.Alloctype , '
      'pqa.Active , '
      'PQA.Qty as PQaqty,'
      'PQABatch.value as Batchnumber , '
      'PQABatch.Qty as BatchQty, '
      'PBin.binlocation , '
      'PBin.binnumber, '
      'PQABins.Qty as binQty , '
      'PQASN.value as Serialnumber '
      'from tblsales S'
      'inner join tblsaleslines SL on S.saleId = SL.saleId '
      
        'inner join tblpqa pqa on pqa.translineId = SaleLineId and pqa.tr' +
        'anstype in ("TInvoiceLine","TSalesOrderline","TRefundSaleLine","' +
        'TPOSCashSaleline","TPOSLaybylines","TCashSaleLine")'
      'Left join tblpqatranstypes TT on TT.Transtype = pqa.TransType'
      'inner join tblparts P on P.partsId = PQA.productId '
      
        'left join `tblPqaDetails` PQABatch  on PQABatch.PQAId '#9'= PQA.PQA' +
        'ID and PQABatch.PQAType = "Batch"  and PQABatch.Active= "T"'
      
        'left join `tblPqaDetails` PQABins  on PQABins.PQAId '#9'= PQA.PQAID' +
        ' '
      'and PQABins.PQAType = "Bin" and PQABins.Active = "T" and  '
      
        '( (PQABatch.globalref = IFNULL(PQABins.ParentRef,"") and P.Batch' +
        '="T" and P.multiplebins = "T")  or (ifnull(PQABins.Parentref ,""' +
        ') = "" AND P.Batch="F" and P.multiplebins = "T"))'
      
        'Left join `tblProductBin` PBin     on PBin.binId '#9'= PQABins.BinI' +
        'D'
      
        'left join `tblPqaDetails` PQASN    on (PQASN.PQAId '#9'= PQA.PQAID ' +
        'and PQASN.PQAType = "SN"   and PQASN.Active = "T" and PQASN.Qty ' +
        '<> 0'
      
        'and ((PQABatch.globalref = IFNULL(PQASN.ParentRef,"") and P.Batc' +
        'h="T" and P.multiplebins = "F" and P.SNTracking = "T") or (PQAbi' +
        'ns.Globalref = IFNULL(PQASn.ParentRef,"") and P.Multiplebins = "' +
        'T" and P.SNTracking = "T")  or ((ifnull(PQASN.Parentref ,"") = "' +
        '") and P.Batch="F" and P.Multiplebins = "F" and P.SNTracking = "' +
        'T")))'
      
        'Where (sl.saleId = :SaleID or :SaleID =0) and (sl.SaleLineId = :' +
        'SaleLineId or :SaleLineId =0)'
      'union all '
      'Select '
      'S.saleID, SL.saleLineID,'
      'S.saleDAte , '
      'SL.productname , '
      'SL.UnitofMeasureShipped , '
      
        'concat(SL.UnitofMeasureSaleLines , "(" , SL.UnitofMeasureMultipl' +
        'ier , ")") as uom, '
      'PT.SequenceDown,'
      
        'Convert(TreeNodeCaption(PT.Level, PT.caption ) ,char(255)) as Ca' +
        'ption, '
      'TT.Description as Transtype,'
      'pqa.alloctype as alloctype,'
      'pqa.Active , '
      'PQA.Qty as PQaqty,'
      'PQABatch.value as Batchnumber , '
      'PQABatch.Qty as BatchQty, '
      'PBin.binlocation , '
      'PBin.binnumber, '
      'PQABins.Qty as binQty , '
      'PQASN.value as Serialnumber '
      'from tblsales S'
      'inner join tblsaleslines SL on S.saleId = SL.saleId '
      
        'inner join tblproctree PT on PT.masterId =SL.saleLineId and PT.m' +
        'astertype <> "mtProduct"'
      'inner join tblproctreePArt PTP on PTP.proctreeId = PT.ProcTreeId'
      
        'inner join tblpqa pqa on pqa.translineId = PTP.proctreepartID an' +
        'd pqa.transtype in ("TProcTreePart" , "TProcTreePartIN")'
      'Left join tblpqatranstypes TT on TT.Transtype = pqa.TransType'
      'inner join tblparts P on P.partsId = PQA.productId '
      
        'left join `tblPqaDetails` PQABatch  on PQABatch.PQAId '#9'= PQA.PQA' +
        'ID and PQABatch.PQAType = "Batch"  and PQABatch.Active= "T"'
      
        'left join `tblPqaDetails` PQABins  on PQABins.PQAId '#9'= PQA.PQAID' +
        ' '
      'and PQABins.PQAType = "Bin" and PQABins.Active = "T" and  '
      
        '( (PQABatch.globalref = IFNULL(PQABins.ParentRef,"") and P.Batch' +
        '="T" and P.multiplebins = "T")  or (ifnull(PQABins.Parentref ,""' +
        ') = "" AND P.Batch="F" and P.multiplebins = "T"))'
      
        'Left join `tblProductBin` PBin     on PBin.binId '#9'= PQABins.BinI' +
        'D'
      
        'left join `tblPqaDetails` PQASN    on (PQASN.PQAId '#9'= PQA.PQAID ' +
        'and PQASN.PQAType = "SN"   and PQASN.Active = "T" and PQASN.Qty ' +
        '<> 0'
      
        'and ((PQABatch.globalref = IFNULL(PQASN.ParentRef,"") and P.Batc' +
        'h="T" and P.multiplebins = "F" and P.SNTracking = "T") or (PQAbi' +
        'ns.Globalref = IFNULL(PQASn.ParentRef,"") and P.Multiplebins = "' +
        'T" and P.SNTracking = "T")  or ((ifnull(PQASN.Parentref ,"") = "' +
        '") and P.Batch="F" and P.Multiplebins = "F" and P.SNTracking = "' +
        'T")))'
      
        'Where (sl.saleId = :SaleID or :SaleID =0) and (sl.SaleLineId = :' +
        'SaleLineId or :SaleLineId =0)'
      'union all '
      'Select '
      'S.saleID, SL.saleLineID,'
      'S.saleDAte , '
      'SL.productname , '
      'SL.UnitofMeasureShipped , '
      
        'concat(SL.UnitofMeasureSaleLines , "(" , SL.UnitofMeasureMultipl' +
        'ier , ")") as uom, '
      'PT.SequenceDown,'
      
        'Convert(TreeNodeCaption(PT.Level, PT.caption ) ,char(255)) as Ca' +
        'ption, '
      'TT.Description as Transtype,'
      'pqa.Alloctype , '
      'pqa.Active , '
      'PQA.Qty as PQaqty,'
      'PQABatch.value as Batchnumber , '
      'PQABatch.Qty as BatchQty, '
      'PBin.binlocation , '
      'PBin.binnumber, '
      'PQABins.Qty as binQty , '
      'PQASN.value as Serialnumber '
      'from tblsales S'
      'inner join tblsaleslines SL on S.saleId = SL.saleId '
      
        'inner join tblproctree PT on PT.masterId =SL.saleLineId and PT.m' +
        'astertype <> "mtProduct"'
      
        'inner join tblprocprogress PPG on PT.proctreeId = PPG.ProcTreeId' +
        ' and PPG.Alloctype ="IN"'
      
        'inner join tblpqa pqa on pqa.translineId = PPG.ProcProgressID an' +
        'd pqa.transtype in ("TProcProgressIn")'
