  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'SO.SmartOrderID,'
      'SO.CreationDate,'
      'SO.SmartOrderDesc,'
      'SL.PARTSNAME,'
      'SL.SmartOrderLinesID , '
      'SL.ClassName,'
      'SL.Qty,'
      'SL.UnitofMeasure,'
      'PL.QtySold,'
      'Date(PL.receiveddate) as ReceivedDate,'
      
        'ifnull(PL.UnitofMeasureShipped,0)+ifnull((Select Sum(BOL.Shipped' +
        ') from tblpurchaselines BOL where  BOL.baseLineNo = PL.PurchaseL' +
        'ineId),0)  as Shipped,'
      
        'ifnull(PL.TotalLineAmount,0)+ifnull((Select Sum(BOL.TotalLineAmo' +
        'untinc) from tblpurchaselines BOL where  BOL.baseLineNo = PL.Pur' +
        'chaseLineId),0)  as TotalLineAmount,'
      
        'ifnull((Select Max(BOL.LineCost) from tblpurchaselines BOL where' +
        '  BOL.baseLineNo = PL.PurchaseLineId and ifnull(BOL.Shipped,0)=0' +
        '),SL.LinePrice)   as BOCost,'
      'PL.UnitofMeasurePOLines, '
      'PL.PurchaseOrderID,'
      'P.ETADate'
      'FROM tblsmartorder SO'
      
        'inner join tblsmartorderlines AS SL  on SO.SmartOrderId = SL.Sma' +
        'rtOrderID'
      
        'Left Join tblpurchaselines AS PL ON SL.SmartOrderID = PL.SmartOr' +
        'derID AND SL.SmartOrderLinesId = PL.SmartOrderLineID and ifnull(' +
        'baselineno ,0) =0'
      
        'left join tblpurchaseorders P on P.PurchaseOrderId = PL.Purchase' +
        'OrderID'
      'inner join tblsales on tblsales.GlobalRef = :SalesGlobalref'
      
        'inner join tblsaleslines on tblsales.SaleID = tblsaleslines.Sale' +
        'ID'
      'WHERE SO.SalesGlobalref = :SalesGlobalref '
      ''
      
        '/* Exclude Smart Orders that have been merged into a new Smart O' +
        'rder */'
      'and SL.SaleLineID = tblsaleslines.SaleLineID '
      
        'and ((SO.Done = "F") or (SO.Done = "T" and (select count(tblpurc' +
        'haselines.SmartOrderId) from tblpurchaselines where tblpurchasel' +
        'ines.SmartOrderId = SO.SmartOrderID) > 0))'
      ''
      '/* Also link to sales that have a link via AutoSmartOrderRef */'
      'or SL.SmartOrderLinesID in '
      '(  '
      'select tblsmartorderlines.SmartOrderLinesID '
      'from tblsales, tblsaleslines, tblsmartorder, tblsmartorderlines '
      'where tblsales.GlobalRef = :SalesGlobalref '
      'and tblsales.SaleId = tblsaleslines.SaleID'
      'and tblsales.AutoSmartOrderRef = tblsmartorder.GlobalRef'
      'and tblsmartorder.SmartOrderID = tblsmartorderlines.SmartOrderID'
      'and tblsmartorderlines.SaleLineID = tblsaleslines.SaleLineID'
      
        'and ((tblsmartorder.Done = "F") or (tblsmartorder.Done = "T" and' +
        ' (select count(tblpurchaselines.SmartOrderId) from tblpurchaseli' +
        'nes where tblpurchaselines.SmartOrderId = tblsmartorder.SmartOrd' +
        'erID) > 0))'
      ')'
      ''
      
        'group by SO.SmartOrderID , SL.SmartOrderLinesID , PL.PurchaseOrd' +
        'erID '
      'Order by SmartOrderID , PurchaseOrderID;'
      ''
      '/*'
      'SELECT'
      'SO.SmartOrderID,'
      'SO.CreationDate,'
      'SO.SmartOrderDesc,'
      'SL.PARTSNAME,'
      'SL.SmartOrderLinesID , '
      'SL.ClassName,'
      'SL.Qty,'
      'SL.UnitofMeasure,'
      'PL.QtySold,'
      'Date(PL.receiveddate) as ReceivedDate,'
      
        'ifnull(PL.UnitofMeasureShipped,0)+ifnull((Select Sum(BOL.Shipped' +
        ') from tblpurchaselines BOL where  BOL.baseLineNo = PL.PurchaseL' +
        'ineId),0)  as Shipped,'
      
        'ifnull(PL.TotalLineAmount,0)+ifnull((Select Sum(BOL.TotalLineAmo' +
        'untinc) from tblpurchaselines BOL where  BOL.baseLineNo = PL.Pur' +
        'chaseLineId),0)  as TotalLineAmount,'
      
        'ifnull((Select Max(BOL.LineCost) from tblpurchaselines BOL where' +
        '  BOL.baseLineNo = PL.PurchaseLineId and ifnull(BOL.Shipped,0)=0' +
        '),SL.LinePrice)   as BOCost,'
      'PL.UnitofMeasurePOLines, '
      'PL.PurchaseOrderID,'
      'P.ETADate'
      'FROM tblsmartorder SO'
      
        'inner join tblsmartorderlines AS SL  on SO.SmartOrderId = SL.Sma' +
        'rtOrderID'
      
        'Left Join tblpurchaselines AS PL ON SL.SmartOrderID = PL.SmartOr' +
        'derID AND SL.SmartOrderLinesId = PL.SmartOrderLineID and ifnull(' +
        'baselineno ,0) =0'
      
        'left join tblpurchaseorders P on P.PurchaseOrderId = PL.Purchase' +
        'OrderID'
      'WHERE SO.SalesGlobalref = :SalesGlobalref '
      
        'group by SO.SmartOrderID , SL.SmartOrderLinesID , PL.PurchaseOrd' +
        'erID '
      'Order by SmartOrderID , PurchaseOrderID'
      '*/')
