  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'S.SaleDate as '#39'Sale Date'#39','
      'S.DueDate as '#39'Followup Date'#39','
      'S.SalesCategory as SalesCategory,'
      'C.CUSTFLD1, C.CUSTFLD2, C.CUSTFLD3, C.CUSTFLD4, C.CUSTFLD5,'
      'C.CUSTFLD6, C.CUSTFLD7, C.CUSTFLD8, C.CUSTFLD9, C.CUSTFLD10,'
      'C.CUSTFLD11, C.CUSTFLD12, C.CUSTFLD13, C.CUSTFLD14, C.CUSTFLD15,'
      'C.CUSTDATE1, C.CUSTDATE2, C.CUSTDATE3,'
      'C.Printname as Printname, '
      'If(S.IsQuote='#39'T'#39','#39'Quote     '#39','#39'Unknown'#39') as '#39'Type'#39', '
      
        'CONCAT_WS(" ", If(InStr(C.Company,'#39'^'#39') -1 >0,Left(C.Company,InSt' +
        'r(C.Company,'#39'^'#39')-1),C.Company),IF(char_length(C.JobName)>0, Conc' +
        'at("(",C.JobName,")"),'#39#39')) as '#39'Customer Name'#39' ,'
      'CT.TypeName as '#39'CustomerType'#39','
      'S.GlobalRef as '#39'Global #'#39','
      'S.OriginalNo as '#39'Original #'#39','
      'S.InvoiceDocNumber as '#39'Invoice Number'#39','
      'S.PONumber as '#39'PO Number'#39','
      'S.TotalAmount as '#39'Total Amount (Ex)'#39','
      'S.TotalTax as '#39'Total Tax'#39','
      'S.TotalAmountInc as '#39'Total Amount (Inc)'#39' ,'
      'S.TotalQuoteAmount as '#39'Total Quote Amount'#39' ,'
      'S.EmployeeName AS '#39'Employee Name'#39','
      's.employeeId as EmployeeId,'
      'S.Class as '#39'Class Name'#39','
      'S.QuoteStatus,'
      'C.ClientID as ClientID,'
      'S.SaleID as SaleID,'
      'SUBSTRING(S.Comments,1,255)  as '#39'Comments'#39','
      'SUBSTRING(S.PickMemo,1,255)  as '#39'PickMemo'#39','
      'S.Deleted as Done,'
      'S.terms as Terms,'
      'S.Converted as Converted,'
      'S.Reference as Reference,'
      'S.InvoiceTo as InvoiceTo,'
      'S.Employeename as Rep, '
      'S.EnteredBy as EnteredBy, '
      'S.Class as class, '
      'S.ShipTo as ShipTo,'
      'RC.Mastercompany as FundingBody,'
      'RC.MasterClientID as FundingBodyId, '
      'if(ifnull(Variedto.TransRef,'#39#39')<>'#39#39' , '#39'T'#39' , '#39'F'#39') as IsVaried,'
      'FromVaried.PreviousREf as VariedFromRef,'
      'Variedto.TransRef as VariedtoRef'
      'FROM tblSales S'
      'inner join tblSaleslines Sl on S.SaleId = Sl.sAleid'
      'INNER JOIN tblclients C ON C.ClientID=S.ClientID'
      'LEFT JOIN tblclienttype Ct Using(ClientTypeID)'
      'lEFT JOIN tblrelatedclients RC ON RC.ChildclientId = S.ClientID'
      
        'Left join tblrelatedsales FromVaried on FromVaried.TransRef = S.' +
        'globalref'
      
        'Left join tblrelatedsales Variedto on Variedto.PreviousREf = S.g' +
        'lobalref'
      
        'WHERE S.IsQuote='#39'T'#39'  and S.IsRefund ='#39'F'#39' And S.IsCashSale='#39'F'#39' An' +
        'd S.IsInvoice='#39'F'#39' AND S.IsSalesOrder='#39'F'#39' and sL.invoiced = '#39'T'#39
      'and S.SaleDate Between :txtfrom AND :txtto'
      'GROUP BY S.SaleID'
      'ORDER BY S.SaleID DESC;')
    Left = 47
    ParamData = <
      item
        DataType = ftString
        Name = 'txtfrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end>
