  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      '1 as details , '
      'tblsalespayments.SaleId , '
      
        'group_concat(DISTINCT tblsalespayments.Paymethod) as Paymethod ,' +
        ' '
      'sum(tblsalespayments.Amount) as Salesamount , '
      
        'Sum(ifnull(tblbankdepositlines.Amount,ifnull(tblbankdepositlines' +
        'sales.amount,0))) as Deposited '
      
        'from tblsalespayments  Left  join  tblbankdepositlines  on tblsa' +
        'lespayments.PaymentId = tblbankdepositlines.TransPaymentID Left ' +
        'join tblbankdepositlinessales on tblsalespayments.PaymentId = tb' +
        'lbankdepositlinessales.PaymentID'
      'Where   ifnull(tblsalespayments.amount,0) <>0'
      'group by tblsalespayments.SaleId'
      'union all '
      'select '
      '2 as details , '
      'tblsalespayments.SaleId ,'
      'tblsalespayments.Paymethod, '
      'tblsalespayments.amount,  '
      
        'ifnull(tblbankdepositlines.Amount,ifnull(tblbankdepositlinessale' +
        's.amount,0)) as Deposited '
      
        'from tblsalespayments Left join  tblbankdepositlines   on tblsal' +
        'espayments.PaymentId = tblbankdepositlines.TransPaymentID Left j' +
        'oin tblbankdepositlinessales on tblsalespayments.PaymentId = tbl' +
        'bankdepositlinessales.PaymentID'
      'Where   ifnull(tblsalespayments.amount,0) <>0'
      'order by saleId, details')
    OnCalcFields = qryMainCalcFields
