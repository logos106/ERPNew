  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select distinct '
      '1 as Details, '
      'SD.ID as DispatchID, '
      'SD.Dispatchdate as Dispatchdate , '
      'employeeName as employeeName, '
      'Null as SaleID, '
      'NULL as ProductName, '
      'NULL as Batchno, '
      'NULL as UOM, '
      'NULL as Qty'
      
        'from tblsalesdispatch Sd inner join  tblsalesdispatchlines SDL o' +
        'n SD.Id = SDL.DispatchID'
      'Where SD.Dispatchdate between :DateFrom and :Dateto'
      'union all '
      'select '
      '2 as Details, '
      'SD.ID as DispatchID,  '
      'SD.Dispatchdate as Dispatchdate, '
      'SD.employeeName  as employeeName, '
      'SDL.SaleID as SaleID, '
      'SDL.ProductName as ProductName, '
      'SDL.Batchno as Batchno, '
      'concat(UOM.UnitName , '#39'('#39' , UOM.Multiplier , '#39')'#39') as UOM , '
      'Sum(SDL.UOMQty) as Qty '
      
        'from tblsalesdispatch Sd inner join  tblsalesdispatchlines SDL o' +
        'n SD.Id = SDL.DispatchID inner join tblunitsofmeasure UOM on SDL' +
        '.UOMID = UOM.UnitID '
      'Where SD.Dispatchdate between :DateFrom and :Dateto'
      'group by SD.ID, SaleID, ProductName, Batchno , uomid'
      'Order by DispatchID desc, Dispatchdate desc , Details')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'Dateto'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'Dateto'
      end>
