  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      '0 as DetailType,'
      'tblclients.ParentClientID as ParentClientID,'
      'pc.Company as Customer,'
      '0 as ClientID,'
      'null as JobCustomer,'
      'null as CustomerJobNumber,'
      'null as JobName,'
      'null as ProductName,'
      'null as QtyShipped,'
      'sum(tblsaleslines.Discounts) as TotalDiscount,'
      'sum(tblsaleslines.TotalLineAmount) as TotalAmountEx,'
      'sum(tblsaleslines.TotalLineAmountInc) as TotalAmountInc,'
      'sum(tblsaleslines.LineTax) as TotalTax '
      'from tblsales'
      'inner join tblclients on tblclients.ClientID = tblsales.ClientID'
      
        'inner join tblsaleslines on tblsaleslines.SaleID = tblsales.Sale' +
        'ID'
      
        'left join tblclients pc on pc.clientid = tblclients.ParentClient' +
        'ID'
      'where ((pc.ClientID = :ClientId) or (:ClientId = 0))'
      'and (tblsales.SaleDateTime between :DateFrom and :DateTo)'
      'group by pc.ClientID'
      'union all'
      'select '
      '1 as DetailType,'
      'tblclients.ParentClientID as ParentClientID,'
      'pc.Company as Customer,'
      'tblsales.ClientID as ClientID,'
      'tblsales.CustomerName as JobCustomer,'
      'tblclients.CustomerJobNumber as CustomerJobNumber,'
      'tblclients.JobName as JobName,'
      'tblsaleslines.ProductName as ProductName,'
      'sum(tblsaleslines.UnitOfMeasureShipped) as QtyShipped,'
      'sum(tblsaleslines.Discounts) as TotalDiscount,'
      'sum(tblsaleslines.TotalLineAmount) as TotalAmountEx,'
      'sum(tblsaleslines.TotalLineAmountInc) as TotalAmountInc,'
      'sum(tblsaleslines.LineTax) as TotalTax '
      'from tblsales'
      'inner join tblclients on tblclients.ClientID = tblsales.ClientID'
      
        'inner join tblsaleslines on tblsaleslines.SaleID = tblsales.Sale' +
        'ID'
      
        'left join tblclients pc on pc.clientid = tblclients.ParentClient' +
        'ID'
      'where ((pc.ClientID = :ClientId) or (:ClientId = 0))'
      'and (tblsales.SaleDateTime between :DateFrom and :DateTo)'
      'group by pc.ClientID, tblsaleslines.ProductID'
      'order by Customer, DetailType')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ClientId'
      end
      item
        DataType = ftUnknown
        Name = 'ClientId'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'ClientId'
