  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'tblSales.SaleDate as '#39'Sale Date'#39','
      'If(tblSales.IsCashSale='#39'T'#39' And tblSales.IsPOS='#39'F'#39' ,'#39'Cash Sale'#39','
      'If(tblSales.IsRefund='#39'T'#39','#39'Refund'#39','
      
        'If(tblSales.IsCashSale='#39'T'#39' And tblSales.IsPOS='#39'T'#39','#39'POS'#39',If(tblSa' +
        'les.IsInvoice='#39'T'#39','#39'Invoice'#39','#39'Unknown'#39')))) as Type,'
      
        'CONCAT_WS(" ", If(InStr(tblClients.Company,'#39'^'#39') -1 >0,Left(tblCl' +
        'ients.Company,InStr(tblClients.Company,'#39'^'#39')-1),tblClients.Compan' +
        'y),IF(char_length(tblClients.JobName)>0, Concat("(",tblClients.J' +
        'obName,")"),'#39#39')) as '#39'Customer Name'#39','
      'TypeName as CustomerType,'
      'JobName as JobName,'
      'JobNumber as JobNumber,'
      'tblSales.GlobalRef as '#39'Global #'#39','
      'tblSales.OriginalNo as '#39'Original #'#39','
      'tblSales.InvoiceDocNumber as '#39'Invoice Number'#39','
      'tblSales.PONumber as '#39'PO Number'#39','
      'tblSales.TotalAmount as '#39'Total Amount (Ex)'#39','
      'tblSales.TotalTax as '#39'Total Tax'#39','
      'tblSales.TotalAmountInc as '#39'Total Amount (Inc)'#39','
      'tblEmployees.EmployeeName AS '#39'Employee Name'#39','
      'tblSales.ConNote as '#39'Consignment Note'#39','
      'LEFT(Memo,255) as '#39'POS Memo'#39','
      'PickMemo as PickingMemo,'
      'tblSales.Class as '#39'Class Name'#39','
      'tblSales.Terms as Terms,'
      'tblSales.HoldSale as Held,'
      'tblSales.QuoteGlobalRef as ConvertFromQuote,'
      'tblSales.SalesOrderGlobalRef as ConvertFromSO,'
      'LEFT(tblSales.Comments, 255) as Comments,'
      'tblEmployees.EmployeeID as EmployeeID,'
      'tblClients.ClientID as ClientID,'
      'tblSales.SaleID as SaleID,'
      'tblSales.ShipDate as '#39'Ship Date'#39','
      'tblSales.Paid AS '#39'Paid'#39
      'FROM tblSales'
      'LEFT JOIN tblSalesLines Using(SaleID)'
      'INNER JOIN tblClients ON tblSales.ClientID = tblClients.ClientID'
      'LEFT JOIN tblclienttype Using(ClientTypeID)'
      
        'LEFT JOIN tblEmployees ON (tblSales.EmployeeID = tblEmployees.Em' +
        'ployeeID)'
      'WHERE (tblSales.SaleDate BETWEEN :txtfrom AND :txtto AND'
      'ISNULL(tblSalesLines.SaleID) AND'
      'tblSales.IsLayby = '#39'F'#39' AND'
      'tblSales.IsSalesOrder = '#39'F'#39' AND'
      'tblSales.IsQuote = '#39'F'#39' AND'
      
        '(tblSales.IsPOS = '#39'T'#39' or tblSales.IsRefund = '#39'T'#39' or tblSales.IsC' +
        'ashSale = '#39'T'#39' or tblSales.IsInvoice = '#39'T'#39'))'
      'GROUP BY tblSales.SaleID;'
      '')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'txtfrom'
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end>
