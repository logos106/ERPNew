  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'S.SaleDate as SaleDate,'
      'S.DueDate as FollowupDate,'
      'C.CUSTFLD1, C.CUSTFLD2, C.CUSTFLD3, C.CUSTFLD4, C.CUSTFLD5,'
      'C.CUSTFLD6, C.CUSTFLD7, C.CUSTFLD8, C.CUSTFLD9, C.CUSTFLD10,'
      'C.CUSTFLD11, C.CUSTFLD12, C.CUSTFLD13, C.CUSTFLD14, C.CUSTFLD15,'
      'C.CUSTDATE1, C.CUSTDATE2, C.CUSTDATE3,'
      'SUBSTRING(C.Notes,1,255) as ClientNotes,'
      'C.JobName as JobName,'
      'C.WarrantyFinishDate as WarrantyFinishDate,'
      'C.WarrantyPeriod as WarrantyPeriod,'
      'C.HoursTakenForJob as HoursTakenForJob,'
      'C.RepName as JobRep,'
      'C.JobNumber as JobNumber,'
      'C.JobTitle as JobTitle,'
      'C.SerialNumber as SerialNumber,'
      'C.ModelNumber as ModelNumber,'
      'C.Manufacture as Manufacture,'
      'C.CustomerJobNumber as CustomerJobNumber,'
      'C.Hours as Hours,'
      'C.InsuranceCompanyName as InsuranceCompanyName,'
      'C.ClaimNumber as ClaimNumber,'
      'C.StormLocation as StormLocation,'
      'C.StormDate as StormDate,'
      'C.Year as Year,'
      'C.Colour as Colour,'
      'C.BodyType as BodyType,'
      'C.AssessorsName as AssessorsName,'
      'C.GlobalRef as ClientGlobalRef,'
      'If(S.IsQuote='#39'T'#39','#39'Quote     '#39','#39'Unknown'#39') as '#39'Type'#39', '
      
        'CONCAT_WS(" ", If(InStr(C.Company,'#39'^'#39') -1 >0,Left(C.Company,InSt' +
        'r(C.Company,'#39'^'#39')-1),C.Company),IF(char_length(C.JobName)>0, Conc' +
        'at("(",C.JobName,")"),'#39#39')) as '#39'Customer Name'#39' ,'
      'CT.TypeName as '#39'CustomerType'#39','
      'C.JobRegistration as Registration,'
      'C.BailmentNumber as BailmentNumber,'
      'C.BailmentAmountEx as BailmentAmountEx,'
      'C.StockReceivedDate as StockReceivedDate,'
      'C.NewOrUsed as NewOrUsed,'
      'S.GlobalRef as SaleGlobalRef,'
      'S.OriginalNo as '#39'OriginalNo'#39','
      'S.InvoiceDocNumber as '#39'InvoiceNumber'#39','
      'S.PONumber as '#39'PONumber'#39','
      'S.TotalAmount as '#39'TotalAmountEx'#39','
      'S.TotalTax as '#39'TotalTax'#39','
      'S.TotalAmountInc as '#39'TotalAmountInc'#39' ,'
      'S.TotalQuoteAmount as '#39'TotalQuoteAmount'#39' ,'
      'S.EmployeeName AS '#39'EmployeeName'#39','
      'S.Class as '#39'ClassName'#39','
      'S.QuoteStatus,'
      'C.ClientID as ClientID,'
      'S.SaleID as SaleID,'
      'SUBSTRING(S.Comments,1,255)  as '#39'SaleComments'#39','
      'S.Deleted as Done,'
      'S.Converted as Converted,'
      'S.Reference as Reference,'
      'S.InvoiceTo as InvoiceTo,'
      'S.Employeename as SaleRep, '
      'S.EnteredBy as EnteredBy, '
      'S.ShipTo as ShipTo,'
      'C.ExcessAmount as ExcessAmount'
      'FROM tblSales S, tblClients C'
      'LEFT JOIN tblclienttype Ct Using(ClientTypeID)'
      'WHERE S.ClientID = C.ClientID '
      'and S.IsQuote= "T" and C.IsJob = "T"'
      'and S.SaleDate Between :txtfrom AND :txtto'
      'GROUP BY S.SaleID'
      'ORDER BY S.SaleID DESC;')
