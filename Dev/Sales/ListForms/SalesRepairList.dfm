  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'R.RepairID AS RepairID,'
      'R.GlobalRef AS GlobalRef,'
      'R.CreationDate AS CreationDate,'
      'R.Done AS Done,'
      'R.CustomerDetails AS CustomerDetails,'
      'R.JobDueDate AS JobDueDate,'
      'R.Converted AS Converted,'
      'R.Status AS Status,'
      'R.SOGlobalRef AS SOGlobalRef,'
      'R.QuoteGlobalRef AS QuoteGlobalRef,'
      'if(R.RepairID = ifnull(S.repairId,0) , '
      
        'concat('#39'Repair from '#39' , If(S.IsCashSale="T" And S.IsPOS="F", "Ca' +
        'sh Sale",If(S.IsRefund="T", "Refund",If(S.ISSalesOrder="T", "Sal' +
        'es Order",If(S.IsQuote="T", "Quote",If(S.IsCashSale="T" And S.Is' +
        'POS="T", "POS",If(S.Islayby="T" And IsPOS="T", "Layby",If(S.IsIn' +
        'voice="T" , "Invoice","Unknown"))))))) ) , '
      
        'concat('#39'Repair to '#39' , If(S.IsCashSale="T" And S.IsPOS="F", "Cash' +
        ' Sale",If(S.IsRefund="T", "Refund",If(S.ISSalesOrder="T", "Sales' +
        ' Order",If(S.IsQuote="T", "Quote",If(S.IsCashSale="T" And S.IsPO' +
        'S="T", "POS",If(S.Islayby="T" And IsPOS="T", "Layby",If(S.IsInvo' +
        'ice="T" , "Invoice","Unknown"))))))) ))   description '
      'from tblsales  S'
      
        'Left join tblRepairs R on R.RepairID = ifnull(S.repairId,0) or R' +
        '.globalref = ifnull( repairglobalref   , '#39#39')'
      'Where S.SaleId = :SaleID')
    ParamData = <
      item
        DataType = ftInteger
        Name = 'SaleID'
        Value = 7
      end>
