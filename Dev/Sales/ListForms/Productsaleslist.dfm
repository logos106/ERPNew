  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'SL.saleLineID, '
      'S.SaleId, '
      'S.CustomerName , '
      'S.ContactName,'
      
        'if(s.isinvoice="T" , "Invoice" ,if(S.issalesorder="T", "SalesOrd' +
        'er" ,if(S.ISCashSale="T" , "Cash Sale" , "POS"))) as salestype ,' +
        ' '
      'SL.productname, '
      'P.ProductPrintName as ProductPrintName,'
      'SL.UnitofMeasureQtySold , '
      'SL.UnitofMeasureShipped , '
      'SL.QtySold , '
      'SL.Shipped , '
      'S.sAledatetime as SaleDate,'
      
        'concat(SL.UnitofMeasureSaleLines ,  '#39' ('#39' , SL.UnitofMeasureMulti' +
        'plier , '#39')'#39') as UOM'
      'from tblsales S '
      'inner join tblsaleslines SL using (SaleID) '
      'inner join tblparts P on P.partsId = Sl.productId'
      
        'where (S.IsInvoice ="T" or S.issalesorder="T" or S.IsCashSAle="T' +
        '" or S.ISPOS ="T") and SL.Invoiced ="T" and S.Deleted="F" and Ca' +
        'ncelled ="F" '
      'and (S.clientID =:ClientID or :ClientID=0)'
      'and (SL.ProductID =:ProductID or :ProductID=0)'
      'and (S.ClassID =:ClassID or :ClassID=0)'
      'and S.sAledatetime between :DateFrom and :DateTo'
      'order by SaleId, productName')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ClientID'
      end
      item
        DataType = ftUnknown
        Name = 'ClientID'
      end
      item
        DataType = ftUnknown
        Name = 'ProductID'
      end
      item
        DataType = ftUnknown
        Name = 'ProductID'
      end
      item
        DataType = ftUnknown
        Name = 'ClassID'
      end
      item
        DataType = ftUnknown
        Name = 'ClassID'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end>
