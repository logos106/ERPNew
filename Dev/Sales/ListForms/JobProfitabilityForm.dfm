  inherited qryMain: TERPQuery
    SQL.Strings = (
      '# THIS IS A DUMMY SQL - SEE RefreshQuery()'
      ''
      'SELECT '
      
        'If(InStr(Company,'#39'^'#39') -1 >0,Left(Company,InStr(Company,'#39'^'#39')-1),C' +
        'ompany) AS'
      'CompanyName, JobName, JobNumber, s.SaleID,'
      
        'If(s.IsCashSale='#39'T'#39' And s.IsPOS='#39'F'#39', '#39'Cash Sale                 ' +
        '            '#39','
      'If(s.IsRefund='#39'T'#39', '#39'Refund                                '#39','
      
        'If(s.IsCashSale='#39'T'#39' And s.IsPOS='#39'T'#39', '#39'POS                       ' +
        '            '#39','
      
        'If(s.IsInvoice='#39'T'#39' OR s.IsQuote='#39'T'#39', '#39'Invoice                   ' +
        '            '#39', '
      
        #39'Unknown                               '#39')))) as '#39'Transaction Typ' +
        'e'#39','
      's.ClientID,'
      'InvoiceDocNumber as TransactionNo,'
      'Sum(sl.LineCost * Shipped) as '#39'CostEx'#39','
      
        'IF(s.IsInvoice='#39'T'#39' AND sl.IsQuote='#39'F'#39', Sum(sl.TotalLineAmount), ' +
        'Sum(0.00)) as '#39'IncomeEx'#39','
      
        'IF(s.IsQuote='#39'T'#39' AND sl.IsQuote='#39'T'#39', Sum(sl.TotalLineAmount), Su' +
        'm(0.00)) as '#39'QuotedEx'#39','
      
        'IF((s.IsInvoice='#39'T'#39' OR s.IsQuote='#39'T'#39') AND sl.IsQuote='#39'F'#39', Sum(sl' +
        '.TotalLineAmount)-Sum(sl.LineCost*Shipped), Sum(0.00)) as '#39'Amoun' +
        't Diff (Income-Cost)'#39','
      
        'IF((s.IsInvoice='#39'T'#39' OR s.IsQuote='#39'T'#39') AND sl.IsQuote='#39'F'#39', ((Sum(' +
        'sl.TotalLineAmount)-Sum(sl.LineCost*Shipped)) / Sum(sl.LineCost*' +
        'Shipped))*100, Sum(0.00)) as '#39'PercentDiffIncomeCost'#39','
      
        'IF((s.IsInvoice='#39'T'#39' OR s.IsQuote='#39'T'#39') AND sl.IsQuote='#39'T'#39', Sum(sl' +
        '.TotalLineAmount)-Sum(sl.LineCost*Shipped), Sum(0.00)) as '#39'Amoun' +
        't Diff (Quote-Cost)'#39','
      
        'IF((s.IsInvoice='#39'T'#39' OR s.IsQuote='#39'T'#39') AND sl.IsQuote='#39'T'#39', ((Sum(' +
        'sl.TotalLineAmount)-Sum(sl.LineCost*Shipped)) / Sum(sl.LineCost*' +
        'Shipped))*100, Sum(0.00)) as '#39'PercentDiffQuoteCost'#39','
      
        'Sum(IF(sl.Invoiced = "F", sl.QtySold, 0.00)) As "Backorders", 0.' +
        '00 AS "ProfitPercent",'
      'sl.ProductName,'
      
        'LEFT(SUBSTRING_INDEX( Concat_WS("^(",tp.PRODUCTGROUP,"") ,"^",1)' +
        ',255) AS FirstColumn,'
      
        'LEFT(MID(SUBSTRING_INDEX( Concat_WS("^",tp.PRODUCTGROUP,"") ,"^"' +
        ',2),char_length(SUBSTRING_INDEX( Concat_WS("^",tp.PRODUCTGROUP,"' +
        '") ,"^",1))+2,char_length(SUBSTRING_INDEX(Concat_WS("^",tp.PRODU' +
        'CTGROUP,"") ,"^",2))-char_length(SUBSTRING_INDEX( Concat_WS("^",' +
        'tp.PRODUCTGROUP,"") ,"^",1))),255) AS SecondColumn,'
      
        'LEFT(MID(SUBSTRING_INDEX( Concat_WS("^",tp.PRODUCTGROUP,"") ,"^"' +
        ',3),char_length(SUBSTRING_INDEX( Concat_WS("^",tp.PRODUCTGROUP,"' +
        '") ,"^",2))+2,char_length(SUBSTRING_INDEX(Concat_WS("^",tp.PRODU' +
        'CTGROUP,"") ,"^",3))-char_length(SUBSTRING_INDEX( Concat_WS("^",' +
        'tp.PRODUCTGROUP,"") ,"^",2))),255) AS ThirdColumn, '
      'tp.Area AS "Area"'
      'FROM tblSales s'
      'INNER JOIN tblSalesLines sl on (s.SaleID = sl.SaleID)'
      'INNER JOIN tblparts tp on (tp.PartsID = sl.ProductID)'
      'LEFT JOIN tblclients on tblclients.ClientID = s.ClientID'
      
        'WHERE (IsRefund='#39'T'#39' or IsInvoice='#39'T'#39' or IsCashSale='#39'T'#39' or s.IsQu' +
        'ote='#39'T'#39') AND ((s.Converted='#39'T'#39' AND s.Deleted='#39'T'#39') OR s.Deleted <' +
        '> '#39'T'#39')'
      'GROUP By s.ClientID, s.IsInvoice, s.IsQuote'
      ''
      'UNION ALL'
      ''
      'SELECT'
      
        'If(InStr(Company,'#39'^'#39') -1 >0,Left(Company,InStr(Company,'#39'^'#39')-1),C' +
        'ompany) AS CompanyName,'
      'JobName, JobNumber, P.PurchaseOrderID,'
      
        'If(IsPO='#39'T'#39','#39'Purchase Order'#39',If(IsBill='#39'T'#39','#39'Bill'#39',If(IsCredit='#39'T' +
        #39','#39'Credit'#39','#39'Unknown'#39'))) as '#39'TransactionType'#39','
      'PL.CustomerJobID, PurchaseOrderNumber as InvoiceNo,'
      'Sum(TotalLineAmount) as '#39'CostEx'#39','
      '0.00 as '#39'IncomeEx'#39', 0.00 as '#39'QuotedEx'#39','
      'Sum(TotalLineAmount)-Sum(0.00) as '#39'AmountDiffIncomeCost'#39','
      
        '(Sum(TotalLineAmount)-Sum(0.00))/Sum(0.00)*100 as '#39'Percent Diff ' +
        '(Income/Cost)'#39','
      
        '0.00 as '#39'AmountDiffQuoteCost'#39', 0.00 as '#39'Percent Diff (Quote/Cost' +
        ')'#39','
      'Sum(PL.Backorder) AS "Backorders", 0.00 AS "ProfitPercent",'
      'PL.ProductName,'
      
        'LEFT(SUBSTRING_INDEX( Concat_WS("^(",tp.PRODUCTGROUP,"") ,"^",1)' +
        ',255) AS FirstColumn,'
      
        'LEFT(MID(SUBSTRING_INDEX( Concat_WS("^",tp.PRODUCTGROUP,"") ,"^"' +
        ',2),char_length(SUBSTRING_INDEX( Concat_WS("^",tp.PRODUCTGROUP,"' +
        '") ,"^",1))+2,char_length(SUBSTRING_INDEX(Concat_WS("^",tp.PRODU' +
        'CTGROUP,"") ,"^",2))-char_length(SUBSTRING_INDEX( Concat_WS("^",' +
        'tp.PRODUCTGROUP,"") ,"^",1))),255) AS SecondColumn,'
      
        'LEFT(MID(SUBSTRING_INDEX( Concat_WS("^",tp.PRODUCTGROUP,"") ,"^"' +
        ',3),char_length(SUBSTRING_INDEX( Concat_WS("^",tp.PRODUCTGROUP,"' +
        '") ,"^",2))+2,char_length(SUBSTRING_INDEX(Concat_WS("^",tp.PRODU' +
        'CTGROUP,"") ,"^",3))-char_length(SUBSTRING_INDEX( Concat_WS("^",' +
        'tp.PRODUCTGROUP,"") ,"^",2))),255) AS ThirdColumn, '
      'tp.Area AS "Area"'
      ''
      'FROM tblpurchaseorders P'
      
        'INNER JOIN tblpurchaselines PL on P.PurchaseOrderID = PL.Purchas' +
        'eOrderID'
      'INNER JOIN tblparts tp on (tp.PartsID = PL.ProductID)'
      'LEFT JOIN tblclients on tblclients.ClientID = PL.CustomerJobID'
      
        'WHERE (IsPO='#39'T'#39' or IsBill='#39'T'#39' or IsCredit='#39'T'#39') AND P.Deleted <> ' +
        #39'T'#39' And UseTimeCost <> '#39'T'#39
      'GROUP BY P.PurchaseOrderID'
      ''
      'UNION ALL'
      ''
      'SELECT'
      
        'If(InStr(Company,'#39'^'#39') -1 >0,Left(Company,InStr(Company,'#39'^'#39')-1),C' +
        'ompany) AS CompanyName,'
      'JobName, JobNumber, P.PayID, '#39'Payroll'#39' AS '#39'TransactionType'#39','
      'PL.ClientID, 0 AS InvoiceNo, Sum(LineTotal) AS '#39'CostEx'#39','
      '0.00 AS '#39'IncomeEx'#39', 0.00 as '#39'QuotedEx'#39','
      'Sum(LineTotal) - Sum(0.00) AS '#39'AmountDiffIncomeCost'#39','
      
        '(Sum(LineTotal) - Sum(0.00)) / Sum(0.00) * 100 AS '#39'Percent Diff ' +
        '(Income/Cost)'#39','
      
        '0.00 as '#39'AmountDiffQuoteCost'#39', 0.00 as '#39'Percent Diff (Quote/Cost' +
        ')'#39','
      '0.00 AS "Backorders", 0.00 AS "ProfitPercent",'
      '"" as ProductName,'
      '"" as FirstColumn,'
      '"" as SecondColumn,'
      '"" as ThirdColumn,'
      '"" as Area'
      'FROM tblpays P'
      
        'INNER JOIN tblpayspayrates PL on (P.PayID = PL.PayID) AND (NOT I' +
        'SNULL(PL.ClientID) AND PL.ClientID <> '#39#39')'
      'INNER JOIN tblclients C on C.ClientID = PL.ClientID'
      'WHERE (Paid = '#39'T'#39')'
      'GROUP BY C.ClientID'
      ''
      'UNION ALL'
      ''
      'SELECT'
      
        'If(InStr(Company,'#39'^'#39') -1 >0, Left(Company,InStr(Company,'#39'^'#39')-1),' +
        'Company) AS CompanyName,'
      
        'JobName, JobNumber, P.EmployeeID, '#39'Timesheet'#39' AS '#39'Transaction Ty' +
        'pe'#39','
      'P.JobID, 0 AS InvoiceNo, Sum(Total) AS '#39'CostEx'#39', '
      '0.00 AS '#39'IncomeEx'#39', 0.00 as '#39'QuotedEx'#39','
      'Sum(Total) - Sum(0.00) AS '#39'AmountDiffIncomeCost'#39','
      
        '(Sum(Total) - Sum(0.00)) / Sum(0.00) * 100 AS '#39'Percent Diff (Inc' +
        'ome/Cost)'#39','
      
        '0.00 as '#39'AmountDiffQuoteCost'#39', 0.00 as '#39'Percent Diff (Quote/Cost' +
        ')'#39','
      '0.00 AS "Backorders", 0.00 AS "ProfitPercent",'
      '"" as ProductName,'
      '"" as FirstColumn,'
      '"" as SecondColumn,'
      '"" as ThirdColumn,'
      '"" as Area'
      'FROM tbltimesheets P'
      'INNER JOIN tblclients C on C.ClientID = P.JobID'
      'INNER JOIN tblPays PH on PH.EmployeeID = P.EmployeeID'
      
        'WHERE (P.Active = '#39'T'#39' OR (P.Active = '#39'F'#39' AND PH.IsTimesheet = '#39'T' +
        #39' AND PH.Paid = '#39'F'#39'))'
      'GROUP BY P.JobID;')
    Options.StrictUpdate = False
