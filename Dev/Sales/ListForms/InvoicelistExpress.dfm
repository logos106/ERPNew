  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select Distinct'
      'S.clientId as ClientId,'
      'S.InvoiceDocNumber as SaleNo,  '
      'S.SaleId as SaleId, '
      'S.BOID as BORef, '
      'S.Customername as CustomerName, '
      'S.ClientPrintName as PrintName,'
      'S.totalAmountinc As TotalAmountInc, '
      'S.totalAmount As TotalAmount, '
      'S.Payment As Payment, '
      'S.TotalTax As TotalTax,'
      'S.balance  as Balance, '
      'S.SaleDate as SaleDate, '
      'Date(S.SaleDateTime) as TransDate,'
      'Time(S.SaleDateTime) as TransTime,'
      'S.Converted as Converted,'
      'S.futureSo as FutureSo,'
      'S.Deleted as Done,'
      'S.Class as ClassName, '
      'S.dueDate as dueDate,'
      'S.employeename as employeename, '
      
        'If(S.IsVoucher='#39'T'#39' and isinvoice="T", "Invoice Voucher" , if(S.I' +
        'sVoucher='#39'T'#39' ,'#39'Voucher'#39' , If(S.IsCashSale='#39'T'#39' And S.IsPOS='#39'F'#39' ,'#39 +
        'Cash Sale'#39', If(S.IsRefund='#39'T'#39','#39'Refund'#39',If(S.IsInvoice='#39'T'#39','#39'Invoi' +
        'ce'#39',If(S.IsCashSale='#39'T'#39' And S.IsPOS='#39'T'#39',if(S.IsLaybyPayment='#39'T'#39' ' +
        ',if( S.TotalAmountInc <0 , '#39'Layby Reverse Transaction '#39','#39'Layby P' +
        'ayment'#39' ), '#39'POS'#39'),'#39'Unknown'#39')))))) as Type,'
      'S.QuoteStatus as Status, '
      'S.ShipDate as ShipDate, '
      'S.HoldSale as Held,'
      'S.PONumber, '
      'S.Terms,'
      'S.Comments,'
      ''
      'If(IsNull(ClientSignature),"F","T") as HasSignature'
      'From tblSales S'
      'inner join tblsaleslines SL on S.saleID = SL.SaleId'
      'Where '
      '(S.clientID= :CustomerId or :CustomerId=0) '
      'and SaleDate between :DateFrom and :Dateto '
      'AND  SL.Invoiced = '#39'T'#39' '
      'and S.isinternalOrder ="F"'
      'AND (:IgnoreClassfilter ="T" or S.ClassID = :ClassID )'
      
        'ANd ((:Listtype = '#39'Invoice'#39' '#9'and (S.IsPOS = '#39'T'#39' or S.IsRefund = ' +
        #39'T'#39' or S.IsCashSale = '#39'T'#39' or S.IsInvoice = '#39'T'#39') and S.islayby ='#39 +
        'F'#39')  OR'
      
        #9'(:Listtype = '#39'Salesorder'#39' '#9'and S.IsSalesOrder= '#39'T'#39' AND (S.IsCas' +
        'hSale='#39'F'#39' OR S.IsLayByTOS= '#39'T'#39') ) Or '
      #9'(:Listtype = '#39'Quote'#39' '#9#9'and  S.IsQuote='#39'T'#39') )'
      'Order by SaleID Desc')
