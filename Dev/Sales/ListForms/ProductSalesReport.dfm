  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      's.SaleId as SaleID, '
      'SL.saleLineId as SaleLineID,'
      'S.InvoiceDocNumber DocNumber , '
      'S.shipdate as Shipdate,'
      'S.customername as customername , '
      'ifnull(SA.CompanyName, S.customername) as ShiptoName , '
      'ifnull(SA.ShipAddress, C.street) as ShipToAddress1,'
      'ifnull(SA.ShipAddress1, C.street2) as ShipToAddress2,'
      'ifnull(SA.ShipAddress2, C.street3) as ShipToAddress3,'
      'ifnull(SA.ShipCity, C.Suburb) as ShipToCity ,'
      'ifnull(SA.ShipState, C.state) as ShipToState,'
      
        'If(S.IsCashSale="T" And S.IsPOS="F", "Cash Sale",If(S.IsRefund="' +
        'T","Refund",If(S.IsCashSale="T" And S.IsPOS="T","POS",If(S.IsInv' +
        'oice="T","Invoice","Unknown")))) as TransactionType,'
      'ifnull(SA.ShipPostCode, C.Postcode) as ShipToPostCode, '
      'ifnull(SA.email,c.Email) as Shipttoemail,'
      'ifnull(SA.Phone,C.Phone) as Shiptophone, '
      'S.Quotestatus as Salestatus,'
      'space(100) as Rushorder,'
      'SL.productName as Productname,'
      'SL.partbarcode as Partbarcode, '
      
        'concat(SL.UnitofMeasureSaleLines ,'#39'/'#39' , SL.UnitofMeasureMultipli' +
        'er) as UOM,'
      'SL.UnitofMeasureShipped as Qtyshipped,'
      'SL.UnitofMeasureQtySold as QtySold,'
      'Space(100) as SpecialInstruction,'
      'S.comments as comments'
      'FROM tblSales S '
      'INNER JOIN tblSalesLines SL ON  S.SaleID = SL.SaleID '
      'inner join tblclients c on S.clientId = c.clientID'
      'left join tblshippingaddress SA on SA.ShipAddressID = S.ShiptoID'
      
        'WHERE (S.IsRefund ="T" OR S.IsCashSale="T" OR S.IsInvoice="T" OR' +
        ' S.IsPOS="T") AND S.IsQuote="F" AND S.IsLayby="F" '
      'and  S.shipdate  between :Datefrom and :DateTo'
      'order by S.SaleId desc , SL.sortId')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Datefrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end>
