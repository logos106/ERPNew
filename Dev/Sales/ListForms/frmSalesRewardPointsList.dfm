  inherited qryMain: TERPQuery
    SQL.Strings = (
      
        'SELECT S.SaleId, S.SaleDate, S.CustomerName, S.InvoiceDocNumber,' +
        ' S.Paid, S.EmployeeName,'
      'SL.ProductName, SL.Shipped, SL.TotalLineAmountInc,'
      'SLP.LinePoints'
      'FROM tblSales S, tblSalesLines SL, tblSalesLinesPoints SLP'
      'WHERE SLP.SaleLineId = SL.SaleLineId'
      'AND SL.SaleId = S.SaleId'
      'AND S.SaleDate BETWEEN :DateFrom AND :DateTo'
      
        'AND ((:PaidStatus = "Both") OR (:PaidStatus = "Paid" AND S.Paid ' +
        '= "T") OR (:PaidStatus = "Not Paid" AND S.Paid = "F"))'
      'ORDER BY S.SaleDate, S.SaleId, S.ClientId')
    ParamData = <
      item
        DataType = ftString
        Name = 'DateFrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'PaidStatus'
      end
      item
        DataType = ftUnknown
        Name = 'PaidStatus'
      end
      item
        DataType = ftUnknown
        Name = 'PaidStatus'
      end>
    object qryMainSaleDate: TDateField
      DisplayLabel = 'Sale Date'
