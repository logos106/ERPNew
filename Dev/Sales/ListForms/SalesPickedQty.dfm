  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      
        'convert(  if(:groupProduct="T", NULL , S.saLeId), SIGNED)  as Sa' +
        'leId,  '
      ' SL.productname as ProductName, '
      'Product_Description as ProductDescription ,'
      ' sum(Shipped) as Totalshipped,'
      
        ' (Select  sum(if(:SaleID<>0 or :groupProduct ="F", SLBO.shipped,' +
        '0) + if(ifnull(SBO.BOID,"")="" , backorder , 0) )  '
      '         from tblsales SBO  '
      
        '         inner join tblsaleslines SLBO on SBO.saleID = SLBO.sale' +
        'Id  '
      '         where SLBO.ProductID = SL.productId  '
      '         and SLBO.SeqNo = SL.seqno  '
      '         and  SBO.saleID <> S.saleId  '
      '         and SBO.Originalno = S.originalno)  as QtyBackorder, '
      'sum( (Select Sum(Qty) from tblpickingassemblylines PAL '
      
        '         inner join tblpickingassembly PA on PA.PickingAssemblyI' +
        'D = PAL.PickingAssemblyID  '
      '         where PA.SaleID = s.saleid '
      '         and  PA.PickingAssemblyID = PAL.PickingAssemblyID  '
      '         and PAL.ProductID = SL.productID ))  as QtyPacked,'
      ' UnitofMeasureSaleLines as UOM,'
      ' UnitOfMeasureID as UOMId,'
      ' UnitofMeasureMultiplier UOMMultiplier'
      ' From tblsales S'
      ' inner join tblsalesLines SL on S.saleId = SL.saleID'
      
        ' where  ((S.Originalno = :OriginalNo and :OriginalNo <> '#39#39') or (' +
        'S.SaleId = :SaleID and :SaleId <>0) or (:SaleId =0 and :Original' +
        'No ="" and S.SAleDate between :Datefrom and :Dateto) )'
      '  group by if(:groupProduct="T", 1 , s.SaleId), Sl.productID'
      '  order by s.SaleId, SL.SortId , SL.productId')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'groupProduct'
      end
      item
        DataType = ftUnknown
        Name = 'SaleID'
      end
      item
        DataType = ftUnknown
        Name = 'groupProduct'
      end
      item
        DataType = ftUnknown
        Name = 'OriginalNo'
      end
      item
        DataType = ftUnknown
        Name = 'OriginalNo'
      end
      item
        DataType = ftUnknown
        Name = 'SaleID'
      end
      item
        DataType = ftUnknown
        Name = 'SaleId'
      end
      item
        DataType = ftUnknown
        Name = 'SaleId'
      end
      item
        DataType = ftUnknown
        Name = 'OriginalNo'
      end
      item
        DataType = ftUnknown
        Name = 'Datefrom'
      end
      item
        DataType = ftUnknown
        Name = 'Dateto'
      end
      item
        DataType = ftUnknown
        Name = 'groupProduct'
      end>
