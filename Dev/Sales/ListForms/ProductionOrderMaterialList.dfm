  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select distinct '
      
        'convert( @rownum:=if(@SaleLineId <> Sl.SaleLineId,  @rownum:= 1 ' +
        ',  @rownum+1) , unsigned) as rownum ,'
      
        'convert( @SaleLineId := sl.SaleLineId , unsigned)  as SaleLineId' +
        '1,'
      'convert(sl.SaleLineId  , unsigned)  as SaleLineId,'
      'S.saleId as SAleId,'
      'SL.Productname as SaleProduct,'
      'SL.Product_Description  as SaleProductDescription,'
      
        'concat(SL.UnitofMeasureSaleLines , '#39'('#39' , SL.UnitofMeasureMultipl' +
        'ier , '#39')'#39') as SaleProductUOM, '
      'SL.UnitofMeasureQtySold as UnitofMeasureQtySold, '
      'SL.UnitofMeasureShipped as UnitofMeasureShipped, '
      'SL.UnitofMeasureBackorder as UnitofMeasureBackorder,'
      'PT.BuildLineNo as Lineno, '
      'PT.Caption as Item,'
      
        'Convert(TreeNodeCaption(PT.level, PT.caption),char(255))  as Tre' +
        'eItem,  '
      
        'concat(PT.TreePartUOM , '#39'('#39' , PT.TreePartUOMMultiplier , '#39')'#39') as' +
        ' Itemuom, '
      'PT.Description as ItemDEscription, '
      'PT.Info as ItemInfo, '
      'PT.TreePartUOMTotalQty as TreePartUOMTotalQty,'
      'PT.FromStockUOMQty as FromStockUOMQty, '
      'PT.ManufactureUOMQty as ManufactureUOMQty, '
      'PT.ManufacturedUOMQty as ManufacturedUOMQty , '
      'PT.ManufactureUOMQty -PT.ManufacturedUOMQty  as tobeBuitQty,'
      'PT.ManufacturingUOMQty as ManufacturingUOMQty , '
      'PT.OnOrderUOMQty as OnOrderUOMQty , '
      
        'if(PT.ManufacturedUOMQty = PT.TreePartUOMTotalQty , "T" , "F") a' +
        's Completed,'
      '0.0 as completeQty'
      'from  (SELECT @rownum:=0) r ,'
      ' (SELECT @SaleId:=0) s,'
      'tblsales S '
      'inner join tblSaleslines SL on S.saleId = SL.saleId '
      
        'inner join tblproctree PT on SL.salelineid = PT.MasterId and PT.' +
        'MasterType <> '#39'mtProduct'#39
