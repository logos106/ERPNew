  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select'
      'S.OriginalNo,'
      'S.SaleID,'
      'space(100) as LeadEmployeeName,'
      'S.CustomerName, '
      'S.SaleDate,'
      
        'if(S.IsQuote="T", "Quote" , if(S.IsSalesOrder="T", "Sales Order"' +
        ' , if(S.IsCashSale="T", "Cash Sale" , if(S.ISInvoice="T", "Invoi' +
        'ce" ,"Sale" )))) as salestype,'
      'S.totalAmountinc,'
      'S.employeename'
      'from tblsales s '
      'where S.deleted ="F" and S.cancelled ="F"'
      'Order by Originalno desc , saleDate desc')
    Active = True
    object qryMainOriginalNo: TWideStringField
      DisplayLabel = 'Sales Group Ref'
