  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'S.SaleID as SaleID,'
      'S.ShipDate as ShipDate,'
      'C.ClientID as ClientID,'
      'E.EmployeeID as EmployeeID,'
      'SL.ProductID as ProductID, '
      'S.SaleDate as SaleDate,'
      '" " as Firstcolumn, '
      '" " as Secondcolumn, '
      '" " as Thirdcolumn,'
      
        'CONCAT_WS(" ", If(InStr(C.Company,'#39'^'#39') -1 >0,Left(C.Company,InSt' +
        'r(C.Company,'#39'^'#39')-1),C.Company),IF(char_length(C.JobName)>0, Conc' +
        'at("(",C.JobName,")"),'#39#39')) as CustomerName,'
      'C.PrintName as PrintName,'
      'TypeName as CustomerType,'
      'S.GlobalRef as GlobalRef,'
      'S.OriginalNo as OriginalNo,'
      'S.OriginalCreationDate as OriginalCreationDate,'
      'S.DueDate as DueDate ,'
      'S.InvoiceDocNumber as InvoiceDocNumber,'
      'S.PONumber as PONumber,'
      'SL.BackOrder as BackOrder,'
      'SL.ProductName as ProductName,'
      'SL.Product_Description as Product_Description,'
      'SL.LinePrice as LinePrice,'
      'SL.LinePriceInc as LinePriceInc,'
      'SL.LineWholesalePrice as LineWholesalePrice,'
      'SL.LineCost as LineCost,'
      'SL.LineCostInc as LineCostInc,'
      'SL.BackOrder*SL.LineCost as TotalLineCost,'
      'SL.BackOrder*SL.LineCostInc as TotalLineCostinc,'
      'SL.BackOrder*SL.LinePrice as TotalLinePrice,'
      'SL.BackOrder*SL.LinePriceInc as TotalLinePriceInc,'
      'SL.SalesLinesCustField1,'
      'SL.SalesLinesCustField2,'
      'SL.SalesLinesCustField3,'
      'SL.SalesLinesCustField4,'
      'SL.SalesLinesCustField5,'
      'SL.SalesLinesCustField6,'
      'SL.SalesLinesCustField7,'
      'SL.SalesLinesCustField8,'
      'SL.SalesLinesCustField9,'
      'SL.SalesLinesCustField10,'
      'SL.WarrantyEndsOn as WarrantyEndsOn,'
      'SL.WarrantyPeriod as WarrantyPeriod,'
      'SL.ShipDate as LineShipDate,'
      'S.Employeename as Rep, '
      'S.EnteredBy as EnteredBy, '
      'S.PickUpFrom as PickUpFrom,'
      'S.Deleted as Deleted,'
      'SL.SaleLineID as SaleLineID,'
      'S.Converted as Converted,'
      'S.QuoteGlobalRef as ConvertFromQuote,'
      'S.SalesOrderGlobalRef as ConvertFromSO,'
      'C.CUSTFLD1 AS CUSTFLD1,'
      'C.CUSTFLD2 AS CUSTFLD2,'
      'C.CUSTFLD3 AS CUSTFLD3,'
      'C.CUSTFLD4 AS CUSTFLD4,'
      'C.CUSTFLD5 AS CUSTFLD5,'
      'C.CUSTFLD6 AS CUSTFLD6,'
      'C.CUSTFLD7 AS CUSTFLD7,'
      'C.CUSTFLD8 AS CUSTFLD8,'
      'C.CUSTFLD9 AS CUSTFLD9,'
      'C.CUSTFLD10 AS CUSTFLD10,'
      'C.CUSTFLD11 AS CUSTFLD11,'
      'C.CUSTFLD12 AS CUSTFLD12,'
      'C.CUSTFLD13 AS CUSTFLD13,'
      'C.CUSTFLD14 AS CUSTFLD14,'
      'C.CUSTFLD15 AS CUSTFLD15, '
      'C.CUSTdate1 as CUSTdate1, '
      'C.CUSTdate2 as CUSTdate2, '
      'C.CUSTdate3 as CUSTdate3, '
      'S.Class as class, '
      'SL.CreationDate as CreationDate,'
      'P.PreferedSupp as PreferedSupplier , '
      'S.PickMemo ,'
      'SO.SmartOrderId, '
      
        'case when SO.SmartOrderId is null then "F" else "T" end as Pushe' +
        'dToSmartOrder'
      'FROM tblSales S'
      'INNER JOIN tblSalesLines SL Using(SaleID)'
      'INNER JOIN tblParts P ON SL.ProductID=P.PARTSID'
      'INNER JOIN tblClients C ON  (S.ClientID = C.ClientID)'
      'LEFT JOIN tblclienttype CT Using(ClientTypeID)'
      'LEFT JOIN tblEmployees E ON  S.EmployeeID = E.EmployeeID'
      
        'LEFT JOIN tblSmartOrder SO on S.GlobalRef=SO.SalesGlobalRef   Or' +
        ' ifnull(S.originalno , "") = SO.SalesGlobalRef'
      
        'WHERE S.IsInvoice='#39'T'#39' AND S.IsRefund='#39'F'#39' AND S.IsCashSale='#39'F'#39' AN' +
        'D S.IsSalesOrder='#39'F'#39' '
      'AND   S.IsQuote='#39'F'#39' AND S.Deleted='#39'F'#39' '
      'AND   S.SaleDate Between :txtfrom AND :txtto '
      'AND   SL.Invoiced = '#39'F'#39' AND SL.BackOrder <> 0 '
      'AND   ifnull(S.BOID , '#39#39') = '#39#39
      'and (:CustomerId =0 or S.ClientID =:CustomerId)'
      'GROUP BY SL.SaleLineID order by SaleLineID desc')
    Left = 48
    ParamData = <
      item
        DataType = ftString
        Name = 'txtfrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end
      item
        DataType = ftUnknown
        Name = 'CustomerId'
      end
      item
        DataType = ftUnknown
        Name = 'CustomerId'
      end>
