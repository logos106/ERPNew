  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'RS.SaleID RefundID, '
      
        'if (RS.IsCustomerReturn="T", "Customer Return" , "Refund") as Re' +
        'fundType, '
      'RS.Converted , '
      'RS.SaleDate refundDate,'
      'RSL.ProductName,'
      'RSL.UnitofMeasureShipped REfundQty,'
      
        'concat(RSL.UnitofMeasureSaleLines , '#39'('#39' , RSL.UnitofMeasureMulti' +
        'plier,'#39')'#39' ) as RefundUOM,'
      's.sAleID as SaleID,'
      'SL.UnitofMeasureQtySold,'
      'SL.UnitofMeasureShipped, '
      'SL.UnitofMeasureBackorder,'
      
        'concat(SL.UnitofMeasureSaleLines , '#39'('#39' , SL.UnitofMeasureMultipl' +
        'ier,'#39')'#39' ) as SalesUOM,'
      
        'if (S.ISInvoice="T","Invoice", if (S.ISCashSale="T","Cash Sale",' +
        '"POS" )) as SaleType, '
      'S.SaleDate'
      'from tblsales RS'
      'inner  join tblsaleslines RSL on Rs.SaleId = RSL.saleID'
      
        'Left join (tblsaleslines SL inner join tblsales S on S.sAleId = ' +
        'Sl.sAleId) on RSL.RefundedSalesLineID = SL.SaleLineID'
      'Where (RS.IsCustomerReturn ="T" or RS.IsRefund="T")'
      'and RS.saleDate between :Datefrom and :DateTo'
      'order by RS.SAleID desc')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Datefrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end>
