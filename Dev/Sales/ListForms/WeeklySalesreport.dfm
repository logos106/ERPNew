  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      
        'convert(round(DateDiff( s.saleDate , :DateFrom )/7)+1 , signed) ' +
        'as Weekno,'
      
        'convert(Date_add(:DateFrom , Interval round(DateDiff( s.saleDate' +
        ' , :DateFrom )/7) Week) , Date) weekfrom,'
      
        'convert(Date_add(Date_add(:DateFrom , Interval round(DateDiff( s' +
        '.saleDate ,:DateFrom )/7) Week), interval 6 day), Date) weekto,'
      's.SaleID, '
      's.Saledate as SaleDate,'
      
        'if(s.Isinvoice ="T", "Invoice", if(S.IsCashSale="T" , "cash Sale' +
        '" , if(S.IsPOS="T" , "POS" , if(S.IsRefund="T" , "Refund", "")))' +
        ') as Type,'
      'customername , '
      'SL.productname, '
      
        'concat(SL.UnitofMeasureSaleLines , '#39'('#39' , SL.UnitofMeasureMultipl' +
        'ier , '#39')'#39') as UOM, '
      'SL.UnitofMeasureShipped , '
      'SL.LinePriceInc, '
      'SL.TotalLineAmountInc , '
      'SL.LinePrice , '
      'SL.TotalLineAmount , '
      'SL.LineCost , '
      'SL.LineCostInc ,  '
      'SL.LineCost * SL.Shipped as totalLineCost , '
      'SL.LineCostInc * SL.Shipped as totalLineCostinc'
      'from tblsales S'
      'inner join tblsaleslines SL on S.SAleId = SL.sAleID'
      
        'where (s.Isinvoice ="T" or S.IsCashSale="T" or S.IsPOS="T" or S.' +
        'IsRefund="T") '
      'and S.Saledate between :DateFrom and :Dateto'
      'order by Customername, weekno , Productname')
