  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'S.SaleID as SaleID,'
      'S.InvoiceDocNumber As InvoiceDocNumber, '
      'S.CustomerName as CustomerName,'
      'S.SaleDate as SaleDate, '
      'S.EmployeeName as EmployeeName, '
      'S.Converted as Converted,'
      'SL.LinePrice as LinePrice, '
      'SL.SalesLinesCustField1 as CustomField1,'
      'SL.SalesLinesCustField2 as CustomField2,'
      'SL.SalesLinesCustField3 as CustomField3,'
      'SL.SalesLinesCustField4 as CustomField4,'
      'SL.SalesLinesCustField5 as CustomField5,'
      'SL.SalesLinesCustField6 as CustomField6,'
      'SL.SalesLinesCustField7 as CustomField7,'
      'SL.SalesLinesCustField8 as CustomField8,'
      'SL.SalesLinesCustField9 as CustomField9,'
      'SL.SalesLinesCustField10 as CustomField10,'
      'P.PartName as PartName,'
      'SL.Product_Description as Description,'
      'SL.Product_Description_Memo as Product_Description_Memo,'
      'SUBSTRING_INDEX(P.PRODUCTGROUP,"^",1) AS FirstColumn,'
      
        'IF(INSTR(P.PRODUCTGROUP,"^")<>0, SUBSTRING_INDEX(SUBSTRING_INDEX' +
        '(P.PRODUCTGROUP,"^",2),"^",-1),"")  AS SecondColumn,'
      
        'IF(INSTR(IF(INSTR(P.PRODUCTGROUP,"^")<>0, SUBSTRING_INDEX(P.PROD' +
        'UCTGROUP,"^",2),""),"^")<>0, SUBSTRING_INDEX(P.PRODUCTGROUP,"^",' +
        '-1),"")  AS ThirdColumn,'
      '`SL`.`Shipped` AS `Quantity`,'
      '`SL`.`COGSACCNT`, '
      
        'S.QuoteStatus,SL.Linecost, SL.LinecostInc , SL.LinePriceinc, SL.' +
        'TotalLineAmount, SL.TotalLineAmountInc'
      'FROM tblSales S'
      'INNER JOIN tblSalesLines SL USING(SaleID)'
      'INNER JOIN tblParts P ON P.PartsID = SL.ProductID'
      'WHERE S.IsQuote = "T"'
      'AND S.SaleDate Between :txtfrom AND :txtto')
    ParamData = <
      item
        DataType = ftString
        Name = 'txtfrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end>
