  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      '1 as Detail, '
      'ClassID as DepartmentID,'
      'Class as Department,'
      'SUM(SL.QtySold) as QuantitySold, '
      'Sum(SL.totalLineamount) as TotalEx,'
      'Sum(SL.totalLineamountInc) as TotalInc,'
      'Sum(SL.LineTax) as LineTax,'
      'SL.ProductID as ProductID, '
      'SL.ProductName as ProductName,'
      'P.ProductPrintName as ProductPrintName,'
      'P.PartsDescription as Description,'
      'P.PREFEREDSUPP as PREFEREDSUPP,'
      '"" AS FirstColumn,'
      '"" AS SecondColumn,'
      '"" AS ThirdColumn, '
      '0 as SaleID, '
      '"" as Saletype'
      'FROM tblSalesLines SL '
      'INNER JOIN tblSales S Using(SaleID)  '
      'LEFT JOIN tblParts P ON SL.ProductID = P.PartsID'
      'WHERE  S.IsQuote = "F" '
      'AND S.Converted = "F" '
      'AND S.Deleted = "F" AND  '
      '(S.IsInvoice = "T" OR S.IsRefund = "T" OR S.IsCashSale = "T") '
      'AND P.PartType = "INV"'
      'AND S.SaleDate BETWEEN :DateFrom2 AND :DateTo2'
      'GROUP BY ProductID, ClassID'
      'union ALL'
      'SELECT '
      '2 as Detail, '
      'ClassID as DepartmentID,'
      'Class as Department,'
      'SUM(SL.QtySold) as QuantitySold, '
      'Sum(SL.totalLineamount) as TotalEx,'
      'Sum(SL.totalLineamountInc) as TotalInc,'
      'Sum(SL.LineTax) as LineTax,'
      'SL.ProductID as ProductID, '
      'SL.ProductName as ProductName,'
      'P.ProductPrintName as ProductPrintName,'
      'P.PartsDescription as Description,'
      'P.PREFEREDSUPP as PREFEREDSUPP,'
      '"" AS FirstColumn,'
      '"" AS SecondColumn,'
      '"" AS ThirdColumn, '
      'S.SaleID as SaleID, '
      
        'if(S.ISQuote='#39'T'#39' , '#39'Quote'#39' , if(S.ISSalesOrder ='#39'T'#39', '#39'SalesORder' +
        #39' ,if(S.ISInvoice ='#39'T'#39', '#39'Invoice'#39' ,if(S.ISRefund='#39'T'#39' , '#39'Refund'#39' ' +
        ', '#39'CashSale'#39') ) )) as Saletype'
      'FROM tblSalesLines SL'
      'INNER JOIN tblSales S Using(SaleID) '
      'LEFT JOIN tblParts P ON SL.ProductID = P.PartsID'
      'WHERE  S.IsQuote = "F"'
      'AND S.Converted = "F"'
      'AND S.Deleted = "F"'
      
        'AND (S.IsInvoice = "T" OR S.IsRefund = "T" OR S.IsCashSale = "T"' +
        ') '
      'AND P.PartType = "INV"'
      'AND S.SaleDate BETWEEN :DateFrom1 AND :DateTo1'
      'GROUP BY ProductID, ClassID, S.SaleId')
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'DateFrom2'
      end
      item
        DataType = ftDateTime
        Name = 'DateTo2'
      end
      item
        DataType = ftDateTime
        Name = 'DateFrom1'
      end
      item
        DataType = ftDateTime
        Name = 'DateTo1'
      end>
