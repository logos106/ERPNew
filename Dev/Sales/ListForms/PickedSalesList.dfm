  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      's.saleId  AS  saleId ,'
      'if(S.IsInvoice="T", "Invoice" , "Sales Order") as Transtype,'
      'S.CustomerName AS CustomerName,'
      'S.ShipDate  AS  ShipDate ,'
      'S.Area  AS  Area ,'
      'S.ShipTo  AS  ShipTo ,'
      'SA.ShipCity AS ShipCity,'
      'S.Shipping  AS  Shipping ,'
      'PAL.Qty  AS  Qty,'
      'P.CUSTFLD1  AS  CUSTFLD1,'
      'P.CUSTFLD2  AS  CUSTFLD2,'
      'P.CUSTFLD3  AS  CUSTFLD3,'
      'P.CUSTFLD4 AS CUSTFLD4,'
      'P.CUSTFLD5 AS CUSTFLD5,'
      'P.CUSTFLD6 AS CUSTFLD6,'
      'P.CUSTFLD7 AS CUSTFLD7,'
      'P.CUSTFLD8 AS CUSTFLD8,'
      'P.CUSTFLD9 AS CUSTFLD9,'
      'P.CUSTFLD10 AS CUSTFLD10,'
      'P.CUSTFLD11 AS CUSTFLD11,'
      'P.CUSTFLD12 AS CUSTFLD12,'
      'P.CUSTFLD13 AS CUSTFLD13,'
      'P.CUSTFLD14 AS CUSTFLD14,'
      'P.CUSTFLD15 AS CUSTFLD15,'
      'S.AddToManifest AS AddToManifest,'
      'ML.ManifestID AS ManifestID,'
      'PA.StartPickingTime AS StartPickingTime,'
      'PA.FinishedPickingTime AS FinishedPickingTime,'
      'PA.StartPackingTime AS StartPackingTime,'
      'PA.FinishedPackingTime AS FinishedPackingTime,'
      
        'if(PA.FinishedPacking="T" , "Packed",if(PA.StartPacking="T" , "P' +
        'acking",if(PA.FinishedPicking="T" , "Picked",if(PA.StartPicking=' +
        '"T" , "Picking",'#39#39'))))  AS  Pickstatus,'
      'uom.Length*uom.width AS ProductArea, '
      'uom.Weight   as ProductWeight,'
      'uom.Length*uom.width* PAL.Qty AS totalProductArea, '
      'uom.Weight * PAL.Qty  as totalProductWeight'
      'from tblsales S '
      'inner join tblpickingassembly PA on S.saleId = PA.SaleID '
      
        'inner join tblpickingassemblylines PAL on PA.PickingAssemblyID =' +
        ' PAL.PickingAssemblyID  and (PA.FinishedPacking="T" or PA.StartP' +
        'acking="T" or PA.FinishedPicking="T"  or PA.StartPicking="T" )'
      'inner join tblparts P on P.partsId = PAL.productID'
      'inner join tblunitsofmeasure uom on uom.UnitID = PAL.UOMID'
      
        'inner join tblsaleslines SL on s.saleId = SL.saleId and SL.Produ' +
        'ctID = PAL.ProductID and SL.UnitOfMeasureID = PAL.UOMID'
      'Left join tblshippingaddress SA on S.ShipToID = SA.ShipAddressID'
      'Left join tblmanifestlines ML on SL.SaleLineID= ML.SaleLineID'
      
        'where ifnull(PAL.qty,0)<>0 and S.Converted ="F" and (s.issalesor' +
        'der ="T" or ISinvoice ="T") and S.ShipDate between :dateFrom and' +
        ' :dateto'
      'Order by ShipDate desc')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'dateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'dateto'
      end>
