  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      ''
      'tblSales.SaleDate as '#39'Sale Date'#39','
      #39'Sales Order'#39' as '#39'Transaction Type'#39', '
      'tblSales.CustomerName as '#39'Customer Name'#39' ,'
      'TypeName as CustomerType,'
      'tblSales.GlobalRef as '#39'Global #'#39' ,'
      'tblSales.OriginalNo as '#39'Original #'#39','
      'tblSales.InvoiceDocNumber as '#39'Sales Order Number'#39' ,'
      'tblSales.PONumber as '#39'PO Number'#39','
      'tblSales.TotalAmount as '#39'Total Amount (Ex)'#39', '
      'tblSales.TotalTax as '#39'Total Tax'#39','
      'tblSales.TotalAmountInc as '#39'Total Amount (Inc)'#39' ,'
      'tblEmployees.EmployeeName AS '#39'Employee Name'#39','
      'tblSales.ConNote as '#39'Consignment Note'#39', '
      'tblSales.Class as '#39'Class Name'#39','
      'TypeDesc as '#39'Product Type'#39','
      
        'Left(if((LOCATE('#39'^'#39',tblparts.PRODUCTGROUP,InStr(tblparts.PRODUCT' +
        'GROUP,'#39'^'#39')+1)) >0, MID(tblparts.PRODUCTGROUP,LOCATE('#39'^'#39',tblparts' +
        '.PRODUCTGROUP, InStr(tblparts.PRODUCTGROUP,'#39'^'#39')+1)+1,char_length' +
        '(tblparts.PRODUCTGROUP)- LOCATE('#39'^'#39',tblparts.PRODUCTGROUP,InStr(' +
        'tblparts.PRODUCTGROUP,'#39'^'#39')+1)),  Null),255)  AS Department ,'
      
        'Left(MID(tblparts.PRODUCTGROUP, InStr(tblparts.PRODUCTGROUP,'#39'^'#39')' +
        '+1 ,If(LOCATE('#39'^'#39',tblparts.PRODUCTGROUP,InStr(tblparts.PRODUCTGR' +
        'OUP,'#39'^'#39')+1) - InStr(tblparts.PRODUCTGROUP,'#39'^'#39') >0,LOCATE('#39'^'#39',tbl' +
        'parts.PRODUCTGROUP,InStr(tblparts.PRODUCTGROUP,'#39'^'#39')+1)-1 - InStr' +
        '(tblparts.PRODUCTGROUP,'#39'^'#39'),IF(InStr(tblparts.PRODUCTGROUP,'#39'^'#39')<' +
        '>0,LENGTH(tblparts.PRODUCTGROUP) - InStr(tblparts.PRODUCTGROUP,'#39 +
        '^'#39'),0))),255)  AS Type,'
      
        'Left(If(InStr(tblparts.PRODUCTGROUP,'#39'^'#39') -1 >0,Left(tblparts.PRO' +
        'DUCTGROUP,InStr(tblparts.PRODUCTGROUP,'#39'^'#39')-1),tblparts.PRODUCTGR' +
        'OUP),255) AS Manufacturer ,'
      'tblSalesLines.ProductName as '#39'Product Name'#39','
      'tblparts.ProductPrintName as ProductPrintName,'
      'tblSalesLines.Product_Description as '#39'Product Description'#39' , '
      'tblSalesLines.LinePrice as '#39'Line Price (Ex)'#39','
      'tblSalesLines.LinePriceInc as '#39'Line Price (Inc)'#39', '
      'tblSalesLines.LineCost as '#39'Line Cost (Ex)'#39','
      'tblSalesLines.LineCostInc as '#39'Line Cost (Inc)'#39', '
      'tblSalesLines.LineTaxCode as '#39'Tax Code'#39', '
      'tblSalesLines.LineTax as '#39'Line Tax'#39','
      'tblSalesLines.Shipped,'
      'tblSalesLines.BackOrder,'
      'tblSalesLines.TotalLineAmount as '#39'Line Total (Ex)'#39','
      'tblSalesLines.TotalLineAmountInc as '#39'Line Total (Inc)'#39','
      'tblparts.PREFEREDSUPP as '#39'Preferred Supplier'#39','
      'tblSales.Deleted,'
      'tblEmployees.EmployeeID,'
      'tblClients.ClientID,'
      'tblSalesLines.WarrantyPeriod,'
      'tblSalesLines.WarrantyEndsOn,'
      'tblSalesLines.ShipDate as LineShipDate,'
      'tblSales.Converted,'
      'tblSales.SaleID,'
      'tblShippingContainer.ContainerName,'
      'tblShippingContainer.ETDeparture as ContainerETD,'
      'tblShippingContainer.ETA as ContainerETA,'
      'tblShippingContainer.ShippingContainerID,'
      '`tblsaleslines`.`SalesLinesCustField1` as custfld1,'
      '`tblsaleslines`.`SalesLinesCustField2` as custfld2,'
      '`tblsaleslines`.`SalesLinesCustField3` as custfld3,'
      '`tblsaleslines`.`SalesLinesCustField4` as custfld4,'
      '`tblsaleslines`.`SalesLinesCustField5` as custfld5,'
      '`tblsaleslines`.`SalesLinesCustField6` as custfld6,'
      '`tblsaleslines`.`SalesLinesCustField7` as custfld7,'
      '`tblsaleslines`.`SalesLinesCustField8` as custfld8,'
      '`tblsaleslines`.`SalesLinesCustField9` as custfld9,'
      '`tblsaleslines`.`SalesLinesCustField10`  as custfld10, '
      
        '`tblsaleslines`.UnitofMeasureSaleLines  as UnitofMeasureSaleLine' +
        's, '
      
        'tblsaleslines.UnitofMeasureMultiplier as UnitofMeasureMultiplier' +
        ', '
      'tblsaleslines.UnitofMeasureQtySold,'
      'tblsaleslines.UnitofMeasureShipped , '
      'tblsaleslines.QtySold, '
      'UOM.`Height` as Height, '
      'UOM.`Width` as Width, '
      'UOM.`Length` as Length, '
      'UOM.`Weight` as Weight, '
      'UOM.`Volume` Volume,'
      'tblSales.FutureSO as FutureSO,'
      'tblsales.QuoteStatus as Status'
      'FROM tblSales'
      
        'inner  JOIN tblSalesLines ON  tblSales.SaleID = tblSalesLines.Sa' +
        'leID'
      
        'inner join tblunitsofmeasure UOM on UOM.UnitID =  tblSalesLines.' +
        'UnitOfMeasureID'
      'inner JOIN tblparts ON PARTSID = ProductID'
      
        'inner JOIN tblparttypes ON tblparts.PARTTYPE = tblparttypes.Type' +
        'Code'
      
        'LEft JOIN tblClients as Supp ON  tblparts.PREFEREDSUPP = Supp.Co' +
        'mpany'
      
        'inner  JOIN tblClients ON  tblSales.ClientID = tblClients.Client' +
        'ID'
      
        'LEFT JOIN tblclienttype on tblclienttype.ClientTypeID = tblClien' +
        'ts.ClientTypeID'
      
        'Inner JOIN tblEmployees ON tblSales.EmployeeName = tblEmployees.' +
        'EmployeeName'
      
        'LEFT JOIN tblPurchaseLines ON tblSalesLines.SaleLineID = tblPurc' +
        'haseLines.SaleLineID'
      
        'LEFT JOIN tblShipContainerDetails ON tblPurchaseLines.PurchaseLi' +
        'neID = tblShipContainerDetails.PurchaseLineID'
      
        'LEFT JOIN tblShippingContainer ON tblShipContainerDetails.Shippi' +
        'ngContainerID = tblShippingContainer.ShippingContainerID'
      
        'WHERE tblSales.IsRefund ='#39'F'#39' And tblSales.IsCashSale='#39'F'#39' And tbl' +
        'Sales.IsInvoice='#39'F'#39' AND tblSalesLines.Invoiced='#39'T'#39
      'AND tblSales.IsSalesOrder='#39'T'#39' And tblSales.IsQuote='#39'F'#39' '
      'AND tblSales.SaleDate Between :txtfrom  AND :txtto'
      'GROUP BY tblSalesLines.SaleLineID'
      'Order BY tblSalesLines.SaleID Desc;')
    Left = 42
    ParamData = <
