  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT DISTINCT'
      'tblSales.GlobalRef,'
      'tblSales.SaleID,'
      'tblSales.BOID,'
      'InvoiceDocNumber,'
      'tblSales.ClientID, '
      'CustomerName, '
      'SaleDate, '
      'tblSales.shipDate,'
      
        '-- if(ifnull(tblPrintdocs.PrintdocId,0)=0 , "F" , "T") as PrintF' +
        'lag,'
      
        'if(IfNull(tblPrintdocs.DocOutputType,"") = "" or IfNull(tblPrint' +
        'docs.DocOutputType,"") = "dotPreview","F","T") as PrintFlag,'
      
        'if((select PrintDocId from tblPrintDocs where tblPrintdocs.DocId' +
        ' = tblSales.SaleId AND tblPrintdocs.DocName = "TSalesOrder" and ' +
        '(tblPrintdocs.printedfrom = "TSalesOrderPrnGUI"  or :PrintedFrom' +
        'AnyForm ="T")  and IfNull(tblPrintdocs.DocOutputType,"") = "dotE' +
        'mail" limit 1) > 0,"T","F") as EmailFlag,'
      
        '(select tblEmployees.EmployeeName from tblPrintDocs, tblEmployee' +
        's where tblPrintDocs.PrintedBy = tblEmployees.EmployeeID and tbl' +
        'Printdocs.DocId = tblSales.SaleId AND tblPrintdocs.DocName = "TS' +
        'alesOrder" and (tblPrintdocs.printedfrom = "TSalesOrderPrnGUI"  ' +
        'or :PrintedFromAnyForm ="T")  and IfNull(tblPrintdocs.DocOutputT' +
        'ype,"") = "dotEmail" limit 1) as EmailedBy,'
      'tblSales.EditedFlag,'
      'tblclienttype.TypeName, '
      'Class,'
      'converted,'
      'tblclients.SendXMLInvoices ,  '
      'tblclients.EmailXML, '
      'tblEmployees.EmployeeName PrintedBy,'
      'tblSales.QuoteStatus as SaleStatus,'
      'tblclients.FTPAddress,'
      'tblclients.FTPUserName,'
      'tblclients.FTPPassword'
      'FROM tblSales'
      
        'INNER JOIN tblSaleslines on tblSaleslines.SaleID = tblSales.Sale' +
        'ID AND Invoiced = "T"'
      
        'and ((:ProductnamefilterOption1 = 0 and :ProductnamefilterOption' +
        '2 = 0)'
      
        'or ((:ProductnamefilterOption1 =1 and tblSaleslines.ProductName ' +
        'like concat(:ProductnamefilterProductName1, '#39'%'#39'))'
      
        'or (:ProductnamefilterOption2 =1 and tblSaleslines.ProductName l' +
        'ike concat(:ProductnamefilterProductName2, '#39'%'#39'))))'
      'INNER JOIN tblclients on tblSales.clientid = tblclients.clientid'
      
        'LEFT JOIN tblclienttype on tblclients.clienttypeid = tblclientty' +
        'pe.clienttypeid'
      
        'LEFT JOIN tblPrintdocs on tblPrintdocs.DocId = tblSales.SaleId A' +
        'ND  tblPrintdocs.DocName = '#39'TSalesOrder'#39' and (tblPrintdocs.print' +
        'edfrom = '#39'TSalesOrderPrnGUI'#39'  or :PrintedFromAnyForm ="T")   and' +
        ' IfNull(tblPrintdocs.DocOutputType,"") = '#39'dotPrint'#39
      
        'LEFT JOIN tblEmployees on tblEmployees.employeeId = tblprintdocs' +
        '.PrintedBy '
      'WHERE IsSalesOrder='#39'T'#39' '
      
        'AND ((:FilterOption =0 and SaleDate BETWEEN :xFrom AND :xTo )  o' +
        'r (:FilterOption =1 and tblsales.ShipDate BETWEEN :xFrom AND :xT' +
        'o ))'
      'AND Invoiced = "T"')
