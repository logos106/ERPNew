  object qryLaybyPayments: TERPQuery
    Connection = MyConnection
    SQL.Strings = (
      'select SL.ProductName, SL.LinePriceInc, S.SaleDate, S.Comments'
      'from tblSalesLines SL'
      'inner join tblSales S on S.SaleId=SL.SaleId'
      
        'where SL.laybyid= (select Globalref from tblSales where saleid=:' +
        'id)')
    OnCalcFields = qryLaybyPaymentsCalcFields
    Left = 579
    Top = 275
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ID'
      end>
