  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT DISTINCT'
      'tblSales.GlobalRef,'
      'tblSales.SaleID,'
      'tblSales.BOID,'
      'InvoiceDocNumber,'
      'tblSales.ClientID, '
      'CustomerName, '
      'converted,'
      'SaleDate, '
      'tblsales.ShipDate,'
      
        'if(IfNull(tblPrintdocs.DocOutputType,"") = "" or IfNull(tblPrint' +
        'docs.DocOutputType,"") = "dotPreview","F","T") as PrintFlag,'
      
        'if((select PrintDocId from tblPrintDocs where tblPrintdocs.DocId' +
        ' = tblSales.SaleId AND tblPrintdocs.DocName = "TInvoice" and tbl' +
        'Printdocs.printedfrom = "TInvoicesPrnGUI" and IfNull(tblPrintdoc' +
        's.DocOutputType,"") = "dotEmail" limit 1) > 0,"T","F") as EmailF' +
        'lag,'
      
        '(select tblEmployees.EmployeeName from tblPrintDocs, tblEmployee' +
        's where tblPrintDocs.PrintedBy = tblEmployees.EmployeeID and tbl' +
        'Printdocs.DocId = tblSales.SaleId AND tblPrintdocs.DocName = "TI' +
        'nvoice" and tblPrintdocs.printedfrom = "TInvoicesPrnGUI" and IfN' +
        'ull(tblPrintdocs.DocOutputType,"") = "dotEmail" limit 1) as Emai' +
        'ledBy,'
      'tblSales.EditedFlag,'
      'tblclienttype.TypeName, '
      'Class,'
      'tblclients.SendXMLInvoices ,  '
      'tblclients.EmailXML, '
      'tblclients.FTPAddress,'
      'tblclients.FTPUserName,'
      'tblclients.FTPPassword,'
      'tblEmployees.EmployeeName PrintedBy '
      'FROM tblSales'
      
        'INNER JOIN tblSaleslines on tblSaleslines.SaleID = tblSales.Sale' +
        'ID AND Invoiced = "T"'
      'INNER JOIN tblclients on tblSales.clientid = tblclients.clientid'
      
        'LEFT JOIN tblclienttype on tblclients.clienttypeid = tblclientty' +
        'pe.clienttypeid'
      
        'LEFT JOIN tblPrintdocs on tblPrintdocs.DocId = tblSales.SaleId A' +
        'ND  tblPrintdocs.DocName = '#39#39
      
        'LEFT JOIN tblEmployees on tblEmployees.employeeId = tblprintdocs' +
        '.PrintedBy '
      
        'WHERE  ((:FilterOption =0 and SaleDate BETWEEN :xFrom AND :xTo )' +
        '  or (:FilterOption =1 and tblsales.ShipDate BETWEEN :xFrom AND ' +
        ':xTo ))'
      
        'AND if(IfNull(tblPrintdocs.DocOutputType,"") = "" or IfNull(tblP' +
        'rintdocs.DocOutputType,"") = "dotPreview","F","T") <> :PrintFlag'
      'AND Invoiced = "T"')
    Left = 41
    Top = 379
