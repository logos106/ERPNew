  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT DISTINCT'
      'tblSales.GlobalRef as GlobalRef,'
      'tblSales.SaleID as SaleID,'
      'tblSales.BOID as BOID,'
      'InvoiceDocNumber as InvoiceDocNumber,'
      'tblSales.ClientID as ClientID, '
      'CustomerName as CustomerName, '
      'SaleDate as SaleDate, '
      'tblsales.shipDate as shipDate,'
      'tblSales.TotalAmount as TotalAmount,'
      'tblSales.TotalAmountInc  TotalAmountInc,'
      
        'if(ifnull(tblPrintdocs.PrintdocId,0)=0 , "F" , "T") as PrintFlag' +
        ','
      
        'if(SalesPrintdocID("TInvoice" , tblsales.saleID , :PrintedFromAn' +
        'yForm , "TInvoicesPrnGUI" , "dotEmail")> 0,"T","F") as EmailFlag' +
        ','
      
        'SalesPrintedby("TInvoice" , tblsales.saleID , :PrintedFromAnyFor' +
        'm , "TInvoicesPrnGUI" , "dotEmail") as EmailedBy,'
      'tblSales.EditedFlag as EditedFlag,'
      'tblclienttype.TypeName as TypeName, '
      'Class as Class,'
      'converted as converted,'
      'tblclients.SendXMLInvoices as SendXMLInvoices,  '
      'tblclients.EmailXML as EmailXML, '
      'tblEmployees.EmployeeName as PrintedBy,'
      'tblRelatedClients.MasterCompany as FundingBody,'
      'tblRelatedClients.MasterClientID as FundingBodyID,'
      'tblclients.FTPAddress,'
      'tblclients.FTPUserName,'
      'tblclients.FTPPassword,'
      'tblSales.QuoteStatus as Status,'
      
        'if(IsInvoice='#39'T'#39' ,'#39'Invoice'#39',if(IsCashSale='#39'T'#39' , '#39'Cash Sale'#39' , '#39#39 +
        ')) as Transtype'
      ''
      'FROM tblSales'
      
        'INNER JOIN tblSaleslines on tblSaleslines.SaleID = tblSales.Sale' +
        'ID AND Invoiced = "T"'
      'INNER JOIN tblclients on tblSales.clientid = tblclients.clientid'
      
        'LEFT JOIN tblclienttype on tblclients.clienttypeid = tblclientty' +
        'pe.clienttypeid'
      
        'LEFT JOIN tblRelatedClients ON tblRelatedClients.ChildclientId =' +
        ' tblSales.ClientID'
      
        'LEFT JOIN tblPrintdocs on tblPrintdocs.DocId = tblSales.SaleId A' +
        'ND  tblPrintdocs.DocName = '#39'TInvoice'#39'  and (tblPrintdocs.printed' +
        'from= '#39'TInvoicesPrnGUI'#39'   or :PrintedFromAnyForm ="T")  and IfNu' +
        'll(tblPrintdocs.DocOutputType,"") = '#39'dotPrint'#39
      
        'LEFT JOIN tblEmployees on tblEmployees.employeeId = tblprintdocs' +
        '.PrintedBy '
      'WHERE (IsInvoice='#39'T'#39'  or IsCashSale='#39'T'#39')'
      
        'AND  ((:FilterOption =0 and SaleDate BETWEEN :xFrom AND :xTo )  ' +
        'or (:FilterOption =1 and tblsales.ShipDate BETWEEN :xFrom AND :x' +
        'To ))'
      'AND Invoiced = "T"')
