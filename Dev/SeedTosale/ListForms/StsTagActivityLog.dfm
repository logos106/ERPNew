  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'STS.ID as TagID,'
      'STS.StsTag as Tag,'
      'STS.UsedOn as TagUsedOn,'
      'STS.Plantcount,'
      'STS.ChangedCount,'
      'STS.WastedCount,'
      'STS.Destroycount,'
      'STS.TransferedCount,'
      'STS.UsedCount,'
      
        'STS.Plantcount-STS.ChangedCount-STS.WastedCount-STS.Destroycount' +
        '-STS.TransferedCount-STS.UsedCount AS Activecount,'
      'STSP.Tagchanged as TagChanged,'
      'STSA.StatusAction as  Action,'
      'STSA.StatusDate as ActionDate,'
      'STSA.Description as ActionDescription,'
      'STSAS.StsTag as NewTag,'
      'STSP.QtyPerTag as NewPhaseQtyPerTag,'
      'STSP.NoofTags as NewPhaseNoOfTags,'
      'STSP.PhaseCreatedOn as NewPhaseCreated,'
      'SeedToSalePhase(STSP.StsPhase) as NewPhase'
      'FROM tblststags STS'
      'LEFT JOIN tblststag_actions STSA ON STS.Id = STSA.TagID'
      'LEFT JOIN tblststags STSAS     ON STSA.ID = STSAS.StSActionID'
      'LEFT JOIN tblststagphases STSP ON STSP.ID = STSAS.StSTagPhaseID')
