  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'OF.FollowUpID as FollowUpID ,'
      'OF.OtherContactID as OtherContactID ,'
      'OF.RepairID as RepairID ,'
      'OF.EmployeeID as EmployeeID ,'
      'OF.ClientID as ClientID ,'
      'OF.FollowUpDate as FollowUpDate ,'
      'OF.Notes as Notes ,'
      'OF.Done as Done ,'
      'OF.AppearDays as AppearDays ,'
      'OF.CreationDate as CreationDate ,'
      'OF.UpdateDate as UpdateDate ,'
      ''
      'OF.IsSupplier as IsSupplier ,'
      'OF.IsOtherContact as IsOtherContact ,'
      'OF.IsCustomer as IsCustomer ,'
      
        'if(OF.IsSupplier ="T","Supplier",if(OF.IsOtherContact ='#39'T'#39' , "Ot' +
        'her Contact" , if(OF.IsCustomer ="T" , "Customer" , if(ifnull(OF' +
        '.EmployeeID,0)<> 0 , '#39'Employee'#39' , if(ifnull(OF.RepairID,0)<>0 , ' +
        #39'Repair'#39' , '#39'Unknown'#39'))))) as Category, '
      'OF.Hours as Hours ,'
      'OF.IsPhoneFollowUp as IsPhoneFollowUp ,'
      'OF.CustomField1 as CustomField1 ,'
      'OF.CustomField2 as CustomField2 ,'
      'OF.CustomField3 as CustomField3 ,'
      'OF.CustomField4 as CustomField4 ,'
      'OF.CustomField5 as CustomField5 ,'
      'OF.CustomField6 as CustomField6 ,'
      'OF.CustomField7 as CustomField7 ,'
      'OF.CustomField8 as CustomField8 ,'
      'OF.CustomField9 as CustomField9 ,'
      'OF.CustomField10 as CustomField10 ,'
      'C.Company as ClientName ,'
      'E.EmployeeName as EmployeeName ,'
      'RT.Name as ResultTypeName ,'
      'T.Name as typeName '
      'FROM `tblotherfollowup` AS `OF`'
      
        'Left Join `tblemployees` AS `E` ON `OF`.`EmployeeID` = `E`.`Empl' +
        'oyeeID`'
      
        'Left Join `tblclients` AS `C` ON `OF`.`ClientID` = `C`.`ClientID' +
        '`'
      
        'Left join tblsimpletypes T on  T.Id = OF.typeID and T.TypeCode =' +
        ' '#39'FollowUpType'#39
      
        'Left join tblsimpletypes RT  on  RT.Id = OF.REsulttypeID and RT.' +
        'TypeCode = '#39'FollowUpResultType'#39)
