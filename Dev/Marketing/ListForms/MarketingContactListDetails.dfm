  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT DISTINCT   '
      'MC.DateEntered as DateEntered, '
      'MC.GlobalRef as GlobalRef, '
      'MC.ID as ID , '
      'MC.COMPANY as CompanyName, '
      'MC.CompanyTypeName as CompanyTypeName, '
      'MC.Phone as Phone, '
      'MC.Email as Email,'
      'MC.STATE as STATE, '
      'MC.FirstName as FirstName, '
      'MC.LastName as LastName, '
      'MC.SUBURB as SUBURB, '
      'MC.Source as Source, '
      'MC.POSTCODE as POSTCODE , '
      'MC.Country as Country , '
      'MLL.updateDate as updateDate , '
      'MLL.ActionRef as ActionRef, '
      'StatusLookup.Name AS Status, '
      'ActionLookup.Name AS Action, '
      
        'concat_Ws(" " , E.Title , E.Firstname, E.lastname) as  EmployeeN' +
        'ame, '
      'MLL.ResultTypeName as ResultTypeName, '
      'MLL.FollowupDate as FollowupDate,  '
      'MLL.Notes as Notes, '
      'MLL.ActionDate  as ActionDate,  '
      'MLL.ISLEad as ISLEad, '
      'if(ifnull(ML.ISLEad,"") = "T" , MLL.LeadID,0) as LeadId, '
      'if(ifnull(MLL.ISLEad,"")<>"T", MLL.LeadID,0) as TMId, '
      'if(ifnull(ML.ISLEad,"") = "F" , MLL.LeadID,0) as FollowupId '
      'FROM tblmarketingcontacts MC '
      
        'Left Join tblmarketingleads AS ML ON ML.MarketingContactID = MC.' +
        'ID '
      
        'Left Join tblmarketingleadlines AS MLL ON ML.LeadID = MLL.LeadID' +
        ' and MLL.done = "T" '
      
        'Left Join tblsimpletypes AS StatusLookup ON MLL.StatusID = Statu' +
        'sLookup.Id '
      
        'left Join tblsimpletypes AS ActionLookup ON MLL.ActionType = Act' +
        'ionLookup.id '
      'left Join tblemployees AS E ON MLL.EmployeeId = E.employeeId '
      
        'Where :FilterOnDate = "T" or MLL.ActionDate BETWEEN :Datefrom AN' +
        'D :Dateto'
      'Order by MC.COMPANY')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'FilterOnDate'
      end
      item
        DataType = ftUnknown
        Name = 'Datefrom'
      end
      item
        DataType = ftUnknown
        Name = 'Dateto'
      end>
