  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      '`tblpurchaseorders`.`PurchaseOrderID`,'
      '`tblpurchaseorders`.`PurchaseOrderNumber`,'
      '`tblpurchaseorders`.`SupplierName`,'
      '`tblpurchaseorders`.`OrderDate`,'
      '`tblpurchaseorders`.`TotalAmount`,'
      '`tblpurchaseorders`.`TotalAmountInc`,'
      '`tblpurchaseorders`.`EmployeeName`,'
      
        'if(`tblpurchaseorders`.`IsPO`="T","PO" , if(`tblpurchaseorders`.' +
        '`IsRA`="T","RA" , if(`tblpurchaseorders`.`IsBill`="T","Bill" , i' +
        'f(`tblpurchaseorders`.`IsCredit`="T", "Credit", if(`tblpurchaseo' +
        'rders`.`IsCheque`="T","Cheque" , if(`tblpurchaseorders`.`IsPOCre' +
        'dit`="T","PO Credit" , "Un-Known")))))) as type , '
      '`tblpurchaseorders`.`Payment`,'
      '`tblpurchaseorders`.`Balance`,'
      '`tblpurchaseorders`.`Paid`'
      'from tblPurchaseOrders '
      
        'where purchaseOrderId not in (Select ifnull(PurchaseOrderID,0) f' +
        'rom tblPurchaseLines)'
      'and ifnull(TotalAmountinc,0) <> 0')
    OnCalcFields = qryMainCalcFields
    Left = 283
