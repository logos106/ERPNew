  object qryETA: TERPQuery
    Connection = MyConnection
    SQL.Strings = (
      '#Dummy SQL - See FormShow()'
      'SELECT S.GlobalRef,S.customername,'
      
        'SL.ProductName, SL.ProductID, Sum(SL.Backorder) AS Backorder, SL' +
        '.SaleID AS "Inv #", S.Class AS "Dept"'
      'FROM tblsaleslines SL'
      'INNER JOIN tblsales S Using(SaleID)'
      'WHERE S.IsInvoice="T" AND SL.ProductID = :xPartID AND'
      'SL.Invoiced="F" AND (SL.PARTTYPE="INV" OR SL.PARTTYPE="GRP")'
      'GROUP BY SL.SaleID;')
    AfterOpen = qryETAAfterOpen
    Options.StrictUpdate = False
    Left = 298
    Top = 182
    ParamData = <
      item
        DataType = ftString
        Name = 'xPartID'
        Value = ''
      end>
