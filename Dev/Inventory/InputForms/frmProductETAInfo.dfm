  object qryETA: TERPQuery
    Connection = MyConnection
    SQL.Strings = (
      '#Dummy SQL - See FormShow()'
      'SELECT P.GlobalRef,'
      
        'PL.ProductName, PL.ProductID, PL.ETADate, Sum(PL.Backorder) AS B' +
        'ackorder, '
      'PL.PurchaseOrderID AS "PO #", PL.Class AS "Dept"'
      'FROM tblpurchaselines AS PL'
      'INNER JOIN tblpurchaseorders AS P Using(PurchaseOrderID)'
      
        'WHERE P.IsPO="T" AND PL.ProductID = :xPartID AND PL.Invoiced="F"' +
        ' AND'
      '               PL.Backorder <> 0'
      'GROUP BY PL.PurchaseOrderID;')
    AfterOpen = qryETAAfterOpen
    Left = 379
    Top = 15
    ParamData = <
      item
        DataType = ftString
        Name = 'xPartID'
        Value = ''
      end>
