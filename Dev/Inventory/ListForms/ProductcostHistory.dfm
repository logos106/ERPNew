  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'pqa.PQAID'#9'as'#9'PQAID'#9','
      'pqa.TransID'#9'as'#9'TransID'#9','
      'pqa.TransLineID'#9'as'#9'TransLineID'#9','
      'pqa.TransType'#9'as'#9'TransType'#9','
      'pqa.TransDate'#9'as'#9'TransDate'#9','
      'pqa.Alloctype'#9'as'#9'Alloctype'#9','
      'pqa.Qty'#9'as'#9'Qty'#9','
      'pqa.UOMQty'#9'as'#9'UOMQty'#9','
      'pqa.StockQty'#9'as'#9'StockQty'#9','
      'pqa.UOM'#9'as'#9'UOM'#9','
      'pqa.UOMMultiplier'#9'as'#9'UOMMultiplier'#9','
      'concat(pqa.UOM, '#39' ('#39' , pqa.UOMMultiplier, '#39')'#39') as UOMdesc,'
      'pqa.UOMID'#9'as'#9'UOMID'#9','
      'pqa.DepartmentID'#9'as'#9'DepartmentID'#9','
      'pqa.ProductID'#9'as'#9'ProductID'#9','
      'pqa.ProductName'#9'as'#9'ProductName'#9','
      'pqa.Active'#9'as'#9'Active'#9','
      'pqa.Linecost'#9'as'#9'Linecost'#9','
      'pqa.linecostinc'#9'as'#9'linecostinc'#9','
      'pqa.totalcost'#9'as'#9'totalcost'#9','
      'pqa.totalcostinc'#9'as'#9'totalcostinc'#9','
      'pqa.Actualtotalcost'#9'as'#9'Actualtotalcost'#9','
      'pqa.Actualtotalcostinc'#9'as'#9'Actualtotalcostinc'#9','
      '(pqa.Actualtotalcost/pqa.Qty)'#9'   as'#9'Actuallinecost'#9','
      '(pqa.Actualtotalcostinc/pqa.Qty)   as'#9'ActualLinecostinc'#9','
      'pqa.RunningQty'#9'as'#9'RunningQty'#9','
      'C.ClassName as classname'
      'from tbltransactionpqas pqa'
      'inner join tblclass C on pqa.DepartmentID = C.ClassID'
      'where productID = :ProductID'
      'order by transdate , pqaId , transId, translineId')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ProductID'
      end>
