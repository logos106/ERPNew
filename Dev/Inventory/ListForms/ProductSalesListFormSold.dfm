  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'PC.ID AS LinesID,'
      'PC.Classname as Class,'
      'PC.ClassId as ClassId,'
      'P.PartsID AS PartsID,'
      'P.PREFEREDSUPP as PREFEREDSUPP,'
      'PartName AS PartName,'
      'PARTSDESCRIPTION AS Description,'
      'PT.TypeDesc AS PartType,'
      'Sum(SL.QtySold) as OnOrder,'
      'Sum(SL.Shipped) AS Shipped,'
      'Avg(SL.LinePrice) AS "Avg Price",'
      'Avg(SL.LineCost) AS "Avg Cost",'
      
        'if (Sum(SL.LineCost) > 0, (Sum(SL.LinePrice)-Sum(SL.LineCost))  ' +
        '/ Sum(SL.LineCost) * 100, 0) AS "Markup",'
      
        '(Sum(SL.LinePrice-SL.LineCost))  / Sum(SL.LinePrice) * 100 AS "M' +
        'argin",'
      
        'Left(SUBSTRING_INDEX(P.ProductGroup,'#39'^'#39',1),255) AS "FirstColumn"' +
        ','
      
        'Left(substring(substring_index(P.ProductGroup,'#39'^'#39',2),char_length' +
        '(substring_index(P.ProductGroup,'#39'^'#39',1))+2),255) AS "SecondColumn' +
        '",'
      
        'Left(substring(substring_index(P.ProductGroup,'#39'^'#39',3),char_length' +
        '(substring_index(P.ProductGroup,'#39'^'#39',2))+2),255) AS "ThirdColumn"' +
        ','
      
        'Round(Sum( if(PQA.Active ="F" , 0,  if(PQA.TransType='#39'TSalesOrde' +
        'rLine'#39' or PQA.IsBO ="T",0, if(PQA.TransType = '#39'TPOSLaybyLines'#39' a' +
        'nd PQA.Reserved = "T" ,0 , if(PQA.alloctype="IN"  , ifnull(PQA.Q' +
        'TY,0), 0-ifnull(PQA.QTY,0)))))),5) as InStockQty,'
      
        'Round(Sum( if(PQA.Active ="F" and  PQA.alloctype ="IN" and PQA.T' +
        'ransType in ('#39'TProcTreePart'#39','#39'TProcTreePartIN'#39'), ifnull(PQA.QTY,' +
        '0),  if((PQA.Active ="F") and  (not(PQA.TransType in ('#39'TProcTree' +
        'Part'#39','#39'TProcTreePartIN'#39')) OR ((PQA.TransType='#39'TProcTreePart'#39') an' +
        'd (PQA.alloctype ="IN")) ), 0,  if(PQA.alloctype="IN" and  PQA.I' +
        'SBo="F", ifnull(PQA.QTY,0),  if(PQA.alloctype="OUT" and (PQA.Tra' +
        'nsType<>'#39'TSalesOrderLine'#39' or PQA.ISBO="F") ,0-ifnull(PQA.QTY,0),' +
        '0))))),5) as AvailableQty,'
      
        'Round(Sum(if(PQA.Active ="F" , 0,  if(((PQA.TransType = '#39'TSalesO' +
        'rderLine'#39') OR (PQA.TransType = '#39'TPOSLaybyLines'#39' and PQA.Reserved' +
        ' = "T")) and (PQA.IsBO ="F"),ifnull(PQA.QTY,0), 0))),5) as "Sale' +
        's Orders",'
      
        'Round(Sum(if(PQA.Active ="F" , 0,  if(PQA.TransType='#39'TInvoiceLin' +
        'e'#39' and PQA.IsBO ="T",ifnull(PQA.QTY,0),0))),5) as "Invoice BO",'
      'PC.ReorderPoint AS "Reorder Point",'
      'PC.PreferredLevel AS PreferredLevel,'
      'PC.ReorderAmount AS "Reorder Amount",'
      'P.Attrib1Sale as Attrib1Sale,'
      'P.Attrib2Sale as Attrib2Sale,'
      'P.Attrib1SaleRate as Attrib1SaleRate,'
      'P.Discontinued as Discontinued'
      'From `tblParts` as P'
      'inner join `tblPQA` as PQA on PQA.ProductId = P.PartsID '
      'inner join `tblclass` as C on PQA.departmentID = C.ClassID '
      
        'Inner join `tblProductClasses` as PC on PC.ProductID = PQA.Produ' +
        'ctID and PC.ClassID = PQA.departmentID'
      ''
      'INNER JOIN tblSalesLines SL ON PQA.TransLineID = SL.SaleLineID '
      
        'and PQA.transtype in ('#39'TInvoiceLine'#39','#39'TSalesOrderline'#39','#39'TRefundS' +
        'aleLine'#39','#39'TPOSCashSaleline'#39','#39'TPOSLaybylines'#39','#39'TCashSaleLine'#39')'
      'INNER JOIN tblSales S ON S.SaleID = SL.SaleID'
      'INNER JOIN tblparttypes PT ON P.PARTTYPE = PT.TypeCode'
      'WHERE P.Active = "T"'
      'AND S.Deleted <> "T"'
      'AND (PC.ClassID = :ClassID or :ClassID=0)'
      'AND (S.SaleDate Between  :Datefrom And  :Dateto)'
      'GROUP BY PartsID, ClassID'
      'ORDER BY "Weekly Profit" desc')
    OnCalcFields = qryMainCalcFields
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ClassID'
      end
      item
        DataType = ftUnknown
        Name = 'ClassID'
      end
      item
        DataType = ftUnknown
        Name = 'Datefrom'
      end
      item
        DataType = ftUnknown
        Name = 'Dateto'
      end>
