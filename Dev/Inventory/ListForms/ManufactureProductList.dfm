  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select P.* ,'
      
        'replace(group_concat(Convert(TreeNodeCaption(PT.Level, PT.captio' +
        'n ), char(255))),'#39','#39' , '#39'\n'#39') Tree'
      'from '
      'tblparts P'
      
        'inner join tblproctree PT on PT.MasterId = P.PartsId and PT.Mast' +
        'ertype = '#39'mtProduct'#39'# and ifnull(Pt.ParentId,0)  = 0'
      'group by P.PartsId')
