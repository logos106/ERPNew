  object Qrysub: TERPQuery
    Connection = MyConnection1
    SQL.Strings = (
      'select '
      'P.partsID,'
      'Convert(TreeNodeCaption(PT.Level, PT.caption ), char(255)) tree,'
      'PT.TreePartUOMQuantity, '
      'PT.TreePartUOMTotalQty, '
      
        'concat(PT.TreePartUOM , '#39' ('#39' , PT.TreePartUOMMultiplier, '#39')'#39') as' +
        ' UOM,'
      'PT.Description as Description, '
      'Subp.partsID as SubpartsID,'
      'Subp.partName as SubPartname'
      'from '
      'tblparts P'
      
        'inner join tblproctree PT on PT.MasterId = P.PartsId and PT.Mast' +
        'ertype = '#39'mtProduct'#39
      'Left join tblparts subp on PT.partsId = Subp.partsID'
      'order by P.partsID , PT.SequenceDown, PT.ParentId')
    FilterOptions = [foCaseInsensitive, foNoPartialCompare]
