  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'AP.id as Id,'
      'AP.ProductId  as ProductId , '
      'AP.ProductName as ProductName, '
      'AP.AlternateProductID as AlternateProductID, '
      'AP.AlternateProductName as AlternateProductName, '
      'AP.isdefault  as isdefault, '
      'C.classname as classname, '
      
        'Round(Sum( if(PQA.TransType ="TSalesOrderInvoiceLines"  and PQA.' +
        'Active ="F" , 0,   if(PQA.TransType ="TSalesOrderInvoiceLines"  ' +
        'and PQA.Active ="T" , ifnull(PQA.QTY,0),  if(PQA.Active ="F" and' +
        ' not (PQA.TransType ='#39'TPRocProgressIN'#39') , 0,  if(PQA.TransType =' +
        #39'TSalesOrderLine'#39' or ifnull(PQA.IsBO , "F") ="T",0, if(PQA.Trans' +
        'Type ='#39'TPOSLaybyLines'#39' and PQA.Reserved = "T" ,0 , if(PQA.TransT' +
        'ype in ('#39'TStockMovementLines'#39')  and PQA.Active ="T"  and PQA.all' +
        'octype ="IN" , ifnull(PQA.QTY,0) , if(PQA.TransType in ('#39'TStockM' +
        'ovementLines'#39')  and PQA.Active ="T"  and PQA.alloctype ="OUT" , ' +
        '0-ifnull(PQA.QTY,0) , if(PQA.TransType in ("THireLines")    and ' +
        'PQA.alloctype  ="OUT" and PQA.Active ="T" , 0-ifnull(PQA.QTY,0) ' +
        ', if(PQA.TransType in ("THireLinesIN")    and PQA.alloctype  ="I' +
        'N" and PQA.Active ="T" , ifnull(PQA.QTY,0) , if(PQA.alloctype ="' +
        'IN"  , ifnull(PQA.QTY,0), 0-ifnull(PQA.QTY,0)))))))))))),5) as I' +
        'nstockQty, '
      'P.Price1 as Price1,'
      'P.Cost1 as Cost1,'
      'P.PREFEREDSUPP as PREFEREDSUPP,'
      'concat(PB.BinLocation , "-" , PB.BinNumber) as Defaultbin,'
      'P.active as AlternateProductActive'
      'from `tblParts` as P'
      'LEFT join `tblPQA` as PQA on PQA.ProductId = P.PartsID '
      'LEFT join `tblclass` as C on PQA.departmentID = C.ClassID '
      
        'LEFT join `tblProductClasses` as PC on PC.ProductID = PQA.Produc' +
        'tID and PC.ClassID = PQA.departmentID'
      ' and PQA.departmentId = '#39#39
      'Left join tblproductbin PB on PB.BinID = PC.DefaultbinId'
      
        'inner join tblpartsalternate  AP on AP.AlternateProductID = P.PA' +
        'RTSID '
      'Group by P.partsId, PQa.departmentID'
      'Order by Productname, classname')
