  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT "S" as Category, '
      'S.SaleDate as SaleDate,'
      'SL.Product_Description,'
      'SL.Product_Description_Memo,'
      'Left(SUBSTRING_INDEX(P.ProductGroup,'#39'^'#39',1),255) as firstColumn,'
      
        'Left(substring(substring_index(P.ProductGroup,'#39'^'#39',2),char_length' +
        '(substring_index(P.ProductGroup,'#39'^'#39',1))+2),255) as Secondcolumn,'
      
        'Left(substring(substring_index(P.ProductGroup,'#39'^'#39',3),char_length' +
        '(substring_index(P.ProductGroup,'#39'^'#39',2))+2),255) as Thirdcolumn,'
      ' Trim(SL.ProductName) as ProductName , '
      ' Space(30) as TransactionType, '
      'Space(30) as formname, '
      ' null as InvoiceNo, '
      ' S.CustomerName as CustomerName, '
      ' Round(Sum(SL.Shipped), 5) as Qty, '
      ' Space(1) as UnitOfMeasure, '
      ' Sum(SL.TotalLineAmount) as `TotalAmountEx`, '
      
        ' Round(Sum(SL.TotalLineAmount) - SUM(SL.LineCost*SL.Shipped),2) ' +
        'as `TotalProfitEx`, '
      
        ' Round(Sum(SL.TotalLineAmountInc) - SUM(SL.LineCostInc*SL.Shippe' +
        'd),2) as `TotalProfitInc`, '
      ' Sum(SL.TotalLineAmountInc) as `TotalAmountInc`, '
      ' SL.TotalLineAmount as GrossProfitTotal, '
      ' Round(SL.LineCost*SL.Shipped,2) as `LineCostEx`,'
      ' 0 as TransactionNo, '
      ' SL.ProductID as ProductId, '
      ' "" as EmployeeName, '
      ' SPACE(20) as Attrib1Sale, '
      ' SPACE(20) as Attrib2Sale,  '
      ' SPACE(20) as Attrib1SaleRate, '
      ' SPACE(20) as ProductDescription, '
      ' "F" as IsSpecial, '
      'S.SalesCategory as SalesCategory,'
      ' 0 as Details ,  '
      ' 0 as SaleLineID,  '
      'CT.TypeName as TypeName,'
      '"F" as Converted,'
      'P.CUSTFLD1,'
      'P.CUSTFLD2,'
      'P.CUSTFLD3,'
      'P.CUSTFLD4,'
      'P.CUSTFLD5,'
      'P.CUSTFLD6,'
      'P.CUSTFLD7,'
      'P.CUSTFLD8,'
      'P.CUSTFLD9,'
      'P.CUSTFLD10,'
      'P.CUSTFLD11,'
      'P.CUSTFLD12,'
      'P.CUSTFLD13,'
      'P.CUSTFLD14,'
      'P.CUSTFLD15,'
      'P.CUSTDATE1,'
      'P.CUSTDATE2,'
      'P.CUSTDATE3'
      ' FROM tblSales as S'
      ' INNER   JOIN tblSalesLines  as SL USING(SaleID) '
      ' INNER   JOIN tblparts       as P ON PARTSID = ProductID '
      ' INNER   JOIN tblclients     as C ON C.ClientID=S.ClientID '
      ' LEFT JOIN tblClientType CT ON C.ClientTypeID=CT.ClientTypeID '
      
        ' LEFT    JOIN tblEmployees ON (S.EmployeeName = Concat(tblEmploy' +
        'ees.FirstName," ", tblEmployees.LastName)) '
      
        'Where  (S.IsPOS ="T" or S.IsRefund ="T"   or S.IsCashSale="T" or' +
        ' S.IsInvoice="T"  or S.IsSalesOrder="T" or S.IsLayby ="T")  AND ' +
        'SL.Invoiced="T" AND S.Deleted ="F"  AND S.IsQuote="F"  AND S.con' +
        'verted = "F"  AND S.SaleDate Between   :Fromdate  AND   :TODate ' +
        ' AND S.ClassID Like :ClassId'
      ' GROUP BY SL.ProductID')
    OnCalcFields = qryMainCalcFields
    Options.FlatBuffers = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Fromdate'
      end
      item
        DataType = ftUnknown
        Name = 'TODate'
      end
      item
        DataType = ftUnknown
        Name = 'ClassId'
      end>
