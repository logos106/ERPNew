  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'PARTSID as PARTSID,'
      'PARTNAME as ProductName,'
      
        'SUBSTRING_INDEX( Concat_WS("^(",PRODUCTGROUP,"") ,"^",1) AS Firs' +
        'tColumn, '
      
        'MID(SUBSTRING_INDEX( Concat_WS("^",PRODUCTGROUP,"") ,"^",2),Leng' +
        'th(SUBSTRING_INDEX( Concat_WS("^",PRODUCTGROUP,"") ,"^",1))+2,Le' +
        'ngth(SUBSTRING_INDEX( Concat_WS("^",PRODUCTGROUP,"") ,"^",2))-Le' +
        'ngth(SUBSTRING_INDEX( Concat_WS("^",PRODUCTGROUP,"") ,"^",1))) A' +
        'S SecondColumn,'
      
        'MID(SUBSTRING_INDEX( Concat_WS("^",PRODUCTGROUP,"") ,"^",3),Leng' +
        'th(SUBSTRING_INDEX( Concat_WS("^",PRODUCTGROUP,"") ,"^",2))+2,Le' +
        'ngth(SUBSTRING_INDEX( Concat_WS("^",PRODUCTGROUP,"") ,"^",3))-Le' +
        'ngth(SUBSTRING_INDEX( Concat_WS("^",PRODUCTGROUP,"") ,"^",2))) A' +
        'S ThirdColumn,'
      'PARTSDESCRIPTION as SalesDescription,'
      'PURCHASEDESC as PURCHASEDESC,'
      'Round(PRICE1 + (PRICE1 * tc.Rate), 2) AS PriceInc,'
      'PartType,'
      'CUSTFLD1,'
      'CUSTFLD2,'
      'CUSTFLD3,'
      'CUSTFLD4,'
      'CUSTFLD5,'
      'CUSTFLD6,'
      'CUSTFLD7,'
      'CUSTFLD8,'
      'CUSTFLD9,'
      'CUSTFLD10,'
      'CUSTFLD11,'
      'CUSTFLD12,'
      'CUSTFLD13,'
      'CUSTFLD14,'
      'CUSTFLD15,'
      'CUSTDATE1,'
      'CUSTDATE2,'
      'CUSTDATE3, '
      'P.BARCODE,'
      
        'if(ifnull(PUOM.UnitName,'#39#39') = '#39#39' , '#39'Units (1)'#39' , concat(PUOM.Uni' +
        'tName,'#39'('#39', PUOM.Multiplier ,'#39')'#39')) as PurchaseUOM,'
      
        'if(ifnull(sUOM.UnitName,'#39#39')='#39#39' ,  '#39'Units (1)'#39' ,concat(sUOM.UnitN' +
        'ame,'#39'('#39', sUOM.Multiplier ,'#39')'#39')) as SalesUOM'
      'FROM tblparts p'
      
        'LEFT JOIN tbltaxcodes AS tc ON (tc.Name = p.TAXCODE) AND (tc.Reg' +
        'ionID = 1)'
      
        'Left join tblunitsofmeasure puom on pUOM.PartID=P.PartsID and  p' +
        'uom.PurchasesDefault = '#39'T'#39' and puom.Active ="T"'
      
        'Left join tblunitsofmeasure suom on suom.PartID=P.PartsID and  s' +
        'uom.SalesDefault= '#39'T'#39' and suom.Active ="T"'
      
        'Where (p.Active="T") AND  char_length(p.PARTNAME) > 0 AND not Is' +
        'Null(p.PARTNAME)')
