  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'PQA.transID as TransID,'
      'PQA.ProductName as ProductName,'
      'PQAT.Description as Transtype,'
      
        'Round(Sum(if(PQA.Active ="F" , 0,  if(PQA.TransType='#39'TPurchaseOr' +
        'derLine'#39' and PQA.IsBO ="T",ifnull(PQA.QTY,0),0))),5) as   Qty, '
      'PO.PurchaseOrderID as PurchaseOrderID, '
      's.saleID as SaleId , '
      'PO.Deleted as PODeleted,'
      'S.Deleted as SalesDeleted,'
      'PO.Cancelled as POCancelled,'
      'S.Cancelled as SalesCancelled'
      'FROM `tblParts` as P'
      'inner join `tblPQA` as PQA on PQA.ProductId = P.PartsID '
      'inner join `tblclass` as C on PQA.departmentID = C.ClassID '
      
        'Inner join `tblProductClasses` as PC on PC.ProductID = PQA.Produ' +
        'ctID and PC.ClassID = PQA.departmentID'
      
        'inner join tblpqatranstypes PQAT on PQAT.TransType = PQa.Transty' +
        'pe '
      
        'Left join tblpurchaseorders PO on PQa.transId = PO.PurchaseOrder' +
        'ID and PQA.transtype ='#39'TPurchaseOrderLine'#39
      
        'Left join tblsales S on PQa.transId = S.SaleID and PQA.transtype' +
        '  in ('#39'TInvoiceLine'#39' , '#39'TSalesOrderline'#39')'
      'WHERE PC.ProductID= 9319'
      'group by PQA.transID')
    Active = True
