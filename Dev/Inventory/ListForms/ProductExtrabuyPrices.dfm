  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      '`C`.`Company`,'
      '`EP`.`GlobalRef`,'
      '`EP`.`ExtraPartPriceID`,'
      '`P`.`PartsID`,'
      '`EP`.`SupplierID`,'
      '`EP`.`Price`,'
      '`P`.`PartName`,'
      '`EP`.`Buy1`,'
      '`EP`.`SupplierProductCode`,'
      '`EP`.`SupplierProductName`,'
      '`EP`.`UsePrice`,'
      '`EP`.`PrintedValue1`,'
      '`EP`.`msTimeStamp`,'
      '`EP`.`UOM`,'
      '`EP`.`UOMID`,'
      '`EP`.`UOMMultiplier`'
      'FROM '
      'tblparts p '
      'Left join (`tblextraprice` AS `EP` '
      
        'Inner Join `tblclients` AS `C` ON `EP`.`SupplierID` = `C`.`Clien' +
        'tID`) on EP.PartsID = P.partsId'
      'Where (:AllProducts="T" or ifnull(EP.ExtraPartPriceID,0)<>0)')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'AllProducts'
      end>
