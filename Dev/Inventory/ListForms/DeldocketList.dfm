      'SL.Shipdate,'
      'PARTNAME as '#39'Product Name'#39', '
      
        'LEFT(if((LOCATE('#39'^'#39',P.PRODUCTGROUP,InStr(P.PRODUCTGROUP,'#39'^'#39')+1))' +
        ' >0, MID(P.PRODUCTGROUP,LOCATE('#39'^'#39',P.PRODUCTGROUP, InStr(P.PRODU' +
        'CTGROUP,'#39'^'#39')+1)+1,   char_length(P.PRODUCTGROUP)- LOCATE('#39'^'#39',P.P' +
