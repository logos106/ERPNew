        'rdColumn'#39' ,'
      
        'LEFT(MID(P.PRODUCTGROUP,InStr(P.PRODUCTGROUP,'#39'^'#39')+1 ,   If(LOCAT' +
        'E('#39'^'#39',P.PRODUCTGROUP,InStr(P.PRODUCTGROUP,'#39'^'#39')+1) - InStr(P.PROD' +
        'UCTGROUP,'#39'^'#39') >0,   LOCATE('#39'^'#39',P.PRODUCTGROUP,InStr(P.PRODUCTGRO' +
        'UP,'#39'^'#39')+1)-1 - InStr(P.PRODUCTGROUP,'#39'^'#39'),    IF(InStr(P.PRODUCTG' +
        'ROUP,'#39'^'#39')<>0,char_length(P.PRODUCTGROUP) - InStr(P.PRODUCTGROUP,' +
        #39'^'#39'),0))),255)   AS '#39'SecondColumn'#39' ,   '
      
        'LEFT(If(InStr(P.PRODUCTGROUP,'#39'^'#39') -1 >0,Left(P.PRODUCTGROUP,InSt' +
        'r(P.PRODUCTGROUP,'#39'^'#39')-1),   P.PRODUCTGROUP),255) AS '#39'FirstColumn' +
        #39' ,'
      'P.PARTSDESCRIPTION as '#39'Sales Description'#39',   '
      'S.SaleID, '
      'S.SaleDate AS '#39'Sale Date'#39',   '
      'if(S.isInvoice = '#39'T'#39' ,'#39'Invoice'#39'  ,'#39'Sales Order'#39' )  AS Type,   '
      'S.Shipping, '
      'S.Comments , '
      'if(isnull(tblPrintDocs.docId) , '#39'F'#39' , '#39'T'#39') PrintFlag, '
      'tblEmployees.EmployeeName PrintedBy ,'
      'P.Area'
      'FROM tblparts AS P '
      'INNER JOIN tblSalesLines AS SL ON P.PartsID = SL.ProductID '
      'INNER JOIN tblSales AS S ON S.SaleID = SL.SaleID '
      
        'left join tblPrintDocs on tblPrintDocs.docId = SL.SaleId and tbl' +
        'PrintDocs.Doctype = '#39'dtDelDock'#39' and Printedfrom = '#39'TDeldocketLis' +
        'tGUI'#39
      
        'LEFT JOIN tblEmployees on tblEmployees.employeeId = tblprintdocs' +
        '.PrintedBy '
      'inner join tblPQA on  tblPQA.TransLineId = SL.SaleLineId'
      
        'WHERE ifnull(UnitofMeasureQtySold,0)<>0 and  ((S.IsInvoice = '#39'T'#39 +
        ')  or (S.ISSalesorder = '#39'T'#39' and S.converted = '#39'F'#39'))'
      ' and s.shipdate Between :dtfrom and :dtto '
      ' and S.ClassID = :ClassID'
      ' and  tblPQA.Active = "T" '
      'ORDER BY S.SaleID desc, Type')
