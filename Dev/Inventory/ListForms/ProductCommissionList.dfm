  inherited qryMain: TERPQuery
    SQL.Strings = (
      
        'SELECT PC.*, concat_Ws('#39' '#39' , E.`Title`,E.`FirstName`,E.`MiddleNa' +
        'me`,E.`LastName`) as employeename,'
      
        'LEFT(SUBSTRING_INDEX( Concat_WS("^(",P.PRODUCTGROUP,"") ,"^",1),' +
        '255) AS Manufacturer,'
      
        'LEFT(MID(SUBSTRING_INDEX( Concat_WS("^",P.PRODUCTGROUP,"") ,"^",' +
        '2),char_length(SUBSTRING_INDEX( Concat_WS("^",P.PRODUCTGROUP,"")' +
        ' ,"^",1))+2,char_length(SUBSTRING_INDEX( Concat_WS("^",P.PRODUCT' +
        'GROUP,"") ,"^",2))-char_length(SUBSTRING_INDEX( Concat_WS("^",P.' +
        'PRODUCTGROUP,"") ,"^",1))),255) AS Type,'
      
        'LEFT(MID(SUBSTRING_INDEX( Concat_WS("^",P.PRODUCTGROUP,"") ,"^",' +
        '3),char_length(SUBSTRING_INDEX( Concat_WS("^",P.PRODUCTGROUP,"")' +
        ' ,"^",2))+2,char_length(SUBSTRING_INDEX( Concat_WS("^",P.PRODUCT' +
        'GROUP,"") ,"^",3))-char_length(SUBSTRING_INDEX( Concat_WS("^",P.' +
        'PRODUCTGROUP,"") ,"^",2))),255) AS Dept'
      ' '
      'FROM tblProductCommission PC'
      'inner join  tblParts P on PC.PartsId = P.PartsId'
      'Left join tblemployees E on E.EmployeeId = PC.employeeID'
      'WHERE  ((:Active = "Both") OR '
      '     (:Active = "Active" AND PC.Active = "T") OR '
      '     (:Active = "Not Active" AND PC.Active <> "T"))'
      'ORDER BY P.PartName')
    ParamData = <
      item
        DataType = ftString
        Name = 'Active'
        Value = ''
      end
      item
        DataType = ftString
        Name = 'Active'
        Value = ''
      end
      item
        DataType = ftString
        Name = 'Active'
        Value = ''
      end>
