  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select'
      'PartsId , Partname  , Parttype, '
      
        'if(ifnull(PU.UnitId,0) =0 , concat(U.Unitname  , '#39'('#39' , U.Multipl' +
        'ier , '#39')'#39') , concat(PU.Unitname  , '#39'('#39' , PU.Multiplier , '#39')'#39') ) ' +
        'as PurchaseUOM ,'
      
        'if(ifnull(SU.UnitId,0) =0 , concat(U.Unitname  , '#39'('#39' , U.Multipl' +
        'ier , '#39')'#39') , concat(SU.Unitname  , '#39'('#39' , SU.Multiplier , '#39')'#39') ) ' +
        'as SalesUOM ,'
      
        'CAST(if(ifnull(PU.UnitId,0) =0 , U.UnitId , PU.UnitId) AS UNSIGN' +
        'ED) as PurchaseUOMID ,'
      
        'CAST(if(ifnull(SU.UnitId,0) =0 , U.UnitId , SU.UnitId) AS UNSIGN' +
        'ED) as SalesUOMID'
      'from tblunitsofmeasure U, tblParts P '
      
        'Left join tblunitsofmeasure PU on ifnull(PU.PartID,0) = P.PartsI' +
        'd and PU.PurchasesDefault ="T"'
      
        'Left join tblunitsofmeasure SU on ifnull(SU.PartID,0) = P.PartsI' +
        'd and SU.SalesDefault ="T"'
      
        'where U.UnitName = :DefaultUOM  and U.Multiplier =1  and ifnull(' +
        'U.partId ,0) = 0 '
      'Order by Partname')
    ParamData = <
      item
        DataType = ftString
        Name = 'DefaultUOM'
      end>
