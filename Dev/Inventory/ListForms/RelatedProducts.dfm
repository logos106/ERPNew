  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'P.partsID as PartsId , '
      '`p`.`PARTNAME` as PARTNAME,'
      '`rp`.`RelatedID`,'
      '`rp`.`PartName` relatedProduct,'
      '`rp`.`Qty`,'
      '`rp`.`ParentQty`,'
      '`rp`.`HideRelated`,'
      '`rp`.`IncurAmount`,'
      '`rp`.`ExtraQty`,'
      '`rp`.`UseIncur`,'
      '`rp`.`IncurAfterTax` , '
      'rp.ParentID,'
      'rp.ProductId, '
      'RP.CanDelete as CanDelete'
      'FROM `tblrelatedparts` AS `rp`'
      'Inner Join `tblparts` AS `p` ON `rp`.`ParentID` = `p`.`PARTSID`')
