  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select 1 as Category , '
      'P.partsId as PartsId,'
      'P.Partname as Partname,'
      'concat(AP.ProductName, "'#39's Alternate Products") as description, '
      'AP.AlternateProductID as AlternateProductID, '
      'AP.AlternateProductName as AlternateProductName, '
      'AP.isdefault  as isdefault, '
      'CL.company as Supplier,'
      
        'convert(ifnull(EP.ExtraPartPriceID,0), unsigned)  as ExtraPriceI' +
        'D'#9','
      'EP.Buy1 as ExtraPriceQuantity,'
      'EP.Price as ExtraPrice,'
      'convert(NULL, DECIMAL) as ForeignPrice,'
      'EP.SupplierProductCode as SupplierProductCode,'
      'EP.SupplierProductName as SupplierProductName,'
      'concat(EP.UOM, "(" , EP.UOMMultiplier, ")") as ExtraPriceUOM,'
      'EP.UOMMultiplier as ExtraPriceUOMMultiplier,'
      'EP.MinimumOrderQty as ExtraPriceMinimumOrderQty  ,'
      'TP.instockQty as instockQty, '
      'P.Price1 as Price1,'
      'P.Cost1 as Cost1,'
      'P.PREFEREDSUPP as PREFEREDSUPP,'
      'TP.Defaultbin as Defaultbin'
      
        ' from tmp_erp_D4F1A9F6_Stock_DFRMTAX51 TP inner join tblparts P ' +
        'on TP.partsId = P.partsID '
      
        'inner join tblpartsalternate  AP on AP.AlternateProductID = P.PA' +
        'RTSID '
      'inner join tblextraprice EP on EP.PartsID = P.partsId'
      'inner join tblclients CL on EP.SupplierID = CL.clientID'
      'Where P.Active <> "F" '
      'union '
      'select 3 as Category , '
      'P.partsId as PartsId,'
      'P.Partname as Partname,'
      'P.partname as description, '
      'NULL as AlternateProductID, '
      'NULL as AlternateProductName, '
      '"F" as isdefault, '
      'null as Supplier,'
      'NULL as ExtraPriceID'#9','
      'NULL as ExtraPriceQuantity,'
      'NULL as ExtraPrice,'
      'NULL as ForeignPrice,'
      'NULL as SupplierProductCode,'
      'NULL as SupplierProductName,'
      'NULL as ExtraPriceUOM,'
      'NULL as ExtraPriceUOMMultiplier,'
      'NULL as ExtraPriceMinimumOrderQty  ,'
      'TP.instockQty as instockQty, '
      'P.Price1 as Price1,'
      'P.Cost1 as Cost1,'
      'P.PREFEREDSUPP as PREFEREDSUPP,'
      'TP.Defaultbin as Defaultbin'
      
        ' from tmp_erp_D4F1A9F6_Stock_DFRMTAX51 TP inner join tblparts P ' +
        'on TP.partsId = P.partsID '
      
        'inner join tblpartsalternate  AP on AP.AlternateProductID = P.PA' +
        'RTSID '
      'Left join tblextraprice EP on EP.PartsID = P.partsId'
      'Left  join tblforeignprice FP on FP.PartsId = P.PartsId'
      
        'Where P.Active <> "F" and ifnull(EP.ExtraPartPriceID,0)=0 and if' +
        'null(FP.ForeignPriceID,0)=0  and P.IsSystemProduct ="F"'
      'union '
      'select 4 as Category , '
      'P.partsId as PartsId,'
      'P.Partname as Partname,'
      'P.partname as description, '
      'NULL as AlternateProductID, '
      'NULL as AlternateProductName, '
      '"F" as isdefault, '
      'CL.company as Supplier,'
      
        'convert(ifnull(EP.ExtraPartPriceID,0), unsigned)  as ExtraPriceI' +
        'D'#9','
      'EP.Buy1 as ExtraPriceQuantity,'
      'EP.Price as ExtraPrice,'
      'NULL  as ForeignPrice,'
      'EP.SupplierProductCode as SupplierProductCode,'
      'EP.SupplierProductName as SupplierProductName,'
      'concat(EP.UOM, "(" , EP.UOMMultiplier, ")") as ExtraPriceUOM,'
      'EP.UOMMultiplier as ExtraPriceUOMMultiplier,'
      'EP.MinimumOrderQty as ExtraPriceMinimumOrderQty  ,'
      'TP.instockQty as instockQty, '
      'P.Price1 as Price1,'
      'P.Cost1 as Cost1,'
      'P.PREFEREDSUPP as PREFEREDSUPP,'
      'TP.Defaultbin as Defaultbin'
      
        ' from tmp_erp_D4F1A9F6_Stock_DFRMTAX51 TP inner join tblparts P ' +
        'on TP.partsId = P.partsID '
      
        'Left join tblpartsalternate  AP on AP.AlternateProductID = P.PAR' +
        'TSID '
      'inner join tblextraprice EP on EP.PartsID = P.partsId'
      'inner join tblclients CL on EP.SupplierID = CL.clientID'
      
        'Where P.Active <> "F" and ifnull(AP.ID,0)=0 and P.IsSystemProduc' +
        't ="F"  '
      'union '
      'select 6 as Category , '
      'P.partsId as PartsId,'
      'P.Partname as Partname,'
      'P.partname as description, '
      'NULL as AlternateProductID, '
      'NULL as AlternateProductName, '
      '"F" as isdefault, '
