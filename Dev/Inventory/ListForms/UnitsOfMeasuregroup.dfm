  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT  '
      'U.UnitID as unitID,'
      'U.UnitName as unitname, '
      'U.Multiplier as Multiplier, '
      'U.UnitDescription as UnitDescription,'
      'U.BaseUnitName as BaseUnitName,'
      'U.`BaseUnitID` as `BaseUnitID`'
      'FROM tblunitsofmeasure U'
      'WHERE U.ACTIVE = '#39'T'#39'  AND ifnull(U.PartID,0) = :xPartID'
      'union all '
      'SELECT  '
      'U.UnitID as UnitID,'
      'U.UnitName as UnitName , '
      'U.Multiplier as Multiplier , '
      'U.UnitDescription as UnitDescription,'
      'U.BaseUnitName as BaseUnitName,'
      'U.`BaseUnitID` as `BaseUnitID`'
      'FROM tblunitsofmeasure U'
      
        'Left join tblunitsofmeasure PU on PU.Unitname = U.unitname and i' +
        'fnull(PU.PartID,0) = :xPartID'
      'WHERE U.ACTIVE = '#39'T'#39'  AND ifnull(U.PartID,0) = 0 '
      'and ifnull(PU.UnitID,0) =0'
      
        '#and Unitname not  in (Select unitname from  tblunitsofmeasure w' +
        'here  ifnull(PartID,0) = :xPartID  )'
      'Order By UnitName;')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'xPartID'
      end
      item
        DataType = ftUnknown
        Name = 'xPartID'
      end>
