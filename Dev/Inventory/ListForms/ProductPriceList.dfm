  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      ' 1                   as Rectype, '
      ' P.partsId           as partsId , '
      ' P.partname          as partname, '
      ' P.Description       as SalesDescription, '
      ' P.PURCHASEDESC      as PurchaseDescription, '
      ' NULL                as UOM , '
      ' NULL                as unitId, '
      ' NULL                as RetailPRICE, '
      ' P.WHOLESALEPRICE    as WHOLESALEPRICE, '
      ' P.COST1             as COST1, '
      ' P.COST2             as COST2, '
      ' P.COST3             as COST3, '
      ' P.COSTINC1          as COSTINC1, '
      ' P.COSTINC2          as COSTINC2, '
      ' P.COSTINC3          as COSTINC3,'
      ' P.PRICE1            as PRICE1  , '
      ' P.PRICE2            as PRICE2,'
      ' P.PRICE3            as PRICE3, '
      ' P.PRICEINC1         as PRICEINC1, '
      ' P.PRICEINC2         as PRICEINC2, '
      ' P.PRICEINC3         as PRICEINC3, '
      ' NULL                as WSP1, '
      ' NULL                as WSP2, '
      ' NULL                as WSP3, '
      ' NULL                as WSP4, '
      'P.CUSTFLD1          as CustFld1, '
      'P.CUSTFLD2          as CustFld2, '
      'P.CUSTFLD3          as CustFld3, '
      'P.CUSTFLD4          as CustFld4, '
      'P.CUSTFLD5          as CustFld5, '
      'P.CUSTFLD6          as CustFld6, '
      'P.CUSTFLD7          as CustFld7, '
      'P.CUSTFLD8          as CustFld8, '
      'P.CUSTFLD9          as CustFld9, '
      'P.CUSTFLD10         as CustFld10, '
      'P.CUSTFLD11         as CustFld11, '
      'P.CUSTFLD12         as CustFld12, '
      'P.CUSTFLD13         as CustFld13, '
      'P.CUSTFLD14         as CustFld14, '
      'P.CUSTFLD15         as CustFld115,'
      'P.CUSTDATE1         as CustDate1, '
      'P.CUSTDATE2         as CustDate2, '
      'P.CUSTDATE3         as CustDate3,'
      'P.Active            as Active '
      'from tblparts  P '
      ' where P.IsSystemProduct ="F" '
      ' union all  '
      'Select Distinct '
      ' 3                   as Rectype, '
      ' P.partsId           as partsId , '
      ' P.partname          as partname, '
      ' P.Description       as SalesDescription, '
      ' P.PURCHASEDESC      as PurchaseDescription, '
      ' concat(UOM.UnitName , "(" , UOM.Multiplier , ")") as UOM , '
      ' UOM.UnitID          as UnitID, '
      ' Retailer.Price1     as RetailPRICE, '
      ' P.WHOLESALEPRICE    as WHOLESALEPRICE, '
      ' P.COST1             as COST1, '
      ' P.COST2             as COST2, '
      ' P.COST3             as COST3, '
      ' P.COSTINC1          as COSTINC1, '
      ' P.COSTINC2          as COSTINC2, '
      ' P.COSTINC3          as COSTINC3,'
      ' P.PRICE1            as PRICE1  , '
      ' P.PRICE2            as PRICE2,'
      ' P.PRICE3            as PRICE3, '
      ' P.PRICEINC1         as PRICEINC1, '
      ' P.PRICEINC2         as PRICEINC2, '
      ' P.PRICEINC3         as PRICEINC3, '
      ' WholeSaler1.Price1  as WSP1, '
      ' WholeSaler2.Price1  as WSP2, '
      ' WholeSaler3.Price1  as WSP3, '
      ' WholeSaler4.Price1  as WSP4, '
      'P.CUSTFLD1          as CustFld1, '
      'P.CUSTFLD2          as CustFld2, '
      'P.CUSTFLD3          as CustFld3, '
      'P.CUSTFLD4          as CustFld4, '
      'P.CUSTFLD5          as CustFld5, '
      'P.CUSTFLD6          as CustFld6, '
      'P.CUSTFLD7          as CustFld7, '
      'P.CUSTFLD8          as CustFld8, '
      'P.CUSTFLD9          as CustFld9, '
      'P.CUSTFLD10         as CustFld10, '
      'P.CUSTFLD11         as CustFld11, '
      'P.CUSTFLD12         as CustFld12, '
      'P.CUSTFLD13         as CustFld13, '
      'P.CUSTFLD14         as CustFld14, '
      'P.CUSTFLD15         as CustFld115,'
      'P.CUSTDATE1         as CustDate1, '
      'P.CUSTDATE2         as CustDate2, '
      'P.CUSTDATE3         as CustDate3, '
      'P.Active            as Active '
      ' From tblParts P '
      ' inner join tblunitsofmeasure UOM on P.PartsId = UOM.PartID '
      
        ' left join tblextrapricesell Retailer on Retailer.UOMID = UOM.Un' +
        'itID and Retailer.PartsID = UOM.PartID and Retailer.ClientTypeID' +
        ' =  1'
      
        ' left join tblextrapricesell WholeSaler1 on WholeSaler1.UOMID = ' +
        'UOM.UnitID and WholeSaler1.PartsID = UOM.PartID and WholeSaler1.' +
        'ClientTypeID =  2'
      
        ' left join tblextrapricesell WholeSaler2 on WholeSaler2.UOMID = ' +
        'UOM.UnitID and WholeSaler2.PartsID = UOM.PartID and WholeSaler2.' +
        'ClientTypeID =  3'
      
        ' left join tblextrapricesell WholeSaler3 on WholeSaler3.UOMID = ' +
        'UOM.UnitID and WholeSaler3.PartsID = UOM.PartID and WholeSaler3.' +
        'ClientTypeID =  4'
      
        ' left join tblextrapricesell WholeSaler4 on WholeSaler4.UOMID = ' +
        'UOM.UnitID and WholeSaler4.PartsID = UOM.PartID and WholeSaler4.' +
        'ClientTypeID =  0'
      '  where P.IsSystemProduct ="F" '
      ' union all  '
      ' Select Distinct '
      ' 4                   as Rectype, '
      ' P.partsId           as partsId , '
      ' P.partname          as partname, '
      ' P.Description       as SalesDescription, '
      ' P.PURCHASEDESC      as PurchaseDescription, '
      ' NULL                as UOM , '
      ' NULL                as UnitID, '
      ' Retailer.Price1     as RetailPRICE, '
      ' P.WHOLESALEPRICE    as WHOLESALEPRICE, '
      ' P.COST1             as COST1, '
      ' P.COST2             as COST2, '
      ' P.COST3             as COST3, '
      ' P.COSTINC1          as COSTINC1, '
      ' P.COSTINC2          as COSTINC2, '
      ' P.COSTINC3          as COSTINC3,'
      ' P.PRICE1            as PRICE1  , '
      ' P.PRICE2            as PRICE2,'
      ' P.PRICE3            as PRICE3, '
      ' P.PRICEINC1         as PRICEINC1, '
      ' P.PRICEINC2         as PRICEINC2, '
      ' P.PRICEINC3         as PRICEINC3, '
      ' WholeSaler1.Price1  as WSP1, '
      ' WholeSaler2.Price1  as WSP2, '
      ' WholeSaler3.Price1  as WSP3, '
      ' WholeSaler4.Price1  as WSP4, '
      ' P.CUSTFLD1          as CustFld1, '
      'P.CUSTFLD2          as CustFld2, '
      'P.CUSTFLD3          as CustFld3, '
      'P.CUSTFLD4          as CustFld4, '
      'P.CUSTFLD5          as CustFld5, '
      'P.CUSTFLD6          as CustFld6, '
      'P.CUSTFLD7          as CustFld7, '
      'P.CUSTFLD8          as CustFld8, '
      'P.CUSTFLD9          as CustFld9, '
      'P.CUSTFLD10         as CustFld10, '
      'P.CUSTFLD11         as CustFld11, '
      'P.CUSTFLD12         as CustFld12, '
      'P.CUSTFLD13         as CustFld13, '
      'P.CUSTFLD14         as CustFld14, '
      'P.CUSTFLD15         as CustFld115,'
      'P.CUSTDATE1         as CustDate1, '
      'P.CUSTDATE2         as CustDate2, '
      'P.CUSTDATE3         as CustDate3, '
      'P.Active            as Active '
      'From tblParts P '
      
        ' LEFT JOIN tblextrapricesell Retailer ON (IFNULL(Retailer.UOMID,' +
        '0)=0 AND Retailer.PartsId = P.partsID) AND Retailer.ClientTypeID' +
        ' = 1'
      
        ' LEFT JOIN tblextrapricesell WholeSaler1 ON (IFNULL(WholeSaler1.' +
        'UOMID,0)=0 AND WholeSaler1.PartsId = P.partsID) AND WholeSaler1.' +
        'ClientTypeID = 2'
      
        ' LEFT JOIN tblextrapricesell WholeSaler2 ON (IFNULL(WholeSaler2.' +
        'UOMID,0)=0 AND WholeSaler2.PartsId = P.partsID) AND WholeSaler2.' +
        'ClientTypeID =  3'
      
        ' LEFT JOIN tblextrapricesell WholeSaler3 ON (IFNULL(WholeSaler3.' +
        'UOMID,0)=0 AND WholeSaler3.PartsId = P.partsID) AND WholeSaler3.' +
        'ClientTypeID =   4'
      
        ' LEFT JOIN tblextrapricesell WholeSaler4 ON (IFNULL(WholeSaler4.' +
        'UOMID,0)=0 AND WholeSaler4.PartsId = P.partsID) AND WholeSaler4.' +
        'ClientTypeID =  0'
      ' where P.IsSystemProduct ="F" '
      ' Order by Partname , UOM')
