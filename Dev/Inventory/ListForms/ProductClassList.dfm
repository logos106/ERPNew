  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'PC.GlobalRef as GlobalRef,'
      'PC.ID as ID,'
      'PC.ProductID as ProductID,'
      'PC.ProductName as ProductName,'
      'PC.ProductGroup as ProductGroup,'
      'PC.ClassID as ClassID,'
      'PC.ClassName as ClassName,'
      'PC.ReOrderPoint as ReOrderPoint,'
      'PC.PreferredLevel as PreferredLevel,'
      'PC.ReOrderAmount as ReOrderAmount,'
      'PC.LastAdj as LastAdj,'
      'PC.LastAdjAccount as LastAdjAccount,'
      'PC.ClassPriceVariance as ClassPriceVariance,'
      'PC.VariantPrice as VariantPrice,'
      'PC.NextStocktakeDate as NextStocktakeDate,'
      'PC.Active as Active,'
      'PB.binlocation as Defaultbinlocation, '
      'PB.binnumber as defaultbinnumber,'
      'P.Description AS SalesDescription,'
      'P.COST1 ,'
      'P.COSTINC1,'
      'P.BuyQTY1,'
      'P.COST2 ,'
      'P.COSTINC2,'
      'P.BuyQTY2,'
      'P.COST3 ,'
      'P.COSTINC3,'
      'P.BuyQTY3,'
      'P.Price1 ,'
      'P.PriceINC1,'
      'P.SellQTY1,'
      'P.Price2 ,'
      'P.PriceINC2,'
      'P.SellQTY2,'
      'P.Price3 ,'
      'P.PriceINC3,'
      'P.SellQTY3,'
      'P.BARCODE Primarybarcode,'
      'P.active ProductActive,'
      'PC.active ProductClassActive,'
      
        'if(P.msTimeStamp > PC.MsTimeStamp , P.MsTimeStamp , PC.MsTimeSta' +
        'mp) AS MsTimeStamp,'
      
        'if(P.msTimeStamp > PC.MsTimeStamp AND trim(P.msUpdateSiteCode)<>' +
        ' "", P.MsUpdateSiteCode , PC.MsUpdateSiteCode) AS MsUpdateSiteCo' +
        'de'
      'FROM tblparts P'
      'INNER JOIN tblProductclasses PC ON P.PARTSID = PC.productId'
      'Left join tblProductBin PB on PB.binId= PC.defaultBinId'
      'order by `ClassName`,`ProductName`')
