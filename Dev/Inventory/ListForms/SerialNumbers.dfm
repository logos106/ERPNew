  inherited qryMain: TERPQuery
    SQL.Strings = (
      '#Tempqry - see refreshQuery'
      'SELECT distinct '
      'P.PartsId as PartsId, '
      'PC.ClassId as DepartmentID,'
      'PQA.UOMID as UOMID, '
      'ifNull(PQA.UOM, '#39'Units'#39') as UOM,'
      'ifnull(PQA.UOMMultiplier, 1) as UOMMultiplier,'
      '3 as gLevel, '
      'P.PartName as ProductName ,'
      'SUBSTRING_INDEX(P.ProductGroup,'#39'^'#39',1) as ProductColumn1,'
      
        'substring(substring_index(P.ProductGroup,'#39'^'#39',2),char_length(subs' +
        'tring_index(P.ProductGroup,'#39'^'#39',1))+2) as ProductColumn2,'
      
