  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select  '
      '1 Seqno ,'
      'SL.SmartOrderID TransID, '
      'SL.SmartOrderLinesID translineId,  '
      '"Smart Order" Category , '
      'SO.CreationDate TransDate, '
      'SO.SmartOrderDesc Description, '
      'SL.PARTSID, '
      'SL.PARTSNAME  , '
      
        'Concat(SL.UnitofMeasure, " (" ,SL.UnitofMeasureMultiplier,")" ) ' +
        'as  UOM , '
      'Sum(SL.UnitofMeasureQty) Qty,    '
      'SL.FormulaQtyValue1 , '
      'SL.FormulaQtyValue2 , '
      'SL.FormulaQtyValue3, '
      'SL.FormulaQtyValue4, '
      'SL.FormulaQtyValue5  '
      
        'from  tblsmartorderlines SL inner join tblsmartorder SO  on SL.S' +
        'martOrderID = SO.SmartOrderID '
      'where Sl.converted = "F"  '
      
        'group by SL.FormulaQtyValue1 , SL.FormulaQtyValue2 , SL.FormulaQ' +
        'tyValue3, SL.FormulaQtyValue4, SL.FormulaQtyValue5 , SL.UnitOfMe' +
        'asureID')
    Active = True
