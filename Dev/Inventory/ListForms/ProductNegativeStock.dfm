        'n ('#39'TProcTreePart'#39','#39'TProcTreePartIN'#39','#39'TProcTreePartINDONE'#39'), ifn' +
        'ull(PQA.QTY,0),if(PQA.TransType = '#39'TSalesOrderInvoiceLines'#39' and ' +
        'PQA.alloctype ="IN" and PQA.Active ="T", 0-ifnull(PQA.QTY,0),0 )' +
        ') as BuildingQty,'
      
        'if(PQA.Active ="T" and  PQA.alloctype  ="IN" and PQA.TransType i' +
        'n ('#39'TProcTreePart'#39'), ifnull(PQA.QTY,0),0) as BuiltQty,'
      
        'if(PQA.Active ="T" and  PQA.alloctype  ="IN" and PQA.TransType i' +
        'n ('#39'TProcTreePartIN'#39'), ifnull(PQA.QTY,0),0) as SubBuiltQty,'
      
        'if(PQA.Active ="T" and  PQA.alloctype  ="OUT" and PQA.TransType ' +
        'in ('#39'TProcTreePart'#39'), ifnull(PQA.QTY,0),0) as USedForBuildQty,'
      
        ' if(PQA.TransType ="TSalesOrderInvoiceLines"  and PQA.Active ="F' +
        '" , 0,   if(PQA.TransType ="TSalesOrderInvoiceLines"  and PQA.Ac' +
        'tive ="T" , ifnull(PQA.QTY,0),  if(PQA.Active ="F" and not (PQA.' +
        'TransType in ('#39'TPRocProgressIN'#39','#39'TPRocProgressOUT'#39')) , 0,  if(PQ' +
        'A.TransType ='#39'TSalesOrderLine'#39' or ifnull(PQA.IsBO , "F") ="T",0,' +
        ' if(PQA.TransType ='#39'TPOSLaybyLines'#39' and PQA.Reserved = "T" ,0 , ' +
        'if(PQA.TransType in ('#39'TStockMovementLines'#39')  and PQA.Active ="T"' +
