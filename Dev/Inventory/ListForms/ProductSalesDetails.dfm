  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT "S" as Category,  '
      'S.SaleDate as SaleDate,'
      'Left(SUBSTRING_INDEX(P.ProductGroup,'#39'^'#39',1),255) as firstColumn,'
      
        'Left(substring(substring_index(P.ProductGroup,'#39'^'#39',2),char_length' +
        '(substring_index(P.ProductGroup,'#39'^'#39',1))+2),255) as Secondcolumn,'
      
        'Left(substring(substring_index(P.ProductGroup,'#39'^'#39',3),char_length' +
        '(substring_index(P.ProductGroup,'#39'^'#39',2))+2),255) as Thirdcolumn,'
      ' Trim(SL.ProductName) as ProductName , '
      
        ' ifnull(TT.Description , if(S.IsInvoice="T" , "Invoice" , if(iss' +
        'alesorder = "T" , "Sales Order" , if(iscashSale="T" , "Cash Sale' +
        '" , if(isRefund = "T" , "Refund" , if(isLayby="T", "Layby" , "")' +
        '))))) as TransactionType,'
      
        ' ifnull(TT.FormName , if(S.IsInvoice="T" , "TInvoiceGUI" , if(is' +
        'salesorder = "T" , "TSalesOrderGUI" , if(iscashSale="T" , "TCash' +
        'SaleGUI" , if(isRefund = "T" , "TRefundGUI" , if(isLayby="T", "T' +
        'SalesOrderGUI" , "")))))) as FormName,'
      ' S.InvoiceDocNumber as InvoiceNo, '
      ' S.CustomerName as CustomerName, '
      ' Round(Sum(SL.Shipped), 5) as Qty, '
      ' SL.UnitOfMeasureSaleLines as UnitOfMeasure, '
      
        ' Convert(FXCode( S.ForeignExchangeCode ,ifnull(SL.ForeignExchang' +
        'eSellRate ,S.ForeignExchangeRate) ,'#39'AUD'#39'), char(100)) as FXCode,'
      ' Sum(SL.ForeignTotalLineAmount) as ForeignTotalLineAmount,'
      ' Sum(SL.TotalLineAmount) as `Total Amount (Ex)`, '
      
        ' Round(Sum(SL.TotalLineAmount) - SUM(SL.LineCost*SL.Shipped),2) ' +
        'as `Total Profit (Ex)`, '
      
        ' Round(Sum(SL.TotalLineAmountInc) - SUM(SL.LineCostInc*SL.Shippe' +
        'd),2) as `Total Profit (Inc)`, '
      ' Sum(SL.TotalLineAmountInc) as `Total Amount (Inc)`, '
      ' SL.TotalLineAmount as GrossProfitTotal, '
      ' Round(SL.LineCost*SL.Shipped,2) as `Line Cost (Ex)`,'
      ' S.SaleID as TransactionNo, '
      ' SL.ProductID as ProductID, '
      ' S.EmployeeName as EmployeeName, '
      ' Sum(SL.Attrib1Sale) as Attrib1Sale, '
      'Sum(SL.Attrib2Sale) as Attrib2Sale,  '
      ' Sum(SL.Attrib1SaleRate) as Attrib1SaleRate, '
      ' SL.Product_Description as ProductDescription, '
      ' IF(INSTR(SL.CustomData, "PriceScheme"),"T","F") as IsSpecial, '
      ' 2 as Details , '
      ' SL.SaleLineID as SaleLineID, '
      'CT.TypeName as TypeName,'
      ' S.converted as converted,'
      ' C.Email , '
      ' C.Street , '
      ' C.Street2 , '
      ' C.Suburb , '
      ' C.State , '
      ' C.Postcode,'
      ' C.Country,'
      ' C.FirstName,'
      ' C.LastName,'
      'P.CommodityCode,'
      'P.NetWeightKg,'
      'P.ProductUOM,'
      'CTRY.Country as CountryOfOrigin,'
      'P.Ex_Works as ExWorks,'
      'C.CUSTFLD1 as CustomerCUSTFLD1,'
      'C.CUSTFLD2 as CustomerCUSTFLD2,'
      'C.CUSTFLD3 as CustomerCUSTFLD3,'
      'C.CUSTFLD4 as CustomerCUSTFLD4,'
      'C.CUSTFLD5 as CustomerCUSTFLD5,'
      'C.CUSTFLD6 as CustomerCUSTFLD6,'
      'C.CUSTFLD7 as CustomerCUSTFLD7,'
      'C.CUSTFLD8 as CustomerCUSTFLD8,'
      'C.CUSTFLD9 as CustomerCUSTFLD9,'
      'C.CUSTFLD10 as CustomerCUSTFLD10,'
      'C.CUSTFLD11 as CustomerCUSTFLD11,'
      'C.CUSTFLD12 as CustomerCUSTFLD12,'
      'C.CUSTFLD13 as CustomerCUSTFLD13,'
      'C.CUSTFLD14 as CustomerCUSTFLD14,'
      'C.CUSTFLD15 as CustomerCUSTFLD15,'
      'C.CUSTDATE1 as CustomerCUSTDATE1,'
      'C.CUSTDATE2 as CustomerCUSTDATE2,'
      'C.CUSTDATE3 as CustomerCUSTDATE3'
      'FROM '
      'tblsales AS S'
      ' INNER JOIN tblSalesLines as SL USING(SaleID) '
      ' INNER JOIN tblparts      as P ON PARTSID = ProductID '
      
        ' Left JOIN tblcountries as CTRY on CTRY.CountryId = P.CountryofO' +
        'riginID'
      
        ' Left  JOIN tblPQA        as PQA on PQA.TransID = S.SaleId and P' +
        'QA.TransLineID =SL.SaleLineId and PQA.TransType in ('#39'TInvoiceLin' +
        'e'#39','#39'TSalesOrderline'#39','#39'TRefundSaleLine'#39','#39'TPOSCashSaleline'#39','#39'TPOSL' +
        'aybylines'#39','#39'TCashSaleLine'#39')'
      
        ' Left JOIN tblpqatranstypes as TT ON TT.TRansType  = PQA.TransTy' +
        'pe '
      ' INNER JOIN tblclients C ON C.ClientID=S.ClientID '
      ' LEFT JOIN tblClientType CT ON C.ClientTypeID=CT.ClientTypeID '
      
        'Where  ((S.IsPOS ="T" or S.IsRefund ="T"   or S.IsCashSale="T" o' +
        'r S.IsInvoice="T"  or S.IsSalesOrder="T" or S.IsLayby ="T")  AND' +
        ' SL.Invoiced="T" AND S.Deleted ="F"  AND S.IsQuote="F"  AND S.co' +
        'nverted = "F"  )'
      ' GROUP BY SL.ProductID, SL.UnitOfMeasureSaleLines, S.SaleID')
    OnCalcFields = qryMainCalcFields
    Options.FlatBuffers = True
