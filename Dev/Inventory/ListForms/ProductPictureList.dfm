  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'P.PARTSID  as PartsId ,'
      'P.PARTNAME as PARTNAME,'
      'PP.Pictype as Pictype,'
      'PP.partpicID as partpicID,'
      'PP.partpic as partpic'
      
        'from tblpartspics PP inner join tblparts P on PP.partID = P.PART' +
        'SID'
      'where ifnull(PP.partPic,'#39#39')<> '#39#39
      'and (P.PARTSID =:ProductID or :ProductID =0)')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ProductID'
      end
      item
        DataType = ftUnknown
        Name = 'ProductID'
      end>
