  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT distinct  '
      'S.customername as customername, '
      'S.InvoiceDocNumber as InvoiceDocNumber,'
      'SL.Shipdate as Shipdate,'
      'SL.RequiresAssembly as RequiresAssembly,'
      'SL.Assembled as Assembled,'
      'SL.SaleLineID as SaleLineID,'
      'PARTNAME as ProductName, '
      
        'LEFT(if((LOCATE('#39'^'#39',P.PRODUCTGROUP,InStr(P.PRODUCTGROUP,'#39'^'#39')+1))' +
        ' >0, MID(P.PRODUCTGROUP,LOCATE('#39'^'#39',P.PRODUCTGROUP, InStr(P.PRODU' +
        'CTGROUP,'#39'^'#39')+1)+1,   char_length(P.PRODUCTGROUP)- LOCATE('#39'^'#39',P.P' +
        'RODUCTGROUP,InStr(P.PRODUCTGROUP,'#39'^'#39')+1)),   Null),255)  AS Thir' +
        'dColumn ,'
      
        'LEFT(MID(P.PRODUCTGROUP,InStr(P.PRODUCTGROUP,'#39'^'#39')+1 ,   If(LOCAT' +
        'E('#39'^'#39',P.PRODUCTGROUP,InStr(P.PRODUCTGROUP,'#39'^'#39')+1) - InStr(P.PROD' +
        'UCTGROUP,'#39'^'#39') >0,   LOCATE('#39'^'#39',P.PRODUCTGROUP,InStr(P.PRODUCTGRO' +
        'UP,'#39'^'#39')+1)-1 - InStr(P.PRODUCTGROUP,'#39'^'#39'),    IF(InStr(P.PRODUCTG' +
        'ROUP,'#39'^'#39')<>0,char_length(P.PRODUCTGROUP) - InStr(P.PRODUCTGROUP,' +
        #39'^'#39'),0))),255)   AS SecondColumn ,   '
      
        'LEFT(If(InStr(P.PRODUCTGROUP,'#39'^'#39') -1 >0,Left(P.PRODUCTGROUP,InSt' +
        'r(P.PRODUCTGROUP,'#39'^'#39')-1),   P.PRODUCTGROUP),255) AS FirstColumn ' +
        ','
      'P.PARTSDESCRIPTION as SalesDescription,   '
      'S.SaleID as SaleID, '
      'S.SaleDate AS SaleDate,   '
      
        'CASE WHEN S.isCashSale = '#39'T'#39' THEN '#39'CashSale'#39' WHEN S.isInvoice = ' +
        #39'T'#39' THEN '#39'Invoice'#39'  WHEN S.ISSalesOrder = '#39'T'#39' THEN '#39'Sales Order'#39 +
        ' WHEN S.IsPos  = '#39'T'#39' THEN '#39'POS Sale'#39' WHEN S.ISQuote = '#39'T'#39' THEN '#39 +
        'Quote'#39'  END AS Type,   '
      'S.Shipping as Shipping, '
      'S.Comments as Comments, '
      'if(isnull(tblPrintDocs.docId) , '#39'F'#39' , '#39'T'#39') as PickPrintFlag, '
      'P.Area as Area,'
      'S.QuoteStatus as Status'
      'FROM tblparts AS P '
      'INNER JOIN tblSalesLines AS SL ON P.PartsID = SL.ProductID '
      'INNER JOIN tblSales AS S ON S.SaleID = SL.SaleID '
      
        'left join tblPrintDocs on tblPrintDocs.docId = SL.SaleId and tbl' +
        'PrintDocs.Doctype = '#39'dtPickSlip'#39' and Printedfrom = '#39'TfmPrintPick' +
        'ingSlips'#39
      'inner join tblPQA on  tblPQA.TransLineId = SL.SaleLineId'
      
        'and tblPQA.transtype  in ('#39'TInvoiceLine'#39','#39'TSalesOrderLine'#39','#39'TWOA' +
        'llocLines'#39','#39'POS'#39','#39'CashSale'#39')'
      'WHERE S.converted ="F" and s.shipdate Between :dtfrom and :dtto '
      'and S.ClassID = :ClassID'
      ' and tblPQA.Active = "T" '
      'ORDER BY S.SaleID desc, Type')
    ParamData = <
      item
        DataType = ftString
        Name = 'dtfrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'dtto'
      end
      item
        DataType = ftUnknown
        Name = 'ClassID'
      end>
