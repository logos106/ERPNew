  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select     '
      '1 as Category, '
      'At.Auditdate as Auditdate,'
      'C.company as Supplier,'
      'currencycode.newValue as currencycode, '
      'uomname.newValue as uomname, '
      'uom.UnitID as unitId, '
      'uom.Multiplier as uommult,'
      'convert(price1.newValue , DECIMAL(15,2)) as Price , '
      'price1.newValue * uom.Multiplier as uomPrice'
      'from '
      'tblforeignprice FP'
      
        'inner join tblaudittrail  AT  on AT.TransGlobalref = FP.GlobalRe' +
        'f and AT.TransType = '#39'ProductForeignBuyPrice'#39
      
        'Left join (tblaudittriallines     clientID inner join tblclients' +
        ' C on c.clientId = ClientID.newvalue ) on AT.auditId = clientID.' +
        'AuditID '#9'and trim(clientID.fieldName)= "clientID"'
      
        'Left join tblaudittriallines     currencycode on AT.auditId = cu' +
        'rrencycode.AuditID '#9'and trim(currencycode.fieldName)= "currencyc' +
        'ode"'
      
        'Left join tblaudittriallines     uomname  on AT.auditId = uomnam' +
        'e.AuditID '#9#9#9'and trim(uomname.fieldName) '#9#9'= "uomname"'
      
        'Left join tblunitsofmeasure uom on uom.UnitName = uomname.newVal' +
        'ue and uom.PartID = FP.PartsID'
      
        'Left join tblaudittriallines     price1 '#9#9'  on AT.auditId = pric' +
        'e1.AuditID '#9#9#9'  and trim(price1.fieldName)'#9#9#9'= "price1"'
      'where FP.PartsID= :PartsId'
      '/*union all select '
      '2 as Category, '
      'FP.mstimestamp as Auditdate,  '
      'C.Company as Supplier, '
      'CC.Code as currencycode,'
      'FP.UOM as uomname, '
      'FP.UOMID as unitId,'
      'FP.UOMMultiplier  as uommult,'
      'FP.Price1 as Price,'
      'FP.price1 * FP.UOMMultiplier as uomPrice'
      'from '
      'tblforeignprice FP '
      'inner join tblclients C on FP.ClientID = C.ClientID'
      
        'inner join tblcurrencyconversion CC on CC.CurrencyID = FP.Curren' +
        'cyId'
      'where partsId =  :PartsId*/'
      'order by  supplier, currencycode,uomname,  Auditdate desc')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'PartsId'
      end>
