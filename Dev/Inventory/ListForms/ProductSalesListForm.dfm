  inherited qryMain: TERPQuery
    SQL.Strings = (
      '#temp query - changed by the form at runtime'
      ''
      'SELECT'
      'PC.ID AS LinesID,'
      'PC.Classid as ClassId, '
      'PC.Classname as Classname,'
      'P.PartsID AS PartsID,'
      'PartName AS PartName,'
      'P.PREFEREDSUPP as PREFEREDSUPP,'
      'space(100) as col1,'
      'space(100) as col2,'
      'space(100) as col3,'
      '0.0 as InStockQty,'
      '0.0 as AvailableQty,'
      'PARTSDESCRIPTION AS Description,'
      'PT.TypeDesc AS PartType,'
      'Sum(SL.Shipped) AS Shipped,'
      'Avg(SL.LinePrice) AS "Avg Price",'
      'Avg(SL.LineCost) AS "Avg Cost",'
      
        'if (Sum(SL.LineCost) > 0, (Sum(SL.LinePrice)-Sum(SL.LineCost))  ' +
        '/ Sum(SL.LineCost) * 100, 0) AS "Markup",'
      
        '(Sum(SL.LinePrice-SL.LineCost))  / Sum(SL.LinePrice) * 100 AS "M' +
        'argin",'
      
        'If(InStr(P.PRODUCTGROUP,"^") -1 >0,Left(P.PRODUCTGROUP,InStr(P.P' +
        'RODUCTGROUP,"^")-1),P.PRODUCTGROUP)  AS "FirstColumn",'
      
        'MID(P.PRODUCTGROUP, InStr(P.PRODUCTGROUP,"^")+1 ,If(LOCATE("^",P' +
        '.PRODUCTGROUP,InStr(P.PRODUCTGROUP,"^")+1) - InStr(P.PRODUCTGROU' +
        'P,"^") >0,LOCATE("^",P.PRODUCTGROUP,InStr(P.PRODUCTGROUP,"^")+1)' +
        '-1 - InStr(P.PRODUCTGROUP,"^"),IF(InStr(P.PRODUCTGROUP,"^")<>0,L' +
        'ENGTH(P.PRODUCTGROUP) - InStr(P.PRODUCTGROUP,"^"),0)))  AS "Seco' +
        'ndColumn",'
      
        'IF((LOCATE("^",P.PRODUCTGROUP,InStr(P.PRODUCTGROUP,"^")+1)) >0, ' +
        'MID(P.PRODUCTGROUP,LOCATE("^",P.PRODUCTGROUP, InStr(P.PRODUCTGRO' +
        'UP,"^")+1)+1,char_length(P.PRODUCTGROUP)- LOCATE("^",P.PRODUCTGR' +
        'OUP,InStr(P.PRODUCTGROUP,"^")+1)),  Null)   AS "ThirdColumn",'
      
        '-- Sum(if(PQA.Active ="F" , 0, if(PQA.TransType="TPurchaseOrderL' +
        'ine" and PQA.IsBO ="T",ifnull(PQA.QTY,0),0))) as "On Order",'
      
        '-- Sum(if(PQA.Active ="F" , 0,if(PQA.TransType="TSalesOrderLine"' +
        ' or PQA.IsBO ="T",0,if(PQA.alloctype="IN"  , ifnull(PQA.QTY,0), ' +
        '0-ifnull(PQA.QTY,0))))) as InStockQty,'
      
        '-- Sum(if(PQA.Active ="F" , 0,if(PQA.TransType="TSalesOrderLine"' +
        ' or PQA.IsBO ="T",0,if(PQA.alloctype="IN"  , ifnull(PQA.QTY,0), ' +
        '0-ifnull(PQA.QTY,0))))) as AvailableQty,'
      
        'Sum(if(PQA.Active ="F" , 0, if(PQA.TransType="TSalesOrderLine" a' +
        'nd PQA.IsBO ="F",ifnull(PQA.QTY,0), 0))) as "Sales Orders",'
      
        'Sum(if(PQA.Active ="F" , 0, if(PQA.TransType="TInvoiceLine" and ' +
        'PQA.IsBO ="T",ifnull(PQA.QTY,0),0))) as "Invoice BO",'
      
        '-- Sum(if(PQA.Active ="F" , 0,if(PQA.TransType="TSalesOrderLine"' +
        ' or PQA.IsBO ="T",0,if(PQA.alloctype="IN"  , ifnull(PQA.QTY,0), ' +
        '0-ifnull(PQA.QTY,0))))) *Avg(SL.LinePrice) as StockValue,'
      'PC.ReorderPoint AS "Reorder Point",'
      'PC.PreferredLevel AS PreferredLevel,'
      'PC.ReorderAmount AS "Reorder Amount",'
      'SL.Attrib1Sale as Attrib1sale,'
      'SL.Attrib2Sale as Attrib2sale,'
      'SL.Attrib1SaleRate as Attrib1SaleRate'
      ' From tblParts as P'
      'inner join tblPQA as PQA on PQA.ProductId = P.PartsID '
      'inner join tblclass as C on PQA.departmentID = C.ClassID '
      
        'Left join tblProductClasses as PC on PC.ProductID = PQA.ProductI' +
        'D and PC.ClassID = PQA.departmentID'
      'INNER JOIN tblSalesLines SL ON PQA.TransLineID = SL.SaleLineID '
      
        'and PQA.transtype in ('#39'TInvoiceLine'#39','#39'TSalesOrderLine'#39','#39'TRefundS' +
        'aleLine'#39','#39'TCashSaleLine'#39')'
      'INNER JOIN tblSales S ON S.SaleID = SL.SaleID'
      'INNER JOIN tblparttypes PT ON P.PARTTYPE = PT.TypeCode'
      'WHERE P.Active = "T"'
      'AND S.Deleted <> "T"'
      'AND PC.ClassID = 1'
      'AND S.ClassID = PC.ClassID'
      '-- AND (S.IsInvoice = "T" OR S.IsCashSale="T")'
      'AND (S.SaleDate Between  '#39'2007-04-24'#39' And  '#39'2008-06-18'#39')'
      'GROUP BY PartsID'
      'ORDER BY '#39'Weekly Profit'#39' desc')
    FetchRows = 1
    OnCalcFields = qryMainCalcFields
    OnFilterRecord = qryMainFilterRecord
    Left = 251
    Top = 272
