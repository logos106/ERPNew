      FieldName = 'PartName'
      Size = 60
    end
    object qryMainProductPrintName: TWideStringField
      FieldName = 'ProductPrintName'
      Size = 255
    end
    object qryMainAvailableQty: TFloatField
      FieldName = 'AvailableQty'
    end
    object qryMainInStockQty: TFloatField
      FieldName = 'InStockQty'
    end
    object qryMainSOQty: TFloatField
      FieldName = 'SOQty'
    end
    object qryMainSOBOQty: TFloatField
      FieldName = 'SOBOQty'
    end
    object qryMainBOQty: TFloatField
      FieldName = 'BOQty'
    end
    object qryMainOnOrderQty: TFloatField
      FieldName = 'OnOrderQty'
    end
  end
