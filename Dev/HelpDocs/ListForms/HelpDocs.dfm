  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'HD.ID AS ID,'
      'HD.Description AS Description,'
      'HD.Modulename as Modulename,'
      'HD.Filetype AS Filetype,'
      'HD.IsERPTaskItem AS IsERPTaskItem,'
      'HD.FileName AS FileName, '
      'HD.msTimeStamp AS  msTimeStamp,'
      
        'if(IFNULL(HDB.ID,0)=0 , "F" , if(IFNULL(HDB.HelpDoc , '#39#39')='#39#39' , '#39 +
        'F'#39' , '#39'T'#39')) AS HasBlob'
      'from erpdocumentaion.tblhelpdocs  HD'
      
        'inner JOIN  erpdocumentaion.tblhelpdocblobs HDB ON HD.ID = HDB.I' +
        'D'
      'Where HD.active ="T"'
      'having HasBlob ="T"'
      ' ORDER BY Modulename,Description')
    Active = True
