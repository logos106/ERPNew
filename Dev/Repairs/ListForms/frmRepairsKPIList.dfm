  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'R.RepairID as RepairID,'
      'R.GlobalRef as GlobalRef,'
      'R.CreationDate as CreationDate,'
      'R.Done as Done,'
      'R.Notes as Notes,'
      'R.FeedbackNotes as FeedbackNotes,'
      'R.CustomerDetails,'
      'R.Phone,'
      'R.AltPhone,'
      'R.Mobile,'
      'R.Fax,'
      'R.JobDueDate,'
      'R.Converted,'
      'R.Status,'
      'R.BillToCustomerDetails,'
      'R.BillCusID,'
      'R.UseBillCust,'
      'R.BillPhone,'
      'R.BillAltPhone,'
      'R.BillFax,'
      'R.SOGlobalRef,'
      'R.QuoteGlobalRef,'
      'R.CustomerPONumber,'
      'R.CompletionTime,'
      'R.Shipping,'
      'R.AllocatedEmployeeID,'
      'R.Terms,'
      'R.BillMobile,'
      'R.QuotedAmount,'
      'R.QuotedAmountinc,'
      'R.HoldRepair,'
      'R.CUSTFLD1,'
      'R.CUSTFLD2,'
      'R.CUSTFLD3,'
      'R.CUSTFLD4,'
      'R.CUSTFLD5,'
      'R.CUSTFLD6,'
      'R.CUSTFLD7,'
      'R.CUSTFLD8,'
      'R.CUSTFLD9,'
      'R.CUSTFLD10,'
      'R.CUSTFLD11,'
      'R.CUSTFLD12,'
      'R.CUSTFLD13,'
      'R.CUSTFLD14,'
      'R.CUSTFLD15,'
      'R.CUSTDATE1,'
      'R.CUSTDATE2,'
      'R.CUSTDATE3,'
      'C.Company AS RepairClient,'
      'C2.Company AS Billtoclient,'
      'E.EmployeeName as RepName,'
      'class.ClassName,'
      'DateDiff(R.CUSTDATE3,R.CUSTDATE2) '
      '- (Truncate(DateDiff(R.CUSTDATE3,R.CUSTDATE2) / 7,0) * 2) '
      
        '- (select Count(*) from tblPublicHolidays H where H.Day Between ' +
        'R.CUSTDATE2 and R.CUSTDATE3 and not (DayOfWeek(H.Day)) in (1,7))' +
        ' as CollectionToDeliverDays'
      'FROM tblrepairs AS R'
      'Left Join tblclients AS C ON R.CusID = C.ClientID'
      'Left Join tblclients AS C2 ON R.BillCusID = C2.ClientID'
      'Left Join tblclass AS class ON R.ClassID = class.ClassID'
      'Left Join tblemployees AS E ON R.EmployeeID = E.EmployeeID'
      
        'Where ((R.CreationDate Between   :dtfrom and  :dtto) or (isnull(' +
        'R.CreationDate)) or (R.CreationDate='#39'1899-12-30'#39' ) ) AND R.Class' +
        'ID Like  :ClassID AND R.CusID Like  :ClientId'
      ' and R.Cancelled = "F"'
      'ORDER BY jobdueDate, C.Company')
    OnCalcFields = qryMainCalcFields
