  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'RF.RepairFaultID as RepairFaultID,'
      'RF.Globalref as Globalref,'
      'RF.code as Code,'
      'RF.Category as Category,'
      
        'case  ifnull(RF.Category,'#39#39') when  '#39'C'#39' then '#39'Condition'#39' when '#39'S'#39 +
        ' then '#39'Symptom'#39' when '#39'D'#39' then '#39'Defect'#39' when '#39'R'#39' then '#39'Repair'#39' wh' +
        'en '#39'L'#39' then '#39'Section'#39' else   '#39'Unknown'#39' end as CategoryDesc, '
      'RF.Description as Description,'
      'RF.Active as Active,'
      '`RF`.`Manufacture` as Manufacture,'
      'RF.msTimestamp as msTimestamp'
      'FROM tblrepairfaults AS RF')
