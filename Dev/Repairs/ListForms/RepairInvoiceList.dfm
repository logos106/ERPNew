  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      '`R`.`RepairID`,'
      '`R`.`GlobalRef`,'
      '`R`.`CreationDate`,'
      '`R`.`Done`,'
      '`R`.`Notes`,'
      '`R`.`FeedbackNotes`,'
      '`R`.`CustomerDetails`,'
      '`R`.`Phone`,'
      '`R`.`AltPhone`,'
      '`R`.`Mobile`,'
      '`R`.`Fax`,'
      '`R`.`JobDueDate`,'
      '`R`.`Converted`,'
      '`R`.`Status`,'
      '`R`.`BillToCustomerDetails`,'
      '`R`.`BillCusID`,'
      '`R`.`UseBillCust`,'
      '`R`.`BillPhone`,'
      '`R`.`BillAltPhone`,'
      '`R`.`BillFax`,'
      '`R`.`SOGlobalRef`,'
      '`R`.`QuoteGlobalRef`,'
      '`R`.`CustomerPONumber`,'
      '`R`.`CompletionTime`,'
      '`R`.`Shipping`,'
      '`R`.`AllocatedEmployeeID`,'
      '`R`.`Terms`,'
      '`R`.`BillMobile`,'
      '`R`.`QuotedAmount`,'
      '`R`.`QuotedAmountinc`,'
      '`C`.`Company` AS `RepairClient`,'
      '`C2`.`Company` AS `Billtoclient`,'
      '`E`.`EmployeeName` as RepName,'
      '`class`.`ClassName`, S.`InvoiceDocNumber`, S.SaleId'
      'FROM'
      '`tblrepairs` AS `R`'
      'Left Join `tblclients` AS `C` ON `R`.`CusID` = `C`.`ClientID`'
      
        'Left Join `tblclients` AS `C2` ON `R`.`BillCusID` = `C2`.`Client' +
        'ID`'
      
        'Left Join `tblclass` AS `class` ON `R`.`ClassID` = `class`.`Clas' +
        'sID`'
      
        'Left Join `tblemployees` AS `E` ON `R`.`EmployeeID` = `E`.`Emplo' +
        'yeeID`'
      'Inner Join `tblsales`  S ON `R`.`RepairID` = S.`RepairID`'
      
        'Where ((R.CreationDate Between   :dtfrom and  :dtto) or (isnull(' +
        'R.CreationDate)) '
      'or (R.CreationDate='#39'1899-12-30'#39' ) )'
      'AND R.ClassID Like  :ClassID'
      'AND R.RepairId Like  :RepairId'
      'ORDER BY jobdueDate, C.Company')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'dtfrom'
      end
      item
        DataType = ftUnknown
        Name = 'dtto'
      end
      item
        DataType = ftUnknown
        Name = 'ClassID'
      end
      item
        DataType = ftUnknown
        Name = 'RepairId'
      end>
