  object qryCompanyInfo: TERPQuery
    SQL.Strings = (
      
        'Select SetupID,CompanyName, Concat(Address,'#39' '#39',Address2,'#39' '#39',Addr' +
        'ess3) As Address, Concat(City,'#39' '#39',State,'#39' '#39',Postcode) As Suburb,'
      
        'Country, Concat('#39'Phone: '#39',PhoneNumber, '#39'  Fax: '#39',FaxNumber) As P' +
        'hone, Concat('#39'ABN: '#39',ABN) As ABN,'
      'Date_Format(Now(),'#39'%W,%d %M %Y'#39') AS FaxDate,'
      
        'HideCompanyName, HideAddress, HideAddress2, HideAddress3, HideCi' +
        'ty, HideState, HidePostcode, HideCountry, HidePhoneNumber, HideF' +
        'axNumber, HideABN'
      'From tblCompanyInformation;')
