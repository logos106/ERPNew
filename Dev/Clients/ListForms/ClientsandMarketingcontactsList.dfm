  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select  distinct '
      '"Marketing Contact" as Type, '
      'MC.globalref as Globalref,'
      'MC.ID  as ID,'
      'MC.clientID as ClientID,'
      'MC.CompanyTypeName as CompanyTypeName,'
      'MC.COMPANY as COMPANY, '
      'MC.Title as Title,'
      'MC.FirstName as FirstName,'
      'MC.MiddleName as MiddleName,'
      'MC.LastName as LastName,'
      'MC.Street as Street,'
      'MC.Street2 as Street2, '
      'MC.Street3 as Street3, '
      'MC.SUBURB as SUBURB,'
      'MC.State as State,'
      'MC.Country as Country,'
      'MC.Source as Source,'
      'MC.FaxNumber as FaxNumber,'
      'MC.Phone as Phone, '
      'MC.Mobile as Mobile, '
      'MC.POSTCODE as POSTCODE, '
      'MC.Email as Email,'
      'C.Customer as ISCustomer,'
      'C.Supplier as ISSupplier,'
      'C.OtherContact as Isothercontact,'
      '"T" as IsMc, '
      'ML.LeadID,'
      'MC.URL as URL,'
      'MC.active as Active,'
      'MC.DateEntered as DateEntered,'
      'Max(MLL.ActionDate) as LastLeaddone,'
      'MLL.FollowupDate as FollowupDate,'
      'MC.DoNotContact'
      'from tblmarketingcontacts MC'
      'Left  join tblclients C on MC.clientID = c.clientID'
      'LEFT JOIN tblMarketingleads ML  on ML.MarketingContactID = MC.ID'
      
        'LEFT join tblMarketingleadlines MLL on ML.LeadID = MLL.LeadID an' +
        'd MLL.done = "T"'
      'Where ifnull(C.ClientID,0)=0'
      'group by MC.globalref'
      'union all '
      'Select '
      'distinct '
      
        'Concat(if (ifnull(Mc.Id,0) <> 0 , "Marketing contact / " , "") ,' +
        ' CASE WHEN C.IsJob = "T" THEN "Job" WHEN C.Customer = "T" AND C.' +
        'Supplier = "T"  THEN "Customer / Supplier" WHEN C.Customer = "T"' +
        ' THEN "Customer" WHEN C.OtherContact = "T" THEN "Prospect" WHEN ' +
        'C.Supplier = "T" THEN "Supplier" ELSE "?" END) AS Type,'
      'C.globalref as Globalref,'
      'MC.ID  as ID,'
      'C.clientID as ClientID,'
      'CT.TypeName as CompanyTypeName,'
      'C.COMPANY as COMPANY, '
      'C.Title as Title,'
      'C.FirstName as FirstName,'
      'C.MiddleName as MiddleName,'
      'C.LastName as LastName,'
      'C.Street as Street,'
      'C.Street2 as Street2, '
      'C.Street3 as Street3, '
      'C.SUBURB as SUBURB,'
      'C.State as State,'
      'C.Country as Country,'
      'S.MediaType as Source,'
      'C.FaxNumber as FaxNumber,'
      'C.Phone as Phone, '
      'C.Mobile as Mobile, '
      'C.POSTCODE as POSTCODE, '
      'C.Email as Email,'
      'C.Customer as ISCustomer,'
      'C.Supplier as ISSupplier,'
      'C.OtherContact as Isothercontact,'
      'if(Ifnull(MC.ID,0)=0,"F", "T") as IsMC,'
      'ML.leadId,'
      'C.URL as URL,'
      'C.active as Active,'
      'C.DateEntered as DateEntered,'
      'Max(MLL.ActionDate) as LastLeaddone,'
      'MLL.FollowupDate as FollowupDate,'
      'MC.DoNotContact'
      'from tblclients C'
      
        'left join tblclienttype as CT on C.ClientTypeID = CT.ClientTypeI' +
        'D'
      'Left join tblmarketingcontacts MC on MC.ClientID = C.clientID'
      'LEFT JOIN tblMarketingleads ML  on ML.MarketingContactID = MC.ID'
      
        'LEFT join tblMarketingleadlines MLL on ML.LeadID = MLL.LeadID an' +
        'd MLL.done = "T"'
      'Left join tblSource S on S.MedtypeId = C.MedTypeID'
      'group by C.Globalref'
      'order by iscustomer, ismc,  Company, type')
