  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'clientId, '
      
        'CONCAT_WS(" ", If(InStr(Company,'#39'^'#39') -1 >0,Left(Company,InStr(Co' +
        'mpany,'#39'^'#39')-1),Company),IF(char_length(JobName)>0, Concat("(",Job' +
        'Name,")"),'#39#39')) as Company,'
      'C.Email AS Email,'
      'C.Phone AS Phone,'
      'C.Mobile AS Mobile,'
      'C.IsJob AS IsJob,'
      'C.Customer as IsCustomer,'
      'C.Supplier as IsSupplier,'
      'C.OtherContact as IsOtherContact,'
      
        '(Select concat_ws('#39' '#39' , contacttitle, contactfirstname, contactS' +
        'urname) from tblcontacts where clientId = C.clientId and ISPrima' +
        'rycontact '#9#9#9#9' ="T" limit 1) as PrimaryContact, '
      
        '(Select concat_ws('#39' '#39' , contacttitle, contactfirstname, contactS' +
        'urname) from tblcontacts where clientId = C.clientId and MainCon' +
        'tactforCashSale        ="T" limit 1) as MainContactforCashSale  ' +
        '   ,'
      
        '(Select concat_ws('#39' '#39' , contacttitle, contactfirstname, contactS' +
        'urname) from tblcontacts where clientId = C.clientId and MainCon' +
        'tactforVoucher         ="T" limit 1) as MainContactforVoucher   ' +
        '   ,'
      
        '(Select concat_ws('#39' '#39' , contacttitle, contactfirstname, contactS' +
        'urname) from tblcontacts where clientId = C.clientId and MainCon' +
        'tactforPOS             ="T" limit 1) as MainContactforPOS       ' +
        '   ,'
      
        '(Select concat_ws('#39' '#39' , contacttitle, contactfirstname, contactS' +
        'urname) from tblcontacts where clientId = C.clientId and MainCon' +
        'tactforRefund          ="T" limit 1) as MainContactforRefund    ' +
        '   ,'
      
        '(Select concat_ws('#39' '#39' , contacttitle, contactfirstname, contactS' +
        'urname) from tblcontacts where clientId = C.clientId and MainCon' +
        'tactforInvoice         ="T" limit 1) as MainContactforInvoice   ' +
        '   ,'
      
        '(Select concat_ws('#39' '#39' , contacttitle, contactfirstname, contactS' +
        'urname) from tblcontacts where clientId = C.clientId and MainCon' +
        'tactforSalesOrder      ="T" limit 1) as MainContactforSalesOrder' +
        '   ,'
      
        '(Select concat_ws('#39' '#39' , contacttitle, contactfirstname, contactS' +
