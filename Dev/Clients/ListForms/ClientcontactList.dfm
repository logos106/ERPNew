  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'CON.`ContactID` as `ContactID`,'
      'C.ClientID as ClientID,'
      
        'CONCAT_WS(" ", If(InStr(C.Company,'#39'^'#39') -1 >0,Left(C.Company,InSt' +
        'r(C.Company,'#39'^'#39')-1),C.Company),IF(char_length(JobName)>0, Concat' +
        '("(",JobName,")"),'#39#39')) as CompanyName,'
      'C.Company as Company,'
      'C.JobName,'
      
        'concat_ws('#39' '#39' ,C.Title , C.FirstName ,C.MiddleName, C.LastName )' +
        'as ClientName,'
      'C.Mobile AS Mobile,'
      'C.Customer as ContactIsCustomer,'
      'C.Supplier as ContactIsSupplier,'
      'C.OtherContact as ContactIsOtherContact,'
      'C.IsJob as ContactIsJob,'
      'C.Country,'
      'C.Billcountry,'
      'CON.`Company` as contact,'
      
        'concat_ws('#39' '#39', CON.`ContactTitle` ,CON.`ContactFirstName` , CON.' +
        '`ContactSurName` ) as ContactName,'
      'CON.`ContactCountry` as `ContactCountry`,'
      'CON.`ContactMOB` as `ContactMOB`,'
      'if(ifnull(CON.contactID,0)=0 , "F" , "T") as hasContact,'
      'if(C.Customer="T" , C.clientID, 0) as CustomerId'
      
        'FROM tblclients C Left join `tblcontacts` CON on CON.ClientID = ' +
        'C.ClientID'
      'where ifnull(C.Company,'#39#39')<> '#39#39
      'Order by CompanyName , ContactName,jobname')
