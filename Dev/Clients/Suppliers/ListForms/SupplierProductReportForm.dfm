  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT tblpurchaseorders.OrderDate as "Order Date",'
      
        'If(tblpurchaseorders.IsBill="T","Bill",If(tblpurchaseorders.IsPO' +
        '="T","Purchase Order",If(tblpurchaseorders.IsCredit="T","Credit"' +
        ',"Return Authority"))) as "Transaction Type",'
      'tblpurchaseorders.SupplierName as "Supplier Name" ,'
      'tblpurchaseorders.GlobalRef as "Global #" ,'
      'tblpurchaseorders.OriginalNo as "Original #",'
      
        'tblpurchaseorders.PurchaseOrderNumber as "Purchase Order Number"' +
        ' ,'
      'tblpurchaseorders.InvoiceNumber as "Invoice Number",'
      
        'If(tblpurchaseorders.IsCredit="T",-tblpurchaseorders.TotalAmount' +
        ',tblpurchaseorders.TotalAmount) as "Total Amount (Ex)",'
      
        'If(tblpurchaseorders.IsCredit="T",-(tblpurchaseorders.TotalAmoun' +
        'tInc-tblpurchaseorders.TotalAmount),(tblpurchaseorders.TotalAmou' +
        'ntInc-tblpurchaseorders.TotalAmount))  as "Total Tax",'
      
        'If(tblpurchaseorders.IsCredit="T",-tblpurchaseorders.TotalAmount' +
        'Inc,tblpurchaseorders.TotalAmountInc) as "Total Amount (Inc)" ,'
      'tblpurchaselines.Qtysold as Ordered, '
      'tblpurchaselines.ETADate as ETADate, '
      
        'Concat(tblEmployees.FirstName," ", tblEmployees.LastName) AS "Em' +
        'ployee Name",'
      'tblpurchaseorders.Comments as "Comments", '
      'tblpurchaseorders.SalesComments as "Sales Comments",'
      'tblpurchaselines.Class as "Class Name",'
      'tblpurchaselines.AccountName as "AccountName",'
      'TypeDesc as "Product Type",'
      
        'Left(If(InStr(tblparts.PRODUCTGROUP,"^") -1 >0,Left(tblparts.PRO' +
        'DUCTGROUP,InStr(tblparts.PRODUCTGROUP,"^")-1),tblparts.PRODUCTGR' +
        'OUP),255) AS Department ,'
      
        'Left(MID(tblparts.PRODUCTGROUP, InStr(tblparts.PRODUCTGROUP,"^")' +
        '+1 ,If(LOCATE("^",tblparts.PRODUCTGROUP,InStr(tblparts.PRODUCTGR' +
        'OUP,"^")+1) - InStr(tblparts.PRODUCTGROUP,"^") >0,LOCATE("^",tbl' +
        'parts.PRODUCTGROUP,InStr(tblparts.PRODUCTGROUP,"^")+1)-1 - InStr' +
        '(tblparts.PRODUCTGROUP,"^"),IF(InStr(tblparts.PRODUCTGROUP,"^")<' +
        '>0,LENGTH(tblparts.PRODUCTGROUP) - InStr(tblparts.PRODUCTGROUP,"' +
        '^"),0))),255)  AS Type,'
      
        'Left(if((LOCATE("^",tblparts.PRODUCTGROUP,InStr(tblparts.PRODUCT' +
        'GROUP,"^")+1)) >0, MID(tblparts.PRODUCTGROUP,LOCATE("^",tblparts' +
        '.PRODUCTGROUP, InStr(tblparts.PRODUCTGROUP,"^")+1)+1,char_length' +
        '(tblparts.PRODUCTGROUP)- LOCATE("^",tblparts.PRODUCTGROUP,InStr(' +
        'tblparts.PRODUCTGROUP,"^")+1)),  Null),155)  AS Manufacturer,'
      'tblpurchaselines.ProductName as "Product Name",'
      'tblpurchaselines.Product_Description as "Product Description" ,'
      
        'If(tblpurchaseorders.IsCredit="T",-tblpurchaselines.LineCost,tbl' +
        'purchaselines.LineCost) as "Line Cost (Ex)",'
      
        'If(tblpurchaseorders.IsCredit="T",-tblpurchaselines.LineCostInc,' +
        'tblpurchaselines.LineCostInc) as "Line Cost (Inc)",'
      
        'If(tblpurchaseorders.IsCredit="T",-tblpurchaselines.TotalLineAmo' +
        'unt,tblpurchaselines.TotalLineAmount) as "Line Cost Total(Ex)",'
      
        'If(tblpurchaseorders.IsCredit="T",-tblpurchaselines.TotalLineAmo' +
        'untInc,tblpurchaselines.TotalLineAmountInc) as "Line Cost Total(' +
        'Inc)",'
      
        'If(tblpurchaseorders.IsCredit="T",-tblpurchaselines.LineTax,tblp' +
        'urchaselines.LineTax) as "Line Tax",'
      
        'If(tblpurchaseorders.IsCredit="T",-(tblpurchaselines.LineCostInc' +
        ' * tblpurchaselines.Backorder),(tblpurchaselines.LineCostInc * t' +
        'blpurchaselines.Backorder)) as `BO Line Cost Total(Inc)`,'
      
        'If(tblpurchaseorders.IsCredit="T",-(tblpurchaselines.LineCost * ' +
        'tblpurchaselines.Backorder),(tblpurchaselines.LineCost * tblpurc' +
        'haselines.Backorder)) as `BO Line Cost Total(Ex)`,'
      'tblpurchaselines.LineTaxCode as "Tax Code",'
      'tblpurchaselines.Shipped as Shipped, '
      'tblpurchaselines.BackOrder as BackOrder, '
      'tblpurchaselines.ProductID as ProductID,'
      'tblpurchaseorders.Deleted as Deleted,'
      'tblEmployees.EmployeeID as EmployeeID,'
      'tblClients.ClientID as ClientID,'
      'tblpurchaseorders.PurchaseOrderID as PurchaseOrderID,'
      'tblpurchaselines.Attrib1Purchase as Attrib1Purchase, '
      'tblpurchaselines.Attrib2Purchase as Attrib2Purchase, '
      'tblpurchaselines.Attrib1PurchaseRate as Attrib1PurchaseRate,'
      'tblpurchaselines.ReceivedDate as ReceivedDate,'
      'tblpurchaselines.DocketNumber as DocketNumber,'
      'tblParts.Barcode as Barcode,'
      'tblchartofaccounts.AccountNumber as AccountNumber , '
      'COGS.AccountNumber as COGSAccountNumber,'
      'Asset.AccountNumber as AssetAccountNumber,'
      'Income.AccountNumber as IncomeAccountNumber,'
      'COGS.AccountName as COGSAccountName,'
      'Asset.AccountName as AssetAccountName,'
      'Income.AccountName as IncomeAccountName'
      'FROM tblpurchaseorders'
      
        'INNER JOIN tblpurchaselines ON tblpurchaselines.PurchaseOrderID ' +
        '= tblpurchaseorders.PurchaseOrderID'
      
        'LEFT JOIN tblparts ON tblparts.PARTSID = tblpurchaselines.Produc' +
        'tID'
      
        'LEFT JOIN tblparttypes ON tblparts.PARTTYPE = tblparttypes.TypeC' +
        'ode'
      
        'LEFT JOIN tblchartofaccounts ON tblchartofaccounts.AccountName =' +
        ' tblpurchaselines.AccountName'
      
        'LEFT JOIN tblchartofaccounts COGS on COGS.AccountID =  tblpurcha' +
        'selines.COGSACCNTID'
      
        'LEFT JOIN tblchartofaccounts Asset on Asset.AccountID =  tblpurc' +
        'haselines.ASSETACCNTID'
      
        'LEFT JOIN tblchartofaccounts Income on Income.AccountID =  tblpu' +
        'rchaselines.INCOMEACCNTID'
      
        'LEFT JOIN tblClients  ON  tblpurchaseorders.SupplierName = tblCl' +
        'ients.Company'
      
        'LEFT JOIN tblEmployees ON  tblpurchaseorders.EmployeeID = tblEmp' +
        'loyees.employeeId #(tblpurchaseorders.EmployeeName = Concat(tblE' +
        'mployees.FirstName," ", tblEmployees.LastName))'
      
        'WHERE (IsBill ='#39'T'#39' OR IsPO ='#39'T'#39' OR IsRA ='#39'T'#39' OR IsCredit ='#39'T'#39')  ' +
        'AND'
      
        '(tblpurchaselines.ProductName >'#39#39' OR tblpurchaselines.AccountNam' +
        'e > '#39#39') And OrderDate Between :txtfrom AND :txtto'
      'AND tblpurchaseorders.Deleted<>'#39'T'#39
      'Order by tblpurchaseorders.OrderDate DESC')
    ParamData = <
      item
        DataType = ftString
        Name = 'txtfrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end>
