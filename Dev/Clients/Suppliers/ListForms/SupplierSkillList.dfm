  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'S.clientId as clientId, '
      'S.company as company, '
      'SS.skillname as skillname, '
      'SS.active as  skillactive, '
      'SS.Id as providerSkillId,'
      's.active as supplieractive, '
      'SS.skillid as SkillId'
      'from tblclients S '
      'Left join `tblSupplierskills` SS on SS.ProviderId = S.clientId'
      
        'Where S.Supplier='#39'T'#39' AND (ifnull(SS.ProviderId,0)<> 0 or :ShowAl' +
        'l ="T")'
      'and (SS.skillId =:SkillIDforlist or :SkillIDforlist  =0)')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ShowAll'
      end
      item
        DataType = ftUnknown
        Name = 'SkillIDforlist'
      end
      item
        DataType = ftUnknown
        Name = 'SkillIDforlist'
      end>
