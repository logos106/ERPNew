  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT Space(50) as Company,'
      'Space(50) as CompanyH,'
      
        'If(tblpurchaseorders.IsBill="T","Bill",If(tblpurchaseorders.IsPO' +
        '="T","Purchase Order",If(tblpurchaseorders.IsCredit="T","Credit"' +
        ',"Return Authority"))) as Type,'
      
        'If(tblpurchaseorders.IsBill="T","Bill",If(tblpurchaseorders.IsPO' +
        '="T","Purchase Order",If(tblpurchaseorders.IsCredit="T","Credit"' +
        ',"Return Authority"))) as TypeH,'
      'tblpurchaseorders.OrderDate as OrderDate,'
      'tblpurchaseorders.DueDate as DueDate,'
      'tblpurchaseorders.InvoiceDate as InvoiceDate,'
      'tblpurchaseorders.SupplierName as Name,'
      'tblpurchaseorders.GlobalRef as "Global #" ,'
      'tblpurchaseorders.OriginalNo as "Original #",'
      
        'tblpurchaseorders.PurchaseOrderNumber as "Purchase Order Number"' +
        ' ,'
      'tblpurchaseorders.InvoiceNumber as "Invoice Number",'
      
        'If(IsCredit='#39'T'#39',-tblpurchaseorders.TotalAmount,tblpurchaseorders' +
        '.TotalAmount) as "Total Amount (Ex)",'
      
        'If(IsCredit='#39'T'#39',-tblpurchaseorders.TotalAmountInc,tblpurchaseord' +
        'ers.TotalAmountInc) as "Total Amount (Inc)" ,'
      'tblpurchaseorders.Comments as "Comments",'
      'tblpurchaseorders.SalesComments as "WareHouse Notes",'
      'tblpurchaseorders.PurchaseOrderID as PurchaseOrderID,'
      'tblpurchaseorders.ClientID,'
      '2 as Details'
      'FROM tblpurchaseorders'
      'INNER JOIN tblpurchaselines USING(PurchaseOrderID)'
      
        'LEFT JOIN tblClients  ON  tblpurchaseorders.ClientID = tblClient' +
        's.ClientID'
      'WHERE (IsBill ='#39'T'#39' OR IsPO ='#39'T'#39' OR IsRA ='#39'T'#39' OR IsCredit ='#39'T'#39')  '
      'And OrderDate Between :txtfrom3 AND :txtto3'
      'AND tblpurchaseorders.Deleted<>'#39'T'#39
      
        'GROUP BY tblpurchaseorders.ClientID,tblpurchaseorders.PurchaseOr' +
        'derID'
      ''
      'UNION ALL'
      ''
      'SELECT tblpurchaseorders.SupplierName as Company ,'
      'tblpurchaseorders.SupplierName as CompanyH ,'
      #39#39' as Type,'
      #39#39' as TypeH,'
      'Space(50) as OrderDate,'
      'Space(50) as DueDate,'
      'Space(50) as InvoiceDate,'
      'tblpurchaseorders.SupplierName as Name,'
      'Space(50) as "Global #" ,'
      'Space(50) as "Original #",'
      'Space(50) as "Purchase Order Number" ,'
      'Space(50) as "Invoice Number",'
      
        'Sum( If(IsCredit="T",-tblpurchaseorders.TotalAmount,tblpurchaseo' +
        'rders.TotalAmount) ) as "Total Amount (Ex)",'
      
        'Sum( If(IsCredit="T",-tblpurchaseorders.TotalAmountInc,tblpurcha' +
        'seorders.TotalAmountInc) ) as "Total Amount (Inc)",'
      'Space(50) as "Comments",'
      'Space(50) as "WareHouse Notes",'
      '0 as PurchaseOrderID,'
      'tblpurchaseorders.ClientID,'
      '0 as Details'
      'FROM tblpurchaseorders'
      
        'LEFT JOIN tblClients  ON  tblpurchaseorders.SupplierName = tblCl' +
        'ients.Company'
      'WHERE (IsBill ='#39'T'#39' OR IsPO ='#39'T'#39' OR IsRA ='#39'T'#39' OR IsCredit ='#39'T'#39')'
      'And OrderDate Between :txtfrom1 AND :txtto1'
      'AND tblpurchaseorders.Deleted<>'#39'T'#39
      'GROUP BY tblpurchaseorders.ClientID'
      ''
      'Union All'
      ''
      'SELECT '#39#39' as Company ,'
      #39#39' as CompanyH ,'
      
        'If(tblpurchaseorders.IsBill="T","Bill",If(tblpurchaseorders.IsPO' +
        '="T","Purchase Order",If(tblpurchaseorders.IsCredit="T","Credit"' +
        ',"Return Authority"))) as Type,'
      
        'If(tblpurchaseorders.IsBill="T","Bill",If(tblpurchaseorders.IsPO' +
        '="T","Purchase Order",If(tblpurchaseorders.IsCredit="T","Credit"' +
        ',"Return Authority"))) as HType,'
      #39#39' as OrderDate,'
      #39#39' as DueDate,'
      'Space(50) as InvoiceDate,'
      'tblpurchaseorders.SupplierName as Name,'
      #39#39' as "Global #" ,'
      #39#39' as "Original #",'
      #39#39' as "Purchase Order Number" ,'
      #39#39' as "Invoice Number",'
      
        'Sum( If(IsCredit="T",-tblpurchaseorders.TotalAmount,tblpurchaseo' +
        'rders.TotalAmount) ) as "Total Amount (Ex)",'
      
        'Sum( If(IsCredit="T",-tblpurchaseorders.TotalAmountInc,tblpurcha' +
        'seorders.TotalAmountInc) ) as "Total Amount (Inc)",'
      #39#39' as "Comments",'
      #39#39' as "WareHouse Notes",'
      '0 as PurchaseOrderID,'
      'tblpurchaseorders.ClientID,'
      '1 as Details'
      'FROM tblpurchaseorders'
      
        'LEFT JOIN tblClients  ON  tblpurchaseorders.SupplierName = tblCl' +
        'ients.Company'
      'WHERE (IsBill ='#39'T'#39' OR IsPO ='#39'T'#39' OR IsRA ='#39'T'#39' OR IsCredit ='#39'T'#39')  '
      'And OrderDate Between :txtfrom2 AND :txtto2'
