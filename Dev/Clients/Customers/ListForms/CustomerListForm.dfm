  object CustomerJobBalance: TERPQuery
    Connection = MyConnection1
    SQL.Strings = (
      'SELECT'
      'If(ParentClientID=0,ClientID,ParentClientID)  as ClientID,'
      
        'If(InStr(C.Company,'#39'^'#39') -1 >0,Left(C.Company,InStr(C.Company,'#39'^'#39 +
        ')-1),C.Company) AS CompanyName,'
      'Sum(Balance) as TotalBalance '
      'FROM tblClients C '
      'WHERE Customer='#39'T'#39'  AND char_length(C.Company)>0'
      'Group By CompanyName'
      'HAVING TotalBalance<>0')
    Options.LongStrings = False
    Left = 245
    Top = 377
  end
