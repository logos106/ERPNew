  inherited qryMain: TERPQuery
    SQL.Strings = (
      '#DUMMY SQL - See RefreshQuery()'
      'SELECT '
      
        'C.Company, OFU.ClientID, OFU.FollowupDate AS "Follow Up Date", O' +
        'FU.Notes, OFU.Done, OFU.Hours,'
      
        'OFU.IsCustomer AS "Customer", OFU.IsSupplier AS "Supplier", OFU.' +
        'IsOtherContact AS "Other Contact",'
      
        'C.Title, C.FirstName, C.MiddleName, C.LastName, C.Street, C.Stre' +
        'et2, C.Suburb, C.State, C.PostCode,'
      'C.Phone, C.Mobile, C.FaxNumber AS "Fax", C.Email'
      ''
      'FROM tblotherfollowup AS OFU'
      'INNER JOIN tblclients AS C Using(ClientID)'
      
        'WHERE C.IsJob = "T" AND (OFU.FollowupDate BETWEEN '#39'2004-07-01'#39' A' +
        'ND '#39'2004-11-01'#39')'
      'ORDER BY C.Company;')
    Left = 85
