  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT 1 as Details, '
      'C.GlobalRef as GlobalRef,'
      'SUBSTRING_INDEX(C.Company,'#39'^'#39',1) AS CompanyName,'
      'Space(80) AS "Related Company",'
      'C.SerialNumber AS "Serial #", '
      'C.ModelNumber AS "Model #", '
      'C.Manufacture AS "Manufacture",'
      'CT.TypeName AS CustomerType,'
      'C.ARBalance AS "AR Balance",'
      'C.APBalance AS "AP Balance",'
      'C.Balance AS Balance,'
      
        'IFNULL(C.Balance,0) + SUM(IFNULL(CP.Balance,0)) AS RelatedBalanc' +
        'e,'
      'IFNULL(C.Balance,0) + SUM(IFNULL(CJ.Balance,0)) AS TotalBalance,'
      'C.FirstName, C.LastName,'
      'Concat_WS(" ",C.FirstName, C.LastName) AS ContactName,'
      'C.Email AS Email,'
      'C.Street AS Street,'
      'C.Street2 AS Street2,'
      'C.Suburb AS Suburb,'
      'C.State AS State,'
      'C.Postcode AS Postcode,'
      'C.Country AS Country,'
      'C.Phone AS Phone,'
      'C.FaxNumber AS FaxNumber,'
      'C.Mobile AS Mobile,'
      'C.AltContact AS AltContact,'
      'C.AltPhone AS AltPhone,'
      'C.BillStreet AS BillStreet,'
      'C.BillStreet2 AS BillStreet2,'
      'C.BillSuburb AS BillSuburb,'
      'C.BillState AS BillState,'
      'C.BillPostcode AS BillPostcode,'
      'C.BillCountry AS BillCountry,'
      'C.ClientNo as AccountNo,'
      'EMP.EmployeeName as Rep,'
      'C.CUSTFLD1 AS CUSTFLD1,'
      'C.CUSTFLD2 AS CUSTFLD2,'
      'C.CUSTFLD3 AS CUSTFLD3,'
      'C.CUSTFLD4 AS CUSTFLD4,'
      'C.CUSTFLD5 AS CUSTFLD5,'
      'C.CUSTFLD6 AS CUSTFLD6,'
      'C.CUSTFLD7 AS CUSTFLD7,'
      'C.CUSTFLD8 AS CUSTFLD8,'
      'C.CUSTFLD9 AS CUSTFLD9,'
      'C.CUSTFLD10 AS CUSTFLD10,'
      'C.CUSTFLD11 AS CUSTFLD11,'
      'C.CUSTFLD12 AS CUSTFLD12,'
      'C.CUSTFLD13 AS CUSTFLD13,'
      'C.CUSTFLD14 AS CUSTFLD14,'
      'C.CUSTFLD15 AS CUSTFLD15,'
      'C.CUSTDATE1 AS CUSTDATE1,'
      'C.CUSTDATE2 AS CUSTDATE2,'
      'C.CUSTDATE3 AS CUSTDATE3,'
      'C.IsJob AS IsJob,'
      'C.Active AS Active,'
      'C.ClientID AS ClientID,'
      'C.ParentClientID AS ParentClientID,'
      'C.ParentRelatedClientID AS ParentRelatedClientID,'
      'C.CreditLimit AS CreditLimit,'
      'C.TermsID AS TermsID,'
      'C.GracePeriod AS GracePeriod,'
      'C.PickingPriority AS PickingPriority,'
      'C.Area AS Area,'
      'C.DischargeDate AS DischargeDate,'
      'C.CreationDate AS CreationDate,'
      'C.CreditCardType AS CreditCardType,'
      'C.CreditCardNumber AS CreditCardNumber,'
      'C.CreditCardExpiryDate AS CreditCardExpiryDate,'
      'C.CreditCardNotes AS CreditCardNotes,'
      'C.ABN AS ABN'
      'FROM tblClients C'
      'LEFT JOIN tblclienttype CT Using(ClientTypeID)'
      'LEFT JOIN tblemployees EMP ON C.RepID = EMP.EmployeeID'
      'LEFT JOIN tblClients CP ON CP.ParentRelatedClientID = C.ClientID'
      'LEFT JOIN tblClients CJ ON CJ.ParentClientID = C.ClientID'
      'WHERE C.Customer='#39'T'#39' AND '
      'C.IsJob = '#39'F'#39' AND '
      'C.ParentRelatedClientID = 0'
      'GROUP BY C.ClientID'
      'UNION ALL'
      'SELECT 2 as Details, C.GlobalRef,'
      'RC.MasterCompany AS CompanyName,'
      'C.Company AS "Related Company",'
      'C.SerialNumber AS "Serial #", '
      'C.ModelNumber AS "Model #", '
      'C.Manufacture AS "Manufacture",'
      'CT.TypeName AS CustomerType,'
      'C.ARBalance AS "AR Balance",'
      'C.APBalance AS "AP Balance",'
      'C.Balance AS Balance,'
      '0.0 AS RelatedBalance,'
      'IFNULL(C.Balance,0) + SUM(IFNULL(CJ.Balance,0)) AS TotalBalance,'
      'C.FirstName, C.LastName,'
      'Concat_WS(" ",C.FirstName, C.LastName) AS ContactName,'
      'C.Email AS Email,'
      'C.Street AS Street,'
      'C.Street2 AS Street2,'
      'C.Suburb AS Suburb,'
      'C.State AS State,'
      'C.Postcode AS Postcode,'
      'C.Country AS Country,'
      'C.Phone AS Phone,'
      'C.FaxNumber AS FaxNumber,'
      'C.Mobile AS Mobile,'
      'C.AltContact AS AltContact,'
      'C.AltPhone AS AltPhone,'
      'C.BillStreet AS BillStreet,'
      'C.BillStreet2 AS BillStreet2,'
      'C.BillSuburb AS BillSuburb,'
      'C.BillState AS BillState,'
      'C.BillPostcode AS BillPostcode,'
      'C.BillCountry AS BillCountry,'
      'C.ClientNo as AccountNo,'
      'EMP.EmployeeName as Rep,'
      'C.CUSTFLD1 AS CUSTFLD1,'
      'C.CUSTFLD2 AS CUSTFLD2,'
      'C.CUSTFLD3 AS CUSTFLD3,'
      'C.CUSTFLD4 AS CUSTFLD4,'
      'C.CUSTFLD5 AS CUSTFLD5,'
      'C.CUSTFLD6 AS CUSTFLD6,'
      'C.CUSTFLD7 AS CUSTFLD7,'
      'C.CUSTFLD8 AS CUSTFLD8,'
      'C.CUSTFLD9 AS CUSTFLD9,'
      'C.CUSTFLD10 AS CUSTFLD10,'
      'C.CUSTFLD11 AS CUSTFLD11,'
      'C.CUSTFLD12 AS CUSTFLD12,'
      'C.CUSTFLD13 AS CUSTFLD13,'
      'C.CUSTFLD14 AS CUSTFLD14,'
      'C.CUSTFLD15 AS CUSTFLD15,'
      'C.CUSTDATE1 AS CUSTDATE1,'
      'C.CUSTDATE2 AS CUSTDATE2,'
      'C.CUSTDATE3 AS CUSTDATE3,'
      'C.IsJob AS IsJob,'
      'C.Active AS Active,'
      'C.ClientID AS ClientID,'
      'C.ParentClientID AS ParentClientID,'
      'C.ParentRelatedClientID AS ParentRelatedClientID,'
      'C.CreditLimit AS CreditLimit,'
      'C.TermsID AS TermsID,'
      'C.GracePeriod AS GracePeriod,'
      'C.PickingPriority AS PickingPriority,'
      'C.Area AS Area,'
      'C.DischargeDate AS DischargeDate,'
      'C.CreationDate AS CreationDate,'
      'C.CreditCardType AS CreditCardType,'
      'C.CreditCardNumber AS CreditCardNumber,'
      'C.CreditCardExpiryDate AS CreditCardExpiryDate,'
      'C.CreditCardNotes AS CreditCardNotes,'
      'C.ABN AS ABN'
      'FROM tblClients C'
      'LEFT JOIN tblclienttype CT Using(ClientTypeID)'
      
        'INNER JOIN tblrelatedclients RC ON C.ParentRelatedClientID = RC.' +
        'MasterClientID'
      'LEFT JOIN tblemployees EMP ON C.RepID = EMP.EmployeeID'
      'LEFT JOIN tblClients CJ ON CJ.ParentClientID = C.ClientID'
      'WHERE C.Customer='#39'T'#39' AND '
      'C.IsJob = '#39'F'#39' AND '
      'C.ParentRelatedClientID <> 0'
      'GROUP BY C.ClientID, `Related Company`, CompanyName'
      'ORDER BY CompanyName, `Related Company`')
