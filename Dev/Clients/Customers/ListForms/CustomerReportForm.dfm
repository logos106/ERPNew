  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'SPACE(30) as Company,'
      
        'If(tblSales.IsCashSale='#39'T'#39' And tblSales.IsPOS='#39'F'#39' ,'#39'Cash Sale'#39',I' +
        'f(tblSales.IsRefund='#39'T'#39','#39'Refund'#39','
      '  If(tblSales.IsCashSale='#39'T'#39' And tblSales.IsPOS='#39'T'#39','#39'POS'#39','
      '  If(tblSales.IsInvoice='#39'T'#39','#39'Invoice'#39','#39'Unknown'#39')))) as Type,'
      'SaleDate as SaleDate,'
      'DueDate as DueDate,'
      'NULL as LastSaleDate,'
      'TRIM(tblclients.Company) as Name,'
      'CT.TypeName as ClientType,'
      'InvoiceDocNumber as '#39'Invoice Number'#39','
      'Sum(tblSalesLines.TotalLineAmount) as '#39'Total Amount (Ex)'#39','
      'Sum(tblSalesLines.TotalLineAmountInc) as '#39'Total Amount (Inc)'#39','
      
        'Sum((tblSalesLines.LinePriceInc - tblSalesLines.LineCostInc) * S' +
        'hipped) as '#39'Gross Profit'#39','
      
        'Sum((tblSalesLines.LinePrice- tblSalesLines.LineCost) * Shipped)' +
        ' as '#39'Gross Profit(Ex)'#39','
      'Sum(tblSalesLines.LineCost * Shipped) as '#39'Total Cost'#39', '
      'SPACE(30) as Address,'
      'SPACE(30) as '#39'Address 2'#39','
      'SPACE(30) as '#39'Address 3'#39','
      'SPACE(30) as Suburb,'
      'SPACE(30) as Postcode,'
      'SPACE(30) as State,'
      'SPACE(30) as Country,'
      'SPACE(30) as Phone,'
      'SPACE(40) as Email,'
      'SPACE(30) as FaxNumber,'
      'SPACE(30) as REP,'
      'tblSales.SaleID,'
      'tblSales.ClientID,'
      '2 as Details'
      'FROM tblSales'
      'INNER JOIN tblSalesLines USING(SaleID)'
      'LEFT JOIN tblclients on tblSales.ClientID = tblclients.ClientID'
      
        'LEFT JOIN tblclienttype CT on  tblclients.ClientTypeID = CT.Clie' +
        'ntTypeID'
      
        'WHERE (tblSales.IsPOS ='#39'T'#39' or tblSales.IsRefund ='#39'T'#39' or tblSales' +
        '.IsCashSale='#39'T'#39' or tblSales.IsInvoice='#39'T'#39')'
      
        'AND tblSalesLines.Invoiced='#39'T'#39' AND tblSales.IsLayby <>'#39'T'#39' AND tb' +
        'lSales.Deleted ='#39'F'#39
      'AND tblSales.IsSalesOrder='#39'F'#39' And tblSales.IsQuote='#39'F'#39
      
        'AND ((:UseShipDate = "F" and tblSales.SaleDate Between :DateFrom' +
        '  AND :DateTo) or (:UseShipDate = "T" and tblSales.ShipDate Betw' +
        'een :DateFrom  AND :DateTo))'
      'GROUP BY tblSales.ClientID,tblSales.SaleID')
