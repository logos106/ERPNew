  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      '2 AS `category`,'
      '`SP`.`SaleID` as SaleID,'
      '`SP`.`SaleLineID` as saleLineID,'
      'SP.SalesLinesPointsId as SalesLinesPointsId,'
      '`SP`.`ProductID` as ProductID,'
      '`SP`.`ProductName` as Productname,'
      '`SP`.`LinePoints` as `LinePoints`,'
      '`SP`.`LineValue` as `LineValue`,'
      '`SP`.`PointExpiresOn` as `PointExpiresOn`,'
      
        '`SP`.`ExpiresOnNoOfMonthsAfterPurchase` as `ExpiresOnNoOfMonthsA' +
        'fterPurchase`,'
      '`SP`.`PointsUOMID` as `PointsUOMID`,'
      '`SP`.`PointsUOMMultiplier` as `PointsUOMMultiplier`,'
      '`SP`.`Active` as `Active`,'
      '`SP`.`clientID` as `clientID`,'
      '`SP`.`RewardPointID` as `RewardPointID`,'
      '`SP`.`ValueforRewardPoint` as `ValueforRewardPoint`,'
      '`SP`.`IsOpeningBalance` as `IsOpeningBalance`,'
      '`SP`.`OpeningBalanceAsOn` as `OpeningBalanceAsOn`,'
      '`SP`.`DeptID` as `DeptID`,'
      '`SP`.`Deptname` as `Deptname`,'
      '`c`.`Company` as `Company`,'
      'ifnull(round(Sum(SRP.UsedPoints),2),0) UsedPoints , '
      'round(Sum(SRP.RedeemAmount),2) RedeemAmount, '
      
        'Cast( if(sp.IsOpeningBalance="T", SP.OpeningBalanceAsOn ,S.SaleD' +
        'ate)  as DAteTime ) as SaleDate, '
      
        'ifnull(CAST(if(Ifnull(SP.PointExpiresOn,0)<> 0 , SP.PointExpires' +
        'On ,  if(ifnull(SP.ExpiresOnNoOfMonthsAfterPurchase,0)  <> 0,  D' +
        'ate_Add(S.SaleDate , Interval   ExpiresOnNoOfMonthsAfterPurchase' +
        ' MONTH),NULL )) as DAteTime) , Date_Add(CURDATE() , Interval   1' +
        '000 YEAR))  as ExpiryDate ,'
      
        'if(sp.IsOpeningBalance="T" , "OpeningBalance", if(S.IsInvoice="T' +
        '", "Invoice" , "CashSale")) as TRanstype'
      'from tblsaleslinespoints SP'
      'inner join tblClients c on C.clientID = SP.clientID'
      'Left Join tblsales AS S ON SP.SaleID = S.SaleID'
      
        'Left join tblsalesredeempoints SRP on SRP.SalesLinesPointsId = S' +
        'P.SalesLinesPointsId and SRP.active ="T"'
      'where SP.active = "T"'
      'and C.ClientID = :ClientID'
      
        'and ifnull(CAST(if(Ifnull(SP.PointExpiresOn,0)<> 0 , SP.PointExp' +
        'iresOn ,  if(ifnull(SP.ExpiresOnNoOfMonthsAfterPurchase,0)  <> 0' +
        ',  Date_Add(S.SaleDate , Interval   ExpiresOnNoOfMonthsAfterPurc' +
        'hase MONTH),NULL )) as DAteTime) , Date_Add(CURDATE() , Interval' +
        '   1000 YEAR)) >= :Salesdate '
      'group by SP.SalesLinesPointsId'
      'union all'
      'Select '
      '1 AS `category`,'
      'NULL as saleID,'
      'NULL as SaleLineID,'
      'NULL as SalesLinesPointsId,'
      'NULL as ProductId,'
      'NULL ProductName ,'
      'Sum(`SP`.`LinePoints`) as LinePoints,'
      'Sum(`SP`.`LineValue`) as `LineValue`,'
      'NULL `PointExpiresOn` ,'
      'NULL `ExpiresOnNoOfMonthsAfterPurchase`'#9','
      'NULL `PointsUOMID`,'
      'NULL `PointsUOMMultiplier`,'
      'NULL `Active`,'
      '`SP`.`clientID`  `clientID`,'
      'NULL  `RewardPointID`,'
      'NULL `ValueforRewardPoint`,'
      'NULL `IsOpeningBalance`,'
      'NULL `OpeningBalanceAsOn`,'
      'NULL as `DeptID`,'
      'NULL as `Deptname`,'
      '`c`.`Company` as `Company`,'
      
        'sum((Select ifnull(round(Sum(SRP.UsedPoints),2),0)  from tblsale' +
        'sredeempoints SRP where  SRP.SalesLinesPointsId = SP.SalesLinesP' +
        'ointsId and SRP.active ="T"))  UsedPoints , '
      
        'sum((Select round(Sum(SRP.RedeemAmount),2)   from tblsalesredeem' +
        'points SRP where  SRP.SalesLinesPointsId = SP.SalesLinesPointsId' +
        ' and SRP.active ="T")) RedeemAmount, '
      'NULL SaleDate'#9', '
      'NULL ExpiryDate ,'
      'NULL TRanstype'
      'from tblsaleslinespoints SP'
      'inner join tblClients c on C.clientID = SP.clientID'
      'where SP.active = "T"'
      'and C.ClientID = :ClientID '
      'group by SP.clientID'
      'Order by company, ExpiryDate, deptname, productname , category')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ClientID'
      end
      item
        DataType = ftUnknown
        Name = 'Salesdate'
      end
      item
        DataType = ftUnknown
        Name = 'ClientID'
      end>
