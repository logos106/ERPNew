  inherited qryMain: TERPQuery
    SQL.Strings = (
      '# DUMMY SQL - See Refresh Query'
      'SELECT'
      '1 AS Details, '
      'Left(SUBSTRING_INDEX(C.Company,'#39'^'#39',1),255) as Company, '
      'C.JobName,C.JobNumber AS JobNumber, '
      
        'T.Hours AS Hours, T.ServiceName,T.ServiceDate,SUM(T.Qty) AS Qty ' +
        ', '
      'T.ChargeRate, T.EmployeeName,'
      'T.ClassName, T.Notes, T.ChargeRate As TotalCharge,'
      
        'IF(T2.SaleLineId > 0, LinePriceInc, T2.ChargeRate) AS TotalInvoi' +
        'ced,'
      'if(ifnull(T.SalesID,0) = 0 , "T", "F") AS "BeenInvoiced",'
      
        'S.InvoiceDocNumber AS "Invoice Ref #", S.SaleDate AS "Invoice Da' +
        'te", T.ID,'
      '"" as EquipmentName'
      'FROM tbltimesheets AS T'
      'INNER JOIN tblclients AS C on T.JobID = C.ClientID'
      'LEFT JOIN tblSales AS S on T.SalesID = S.SaleID'
      
        'LEFT JOIN tbltimesheets AS T2 on T2.ID = T.ID AND T2.SalesID <> ' +
        '0'
      'LEFT JOIN tblSalesLines AS SL on SL.SaleLineId = T2.SaleLineId'
      'WHERE  (ifnull(T.CustomerEquipmentID,0) = 0)'
      'AND (T.TimesheetDate BETWEEN '#39'1899-12-30'#39' AND '#39'2034-10-03'#39')'
      'GROUP BY T.ID'
      'UNION ALL'
      'SELECT'
      '1 AS Details, '
      'Left(SUBSTRING_INDEX(C.Company,'#39'^'#39',1),255) as Company, '
      'C.JobName,C.JobNumber AS JobNumber, '
      
        'T.Hours AS Hours, T.ServiceName,T.ServiceDate,SUM(T.Qty) AS Qty ' +
        ', '
      'T.ChargeRate, T.EmployeeName,'
      'T.ClassName, T.Notes, T.ChargeRate As TotalCharge,'
      
        'IF(T2.SaleLineId > 0, LinePriceInc, T2.ChargeRate) AS TotalInvoi' +
        'ced,'
      'if(ifnull(T.SalesID,0) = 0 , "T", "F") AS "BeenInvoiced",'
      
        'S.InvoiceDocNumber AS "Invoice Ref #", S.SaleDate AS "Invoice Da' +
        'te", T.ID,'
      'E.EquipmentName as EquipmentName'
      'FROM tbltimesheets AS T'
      'INNER JOIN tblclients AS C on T.JobID = C.ClientID'
      'LEFT JOIN tblSales AS S on T.SalesID = S.SaleID'
      
        'LEFT JOIN tbltimesheets AS T2 on T2.ID = T.ID AND T2.SalesID <> ' +
        '0'
      'LEFT JOIN tblSalesLines AS SL on SL.SaleLineId = T2.SaleLineId'
      
        'Inner Join tblcustomerequip AS CE ON T.CustomerEquipmentID = CE.' +
        'id'
      'Inner Join tblequipment AS E ON CE.EquipmentID = E.EquipmentID'
      'WHERE  (ifnull(T.CustomerEquipmentID,0) <> 0)'
      'AND (T.TimesheetDate BETWEEN '#39'1899-12-30'#39' AND '#39'2034-10-03'#39')'
      'GROUP BY T.ID'
      'UNION ALL'
      'SELECT'
      
        '0 AS Details, SUBSTRING_INDEX(C.Company,"^",1) as Company, C.Job' +
        'Name, C.JobNumber,'
      
        'SUM(T.Hours), NULL, NULL AS ChargeRate,NULL as ServiceDate,SUM(T' +
        '.Qty) AS Qty, NULL,'
      'NULL, NULL AS Notes, SUM(T.ChargeRate) As TotalCharge,'
      
        'IF(T2.SaleLineId > 0, SUM(LinePriceInc), SUM(T2.ChargeRate)) AS ' +
        'TotalInvoiced,'
      'NULL AS "BeenInvoiced", NULL, NULL, NULL,NULL'
      'FROM tbltimesheets AS T'
      'INNER JOIN tblclients AS C on T.JobID = C.ClientID'
      'LEFT JOIN tblSales AS S on T.SalesID = S.SaleID'
      
        'LEFT JOIN tbltimesheets AS T2 on T2.ID = T.ID AND T2.SalesID <> ' +
        '0'
      'LEFT JOIN tblSalesLines AS SL on SL.SaleLineId = T2.SaleLineId'
      'WHERE (T.TimesheetDate BETWEEN '#39'1899-12-30'#39' AND '#39'2034-10-03'#39')'
      'GROUP BY C.Company, C.JobName')
