  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'TRANS.ClientName as CustomerName,'
      'TRANS.TYPE as Type,'
      'Date as Date,'
      
        'If(TRANS.TYPE = '#39'Customer Payment'#39',TRANS.PaymentID, If(TRANS.TYP' +
        'E='#39'Customer Prepayment'#39',TRANS.PrepaymentID,InvoiceDocNumber)) as' +
        ' '#39'Transaction Number'#39','
      'Sum(DebitsInc) AS '#39'Debits'#39','
      'Sum(CreditsInc) AS '#39'Credits'#39','
      
        'If(TRANS.TYPE = '#39'Customer Payment'#39',0,If(TRANS.TYPE ='#39'Customer Pr' +
        'epayment'#39',0,TRANS.SaleID )) as SaleID,'
      '2 as Details,'
      'TRANS.PaymentID,'
      'TRANS.PrepaymentID,'
      'TRANS.ClientID as ClientID,'
      'TRANS.ClientName as Name'
      'FROM tbltransactions TRANS'
      
        'LEFT JOIN tbldepositline PL ON (TRANS.PaymentID = PL.PaymentID A' +
        'ND TRANS.SaleID = PL.SaleID)'
      'LEFT JOIN tblSales S ON TRANS.SaleID = S.SaleID'
      'Where not isNull(TRANS.AccountID) AND TRANS.AccountType = '#39'AR'#39
      
        'AND (TRANS.TYPE = '#39'Invoice'#39' OR TRANS.TYPE = '#39'Refund'#39' OR TRANS.TY' +
        'PE = '#39'Customer Payment'#39' OR TRANS.TYPE ='#39'Customer Prepayment'#39')'
      'AND (DebitsEx-CreditsEx)<>0.00 '
      'AND TRANS.Date  Between :txtfrom1  AND  :txtto1'
      'Group By ClientID,DATE,Details,SaleID,TRANS.PaymentID'
      ''
      'UNION ALL'
      ''
      'SELECT'
      'TRANS.ClientName as CustomerName,'
      'Null as Type,'
      'Null as Date,'
      'Null as '#39'Transaction Number'#39','
      'Sum(DebitsInc) AS '#39'Debits'#39','
      'Sum(CreditsInc) AS '#39'Credits'#39','
      '0 as SaleID,'
      '0 as Details,'
      '0 as PaymentID,'
      '0 as PrepaymentID,'
      'TRANS.ClientID as ClientID,'
      'TRANS.ClientName as Name'
      'FROM tbltransactions TRANS'
      
        'LEFT JOIN tbldepositline PL ON (TRANS.PaymentID = PL.PaymentID A' +
        'ND TRANS.SaleID = PL.SaleID)'
      'LEFT JOIN tblSales S ON TRANS.SaleID = S.SaleID'
      'Where not isNull(TRANS.AccountID) AND TRANS.AccountType = '#39'AR'#39
      
        'AND (TRANS.TYPE = '#39'Invoice'#39' OR TRANS.TYPE = '#39'Refund'#39' OR TRANS.TY' +
        'PE = '#39'Customer Payment'#39' OR TRANS.TYPE ='#39'Customer Prepayment'#39')'
      'AND (DebitsEx-CreditsEx)<>0.00 '
      'AND TRANS.Date  Between  :txtfrom2  AND  :txtto2'
      'Group By ClientID'
      ''
      'UNION ALL'
      ''
      'SELECT'
      'TRANS.ClientName as CustomerName,'
      'TRANS.TYPE as Type,'
      'Null as Date,'
      'Null as '#39'Transaction Number'#39','
      'Sum(DebitsInc) AS '#39'Debits'#39','
      'Sum(CreditsInc) AS '#39'Credits'#39','
      '0 as SaleID,'
      '1 as Details,'
      '0 as PaymentID,'
      '0 as PrepaymentID,'
      'TRANS.ClientID as ClientID,'
      'TRANS.ClientName as Name'
      'FROM tbltransactions TRANS'
      
        'LEFT JOIN tbldepositline PL ON (TRANS.PaymentID = PL.PaymentID A' +
        'ND TRANS.SaleID = PL.SaleID)'
      'LEFT JOIN tblSales S ON TRANS.SaleID = S.SaleID'
      'Where not isNull(TRANS.AccountID) AND TRANS.AccountType = '#39'AR'#39
      
        'AND (TRANS.TYPE = '#39'Invoice'#39' OR TRANS.TYPE = '#39'Refund'#39' OR TRANS.TY' +
        'PE = '#39'Customer Payment'#39' OR TRANS.TYPE ='#39'Customer Prepayment'#39')'
      'AND (DebitsEx-CreditsEx)<>0.00 '
      'AND TRANS.Date  Between  :txtfrom3  AND  :txtto3'
      'Group By ClientID,Type,Details'
      ''
      'Order By  ClientID,Details,DATE;')
    AutoCalcFields = False
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'txtfrom1'
      end
      item
        DataType = ftUnknown
        Name = 'txtto1'
      end
      item
        DataType = ftUnknown
        Name = 'txtfrom2'
      end
      item
        DataType = ftUnknown
        Name = 'txtto2'
      end
      item
        DataType = ftUnknown
        Name = 'txtfrom3'
      end
      item
        DataType = ftUnknown
        Name = 'txtto3'
      end>
