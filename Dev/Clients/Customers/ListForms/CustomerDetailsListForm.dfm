  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'tblClients.Company, '
      'tblClients.Company as CompanyName,'
      'EmployeeName as Rep,'
      #39'Permanent / Special Product'#39' as DiscountType,'
      'Discount*100 as Discount, '
      'SpecialDiscount*100 as SpecialDiscount, '
      'tblClients.ClientID,'
      'Space(30) as ProductName,'
      'Space(255) as Description,'
      '0.00 as OrigPrice,'
      '0.00 as LinePrice,'
      'Space(20) AS Col1, '
      'Space(20) AS Col2, '
      'Space(20) AS Col3,'
      '1 as Details,'
      
        'tblClients.CUSTFLD1, tblClients.CUSTFLD2, tblClients.CUSTFLD3, t' +
        'blClients.CUSTFLD4, tblClients.CUSTFLD5, tblClients.CUSTFLD6,'
      
        'tblClients.CUSTFLD7, tblClients.CUSTFLD8, tblClients.CUSTFLD9, t' +
        'blClients.CUSTFLD10, tblClients.CUSTFLD11, tblClients.CUSTFLD12,'
      
        'tblClients.CUSTFLD13, tblClients.CUSTFLD14, tblClients.CUSTFLD15' +
        ', tblClients.CUSTDATE1, tblClients.CUSTDATE2, tblClients.CUSTDAT' +
        'E3'
      'FROM tblClients'
      'LEFT JOIN tblemployees ON RepID = EmployeeID '
      'WHERE tblClients.Active = '#39'T'#39' AND Customer = '#39'T'#39
      ''
      'UNION ALL '
      ''
      'SELECT'
      'Space(50) as Company,'
      'Company as CompanyName,'
      #39' '#39' as Rep,'
      #39'Special Price'#39' as DiscountType,'
      '0.00 as Discount,'
      '0.00 as SpecialDiscount,'
      'CustomerID as ClientID,'
      'PartName as ProductName,'
      'PARTSDESCRIPTION as Description,'
      'OrigPrice,'
      'LinePrice,'
      'Space(20) AS Col1, '
      'Space(20) AS Col2, '
      'Space(20) AS Col3,'
      '2 as Details,'
      
        'tblClients.CUSTFLD1, tblClients.CUSTFLD2, tblClients.CUSTFLD3, t' +
        'blClients.CUSTFLD4, tblClients.CUSTFLD5, tblClients.CUSTFLD6,'
      
        'tblClients.CUSTFLD7, tblClients.CUSTFLD8, tblClients.CUSTFLD9, t' +
        'blClients.CUSTFLD10, tblClients.CUSTFLD11, tblClients.CUSTFLD12,'
      
        'tblClients.CUSTFLD13, tblClients.CUSTFLD14, tblClients.CUSTFLD15' +
        ', tblClients.CUSTDATE1, tblClients.CUSTDATE2, tblClients.CUSTDAT' +
        'E3'
      'FROM tblcustomerlines'
      
        'INNER JOIN tblClients ON tblClients.ClientID = tblcustomerlines.' +
        'CustomerID'
      
        'INNER JOIN tblparts ON tblparts.PARTSID = tblcustomerlines.Produ' +
        'ctID'
      ''
      'UNION ALL'
      ''
      'SELECT'
      'Space(50) as Company,'
      'Company as CompanyName,'
      #39' '#39' as Rep,'
      #39'Product Group'#39' as DiscountType,'
      'T1.Discount,'
      'Null as SpecialDiscount,'
      'T1.ClientID,'
      #39' '#39' As ProductName,'
      #39' '#39' as Description,'
      '0.00 as OrigPrice,'
      '0.00 as LinePrice,'
      'Col1, Col2, Col3,'
      '3 as Details,'
      
        'tblClients.CUSTFLD1, tblClients.CUSTFLD2, tblClients.CUSTFLD3, t' +
        'blClients.CUSTFLD4, tblClients.CUSTFLD5, tblClients.CUSTFLD6,'
      
        'tblClients.CUSTFLD7, tblClients.CUSTFLD8, tblClients.CUSTFLD9, t' +
        'blClients.CUSTFLD10, tblClients.CUSTFLD11, tblClients.CUSTFLD12,'
      
        'tblClients.CUSTFLD13, tblClients.CUSTFLD14, tblClients.CUSTFLD15' +
        ', tblClients.CUSTDATE1, tblClients.CUSTDATE2, tblClients.CUSTDAT' +
        'E3'
      'FROM tblprodgroupdiscs as T1'
      'INNER JOIN tblClients USING (ClientID)'
      ''
      'Order By  CompanyName,Details;')
