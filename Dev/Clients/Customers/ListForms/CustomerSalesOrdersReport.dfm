  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT TRIM(CL.Company) as Company,'
      'Space(50) as Type,'
      'Space(50) as SaleDate, '
      'Space(50) as DueDate,'
      'TRIM(CL.Company) as Name,'
      'Space(50) as '#39'Invoice Number'#39','
      'Sum(tblSalesLines.LineCost * Shipped) as '#39'Total Cost (Ex)'#39','
      'Sum(tblSalesLines.LineCostInc * Shipped) as '#39'Total Cost (Inc)'#39','
      'Sum(tblSalesLines.TotalLineAmount) as '#39'Total Amount (Ex)'#39','
      'Sum(tblSalesLines.TotalLineAmountInc) as '#39'Total Amount (Inc)'#39','
      'CL.Street as Address,'
      'CL.Street2 as '#39'Address 2'#39','
      'CL.Suburb,'
      'CL.Postcode,'
      'CL.State,'
      'CL.Phone,'
      'CL.FaxNumber,'
      'E.EmployeeName as REP,'
      '0 as SaleID,'
      'tblSales.ClientID,'
      '0 as Details,'
      #39' '#39' as Converted'
      'FROM tblSales'
      'INNER  JOIN tblSalesLines USING(SaleID)'
      'LEFT JOIN tblclients as CL on tblSales.ClientID = CL.ClientID'
      'LEFT JOIN tblemployees as E ON CL.RepID = E.EmployeeID'
      'WHERE tblSales.IsSalesOrder='#39'T'#39' AND tblSalesLines.Invoiced='#39'T'#39
      'AND tblSales.SaleDate Between :txtfrom1  AND :txtto1'
      'GROUP BY tblSales.ClientID'
      ''
      'Union ALL'
      ''
      'SELECT SPACE(30) as Company,'
      '"Sales Order" as Type,'
      'SaleDate as SaleDate,'
      'DueDate as DueDate,'
      'TRIM(tblclients.Company) as Name,'
      'InvoiceDocNumber as '#39'Invoice Number'#39','
      'Sum(tblSalesLines.LineCost * Shipped) as '#39'Total Cost (Ex)'#39','
      'Sum(tblSalesLines.LineCostInc * Shipped) as '#39'Total Cost (Inc)'#39','
      'Sum(tblSalesLines.TotalLineAmount) as '#39'Total Amount (Ex)'#39','
      'Sum(tblSalesLines.TotalLineAmountInc) as '#39'Total Amount (Inc)'#39','
      #39#39' as Address,'
      #39#39' as '#39'Address 2'#39','
      #39#39' as Suburb,'
      #39#39' as Postcode,'
      #39#39' as State,'
      #39#39' as Phone,'
      #39#39' as FaxNumber,'
      #39#39' as REP,'
      'tblSales.SaleID,'
      'tblSales.ClientID,'
