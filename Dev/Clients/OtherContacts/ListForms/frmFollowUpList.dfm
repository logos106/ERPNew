  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'OF.*, '
      'C.Company, '
      'E.EmployeeName, '
      'FRT.Name as ResultTypeName, '
      'OFT.Duration '
      '/*'
      'FROM tblClients C, tblEmployees E, tblOtherFollowUp OF'
      'LEFT JOIN tblSimpleTypes FT ON OF.TypeId = FT.Id'
      'LEFT JOIN tblSimpleTypes FRT ON OF.ResultTypeId = FRT.Id'
      
        'LEFT JOIN tblOtherFollowUpTime OFT ON OFT.FollowUpId = OF.Follow' +
        'UpId'
      
        'WHERE OF.ClientId = C.ClientId AND OF.EmployeeId = E.EmployeeId ' +
        'AND OF.IsOtherContact = "T" AND (OF.IsPhoneFollowUp = "T" OR FT.' +
        'Name = "Phone") AND OF.CreationDate BETWEEN :DTFrom AND :DTTo AN' +
        'D ((:Done = "Done" AND OF.Done = "T") OR (:Done = "Not Done" AND' +
        ' OF.Done <> "T") OR (:Done = "Both"))'
      'ORDER BY OF.CreationDate'
      '*/'
      'FROM tblOtherFollowUp OF'
      'INNER JOIN tblClients C on C.ClientId = OF.ClientId'
      'INNER JOIN tblEmployees E on OF.EmployeeId = E.EmployeeId'
      'LEFT JOIN tblSimpleTypes FT ON OF.TypeId = FT.Id'
      'LEFT JOIN tblSimpleTypes FRT ON OF.ResultTypeId = FRT.Id'
      
        'LEFT JOIN tblOtherFollowUpTime OFT ON OFT.FollowUpId = OF.Follow' +
        'UpId'
      
        'WHERE  OF.IsOtherContact = "T" AND (OF.IsPhoneFollowUp = "T" OR ' +
        'FT.Name = "Phone") '
      
        'AND OF.CreationDate BETWEEN :DTFrom AND :DTTo AND ((:Done = "Don' +
        'e" AND OF.Done = "T") OR (:Done = "Not Done" AND OF.Done <> "T")' +
        ' OR (:Done = "Both"))'
      'ORDER BY OF.CreationDate')
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'DTFrom'
        Value = 40909d
      end
      item
        DataType = ftDateTime
        Name = 'DTTo'
        Value = 41274d
