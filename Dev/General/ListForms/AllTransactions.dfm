  inherited qryMain: TERPQuery
    SQL.Strings = (
      
        'SELECT Distinct [PurchaseType]'#9' '#9'as Transtype, GlobalRef,'#9'Purcha' +
        'seOrderID '#9'as TransNo,OrderDate'#9#9'as TransDate,Deleted '#9#9'FROM tbl' +
        'PurchaseOrders'#9'Where orderDate '#9'between [DateFrom] and [DateTo] ' +
        'union all'
      
        'SELECT Distinct [Salestype]'#9#9'as Transtype, GlobalRef,'#9'SaleID'#9#9#9'a' +
        's TransNo,SaleDate'#9#9'as TransDate,Deleted '#9#9'FROM tblSales '#9#9'Where' +
        ' SaleDate '#9#9'between [DateFrom] and [DateTo] union all'
      
        'SELECT Distinct "Smart Order"'#9#9'as Transtype, GlobalRef,'#9'SmartOrd' +
        'erID'#9#9'as TransNo,CreationDate'#9'        as TransDate,"F" '#9#9'FROM tb' +
        'lsmartorder '#9'Where CreationDate '#9'between [DateFrom] and [DateTo]' +
        ' union all'
      
        'SELECT Distinct "Journal Entry"'#9#9'as Transtype, GlobalRef, '#9'GJID'#9 +
        #9#9'as TransNo,TransactionDate'#9'as TransDate,Deleted '#9#9'FROM tblgene' +
        'raljournal '#9'Where TransactionDate '#9'between [DateFrom] and [DateT' +
        'o] Union all'
      
        'SELECT Distinct "Suppliler Payment"'#9'as Transtype, GlobalRef, '#9'Pa' +
        'ymentID'#9#9'as TransNo,PaymentDate'#9'        as TransDate,Deleted'#9#9'FR' +
        'OM tblwithdrawal '#9'Where PaymentDate '#9'between [DateFrom] and [Dat' +
        'eTo] union all '
      
        'SELECT Distinct "Suppliler Pre-payment"'#9'as Transtype, GlobalRef,' +
        ' '#9'PrePaymentID'#9#9'as TransNo,PrePaymentDate'#9'as TransDate,Deleted '#9 +
        #9'FROM tblprepayments '#9'Where PrePaymentDate    between [DateFrom]' +
        ' and [DateTo] and  ifnull(Supplier , "F" ) = "T"  union all '
      
        'SELECT Distinct "Customer Prepayment"'#9'as Transtype, GlobalRef, '#9 +
        'PrePaymentID'#9#9'as TransNo,PrePaymentDate'#9'as TransDate,Deleted '#9#9'F' +
        'ROM tblprepayments '#9'Where PrePaymentDate    between [DateFrom] a' +
        'nd [DateTo] and ifnull(customer , "F" ) = "T"  union all '
      
        'SELECT Distinct "Deposit Entry"'#9'        as Transtype, GlobalRef,' +
        #9'PaymentID'#9#9'as TransNo,PaymentDate'#9'        as TransDate,Deleted ' +
        #9#9'FROM tbldeposits '#9'Where PaymentDate '#9'between [DateFrom] and [D' +
        'ateTo] and ifnull(customerPayment , "F") = "F" '#9' union all'
      
        'SELECT Distinct "Customer Payment"'#9'as Transtype, GlobalRef,'#9'Paym' +
        'entID'#9#9'as TransNo,PaymentDate'#9'        as TransDate,Deleted '#9#9'FRO' +
        'M tblDeposits '#9'Where PaymentDate '#9'between [DateFrom] and [DateTo' +
        '] and ifnull(customerPayment , "F") = "T"  union all'
      
        'SELECT Distinct [Deposittype]'#9#9'as Transtype, bd.GlobalRef,'#9'bd.De' +
        'positID'#9#9'as TransNo,bd.DepositDate'#9'as TransDate,bd.Deleted '#9'FROM' +
        ' tblbankdeposit as bd inner join tblbankdepositlines bdl on bdl.' +
        'DepositID = bd.DepositID Where  DepositDate between [DateFrom] a' +
        'nd [DateTo]  and not (bdl.TrnsType in ("Journal Entry" , "Cheque' +
        '" , "Cheque Deposit" , "Customer Prepayment" ,"Supplier Prepayme' +
        'nt" , "Customer Payment" , "Supplier Payment")) '
      'Order by Transtype')
