  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'E.EmployeeID as ID,  '
      'E.EmployeeID as employeeId,  '
      'E.clientID as clientID,'
      'E.EmployeeName as name,  '
      'E.Active as Active,'
      '"T" as ISEmployee, '
      '"F" as ISCustomer,'
      '"F" as ISSupplier, '
      '"F" as ISOthercontact'
      'from tblemployees E '
      'where ifnull(ClientID,0)=0'
      'union all '
      'Select '
      'C.clientID as ID,  '
      'E.EmployeeID as employeeId,  '
      'C.clientID as clientID,'
      'C.Company as NAme , '
      'C.Active as Active, '
      'if(ifnull(E.ClientID,0)=0,"F", "T") as ISEmployee, '
      'C.customer as ISCustomer,'
      'Supplier as ISSupplier, '
      'C.Othercontact as ISOthercontact'
      
        'from tblclients C left join tblEmployees E on E.clientID = C.cli' +
        'entID')
    Active = True
