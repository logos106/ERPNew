  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'ClientID as ClientID,'
      'Company as Company, '
      'Customer as Customer, '
      'supplier as supplier, '
      'Othercontact as Othercontact, '
      '"F" as Marketingcontact , '
      
        'if(Customer="T","Customer",if( supplier="T","Supplier" , if(Othe' +
        'rcontact="T" , "Other Contact" , "Unknown"))) as category'
      'FROM tblclients'
      'LEFT JOIN tblrelatedclients on ChildClientID=ClientID'
      'WHERE Active = '#39'T'#39
      'And (Customer = '#39'F'#39' OR (IsJob = '#39'F'#39' AND IsNull(ChildClientID)))'
      'union all'
      'Select  '
      'ID  as ClientID, '
      'COMPANY  as Company,  '
      '"F"  as Customer, '
      '"F" as supplier, '
      '"F" as Othercontact, '
      '"T"  as Marketingcontact , '
      '"Marketing Contact" as category'
      'from tblmarketingcontacts'
      'Order By Company;')
    object qryMainCompany: TWideStringField
      DisplayLabel = 'Name'
      DisplayWidth = 30
      FieldName = 'Company'
      Origin = '.Company'
      Size = 255
    end
