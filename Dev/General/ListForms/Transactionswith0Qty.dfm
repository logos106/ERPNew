  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      
        'If(S.IsCashSale="T" And S.IsPOS="F", "Cash Sale",If(S.IsRefund="' +
        'T", "Refund",If(S.ISSalesOrder="T", "Sales Order",If(S.IsQuote="' +
        'T", "Quote",If(S.IsCashSale="T" And S.IsPOS="T", "POS",If(S.Isla' +
        'yby="T" And S.IsPOS="T", "Layby",If(S.IsInvoice="T" OR S.IsQuote' +
        '="T", "Invoice","Unknown")))))))  as type,'
      '`S`.`GlobalRef`,'
      '`S`.`SaleID` as TrasnsId,'
      '`S`.`CustomerName` as clientName,'
      '`SL`.`ProductName`,'
      '`SL`.`UnitofMeasureSaleLines`,'
      '`SL`.`UnitofMeasureQtySold`,'
      '`SL`.`UnitofMeasureShipped`,'
      '`SL`.`UnitofMeasureBackorder`, '
      'S1.SaleID as BOID1, '
      'S2.SaleID as BOID2 '
      'FROM'
      '`tblsales` AS `S`'
      
        'inner join tblSaleslines SL on S.SaleId = Sl.SaleID and ifnull(S' +
        'l.shipped,0) = 0  and ifnull(Sl.Qtysold,0) = 0 and ifnull(BackOr' +
        'der,0) = 0'
      
        'inner join tblParts P on P.PartsID = ifnull(Sl.ProductId,0) and ' +
        'ifnull(P.PartType , "") ="INV"'
      
        'Left join tblsales S1 on S1.globalref = S.BOID and S1.Originalno' +
        ' = S.Globalref'
      
        'Left join tblSales S2 on S2.globalref = S1.BOID and S2.Originaln' +
        'o = S.Globalref'
      'union '
      'SELECT'
      
        'If(PO.IsPO="T","Purchase Order",If(PO.IsBill="T","Bill",If(PO.Is' +
        'Credit="T","Credit",If(PO.IsCheque="T","Cheque","Unknown")))) as' +
        ' type,'
      '`PO`.`GlobalRef`,'
      '`PO`.`PurchaseOrderID`,'
      '`PO`.`Suppliername`,'
      '`PL`.`ProductName`,'
      '`PL`.`UnitofMeasurePOLines`,'
      '`PL`.`UnitofMeasureQtySold`,'
      '`PL`.`UnitofMeasureShipped`,'
      '`PL`.`UnitofMeasureBackorder`, '
      '`PO1`.`PurchaseOrderID`  as BOID1,'
      '`PO2`.`PurchaseOrderID` as BOID2 '
      'FROM'
      'tblPurchaseOrders AS `PO`'
      
        'inner join tblPurchaseLines PL on PO.PurchaseOrderId = PL.Purcha' +
        'seOrderId and ifnull(pl.shipped,0) = 0  and ifnull(pl.Qtysold,0)' +
        ' = 0 and ifnull(Pl.BackOrder,0) = 0'
      
        'inner join tblParts P on P.PartsID = ifnull(Pl.ProductId,0) and ' +
        'ifnull(P.PartType , "") ="INV"'
      
        'Left join tblPurchaseOrders PO1 on PO1.globalref = PO.BOID and P' +
        'O1.Originalno = PO.Globalref'
      
        'Left join tblPurchaseOrders PO2 on PO2.globalref = PO1.BOID and ' +
        'PO2.Originalno = PO.Globalref'
      'Order by type, TrasnsId, Productname')
