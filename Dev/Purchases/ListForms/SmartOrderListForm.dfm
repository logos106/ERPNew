  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'SO.GlobalRef as GlobalRef,'
      'SmartOrderID as No,'
      'SO.CreationDate as CreationDate,'
      'Concat(OrderRep.FirstName, " ", OrderRep.Lastname) AS OrderRep,'
      'Concat(Approver.FirstName, " ", Approver.Lastname) AS Approver,'
      'SO.Approved as Approved ,'
      'SmartOrderDesc as SmartOrderDesc,'
      'SO.Done as Done,'
      'SO.EmployeeID as EmployeeID,'
      'SmartOrderID as SmartOrderID,'
      
        'ifnull((Select count(SmartOrderID) from tblsmartOrderlines SSOL ' +
        'where SSOL.smartOrderID = SO.SmartOrderId and ifnull(converted,"' +
        'F") <>"T"),0)  as notconvertedLines,'
      
        'ifnull((Select count(SmartOrderID) from tblsmartOrderlines SSOL ' +
        'where SSOL.smartOrderID = SO.SmartOrderId and ifnull(converted,"' +
        'F") ="T"),0)  as convertedLines,'
      'count(SOL.SmartOrderLinesID) NoOfItems,'
      'Sum(SOL.qty * SOL.LinePrice) as TotalLineamount'
      'FROM tblsmartorder SO'
      'LEFT join tblsmartorderlines SOL using (SmartOrderID)'
      
        'LEFT JOIN tblEmployees OrderRep ON OrderRep.EmployeeID=SO.Employ' +
        'eeID'
      
        'LEFT JOIN tblEmployees Approver  ON Approver.EmployeeID=SO.Appro' +
        'verID'
      'WHERE SO.CreationDate Between :txtfrom  AND  :txtto'
      'group by SO.SmartOrderID'
      'ORDER BY no DESC')
    Top = 386
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'txtfrom'
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end>
