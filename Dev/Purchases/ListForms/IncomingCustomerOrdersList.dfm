  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'S.CustomerName as CustomerName,'
      
        'if (S.IsInvoice = "T", "Invoice",if(S.IsSalesOrder = "T", "Sales' +
        ' Order","Other")) as SaleType,'
      'S.ClientPrintName as CustomerPrintName,'
      'S.InvoiceDocNumber as DocNumber,'
      'S.SaleID as SaleID,'
      'S.SaleDate as SaleDate,'
      'S.EmployeeName as SaleRep,'
      'PO.PurchaseOrderID as PurchaseOrderID,'
      'PO.EmployeeName as PurchaseRep,'
      'PO.PurchaseOrderNumber as PurchaseOrderNumber, '
      'PO.OrderDate as OrderDate,'
      'PO.SupplierName as SupplierName,'
      'PO.ClientPrintName as SupplierPrintName,'
      'POL.ProductID as ProductID,'
      'POL.ProductName as ProductName,'
      'POL.ProductPrintName as ProductPrintName,'
      'POL.Product_Description as ProductDescription,'
      'POL.UnitofMeasureQtySold as UOMQtyOrdered,'
      'POL.UnitofMeasureShipped as UOMQtyReceived,'
      'POL.UnitofMeasureBackorder as UOMQtyBackorder,'
      'CAST(POL.ReceivedDate AS Date) as ReceivedDate,'
      'SL.LinePriceInc as SaleLinePriceInc,'
      'SL.LinePrice as SaleLinePriceEx,'
      'SL.MemoLine as SaleMemoLine,'
      'SL.UnitofMeasureQtySold as SaleaUnitofMeasureQtySold,'
      'SL.UnitofMeasureShipped as SalesUnitofMeasureShipped,'
      'SL.UnitofMeasureBackorder as SalesUnitofMeasureBackorder,'
      'P.SupplierProductCode as SupplierProductCode,'
      'P.GeneralNotes as ProductGeneralNotes,'
      'SML.LineNotes as SmartOrderLineNotes,'
      'SML.SmartOrderID as SmartOrderID , '
      'C.phone as CustomerPhone,'
      'C.mobile as CustomerMobile ,'
      'C.email as customerEmail'
      'from tblpurchaselines POL'
      
        'inner join tblpurchaseorders PO on PO.PurchaseOrderID = POL.Purc' +
        'haseOrderID'
      
        'inner join tblsaleslines POSL on POSL.SaleLineID = POL.SaleLineI' +
        'D '
      
        'inner join tblsaleslines SL on SL.ProductID = POSL.PRoductId and' +
        ' SL.SeqNo = POSL.SeqNo  and ifnull(SL.Shipped,0)=0  and (SL.Base' +
        'Lineno = POSL.SaleLineID or SL.SaleLineID = POSL.SaleLineID)'
      
        'inner join tblSales S on S.SaleId = SL.SaleID and ifnull(S.boId,' +
        #39#39')='#39#39
      'inner join tblparts P on P.PARTSID = POL.ProductID'
      'inner join tblClients C on C.clientId = s.clientId'
      
        'left join tblsmartorderlines SML on SML.SmartOrderLinesID = POL.' +
        'SmartOrderLineID'
      'where S.SaleDate between :DateFrom and :DateTo'
      'and ((:filterindex = 0 and ifnull(PO.BaseNo,'#39#39') = '#39#39') or '
      '     (:filterindex = 1 and ifnull(PO.BOID  ,'#39#39') = '#39#39') or '
      '     (:filterindex = 2)) '
      'order By S.SaleDate, SL.ProductPrintName')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'filterindex'
      end
      item
        DataType = ftUnknown
        Name = 'filterindex'
      end
      item
        DataType = ftUnknown
        Name = 'filterindex'
      end>
