  inherited qryMain: TERPQuery
    SQL.Strings = (
      '## query changed dynamically in the refreshQuery'
      'SELECT 2 as DisplayCat , '
      'tblPurchaseOrders.PurchaseOrderID as PurchaseOrderID,'
      'tblPurchaseOrders.ShipToCustomer as ShipToCustomer,'
      
        'If(tblPurchaseOrders.IsPO='#39'T'#39','#39'Purchase Order'#39', If(tblPurchaseOr' +
        'ders.IsPOCredit='#39'T'#39','#39'PO Credit'#39','#39#39')) as Type,'
      'tblPurchaseOrders.ClientID as ClientID,'
      'tblPurchaseOrders.SupplierName as Company,'
      'tblPurchaseOrders.OriginalNo as OriginalNo,'
      '"     "  as "P/O Num",'
      'tblPurchaseOrders.OrderDate as OrderDate,'
      'tblPurchaseOrders.InvoiceNumber as InvoiceNumber,'
      'tblPurchaseLines.TotalLineAmount,'
      'tblPurchaseLines.TotalLineAmountInc,'
      'tblPurchaseLines.ProductName,'
      'tblPurchaseLines.LineNotes,'
      'tblPurchaseLines.Product_Description,'
      'tblPurchaseLines.UnitofMeasurePOLines,'
      'tblPurchaseLines.UnitofMeasureQtySold,'
      'tblPurchaseLines.UnitofMeasureShipped,'
      'tblPurchaseLines.Class,'
      'tblPurchaseLines.CustomerJob as CustomerJob,'
      'tblpurchaselines.ReceivedDate as ReceivedDate,'
      'tblPurchaseLines.LandedCostsPerItem, '
      'tblPurchaseLines.LandedCostsPercentage, '
      'tblPurchaseLines.LandedCostsTotal,'
      'tblPurchaseLines.CustomField1 as OrderLineCustomField1, '
      'tblPurchaseLines.CustomField2 as OrderLineCustomField2, '
      'tblPurchaseLines.CustomField3 as OrderLineCustomField3, '
      'tblPurchaseLines.CustomField4 as OrderLineCustomField4, '
      'tblPurchaseLines.CustomField5 as OrderLineCustomField5, '
      'tblPurchaseLines.CustomField6 as OrderLineCustomField6, '
      'tblPurchaseLines.CustomField7 as OrderLineCustomField7, '
      'tblPurchaseLines.CustomField8 as OrderLineCustomField8, '
      'tblPurchaseLines.CustomField9 as OrderLineCustomField9, '
      'tblPurchaseLines.CustomField10 as OrderLineCustomField10 '
      'FROM tblPurchaseOrders'
      
        'LEFT JOIN tblPurchaseLines ON  (tblPurchaseOrders.PurchaseOrderI' +
        'D = tblPurchaseLines.PurchaseOrderID)'
      'Where tblPurchaseLines.Invoiced = '#39'T'#39' and'
      '(tblPurchaseOrders.IsPO='#39'T'#39' OR tblPurchaseOrders.IsPOCredit='#39'T'#39')'
      ''
      'union All '
      'SELECT 1 as DisplayCat, '
      'tblPurchaseOrders.PurchaseOrderID as PurchaseOrderID,'
      'tblPurchaseOrders.ShipToCustomer as ShipToCustomer,'
      
        'If(tblPurchaseOrders.IsPO='#39'T'#39','#39'Purchase Order'#39', If(tblPurchaseOr' +
        'ders.IsPOCredit='#39'T'#39','#39'PO Credit'#39','#39#39')) as Type,'
      'tblPurchaseOrders.ClientID as ClientID,'
      'tblPurchaseOrders.SupplierName as Company,'
      'tblPurchaseOrders.OriginalNo as OriginalNo,'
      
        'if (original.PurchaseOrderId = tblPurchaseOrders.PurchaseOrderId' +
        ' , if (ifnull(tblPurchaseOrders.PurchaseOrderNumber ,'#39'-'#39') = '#39'-'#39' ' +
        ', tblPurchaseOrders.PurchaseOrderId , tblPurchaseOrders.Purchase' +
        'OrderNumber), concat_ws('#39#39',tblPurchaseOrders.PurchaseOrderNumber' +
        ', '#39'('#39' , original.PurchaseOrderNumber , '#39'_'#39' , tblPurchaseOrders.S' +
        'eqNo, '#39')'#39'))  as "P/O Num",'
      'tblPurchaseOrders.OrderDate as OrderDate,'
      'tblPurchaseOrders.InvoiceNumber as InvoiceNumber,'
      'tblPurchaseOrders.TotalAmount as TotalAmount,'
      'Round(tblPurchaseOrders.TotalAmountInc,2) as TotalAmountInc , '
      '"   " as ProductName,'
      '"   " as LineNotes,'
      '"   " as Product_Description,'
      '"   " as UnitofMeasurePOLines,'
      '0 as UnitofMeasureQtySold,'
      '0 as UnitofMeasureShipped,'
      '"   " as Class,'
      '"   " as CustomerJob,'
      'NULL as ReceivedDate,'
      'null as LandedCostsPerItem, '
      'null LasndedCostsPercentage, '
      'null as LandedCostsTotal,'
      'null as OrderLineCustomField1, '
      'null as OrderLineCustomField2, '
      'null as OrderLineCustomField3, '
      'null as OrderLineCustomField4, '
      'null as OrderLineCustomField5, '
      'null as OrderLineCustomField6, '
      'null as OrderLineCustomField7, '
      'null as OrderLineCustomField8, '
      'null as OrderLineCustomField9, '
      'null as OrderLineCustomField10 '
      ''
      'FROM tblPurchaseOrders'
      
        'LEFT JOIN tblPurchaseLines ON  (tblPurchaseOrders.PurchaseOrderI' +
        'D = tblPurchaseLines.PurchaseOrderID)'
      
        'LEFT Join tblPurchaseOrders as original  on original.GlobalRef =' +
        '  tblPurchaseOrders.OriginalNo'
      'Where tblPurchaseLines.Invoiced = '#39'T'#39' and'
      '(tblPurchaseOrders.IsPO='#39'T'#39' OR tblPurchaseOrders.IsPOCredit='#39'T'#39')'
      ''
      'GROUP BY tblPurchaseOrders.PurchaseOrderID'
      ''
      ''
      ''
      'Order by PurchaseOrderID , DisplayCat')
    Left = 94
    Top = 153
