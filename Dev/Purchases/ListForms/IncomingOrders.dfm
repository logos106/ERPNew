  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'PO.PurchaseOrderID as PurchaseOrderID, '
      'PO.Suppliername  as Suppliername  , '
      'S.SaleID as SaleID , '
      'S.customername  as customername , '
      'PL.productId as productId,'
      '"" as saletype, '
      'CAST(PO.orderdate AS DATE) as orderdate, '
      'PL.productname as productname , '
      'PL.Product_Description as Product_Description , '
      
        'concat(PL.UnitofMeasurePOLines , '#39'('#39' , PL.UnitofMeasureMultiplie' +
        'r , '#39')'#39') as UOM, '
      'PL.UnitofMeasureQtySold as uomsold, '
      'PL.UnitofMeasureShipped as uomshipped, '
      'PL.UnitofMeasureBackorder as uomBO ,'
      
        '#(Select Max(ReceivedDate) as ReceivedDate from tblPurchaseLines' +
        ' POL where POL.productId = PL.productId) as LastReceivedon ,'
      'CAST(PL.ReceivedDate as DATE) as ReceivedDate, '
      'P.PRICEINC1 as SalePriceinc , '
      'P.PRICE1 as SalePriceEx, '
      'P.SupplierProductCode as SupplierProductCode,'
      'P.GeneralNotes  as GeneralNotes , '
      'SOL.LineNotes SOLLineNotes , '
      'PL.invoiced as ReceivedOrder'
      'FROM tblPurchaseLines PL'
      
        'Left join tblsmartorderlines SOL on SOL.SmartOrderLinesID = PL.s' +
        'martOrderLineId'
      
        'INNER JOIN tblPurchaseOrders PO ON PO.PurchaseOrderID=PL.Purchas' +
        'eOrderID'
      'inner join tblParts P on PL.productId = P.partsId '
      'Left join tblsales S on S.GlobalRef = PO.SalesGlobalref'
      'LEFT JOIN tblEmployees E on E.EmployeeId = PO.SOApprovedBy'
      
        'WHERE PO.IsPO = '#39'T'#39' AND PO.IsRA = '#39'F'#39' AND PO.IsBill = '#39'F'#39' AND PO' +
        '.IsCredit = '#39'F'#39
      'AND PO.Deleted='#39'F'#39'  and PO.OrderDate Between :txtfrom AND :txtto'
      
        'AND ((:Approved = "T" AND PO.Approved = "T") OR (:Approved = "F"' +
        ')) '
      'Order by ReceivedDate')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'txtfrom'
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end
      item
        DataType = ftUnknown
        Name = 'Approved'
      end
      item
        DataType = ftUnknown
        Name = 'Approved'
      end>
