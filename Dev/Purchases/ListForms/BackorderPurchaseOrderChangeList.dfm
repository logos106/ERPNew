  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'PO.SupplierName,'
      'PO.ClientPrintName as PrintName,'
      'PL.Productname, '
      'PL.UnitofMeasureQtySold as BackOrder,'
      
        'concat(PL.UnitofMeasurePOLines, '#39'('#39' , PL.UnitofMeasureMultiplier' +
        ' , '#39')'#39') UnitofMeasurePOLines,'
      'PL.PurchaseLineID as PurchaseLineID,'
      'date(PO.OrderDate) as OrderDate,'
      'PO.EmployeeName as EmployeeName,'
      'PL.ProductID,'
      'PO.Comments as Comments,'
      'PO.GlobalRef as GlobalRef,'
      'PO.OriginalNo as OriginalNo,'
      'PO.PurchaseOrderNumber as "P/O Num",'
      'PL.LineCost as LineCost,'
      'PL.LineCost*PL.BackOrder as LineCostAmount,'
      'PL.ETADate as ETADate,'
      'PO.PurchaseOrderID as PurchaseOrderID,'
      'PL.Class as Class,'
      'PO.Approved as POApproved,'
      'PO.ConNote,'
      'PO.OrderStatus,'
      'PO.CreatedFromSO as SmartOrderId,'
      'PO.FuturePo,'
      'E.EmployeeName as SmartOrderApprovedBy,'
      
        'cast(cast(CONVERT(DATEDIFF(IfNull(PO.ETADate,0),IfNull(PO.OrderD' +
        'ate,0)) , signed) as decimal(0)) as signed) as LeadDays,'
      'PL.CustomField1 as OrderLineCustomField1, '
      'PL.CustomField2 as OrderLineCustomField2, '
      'PL.CustomField3 as OrderLineCustomField3, '
      'PL.CustomField4 as OrderLineCustomField4, '
      'PL.CustomField5 as OrderLineCustomField5, '
      'PL.CustomField6 as OrderLineCustomField6, '
      'PL.CustomField7 as OrderLineCustomField7, '
      'PL.CustomField8 as OrderLineCustomField8, '
      'PL.CustomField9 as OrderLineCustomField9, '
      'PL.CustomField10 as OrderLineCustomField10, '
      'PL.MemoLine as MemoLine,'
      'ifnull(tc.ID,0) as ChangeID,'
      'tc.CreateDate as ChangeCreateDate,'
      'ifnull(tc.Active,"T") as ChangeActive,'
      'ifnull(tc.Approved,"F") as ChangeApproved,'
      'ifnull(ce.EmployeeName,"") as ChangeApprover'
      'FROM tblPurchaseLines PL'
      
        'INNER JOIN tblPurchaseOrders PO ON PO.PurchaseOrderID=PL.Purchas' +
        'eOrderID'
      'LEFT JOIN tblEmployees E on E.EmployeeId = PO.SOApprovedBy'
      
        'LEFT JOIN tbltranschanges tc on tc.TransType = "Purchase Order" ' +
        'and tc.TransID = PO.PurchaseOrderID'
      
        'LEFT JOIN tblEmployees ce on tc.ApproverEmployeeID = ce.Employee' +
        'ID'
      
        'WHERE PO.IsPO = '#39'T'#39' AND PO.IsRA = '#39'F'#39' AND PO.IsBill = '#39'F'#39' AND PO' +
        '.IsCredit = '#39'F'#39
      
        'AND PL.Invoiced = '#39'F'#39' AND PO.Deleted='#39'F'#39' and PO.OrderDate Betwee' +
        'n :txtfrom AND :txtto'
      
        '/* AND ((:Approved = "T" AND PO.Approved = "T") OR (:Approved = ' +
        '"F")) */ '
      'AND   ifnull(PO.BOID , '#39#39') = '#39#39
      'GROUP BY PL.PurchaseLineID'
      'order by PL.PurchaseLineID desc')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'txtfrom'
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end>
