  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select  '
      'SL.PARTSNAME as PARTSNAME,'
      'sum( SL.Qty ) as Qty,  '
      'EP.SupplierProductCode as SupplierProductCode, '
      'EP.SupplierProductName as SupplierProductName, '
      'c.company as suppliername ,'
      'P.COSTINC1 as COSTINC1, '
      'P.COST1 COST1, '
      'EP.Price as ExtraBuyPrice, '
      'C.Email as Email,'
      'C.ClientID as ClientID'
      'from tblsmartorderlines SL '
      'inner join tblparts P on P.partsId = SL.PARTSID '
      
        'inner  join (tblextraprice EP  inner join tblclients C on C.Clie' +
        'ntID = EP.supplierID ) on SL.PARTSID = EP.PARTSID and (ifnull(EP' +
        '.UOMID,0)=0 or  EP.UOMID = SL.UnitOfMeasureID) '
      'where SmartOrderID = :SmartOrderID'
      'group by SL.PARTSNAME ,c.company '
      'union all '
      'Select  '
      'null as PARTSNAME , '
      'null as Qty ,  '
      'null as SupplierProductCode , '
      'null as SupplierProductName , '
      'c.company as suppliername ,'
      'null as COSTINC1 , '
      'null as COST1 , '
      'null as ExtraBuyPrice,  '
      'C.Email as Email,'
      'C.ClientID as ClientID'
      'from  tblclients C '
      
        'left join  (tblextraprice EP inner join tblsmartorderlines SL on' +
        ' SL.SmartOrderID = :SmartOrderID and  SL.PARTSID = EP.PARTSID an' +
        'd (ifnull(EP.UOMID,0) or  EP.UOMID = SL.UnitOfMeasureID) )     o' +
        'n C.ClientID = EP.supplierID '
      'where C.Supplier="T" and  ifnull(EP.PARTSID,0)=0'
      'order by PARTSNAME desc, SupplierName')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'SmartOrderID'
      end
      item
        DataType = ftUnknown
        Name = 'SmartOrderID'
      end>
