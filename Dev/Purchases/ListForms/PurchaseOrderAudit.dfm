  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT distinct '
      'PO.clientID, '
      'PO.Globalref, '
      'PO.PurchaseOrderID as PurchaseOrderID,'
      'PO.PurchaseOrderNumber as PurchaseOrderNumber,'
      'C.Company as Company,'
      
        'if (original.PurchaseOrderId = PO.PurchaseOrderId , if (ifnull(P' +
        'O.PurchaseOrderNumber ,'#39'-'#39') = '#39'-'#39' , PO.PurchaseOrderId , PO.Purc' +
        'haseOrderNumber), concat_ws('#39#39',PO.PurchaseOrderNumber, '#39'('#39' , ori' +
        'ginal.PurchaseOrderNumber , '#39'_'#39' , PO.SeqNo, '#39')'#39'))  as "P/O Num",'
      'date(PO.OrderDate) as OrderDate,'
      'PO.Comments as Comments,'
      'PO.ConNote as ConNote,'
      'C.Contact1 as Contact,'
      'PO.ETADate as ETADate,'
      'C.Phone as Phone,'
      'PO.InvoiceNumber as InvoiceNumber,'
      'PO.Deleted as Deleted,'
      'PO.SalesComments as WarehouseNotes,'
      'PO.TotalAmount as TotalAmount,'
      'Round(PO.TotalAmountInc,2) as TotalAmountInc,'
      'PO.InvoiceDate as InvoiceDate,'
      'PO.Approved, '
      'PO.Paid, '
      'NULL as IsBO,'
      
        'if(IfNull(PD.DocOutputType,"") = '#39'dotPrint'#39', '#39'T'#39' ,'#39'F'#39') as Printe' +
        'd, '
      
        'if(IfNull(PD.DocOutputType,"") = '#39'dotEmail'#39', '#39'T'#39' ,'#39'F'#39') as Emaile' +
        'd  , '
      'F.Description PrintedFrom, '
      'E.EmployeeName PrintedBy, '
      'Count(PD.PrintdocId) totalcount ,'
      
        'sum(if(IfNull(PD.DocOutputType,"") = '#39'dotEmail'#39' , 1, 0)) emailco' +
        'unt, '
      
        'sum(if(IfNull(PD.DocOutputType,"") = '#39'dotPrint'#39' , 1, 0)) printco' +
        'unt,'
      
        'sum(if(IfNull(PD.DocOutputType,"") = '#39'dotPreview'#39' , 1, 0)) Previ' +
        'ewcount,'
      'PD.Printdate,'
      'C.EmailPOXML as EmailPOXML, '
      'C.SendXMLPOs as SendXMLPOs,'
      'C.FTPUserName as FTPUserName,'
      'C.FTPAddress as FTPAddress , '
      'C.FTPPassword as FTPPassword'
      'FROM tblPurchaseOrders PO'
      'LEFT JOIN tblClients C ON  (PO.SupplierName = C.Company)'
      
        'LEFT Join tblPurchaseOrders as original  on original.GlobalRef =' +
        '  PO.OriginalNo'
      'LEFT JOIN tblprintdocs PD on PD.DocId = PO.PurchaseOrderID '
      'LEft join tblforms F on F.FormName = PD.printedfrom '
      'Left join tblemployees E on PD.PrintedBy = E.EmployeeID'
      'Where  PO.IsPO='#39'T'#39
      'AND PO.OrderDate between :txtFrom and :txtTo'
      
        'AND ((:Approved = "T" AND PO.Approved = "T") OR (:Approved = "F"' +
        ')) '
      
        '/*AND (((PO.TotalAmountInc <= :EmployeeApprovalValueH) AND (PO.T' +
        'otalAmountInc >= :EmployeeApprovalValueL)) OR ((:EmployeeApprova' +
        'lValueH = 0)OR (PO.EnteredBy = :EnteredBy)))*/'
      'group by PD.DocId , PD.printedfrom  , PD.PrintedBy '
      'Order by DocId  , PrintedBy , printedfrom')
