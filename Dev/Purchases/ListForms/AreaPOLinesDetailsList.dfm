  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select Distinct'
      '1                 as Details,'
      'PQA.EquipmentId   as EquipmentId ,'
      'PQA.EquipmentName as EquipmentName ,'
      'PQA.Transdate     as Transdate,'
      #39#39'          as AreaCode ,'
      'NULL              as productName ,'
      'NULL              as ProductID,'
      
        'Round(IF(P.PARTTYPE = "INV",Round(Sum(if (PQA.TransType ='#39'TSales' +
        'OrderInvoiceLines'#39' and PQA.Active ="F" ,0 ,  if(PQA.Active ="F" ' +
        'and  PQA.alloctype ="IN" and PQA.TransType in ('#39'TProcTreePart'#39','#39 +
        'TProcTreePartIN'#39'), ifnull(PQA.QTY,0),  if((PQA.Active ="F") and ' +
        ' (not(PQA.TransType in ('#39'TProcTreePart'#39','#39'TProcTreePartIN'#39')) OR (' +
        '(PQA.TransType='#39'TProcTreePart'#39') and (PQA.alloctype ="IN")) ), 0,' +
        '  if( PQA.alloctype="IN" and  PQA.ISBo="F", ifnull(PQA.QTY,0),  ' +
        'if(PQA.alloctype="OUT" and (PQA.TransType<>'#39'TSalesOrderLine'#39' or ' +
        'PQA.ISBO="F") ,0-ifnull(PQA.QTY,0),0)))))),5),0.0),5) AS Availab' +
        'leQty,'
      
        'Round(IF(P.PARTTYPE = "INV",Round(Sum( if(PQA.Active ="F" , 0,  ' +
        'if(PQA.TransType='#39'TSalesOrderLine'#39' or PQA.IsBO ="T",0, if(PQA.Tr' +
        'ansType = '#39'TPOSLaybyLines'#39' and PQA.Reserved = "T" ,0 , if(PQA.al' +
        'loctype="IN"  , ifnull(PQA.QTY,0), 0-ifnull(PQA.QTY,0)))))),5),0' +
        '.0),5) AS InStockqty,'
      
        'Round(IF(P.PARTTYPE = "INV",Round(Sum(if(PQA.Active ="F" , 0,  i' +
        'f(((PQA.TransType = '#39'TSalesOrderLine'#39') OR (PQA.TransType = '#39'TPOS' +
        'LaybyLines'#39' and PQA.Reserved = "T")) and (PQA.IsBO ="F"),ifnull(' +
        'PQA.QTY,0),  if(PQA.TransType = '#39'TSalesOrderInvoiceLines'#39' and PQ' +
        'A.alloctype="IN" , 0-ifnull(PQA.QTY,0),0 )))),5),0.0),5) AS Allo' +
        'catedSOqty,'
      
        'Round(IF(P.PARTTYPE = "INV",Round(Sum(if(PQA.Active ="F" , 0,  i' +
        'f(PQA.TransType='#39'TInvoiceLine'#39' and PQA.IsBO ="T",ifnull(PQA.QTY,' +
        '0),0))),5),0.0),5) AS AllocatedBOQty,'
      
        'Round(IF(P.PARTTYPE = "INV",Round(Sum(if(PQA.Active ="F" , 0,  i' +
        'f(PQA.TransType='#39'TPurchaseOrderLine'#39' and PQA.IsBO ="T",ifnull(PQ' +
        'A.QTY,0),0))),5),0.0),5) AS OnOrderQty,'
      
        'Round(IF(P.PARTTYPE = "INV",Round(Sum(if(PQA.Active ="F" and  PQ' +
        'A.alloctype ="OUT" and PQA.TransType in ('#39'TProcTreePart'#39','#39'TProcT' +
        'reePartIN'#39'), ifnull(PQA.QTY,0),0)),5),0.0),5) AS OnBuildqty,'
      
        'Round(IF(P.PARTTYPE = "INV",Round(Sum(if(PQA.Active ="F" , 0,  i' +
        'f(PQA.TransType='#39'TSalesOrderLine'#39' and PQA.IsBO ="T",ifnull(PQA.Q' +
        'TY,0),0))),5),0.0),5) AS SOBOQty'
      'from  tblparts P'
      'inner join `tblProductClasses` as PC on PC.ProductID = P.PartsId'
      
        'inner join tblpqa   as PQA on PC.ProductID = PQA.ProductID and P' +
        'C.ClassID = PQA.departmentID'
      'group by PQA.equipmentId  ,PQA.EquipmentName ,PQA.transdate ,'#39#39)
