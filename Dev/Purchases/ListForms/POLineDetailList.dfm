  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'PO.PurchaseOrderid as PurchaseOrderId,'
      'POL.ProductId as PartsId,'
      'POL.ProductName as ProductName,'
      'P.PartType as PartType,'
      
        'if(P.PartType = "INV",POL.Shipped, if(not isnull(PQABatch.PQATyp' +
        'e),PQABatch.Qty,if(not isnull(PQABin.PQAType),PQABin.Qty,if(not ' +
        'isnull(PQASN.PQAType),PQASN.Qty,0)))) as Qty,'
      
        'if(P.PartType = "INV",POL.UnitOfMeasureShipped, if(not isnull(PQ' +
        'ABatch.PQAType),PQABatch.UOMQty,if(not isnull(PQABin.PQAType),PQ' +
        'ABin.UOMQty,if(not isnull(PQASN.PQAType),PQASN.UOMQty,0)))) as U' +
        'OMQty,'
      'PQA.UOM as UOM,'
      'PQA.UOMID as UOMID,'
      'POL.LineCost as `Cost`,'
      'if(not isnull(PQABatch.PQAType),"T","F") as Batch,'
      'if(not isnull(PQABin.PQAType),"T","F") as `Bin`,'
      'if(not isnull(PQASN.PQAType),"T","F") as SN,'
      'if(not isnull(PQABin.PQAType),PQABin.BinID,null) as BinId,'
      'if(not isnull(PQABin.PQAType),PQABin.Value,"") as BinNumber,'
      
        'if(not isnull(PQABin.PQAType),BIN.BinLocation,"") as BinLocation' +
        ','
      
        'if(not isnull(PQABatch.PQAType),PQABatch.Value,"") as BatchNumbe' +
        'r,'
      
        'if(not isnull(PQABatch.PQAType),PQABatch.ExpiryDate,null) as Exp' +
        'iryDate,'
      'if(not isnull(PQASN.PQAType),PQASN.Value,null) as SerialNumber'
      'from tblPurchaseOrders PO'
      
        'inner join tblPurchaseLines POL on PO.PurchaseOrderID = POL.Purc' +
        'haseOrderID'
      'inner join tblParts P on POL.ProductId = P.PartsId'
      
        'left join tblPQA PQA on POL.PurchaseLineID = PQA.TransLineID and' +
        ' TransType = "TPurchaseOrderLine" and PQA.Active = "T"'
      
        'left join tblPQADetails PQABatch on PQA.PQAID = PQABatch.PQAID a' +
        'nd PQABatch.PQAType = "Batch" and PQABatch.Active = "T"'
      
        'left join tblPQADetails PQABin on PQA.PQAID = PQABin.PQAID and P' +
        'QABin.PQAType = "Bin" and PQABin.Active = "T"'
      
        'left join tblPQADetails PQASN on PQA.PQAID = PQASN.PQAID and PQA' +
        'SN.PQAType = "SN" and PQASN.Active = "T"'
      'left join tblproductbin BIN on PQABin.BinID = BIN.BinID '
      'where POL.ProductID > 0'
      'and PO.PurchaseOrderId = :PurchaseOrderId')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'PurchaseOrderId'
      end>
