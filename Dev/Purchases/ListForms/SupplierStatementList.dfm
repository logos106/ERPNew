  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'tblpurchaseorders.SupplierName,'
      
        'If(tblpurchaseorders.IsBill='#39'T'#39','#39'Bill'#39',If(tblpurchaseorders.IsPO' +
        '='#39'T'#39','#39'Purchase Order'#39','
      
        'If(tblpurchaseorders.IsCredit='#39'T'#39','#39'Credit'#39',If(tblpurchaseorders.' +
        'IsCheque='#39'T'#39','#39'Cheque'#39','#39'Return Authority'#39')))) as Type,'
      'Convert(tblpurchaseorders.OrderDate, datetime)  as Date,'
      'tblpurchaseorders.GlobalRef as '#39'Global #'#39' ,'
      'tblpurchaseorders.PurchaseOrderNumber as '#39'Transaction No'#39' ,'
      
        'If(tblpurchaseorders.IsCredit='#39'T'#39',-tblpurchaseorders.TotalAmount' +
        ',tblpurchaseorders.TotalAmount) as '#39'Total Amount (Ex)'#39','
      
        'If(tblpurchaseorders.IsCredit='#39'T'#39',-tblpurchaseorders.TotalAmount' +
        'Inc,tblpurchaseorders.TotalAmountInc) as '#39'Total Amount (Inc)'#39' ,'
      
        'If(tblpurchaseorders.IsCredit='#39'T'#39',-tblpurchaseorders.Payment,tbl' +
        'purchaseorders.Payment) as '#39'Paid'#39' ,'
      
        'If(tblpurchaseorders.IsCredit='#39'T'#39',-tblpurchaseorders.balance,tbl' +
        'purchaseorders.balance) as '#39'balance'#39' ,'
      'tblpurchaseorders.Comments as '#39'Comments'#39','
      'tblpurchaseorders.PurchaseOrderID as PurchaseOrderID,'
      
        'tblpurchaseorders.ClientID, tblpurchaseorders.InvoiceNumber,tblp' +
        'urchaseorders.Terms'
      'FROM tblpurchaseorders'
      
        'WHERE (IsBill ='#39'T'#39' OR IsPO ='#39'T'#39' OR IsRA ='#39'T'#39' OR IsCredit ='#39'T'#39' OR' +
