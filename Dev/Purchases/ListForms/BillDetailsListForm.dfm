  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'tblPurchaseLines.GlobalRef,'
      'tblPurchaseOrders.PurchaseOrderID as PurchaseOrderID,'
      'tblPurchaseLines.PurchaseLineID as PurchaseLineID,'
      'If(tblPurchaseOrders.IsBill='#39'T'#39','#39'Bill'#39','#39'Credit'#39') AS Type,'
      'tblClients.ClientID as ClientID,'
      'tblClients.Company as Company,'
      
        'Concat_WS('#39#39',tblPurchaseOrders.PurchaseOrderNumber) as BillNumbe' +
        'r,'
      'tblPurchaseOrders.OrderDate as OrderDate,'
      'tblPurchaseOrders.Comments,'
      'tblPurchaseLines.AccountName,'
      'tblPurchaseLines.LineTaxCode as '#39'Tax Code'#39','
      'tblPurchaseLines.LineTax as '#39'Tax'#39','
      'tblPurchaseLines.Product_Description as '#39'Memo'#39','
      'tblPurchaseLines.TotalLineAmount as '#39'Amount (Ex)'#39','
      'tblPurchaseLines.TotalLineAmountInc as '#39'Amount (Inc)'#39','
      'tblClients.Contact1 as Contact,'
      'tblPurchaseOrders.ETADate as ETADate,'
      'tblClients.Phone as Phone,'
      'tblPurchaseLines.Invoiced as Invoiced,'
      'tblPurchaseOrders.IsCredit,'
      'tblPurchaseOrders.InvoiceNumber as InvoiceNumber,'
      'tblPurchaseOrders.IsRA as IsRA,'
      'tblPurchaseOrders.Deleted as Deleted,'
      'tblPurchaseOrders.SalesComments as SalesComments,'
      'tblPurchaseLines.CustomerJob,'
      'tblPurchaseLines.CustomerJobID,'
      'tblPurchaseLines.Class as '#39'Department'#39', '
      'ifnull(tblPurchaseOrders.Balance,0) as Balance'
      'FROM tblPurchaseLines'
      
        'INNER JOIN tblPurchaseOrders ON (tblPurchaseLines.PurchaseOrderI' +
        'D = tblPurchaseOrders.PurchaseOrderID)'
      
        'LEFT JOIN tblClients ON  (tblPurchaseOrders.SupplierName = tblCl' +
        'ients.Company)'
      
        'LEFT JOIN tblParts ON  (tblPurchaseLines.ProductName = tblParts.' +
        'PARTName)'
      
        'WHERE tblPurchaseLines.AccountName <> Space(1) AND tblPurchaseOr' +
        'ders.OrderDate And (tblPurchaseOrders.IsBill = '#39'T'#39' OR tblPurchas' +
        'eOrders.IsCredit = '#39'T'#39')  And OrderDate Between :txtfrom AND :txt' +
        'to')
    FetchRows = 1
    ParamData = <
      item
        DataType = ftString
        Name = 'txtfrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end>
