    SQL.Strings = (
      'SELECT '
      '1 as Seqno,'
      'PO.GlobalRef as GlobalRef,'
      'PO.PurchaseOrderID as PurchaseOrderID,'
      'EC.ExpenseClaimId as ExpenseClaimID,'
      '"Cheque" AS Type,'
      'E.EmployeeName as EmployeeName,'
      'C.ClientID as ClientID,'
      'C.Company as Company,'
      'EC.Description as Description,'
      'PO.PurchaseOrderNumber as ClaimNumber,'
      'Convert(EC.DateTime, Date) as Claimdate,'
      'date(PO.OrderDate) as OrderDate,'
      'PO.Approved as Approved,'
      'PO.Paid as Paid,'
      'PO.Comments as Comments,'
      'C.Contact1 as Contact,'
      'PO.ETADate as ETADate,'
      'C.Phone as Phone,'
      'POL.Invoiced as Invoiced,'
      'PO.IsCredit as IsCredit,'
      'PO.InvoiceNumber as InvoiceNumber,'
      'PO.IsRA as IsRA,'
      'PO.Deleted as Deleted,'
      'PO.SalesComments as SalesComments,'
      'PO.TotalAmount as TotalAmountEx,'
      'PO.TotalTax as TotalTax,'
      'PO.TotalAmountInc as TotalAmountInc,'
      'null as Processed,'
      'ECL.Approval as Status'
      'FROM tblexpenseclaim EC'
      
        'inner join tblexpenseclaimline ECL on EC.ExpenseClaimID = ECL.Ex' +
        'penseClaimID'
      
        'inner join tblPurchaseOrders PO on ECL.ChequeId = PO.purchaseOrd' +
        'erId'
      
        'INNER JOIN tblPurchaseLines POL ON (PO.PurchaseOrderID = POL.Pur' +
        'chaseOrderID)'
      'LEFT JOIN tblClients C ON  (PO.SupplierName = C.Company)'
      'LEFT JOIN tblParts ON  (POL.ProductName = tblParts.PARTName)'
      
        'LEFT JOIN tblemployees E ON (PO.ExpenseClaimEmployee = E.Employe' +
        'eID)'
      'WHERE POL.AccountName <> Space(1) '
      ' AND (PO.IsCheque = "T")'
      ' AND (PO.ExpenseClaimEmployee <> 0)'
      ' AND OrderDate Between :txtfrom AND :txtto'
      
        'and ((PO.OrderStatus = :Status) or (:Status = "All") or (:Status' +
        ' = "Processed" and ECL.Processed = "T"))'
      'GROUP BY PO.PurchaseOrderID'
      'union all '
      'SELECT '
      '2 as Seqno,'
      'PO.GlobalRef as GlobalRef,'
      'PO.PurchaseOrderID as PurchaseOrderID,'
      'EC.ExpenseClaimId as ExpenseClaimID,'
      '"Bill" AS Type,'
      'E.EmployeeName as EmployeeName,'
      'C.ClientID as ClientID,'
      'C.Company as Company,'
      'EC.Description as Description,'
      'PO.PurchaseOrderNumber as ClaimNumber,'
      'Convert(EC.DateTime, Date) as Claimdate,'
      'date(PO.OrderDate) as OrderDate,'
      'PO.Approved as Approved,'
      'PO.Paid as Paid,'
      'PO.Comments as Comments,'
      'C.Contact1 as Contact,'
      'PO.ETADate as ETADate,'
      'C.Phone as Phone,'
      'POL.Invoiced as Invoiced,'
      'PO.IsCredit as IsCredit,'
      'PO.InvoiceNumber as InvoiceNumber,'
      'PO.IsRA as IsRA,'
      'PO.Deleted as Deleted,'
      'PO.SalesComments as SalesComments,'
      'PO.TotalAmount as TotalAmountEx,'
      'PO.TotalTax as TotalTax,'
      'PO.TotalAmountInc as TotalAmountInc,'
      'null as Processed,'
      'ECL.Approval as Status'
      'FROM tblexpenseclaim EC'
      
        'inner join tblexpenseclaimline ECL on EC.ExpenseClaimID = ECL.Ex' +
        'penseClaimID'
      
        'inner join tblPurchaseOrders PO on ECL.BillID = PO.purchaseOrder' +
        'Id'
      
        'INNER JOIN tblPurchaseLines POL ON (PO.PurchaseOrderID = POL.Pur' +
        'chaseOrderID)'
      'LEFT JOIN tblClients C ON  (PO.SupplierName = C.Company)'
      'LEFT JOIN tblParts ON  (POL.ProductName = tblParts.PARTName)'
      
        'LEFT JOIN tblemployees E ON (PO.ExpenseClaimEmployee = E.Employe' +
        'eID)'
      'WHERE POL.AccountName <> Space(1) AND PO.OrderDate'
      ' AND (PO.IsBill = "T")'
      ' AND (PO.ExpenseClaimEmployee <> 0)'
      ' AND OrderDate Between :txtfrom AND :txtto'
      
        'and ((PO.OrderStatus = :Status) or (:Status = "All") or (:Status' +
        ' = "Processed" and ECL.Processed = "T"))'
      'GROUP BY PO.PurchaseOrderID'
      'union all'
      'select'
      '3 as Seqno,'
      'EC.GlobalRef as GlobalRef,'
      'null as PurchaseOrderID,'
      'EC.ExpenseClaimId as ExpenseClaimID,'
      'if(EC.active="F" , "Deleted Request" , "Request") as Type,'
      'EC.EmployeeName as EmployeeName,'
      'null as ClientID,'
      'NULL as Company,'
      'EC.Description as Description,'
      'EC.RefNo as ClaimNumber,'
      'Convert(EC.DateTime, Date) as Claimdate,'
      'Null as OrderDate,'
      'null as Approved,'
      '"F" as Paid,'
      'EC.Comments as Comments,'
      'null as Contact,'
      'Date("1001-01-01") as ETADate,'
      'null as Phone,'
      '"F" as Invoiced,'
      '"F" as IsCredit,'
      'Null as InvoiceNumber,'
      '"F" as IsRA,'
      'not EC.Active as Deleted,'
      'null as SalesComments,'
      'EC.TotalAmountEx as "TotalAmountEx",'
      'EC.TotalTax as "TotalTax",'
      'EC.TotalAmountInc as "TotalAmountInc",'
      'EC.Processed as Processed,'
      'ECL.Approval as Status'
      'from tblExpenseClaim EC'
      
        'inner join tblExpenseClaimLine ECL on ECL.ExpenseClaimId = EC.Ex' +
        'penseClaimId '
      '-- and ifnull(ECL.chequeID,0)=0 and ifnull(ECL.billID,0)=0 '
      'where EC.DateTime Between :txtfrom AND :txtto'
      
        'and ((ECL.Approval = :Status) or (:Status= '#39'All'#39') or (:Status = ' +
        '"Processed" and EC.Processed = "T") )'
      'group by ECL.ExpenseClaimId'
      'union all '
      'SELECT '
      '4 as Seqno,'
      'PO.GlobalRef as GlobalRef,'
      'PO.PurchaseOrderID as PurchaseOrderID,'
      'NULL as ExpenseClaimID,'
      
        'If(PO.IsBill="T",'#39'Bill'#39',If(PO.IsCredit="T",'#39'Credit'#39','#39'PO'#39')) AS Ty' +
        'pe,'
      'E.EmployeeName as EmployeeName,'
      'C.ClientID as ClientID,'
      'C.Company as Company,'
      'NULL as Description,'
      'PO.PurchaseOrderNumber as ClaimNumber,'
      'NULL as Claimdate,'
      'date(PO.OrderDate) as OrderDate,'
      'PO.Approved as Approved,'
      'PO.Paid as Paid,'
      'PO.Comments as Comments,'
      'C.Contact1 as Contact,'
      'PO.ETADate as ETADate,'
      'C.Phone as Phone,'
      'POL.Invoiced as Invoiced,'
      'PO.IsCredit as IsCredit,'
      'PO.InvoiceNumber as InvoiceNumber,'
      'PO.IsRA as IsRA,'
      'PO.Deleted as Deleted,'
      'PO.SalesComments as SalesComments,'
      
        'if(PO.ISCredit="T" , 0-PO.TotalAmount , PO.TotalAmount ) as Tota' +
        'lAmountEx,'
      'if(PO.ISCredit="T" , 0-PO.TotalTax , PO.TotalTax ) as TotalTax,'
      
        'if(PO.ISCredit="T" , 0-PO.TotalAmountInc , PO.TotalAmountInc ) a' +
        's TotalAmountInc,'
      'null as Processed,'
      'ECL.Approval as Status'
      'FROM tblPurchaseOrders PO'
      
        'INNER JOIN tblPurchaseLines POL ON (PO.PurchaseOrderID = POL.Pur' +
        'chaseOrderID)'
      'LEFT JOIN tblClients C ON  (PO.SupplierName = C.Company)'
      
        'LEFT JOIN tblemployees E ON (PO.ExpenseClaimEmployee = E.Employe' +
        'eID)'
      
        'Left join tblexpenseclaimline ECL on ECL.ChequeId = PO.purchaseO' +
        'rderId or ECL.BillId= PO.purchaseOrderId '
      
        'WHERE ifnull(ECL.ExpenseClaimLineID,0)=0 and   POL.AccountName <' +
        '> Space(1) AND PO.OrderDate'
      ' AND (PO.IsBill = "T" OR PO.IsCredit = "T" OR PO.IsPO = "T")'
      ' AND (PO.ExpenseClaimEmployee <> 0)'
      ' AND OrderDate Between :txtfrom AND :txtto'
      
        'and ((PO.OrderStatus = :Status) or (:Status = "All") or (:Status' +
        ' = "Processed" and ECL.Processed = "T"))'
      'GROUP BY PO.PurchaseOrderID'
      'union all '
      'select'
      '5 as Seqno,'
      'EC.GlobalRef as GlobalRef,'
      'null as PurchaseOrderID,'
      'EC.ExpenseClaimId as ExpenseClaimID,'
      
        'if(EC.active="F" or ifnull(ECL.ExpenseClaimLineID,0)=0 , "Delete' +
        'd Request" , "Request") as Type,'
      'EC.EmployeeName as EmployeeName,'
      'null as ClientID,'
      'NULL as Company,'
      'EC.Description as Description,'
      'EC.RefNo as ClaimNumber,'
      'Convert(EC.DateTime, Date) as Claimdate,'
      'Null as OrderDate,'
      'null as Approved,'
      '"F" as Paid,'
      'EC.Comments as Comments,'
      'null as Contact,'
      'Date("1001-01-01") as ETADate,'
      'null as Phone,'
      '"F" as Invoiced,'
      '"F" as IsCredit,'
      'Null as InvoiceNumber,'
      '"F" as IsRA,'
      'not EC.Active as Deleted,'
      'null as SalesComments,'
      'EC.TotalAmountEx as "TotalAmountEx",'
      'EC.TotalTax as "TotalTax",'
      'EC.TotalAmountInc as "TotalAmountInc",'
      'EC.Processed as Processed,'
      'ECL.Approval as Status'
      'from tblExpenseClaim EC'
      
        'left join tblExpenseClaimLine ECL on ECL.ExpenseClaimId = EC.Exp' +
        'enseClaimId '
      'where EC.DateTime Between :txtfrom AND :txtto'
      'and ifnull(ECL.ExpenseClaimLineID,0)=0 and :Status='#39'All'#39
      'group by ECL.ExpenseClaimId'
      
        'order by claimdate desc ,ExpenseClaimID desc, orderdate desc, Ty' +
        'pe,company')
