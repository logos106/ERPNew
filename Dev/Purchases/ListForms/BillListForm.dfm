      'SELECT'
      'NULL as ID,'
      'PO.ContactId as ContactId,'
      'PO.GlobalRef as GlobalRef,'
      'PO.PurchaseOrderID as PurchaseOrderID,'
      'If(PO.IsBill="T","Bill","Credit") AS Type,'
      'C.ClientID as ClientID,'
      'C.Company as Company,'
      'C.PrintName as PrintName,'
      'Concat_WS("",PO.PurchaseOrderNumber) as BillNumber,'
      'Date(PO.OrderDate) as OrderDate,'
      'PO.Comments as Comments,'
      
        'IF(NOT ISNULL(C1.Company),C1.Company,IF(NOT ISNULL(C2.Company),C' +
        '2.Company,Space(100))) as Contact,'
      'PO.ETADate as ETADate,'
      'C.Phone as Phone,'
      'PL.Invoiced as Invoiced,'
      'PO.IsCredit as IsCredit,'
      'PO.InvoiceNumber as InvoiceNumber,'
      'PO.IsRA as IsRA,'
      'PO.Deleted as Deleted,'
      'PO.SalesComments as SalesComments,'
      
        'if(IsCredit = "T", -PO.TotalAmount,PO.TotalAmount) as "Total Amo' +
        'unt (Ex)",'
      'if(IsCredit = "T", -PO.TotalTax,PO.TotalTax) as "Total Tax",'
      
        'if(IsCredit = "T", -PO.TotalAmountInc,PO.TotalAmountInc) as "Tot' +
        'al Amount (Inc)",'
      'PO.Approved as Approved,'
      
        'if(IsCredit = "T","Credit",if(IsRA = "T","Return Authority",null' +
        ') ) as CreditOrReturn,'
      'ifnull(PO.balance,0) as Balance'
      'FROM tblPurchaseOrders PO'
      
        'INNER JOIN tblPurchaseLines PL ON (PO.PurchaseOrderID = PL.Purch' +
        'aseOrderID)'
      'LEFT JOIN tblClients  C ON  (PO.SupplierName = C.Company)'
      'LEFT JOIN tblParts P ON  (PL.ProductName = P.PARTName)'
      'LEFT JOIN tblContacts C1 ON C1.ContactID = PO.contactId'
      
        'LEFT JOIN tblContacts C2 ON (C2.IsPrimarycontact ="T" AND C2.Cli' +
        'entID = C.ClientID)'
      
        'WHERE (PL.AccountName <> Space(1) And (PO.IsBill = "T" OR PO.IsC' +
        'redit = "T") And OrderDate Between :DateFrom AND :DateTo '
      
        'and ((:UseApprovalLevels="F")  or ( (:ShowNonApproved="T" or PO.' +
        'Approved = "T")  '
      
        '/*and  (:ApprovalValueH=0 or  (PO.TotalAmountInc <= :ApprovalVal' +
        'ueH AND  PO.TotalAmountInc >=  :ApprovalValueL OR (PO.EnteredBy ' +
        '= :EmployeeID))))*/) ))'
      'GROUP BY PO.PurchaseOrderID')
