  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      
        'convert( @rownum:=if(@PurchaseOrderID <> pol.PurchaseOrderID,  @' +
        'rownum:= 1 ,  @rownum+1) , UNSIGNED)  as  rownum ,'
      '@PurchaseOrderID := pol.PurchaseOrderID,'
      'PO.PurchaseOrderID , '
      'PO.SupplierName , '
      'PO.InvoiceNumber, '
      'PO.invoicedate, '
      'PO.orderdate   as orderdate, '
      'POL.Productname, '
      
        'concat(POL.UnitofMeasurePOLines , '#39'('#39' , POL.UnitofMeasureMultipl' +
        'ier , '#39')'#39') as uom ,'
      'POL.UnitofMeasureQtySold,'
      'POL.UnitofMeasureShipped , '
      'POL.UnitofMeasureBackorder,'
      'POL.totallineamountinc,'
      'PO.TotalAmountInc,'
      'PO.Payment , '
      'PO.Balance'
      'from  (SELECT @rownum:=0) r ,'
      ' (SELECT @PurchaseOrderID:=0) s, tblpurchaseorders PO'
      
        'inner join tblpurchaselines POL on PO.purchaseorderId =  pol.pur' +
        'chaseorderId '
      
        'where PO.ISPO="T" and  POL.PARTTYPE='#39'INV'#39' and ifnull(POL.Shipped' +
        ',0)<>0 and ifnull(invoicedate,0)=0 and PO.orderdate between :Dat' +
        'eFrom and :DAteTo'
      'Order by PO.PurchaseOrderID desc')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DAteTo'
      end>
