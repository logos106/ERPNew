  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      '1 as details, '
      'PO.PurchaseOrderID as PurchaseOrderId,'
      'PO.PurchaseOrderNumber as PurchaseOrderNumber,'
      'PO.SupplierName as SupplierName,'
      'PO.TotalAmountInc as TotalAmountInc,'
      'Max(PP.PrePaymentDate) as PrePaymentDate,'
      'sum(PP.PayAmount) as PayAmount, '
      'PO.TotalAmountInc - sum(PP.PayAmount) as Balance,'
      '0 as PrePaymentID'
      'from tblPurchaseOrders PO '
      'Left join (tblOrdersPrepayments  OP'
      'inner join tblprepayments PP on PP.GlobalRef = OP.PrepayRef'
      'and PP.Deleted = '#39'F'#39' )  on OP.OrderRef = PO.Globalref'
      'where  PO.OriginalNo = :OrderOriginalRef'
      'group by PurchaseOrderID'
      'union all '
      'Select '
      '2 as details,'
      'PO.PurchaseOrderID as PurchaseOrderId,'
      'PO.PurchaseOrderNumber as PurchaseOrderNumber,'
      'PO.SupplierName as SupplierName,'
      'PO.TotalAmountInc as TotalAmountInc,'
      'PP.PrePaymentDate as PrePaymentDate,'
      'PP.PayAmount as PayAmount, '
      '0.0 as Balance,'
      'PP.PrePaymentID as PrePaymentID'
      'from tblPurchaseOrders PO '
      'inner join (tblOrdersPrepayments  OP'
      'inner join tblprepayments PP on PP.GlobalRef = OP.PrepayRef'
      'and PP.Deleted = '#39'F'#39' )  on OP.OrderRef = PO.Globalref'
      'where  PO.OriginalNo = :OrderOriginalRef'
      'order by PurchaseOrderId desc , details , PrePaymentDate desc')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'OrderOriginalRef'
      end
      item
        DataType = ftUnknown
        Name = 'OrderOriginalRef'
      end>
