  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'PO.SupplierName,'
      'PL.Productname, '
      
        'if(PL.UnitofMeasureShipped=0 , '#39'Purchase'#39', '#39'BackOrder'#39') as POtyp' +
        'e,'
      
        'if(PL.UnitofMeasureShipped=0 , PL.UnitofMeasureQtySold , PL.Unit' +
        'ofMeasureShipped) as Qty,'
      
        'concat(PL.UnitofMeasurePOLines, '#39'('#39' , PL.UnitofMeasureMultiplier' +
        ' , '#39')'#39') UnitofMeasurePOLines,'
      'PL.PurchaseLineID as PurchaseLineID,'
      'date(PO.OrderDate) as OrderDate,'
      'date(PO.InvoiceDate) as InvoiceDate,'
      'PO.EmployeeName as EmployeeName,'
      'PL.ProductID,'
      'PO.Comments as Comments,'
      'PO.PurchaseOrderNumber as "P/O Num",'
      'PL.LineCost as LineCost,'
      
        'if(PL.UnitofMeasureShipped=0 , PL.LineCost*PL.BackOrder , PL.Tot' +
        'alLineAmount) as Amount,'
      'PL.ETADate as ETADate,'
      'PO.PurchaseOrderID as PurchaseOrderID,'
      'PL.Class as Class'
      'FROM tblPurchaseLines PL'
      
        'INNER JOIN tblPurchaseOrders PO ON PO.PurchaseOrderID=PL.Purchas' +
        'eOrderID'
      
        'WHERE PO.IsPO = '#39'T'#39' AND PO.IsRA = '#39'F'#39' AND PO.IsBill = '#39'F'#39' AND PO' +
        '.IsCredit = '#39'F'#39
      'AND PO.Deleted='#39'F'#39' '
      
        'and if(:BasedonDate='#39'I'#39' , PO.Invoicedate , PO.OrderDate)  Betwee' +
        'n :dateFrom AND :DateTo'
      'and PL.productID =:ProductID'
      'order by PL.PurchaseOrderID desc')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'BasedonDate'
      end
      item
        DataType = ftUnknown
        Name = 'dateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'ProductID'
      end>
