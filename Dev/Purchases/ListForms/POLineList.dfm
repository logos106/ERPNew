  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'POL.PurchaseOrderid as PurchaseOrderId,'
      'POL.ProductId as PartsId,'
      'POL.purchaselineID as purchaselineID,'
      'POL.ProductName as ProductName,'
      'POL.UnitOfMeasureQtysold as UnitOfMeasureQtysold ,'
      'POL.UnitOfMeasureShipped as UnitOfMeasureShipped,'
      'POL.UnitofMeasureBackorder as UnitofMeasureBackorder,'
      
        'concat(UnitofMeasurePOLines , '#39'('#39' , UnitofMeasureMultiplier , '#39')' +
        #39') as uom,'
      'POL.LineCost as LineCost,'
      'POL.LineCost *POL.UnitOfMeasureQtysold as TotalEstimated'
      'from tblPurchaseOrders PO'
      
        'inner join tblPurchaseLines POL on PO.PurchaseOrderID = POL.Purc' +
        'haseOrderID'
      'where POL.ProductID > 0'
      'and PO.PurchaseOrderId = :PurchaseOrderId')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'PurchaseOrderId'
      end>
