  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select Distinct '
      '1 as Details,'
      'PO.equipmentId  as equipmentId , '
      'PO.EquipmentName as EquipmentName , '
      'NULL as OrderDate, '
      'POL.AreaCode  as AreaCode , '
      'NULL   as productName ,'
      'sum(POL.QtySold) as Qtysold,'
      'sum(POL.shipped) as QtyShipped,'
      'sum(POL.backorder) as QtyBO'
      'from tblpurchaseorders PO'
      
        'inner join tblpurchaselines POL on PO.purchaseorderID = POL.Purc' +
        'haseOrderID and ifnull(POL.AreaCode,'#39#39')<> '#39#39' #and ifnull(POL.Shi' +
        'pped,0)>0'
      
        'where ifnull(PO.equipmentId,0)<> 0 #and PO.OrderDate between :Da' +
        'teFrom and :DateTo'
      'group by PO.equipmentId  ,PO.EquipmentName  ,POL.AreaCode  '
      'union '
      'Select Distinct '
      '2 as Details,'
      'PO.equipmentId  as equipmentId , '
      'PO.EquipmentName as EquipmentName , '
      'NULL as OrderDate, '
      'NULL  as AreaCode , '
      'POL.productname   as productName ,'
      'sum(POL.QtySold) as Qtysold,'
      'sum(POL.shipped) as QtyShipped,'
      'sum(POL.backorder) as QtyBO'
      'from tblpurchaseorders PO'
      
        'inner join tblpurchaselines POL on PO.purchaseorderID = POL.Purc' +
        'haseOrderID and ifnull(POL.AreaCode,'#39#39')<> '#39#39' #and ifnull(POL.Shi' +
        'pped,0)>0'
      
        'where ifnull(PO.equipmentId,0)<> 0 #and PO.OrderDate between :Da' +
        'teFrom and :DateTo'
      'group by PO.equipmentId  ,PO.EquipmentName ,POL.Productname '
      'union '
      ''
      'Select Distinct '
      '3 as Details,'
      'PO.equipmentId  as equipmentId , '
      'PO.EquipmentName as EquipmentName , '
      'PO.OrderDate as OrderDate, '
      'POL.AreaCode  as AreaCode , '
      'POL.productName  as productName ,'
      'POL.QtySold as Qtysold,'
      'POL.shipped as QtyShipped,'
      'POL.backorder as QtyBO'
      'from tblpurchaseorders PO'
      
        'inner join tblpurchaselines POL on PO.purchaseorderID = POL.Purc' +
        'haseOrderID and ifnull(POL.AreaCode,'#39#39')<> '#39#39' #and ifnull(POL.Shi' +
        'pped,0)>0'
      
        'where ifnull(PO.equipmentId,0)<> 0 #and PO.OrderDate between :Da' +
        'teFrom and :DateTo'
      'order by AreaCode , EquipmentName, Details , OrderDate')
    AfterOpen = qryMainAfterOpen
    Active = True
