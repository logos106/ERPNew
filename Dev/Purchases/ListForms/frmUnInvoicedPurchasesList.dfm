  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'PO.PurchaseOrderNumber  as PurchaseOrderNumber,'
      'PO.PurchaseOrderId as PurchaseOrderId, '
      'PO.OrderDate as OrderDate, '
      'PO.SupplierName as SupplierName, '
      'POL.ProductName as ProductName, '
      'POL.QtySold as QtySold, '
      'POL.Shipped as Shipped, '
      'POL.BackOrder as BackOrder, '
      'POL.LineCostInc as LineCostInc, '
      'POL.QtySold * POL.LineCostInc as '#39'OrderedValue'#39','
      'POL.CustomerJobId as CustomerJobId, '
      'C.Company as Company,'
      'PO.SalesGlobalref as SalesGlobalref'
      
        'FROM tblPurchaseOrders PO Inner join tblPurchaseLines POL on POL' +
        '.PurchaseOrderId = PO.PurchaseOrderId'
      'inner join tblClients C  on POL.CustomerJobId = C.ClientId'
      'WHERE  POL.UseTimeCost <> "T"'
      'AND PO.OrderDate >= :DateStart AND PO.OrderDate <= :DateEnd'
      'ORDER BY PO.OrderDate, POL.PurchaseLineId')
    ParamData = <
      item
        DataType = ftString
        Name = 'DateStart'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'DateEnd'
      end>
