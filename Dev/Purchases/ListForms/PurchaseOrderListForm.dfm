  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'PO.baseno baseno, '
      'PO.BOID  BOID, '
      'PO.PurchaseOrderID as PurchaseOrderID,'
      
        'If(PO.IsPO='#39'T'#39','#39'Purchase Order'#39', If(PO.IsPOCredit='#39'T'#39','#39'PO Credit' +
        #39','#39#39')) as Type,'
      'CL.ClientID as ClientID,'
      'CL.PrintName as PrintName,'
      'CL.Company as SupplierName,'
      'PO.GlobalRef as GlobalRef,'
      'PO.OriginalNo as OriginalNo,'
      
        'if (original.PurchaseOrderId = PO.PurchaseOrderId , if (ifnull(P' +
        'O.PurchaseOrderNumber ,'#39'-'#39') = '#39'-'#39' , PO.PurchaseOrderId , PO.Purc' +
        'haseOrderNumber), concat_ws('#39#39',PO.PurchaseOrderNumber, '#39'('#39' , ori' +
        'ginal.PurchaseOrderNumber , '#39'_'#39' , PO.SeqNo, '#39')'#39'))  as "P/O Num",'
      'date(PO.OrderDate) as OrderDate,'
      'PO.Comments as Comments,'
      'PO.ConNote as ConNote,'
      'CL.Contact1 as Contact,'
      'PO.ETADate as ETADate,'
      'CL.Phone as Phone,'
      'PO.InvoiceNumber as InvoiceNumber,'
      'PO.Deleted as Deleted,'
      'PO.SalesComments as WarehouseNotes,'
      'PO.TotalAmount as TotalAmount,'
      'Round(PO.TotalAmountInc,2) as TotalAmountInc,'
      'PO.CustPONumber as CustPONumber,'
      'PO.InvoiceDate as InvoiceDate,'
      'PO.Approved as Approved, '
      'PO.ShipToCustomer,'
      'SL.SaleID as SaleID,'
      'PO.Paid as Paid, '
      'PO.FuturePO,'
      'PO.CreatedFromSO as SmartOrder,'
      'E.EmployeeName as SmartOrderApprovedBy'
      ''
      'FROM tblPurchaseOrders PO'
      
        'LEFT JOIN tblPurchaseLines PL ON  (PO.PurchaseOrderID = PL.Purch' +
        'aseOrderID)'
      'LEFT JOIN tblClients CL ON  (PO.SupplierName = CL.Company)'
      'LEFT JOIN tblParts P ON  (PL.ProductName = P.PARTName)'
      
        'LEFT Join  tblPurchaseOrders as original  on original.GlobalRef ' +
        '=  PO.OriginalNo'
      
        'LEFT  Join `tblsaleslines` SL ON `PL`.`SaleLineID` = SL.`SaleLin' +
        'eID`'
      'LEFT JOIN tblEmployees E on E.EmployeeId=PO.SOApprovedBy'
      'Where PL.Invoiced = '#39'T'#39' and'
      '(PO.IsPO='#39'T'#39' OR PO.IsPOCredit='#39'T'#39')'
      'AND PO.OrderDate between :txtFrom and :txtTo'
      
        'AND ((:Approved = "T" AND PO.Approved = "T") OR (:Approved = "F"' +
        ')) '
      
        ' AND (((PO.TotalAmountInc <= :EmployeeApprovalValueH) AND (PO.To' +
        'talAmountInc >= :EmployeeApprovalValueL)) OR ((:EmployeeApproval' +
        'ValueH = 0)OR (PO.EnteredBy = :EnteredBy)))'
      'GROUP BY PO.PurchaseOrderID'
      'Order by OrderDate desc')
    Left = 218
    Top = 263
    ParamData = <
      item
        DataType = ftString
        Name = 'txtFrom'
        Value = ''
      end
      item
