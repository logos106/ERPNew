  object qryBills: TERPQuery
    Connection = MyConnection
    SQL.Strings = (
      
        'Select tblPurchaseLines.PurchaseLineID, tblPurchaseLines.Related' +
        'ShipContainerIDUsed, tblPurchaseOrders.OrderDate,tblPurchaseOrde' +
        'rs.ForeignExchangeCode, '
      
        'tblPurchaseOrders.SupplierName,tblPurchaseLines.LineCost, tblPur' +
        'chaseLines.LineTax,tblPurchaseLines.ForeignTotalLineAmount, '
      
        'tblPurchaseLines.LineCostInc,tblPurchaseLines.TotalLineAmountInc' +
        ',If(tblPurchaseOrders.IsBill='#39'T'#39','#39'Bill'#39','#39'Credit'#39') AS Type,'
      
        'tblPurchaseLines.TotalLineAmount,tblPurchaseOrders.PurchaseOrder' +
        'ID,tblClients.Phone as Phone,tblPurchaseOrders.InvoiceNumber as ' +
        'InvoiceNumber'
      ''
      'From tblPurchaseOrders'
      'INNER Join tblPurchaseLines Using(PurchaseOrderID)'
      
        'LEFT JOIN tblClients ON  (tblPurchaseOrders.SupplierName = tblCl' +
        'ients.Company)'
      
        'Where ((tblPurchaseOrders.IsBill = "T") or (tblPurchaseOrders.Is' +
        'Cheque = "T")) and (tblPurchaseLines.RelatedShipContainerID = :x' +
        'ShippingContainerID);')
    Left = 16
    Top = 168
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'xShippingContainerID'
      end>
