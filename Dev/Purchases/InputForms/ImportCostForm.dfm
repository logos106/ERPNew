  object qryExpenseCosts: TERPQuery
    Connection = MyConnection
    SQL.Strings = (
      'Select '
      'POL.PurchaseLineID, '
      'POL.PurchaseOrderID, '
      'POL.RelatedPOIDUsed, '
      'PO.OrderDate, '
      'PO.SupplierName,'
      'POL.ProductId,'
      'POL.Product_description,'
      'POL.accountname,'
      'POL.LineCost, POL.LineTax, '
      'POL.LineCostInc,POL.TotalLineAmountInc,'
      'POL.TotalLineAmount,'
      'IP.Duty, '
      'IP.Handling,'
      'PO.IsBill,'
      'PO.IsCheque'
      'From tblPurchaseOrders PO'
      'INNER Join tblPurchaseLines POL Using(PurchaseOrderID)'
      'left join tblImportProducts IP on ip.ProductId = POL.ProductId'
      
        'Where ((PO.IsBill = "T") or (PO.IsCheque = "T"))  and (POL.Relat' +
        'edPOID = :RelatedPOID)'
      'group by POL.PurchaseLineId'
      'order by PO.PurchaseOrderId, ProductId')
    Left = 489
    Top = 17
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'RelatedPOID'
      end>
