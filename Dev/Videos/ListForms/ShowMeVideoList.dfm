  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'V.ID as ID,'
      'VP.Pagename as Pagename, '
      'V.PageCaption as Pagecaption,'
      'V.Seqno as Seqno ,'
      'V.Pagehint as Pagehint, '
      'V.filename as filename ,'
      'V.Videotype as Videotype,'
      'Null as TaskId,'
      'V.HelpContextID as HelpContextID, '
      'min(WF.ID) as WorkflowID,'
      'NULL as tasktype,'
      'NULL as VersionNo,'
      'V.DateCreated as DateCreated,'
      'Null as TaskDetails'
      'from erpdocumentaion.tblvideos V'
      
        'inner join erpdocumentaion.tblvideospages VP on V.ID = VP.VideoI' +
        'd and VP.active ="T"'
      'Left join tblworkflow WF on WF.VideoName = V.PageCaption '
      'where Videotype ='#39'vtTraining'#39' and V.Active ="T"'
      'group by V.ID'
      'union all '
      'Select '
      'T.ID as ID,'
      'TP.Pagename as Pagename, '
      'T.subject as Pagecaption,'
      'V.Seqno as Seqno ,'
      'V.Pagehint as Pagehint, '
      'V.filename as filename ,'
      '"vtTask"  as Videotype, '
      'T.Id as TaskID,'
      'V.HelpContextID as HelpContextID,'
      'min(WF.ID) as WorkflowID,'
      'T.Tasktype as Tasktype,'
      'T.VersionNo as VersionNo,'
      'T.CreatedOn as DateCreated, '
      'CAST(T.Details AS CHAR CHARACTER SET utf8) as TaskDetails'
      'from '
      'erpdocumentaion.tbltasks T'
      
        'inner join erpdocumentaion.tbltaskspages TP on T.Id = TP.TaskID ' +
        'and TP.active ="T"'
      
        'Left join erpdocumentaion.tblvideos V on V.PageCaption = T.Subje' +
        'ct and Videotype ='#39'vtTask'#39
      'Left join tblworkflow WF on WF.VideoName = V.PageCaption  '
      'Where T.Active ="T"'
      'group by T.ID, V.ID'
      'Order by Pagename ,DateCreated, PageCaption')
