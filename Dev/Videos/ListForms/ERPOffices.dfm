  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'ID as ID, '
      'Location as Location, '
      'Phone as Phone, '
      'if(Sunday'#9#9'="T" , StartsAt  , null) as SundayStartsAt,'
      'if(Monday'#9#9'="T" , StartsAt  , null) as MondayStartsAt,'
      'if(Tuesday'#9'="T" , StartsAt  , null) as TuesdayStartsAt,'
      'if(WednesDay="T" , StartsAt  , null) as WednesDayStartsAt,'
      'if(ThursDay'#9'="T" , StartsAt  , null) as ThursDayStartsAt,'
      'if(Friday'#9#9'="T" , StartsAt  , null) as FridayStartsAt,'
      'if(Saturday'#9'="T" , StartsAt  , null) as SaturdayStartsAt,'
      
        'if(Sunday'#9#9'="T" ,if(ShortDay<>'#39'Sun'#39' ,  StopsAt , ShortDayStopAt)' +
        '  , null) as SundayStopsAt,'
      
        'if(Monday'#9#9'="T" ,if(ShortDay<>'#39'Mon'#39' ,  StopsAt , ShortDayStopAt)' +
        '  , null) as MondayStopsAt,'
      
        'if(Tuesday'#9'="T" ,if(ShortDay<>'#39'Tue'#39' ,  StopsAt , ShortDayStopAt)' +
        '  , null) as TuesdayStopsAt,'
      
        'if(Wednesday="T" ,if(ShortDay<>'#39'Wed'#39' ,  StopsAt , ShortDayStopAt' +
        ')  , null) as WednesdayStopsAt,'
      
        'if(Thursday'#9'="T" ,if(ShortDay<>'#39'Thu'#39' ,  StopsAt , ShortDayStopAt' +
        ')  , null) as ThursdayStopsAt,'
      
        'if(Friday'#9#9'="T" ,if(ShortDay<>'#39'Fri'#39' ,  StopsAt , ShortDayStopAt)' +
        '  , null) as FridayStopsAt,'
      
        'if(Saturday'#9'="T" ,if(ShortDay<>'#39'Sat'#39' ,  StopsAt , ShortDayStopAt' +
        ')  , null) as SaturdayStopsAt,'
      'TimeZone as TimeZone,'
      'Sunday as Sunday, '
      'Monday as Monday, '
      'Tuesday as Tuesday, '
      'WednesDay as WednesDay, '
      'ThursDay AS ThursDay , '
      'Friday as Friday, '
      'Saturday as Saturday,'
      'ShortDay as ShortDay,'
      'ShortDayStopAt as ShortDayStopAt,'
      'StopsAt as StopsAt,'
      'StartsAt as StartsAt,'
      'WebAddress as WebAddress '
      'from  erpdocumentaion.tblerpoffices '
      'order by TimeZone Desc,Location')
    OnCalcFields = qryMainCalcFields
