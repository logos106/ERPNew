    SQL.Strings = (
      
        'Select distinct R.EmployeeId, R.TrainingmoduleId ModuleID, Sum(i' +
        'fnull(R.Hours,0))  Hours  , TM.TrainingHrs'
      'from '
      'tblroster R '
      
        'inner join tbltrainingmodules TM on TM.TrainingModuleID =  R.Tra' +
        'iningmoduleId'
      'Where R.type = '#39'Training'#39
      'and R.employeeId = :EmployeeId or :EmployeeId =0'
