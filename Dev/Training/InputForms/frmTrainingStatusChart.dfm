  object QryStatus: TERPQuery
    Connection = MyConnection
    SQL.Strings = (
      
        'Select distinct EMS.EmployeeId, EMS.ModuleId, EMS.trainingemploy' +
        'eemoduleStatusID  , '
      
        '(Select Count(EMSD1.TrainingEmployeeModuleStatusDetailsID) from ' +
        'tblTrainingEmployeeModuleStatusDetails EMSD1 Where EMSD1.trainin' +
        'gemployeemoduleStatusID = EMS.trainingemployeemoduleStatusID and' +
        ' EMSD1.Done='#39'F'#39' ) notDone,'
      
        '(Select Count(EMSD1.TrainingEmployeeModuleStatusDetailsID) from ' +
        'tblTrainingEmployeeModuleStatusDetails EMSD1 Where EMSD1.trainin' +
        'gemployeemoduleStatusID = EMS.trainingemployeemoduleStatusID  ) ' +
        'AllEntries'
      'from tbltrainingemployeemoduleStatus EMS'
