  object QryMain: TERPQuery
    Connection = MyConnection
    SQL.Strings = (
      
        'Select distinct EMS.TrainingID, T.Description, EMS.EmployeeId, E' +
        'MS.ModuleId, EMS.trainingemployeemoduleStatusID  , '
      'TM.Videofilename  , '
      'ifnull(R.Hours,0) Hours, '
      'R.startTime , R.Endtime, TM.TrainingModuleName , '
      'EMS.EmployeeName,  TM.TrainingHrs'
      'from tbltrainingemployeemoduleStatus EMS'
      'Inner Join tblTraining T on T.TrainingID = EMS.TrainingID'
      
        'Left join tblroster R on R.TrainingmoduleId = EMS.ModuleId and R' +
        '.employeeId = EMS.EmployeeId and R.type = '#39'Training'#39
      
        'inner join tbltrainingmodules TM on TM.TrainingModuleID =  EMS.M' +
        'oduleId'
      'Where EMS.employeeId = :EmployeeId or :EmployeeId =0'
      'Order by employeeID , moduleId , startTime')
    AfterOpen = QryMainAfterOpen
    Options.LongStrings = False
    Left = 346
    Top = 16
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'EmployeeId'
      end
