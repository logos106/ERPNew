  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'ID as ID, '
      'VATDesc as VATDesc ,'
      'Active as Active , '
      'Done as Done,'
      'type as Period, '
      
        'STR_TO_DATE(concat("01/" , MonthNo(month) , "/" , year) ,"%d/%m/' +
        '%Y") as PeriodFrom, '
      
        'Date_add(Date_add(STR_TO_DATE(concat("01/" , MonthNo(month) , "/' +
        '" , year) ,"%d/%m/%Y")    , INTERVAL DateRangetypeToMonth(type )' +
        ' Month) , INTERVAL -1 DAY)  as PeriodTo   '
      'FROM tblvatreturns'
      'ORDER BY Id desc;')
