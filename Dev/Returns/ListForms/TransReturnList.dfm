  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select Distinct'
      'R.Id,'
      'RL.TransDate Date,'
      'R.Tab1_Type Period, '
      
        'if(RL.basCode in ('#39'G1'#39','#39'G2'#39','#39'G3'#39','#39'G7'#39','#39'G4'#39','#39'G11'#39','#39'G13'#39','#39'G14'#39','#39'G1' +
        '5'#39','#39'G10'#39','#39'G18'#39') , STR_TO_DATE(concat("01/" , MonthNo(tab1_month)' +
        ' , "/" , tab1_year) ,"%d/%m/%Y")  ,if(RL.basCode in ('#39'_1C'#39','#39'_1E'#39 +
        ','#39'_1D'#39','#39'_1F'#39','#39'_1G'#39') ,STR_TO_DATE(concat("01/" , MonthNo(Tab2_mon' +
        'th) , "/" , Tab2_year) ,"%d/%m/%Y")  , STR_TO_DATE(concat("01/" ' +
        ', MonthNo(Tab3_month) , "/" , Tab3_year) ,"%d/%m/%Y")) ) DateFro' +
        'm , '
      
        'if(RL.basCode in ('#39'G1'#39','#39'G2'#39','#39'G3'#39','#39'G7'#39','#39'G4'#39','#39'G11'#39','#39'G13'#39','#39'G14'#39','#39'G1' +
        '5'#39','#39'G10'#39','#39'G18'#39') , Date_add(Date_add(STR_TO_DATE(concat("01/" , M' +
        'onthNo(tab1_month) , "/" , tab1_year) ,"%d/%m/%Y")    , INTERVAL' +
        ' DateRangetypeToMonth(tab1_type ) Month) , INTERVAL -1 DAY)   , ' +
        'if(RL.basCode in ('#39'_1C'#39','#39'_1E'#39','#39'_1D'#39','#39'_1F'#39','#39'_1G'#39') ,Date_add(Date_' +
        'add(STR_TO_DATE(concat("01/" , MonthNo(Tab2_month) , "/" , Tab2_' +
        'year) ,"%d/%m/%Y")    , INTERVAL DateRangetypeToMonth(Tab2_type ' +
        ') Month) , INTERVAL -1 DAY)   , Date_add(Date_add(STR_TO_DATE(co' +
        'ncat("01/" , MonthNo(Tab3_month) , "/" , Tab3_year) ,"%d/%m/%Y")' +
        '    , INTERVAL DateRangetypeToMonth(Tab3_type ) Month) , INTERVA' +
        'L -1 DAY) )) Dateto, '
      'R.BasSheetDesc Description, '
      'R.Active Active, '
      'R.AccMethod AccMethod,  '
      'R.allclass allclass, '
      'C.classname  classname,'
      'RL.Bascode Code, '
      'RL.Amount Amount,'
      'RL.Transtype  Transtype, '
      'RL.transglobalref Transref, '
      'RL.transdate TransDate,'
      
        'convert(ifnull(T.EmployeeID , TS.EmployeeID)'#9',SIGNED) as Employe' +
        'eID'#9','
      
        'convert(ifnull(T.FixedAssetID , TS.FixedAssetID),SIGNED)'#9'as Fixe' +
        'dAssetID'#9','
      'convert(ifnull(T.PaymentID, TS.PaymentID),SIGNED)'#9'as PaymentID'#9','
      
        'convert(ifnull(T.PrepaymentID, TS.PrepaymentID),SIGNED)'#9'as Prepa' +
        'ymentID'#9','
      
        'convert(ifnull(T.PurchaseOrderID,TS.PurchaseOrderID),SIGNED)'#9'as ' +
        'PurchaseOrderID'#9','
      'convert(ifnull(T.SaleID,TS.SaleID),SIGNED)'#9'as SaleID'#9
      'from  tblBASReportLines RL '
      'inner join tblBasreports R  on RL.basID = R.ID '
      
        'Left join tblTransactions T on T.globalref = RL.transglobalref a' +
        'nd T.type = RL.transtype and T.Seqno = RL.TRansseqno '
      
        'Left join tbltransactionsummarydetails TS on TS.globalref = RL.t' +
        'ransglobalref and TS.type = RL.transtype and TS.Seqno = RL.TRans' +
        'seqno '
      'Left join tblClass C on C.ClassId = R.ClassID'
      
        'Where (RL.Transglobalref = :Transglobalref or :Transglobalref = ' +
        #39#39')'
      'and (Transtype = :Transtype or :Transtype = '#39#39')'
      'and (R.ID = :ID or :ID =0)'
      'and (RL.bascode = :code or :code = '#39#39')')
