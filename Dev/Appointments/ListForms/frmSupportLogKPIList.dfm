  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'P.GlobalRef as GlobalRef,'
      'P.SupportLogID as SupportLogID,'
      'P.Company as Company,'
      'P.Contact as Contact,'
      'P.DateCreated as DateCreated,'
      'P.Form as Module,'
      'P.Type as Type,'
      'P.Issue As Issue, '
      'P.Solution As Solution,'
      'P.CallTime as CallTime,'
      'P.Finished as Finished,'
      'E.EmployeeName as EnteredBy,'
      'Version as Version,'
      'P.CallSecs as CallSecs,'
      'T.EmployeeName as Assignedto,'
      'M.MessagesID as MessagesID,'
      'ML.DateAndTime as MessageDate,'
      'ML.Details as MessageDetails,'
      'ML.TypeName as MessageType,'
      
        'TIME_TO_SEC(TIMEDIFF(P.DateCreated,ML.DateAndTime)) /60 as Messa' +
        'geMins'
      'FROM tblphonesupportlog P'
      'LEFT Join tblemployees E on E.EmployeeID = P.employeeId'
      'LEFT Join tblemployees T on T.EmployeeID = P.ToEmployeeID'
      'left join tblmessagelines ML on ML.SupportLogID = P.SupportLogID'
      'left join tblmessages M on M.MessagesID = ML.MessagesID'
      'WHERE DateCreated Between :txtfrom AND :txtto'
      'ORDER BY DateCreated DESC')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'txtfrom'
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end>
