  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'tblAppointments.AppDate AS '#39'Date'#39','
      'tblClients.Company as '#39'Customer Name'#39','
      
        'Concat(tblemployees.FirstName, '#39' '#39', tblemployees.LastName) AS Re' +
        'p,'
      'tblsource.MediaType as Source,'
      
        '((TIME_TO_SEC(tblAppointments.EndTime)-TIME_TO_SEC(tblAppointmen' +
        'ts.StartTime))/60)/60 as Hours,'
      'Rate as '#39'Hourly Rate'#39','
      'CalloutValue AS '#39'Callout Fee'#39','
      'tblAppointments.TotalInc, '
      'tblAppointments.TotalCost,'
      'TotalInc-TotalCost as Nett,'
      'tblAppointments.AppointID,'
      'tblAppointments.Converted as Converted,'
      
        'IF(ISNULL(tblservices.ServiceID), "NOT SET", tblservices.Service' +
        'Desc) AS Service'
      'FROM tblAppointments'
      
        'INNER JOIN tblemployees ON tblemployees.EmployeeID=tblAppointmen' +
        'ts.TrainerID'
      
        'INNER JOIN tblClients ON tblAppointments.CUSID=tblClients.Client' +
        'ID'
      
        'LEFT JOIN tblservices on tblAppointments.ServiceID=tblservices.S' +
        'erviceID'
      'LEFT JOIN tblsource ON tblsource.MedTypeID=tblClients.MedTypeID'
      'WHERE tblAppointments.AppDate Between :txtfrom AND :txtto'
      'ORDER BY tblAppointments.AppDate DESC;')
    Left = 89
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'txtfrom'
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end>
