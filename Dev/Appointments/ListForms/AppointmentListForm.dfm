  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      
        'if(A.IsGroupParent='#39'T'#39', "", if(isnull(AA.AppointID),"" , Concat(' +
        'AA.AppointId, '#39'-Repeated'#39') )) as RepeatStatus,'
      'A.Start_Time as Start_time,'
      'A.GlobalRef as GlobalRef,'
      'A.AppointID as AppointID,'
      'A.AppointID as AppointmentNo,'
      'A.CusID AS ClientID,'
      'C.JobName AS JobName,'
      'C.JobNumber AS JobNumber,'
      'A.ENDTIME as EndDate,'
      'A.ClassID as ClassID,'
      
        'if(DayofWeek(A.AppDate)=1, '#39'Sun'#39',if(DayofWeek(A.AppDate)=2, '#39'Mon' +
        #39', if(DayofWeek(A.AppDate)=3, '#39'Tue'#39', if(DayofWeek(A.AppDate)=4, ' +
        #39'Wed'#39', if(DayofWeek(A.AppDate)=5, '#39'Thu'#39', if(DayofWeek(A.AppDate)' +
        '=6, '#39'Fri'#39','#39'Sat'#39' ))))) ) as AppDateDay,'
      'A.AppDate as AppDate,'
      'A.STARTTIME as '#39'Start Time'#39','
      'A.END_TIME as '#39'End Time'#39','
      'A.END_TIME as '#39'End_Time'#39','
      
        'CONCAT_WS(" ", If(InStr(C.Company,'#39'^'#39') -1 >0,Left(C.Company,InSt' +
        'r(C.Company,'#39'^'#39')-1),C.Company),IF(char_length(C.JobName)>0, Conc' +
        'at("(",C.JobName,")"),'#39#39')) as Company,'
      
        'Trim(SUBSTRING_INDEX(REPLACE(A.CustomerDetails,Concat(Char(13),c' +
        'har(10)),'#39' '#39'),Trim(C.Company),-1)) as AppointmentDetails,'
      'A.TrainerID as TrainerID,'
      'Concat(E.FirstName, '#39' '#39', E.LastName) AS Rep,'
      'C.IsJob as IsJob,'
      'A.Feedback as Feedback,'
      'A.Cancellation as Cancellation,'
      'A.Reschedulled as Rescheduled,'
      'tblClass.ClassName as ClassName,'
      'A.Converted as Converted,'
      
        'if(C.FirstName is Null, Space(0),Concat(C.FirstName, " ",C.LastN' +
        'ame)) AS '#39'Contact Name'#39','
      'C.Email as Email,'
      'C.Street as Street,'
      'C.Suburb as Suburb,'
      'C.State as State,'
      'C.Postcode as Postcode ,'
      '`A`.`Phone` as Phone,'
      '`A`.`Fax` as FaxNumber,'
      '`A`.`Mobile` as Mobile,'
      '`A`.`AltPhone` as AltPhone,'
      'A.Unavailable as Unavailable, '
      'A.LocalApp as GoLiveApp,'
      ''
      'C.Phone as CustomerPhone,'
      'C.FaxNumber as CustomerFaxNumber,'
      'C.Mobile as CustomerMobile,'
      'C.AltPhone as CustomerAltPhone,'
      'C.AltContact as CustomerAltContact,'
      ''
      'BillStreet as BillStreet,'
      'BillSuburb as BillSuburb,'
      'BillState as BillState,'
      'BillPostcode as BillPostcode,'
      'A.TotalInc as TotalInc,'
      'A.TotalCost as TotalCost,'
      'SVC.ServiceDesc AS "Service Description",'
      'tblsource.MediaType as Source,'
      'A.Active AS Active,'
      'if (A.Active='#39'T'#39', '#39'F'#39','#39'T'#39') as Deleted,'
      'A.CreationDate as CreationDate,'
      'A.EnteredByEmployeeName as EnteredBy,'
      'A.Status as Status,'
      'ET.TypeName as '#39'Rep Type'#39','
      'CASE'
      
        '  WHEN C.Customer = '#39'T'#39' THEN IF(C.IsJob = '#39'T'#39', '#39'Job'#39', '#39'Customer'#39 +
        ') '
      '  WHEN C.Supplier = '#39'T'#39' THEN '#39'Supplier'#39
      '  WHEN C.OtherContact = '#39'T'#39' THEN '#39'OtherContact'#39
      'END AS ClientType, '
      'Ct.typeName as CustomerType,'
      '`A`.`Actual_AppDate`,'
      '`A`.`Actual_Endtime`,'
      '`A`.`Actual_Start_time`,'
      '`A`.`Actual_End_time` , '
      ' A.Actual_StartTime'
      'FROM tblAppointments AS A'
      
        'LEFT JOIN tblAppointments AS AA ON A.LinkAppointID = AA.AppointI' +
        'D'
      'LEFT JOIN tblClients AS C ON A.CUSID = C.ClientID'
      'LEFT JOIN tblemployees E ON A.TrainerID = E.EmployeeID'
      'LEFT JOIN tblClass ON A.ClassID = tblClass.ClassID'
      'LEFT JOIN tblsource ON tblsource.MedTypeID = C.MedTypeID'
      'LEFT JOIN tblServices SVC ON A.ServiceID = SVC.ServiceID'
      
        'LEFT JOIN tblemployeetypes ET ON E.EmployeeTypeID = ET.EmployeeT' +
        'ypeID'
      
        'lefT Join `tblclienttype` CT ON `C`.`ClientTypeID` = CT.`ClientT' +
        'ypeID`'
      
        'WHERE ((C.ClientID IS Not Null) OR (A.LocalApp="T")) AND A.AppDa' +
        'te Between  :dtfrom and  :dtto'
      
        'ORDER BY if(isnull(A.LinkAppointId) or A.LinkAppointId=0, A.Appo' +
        'intID, if(A.IsGroupParent='#39'T'#39',A.AppointID,A.LinkAppointID)) desc' +
        ',A.AppDate desc, '#39'Start Time'#39)
    OnCalcFields = qryMainCalcFields
    Left = 44
    ParamData = <
      item
        DataType = ftString
        Name = 'dtfrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'dtto'
      end>
