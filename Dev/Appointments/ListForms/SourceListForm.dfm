  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'tblSource.GlobalRef, '
      'tblSource.MedTypeID,'
      'tblSource.MediaType as '#39'Source Name'#39', '
      'Enquires as Enquiries, '
      'tblSource.Account, '
      'tblSource.Cost as '#39'Estimated Cost'#39','
      'tblChartofAccounts.AccountName as '#39'Account Name'#39','
      'tblSource.Active, '
      'Count(tblAppointments.MedTypeID) as '#39'Appointment Count'#39
      'FROM tblSource'
      
        'LEFT JOIN tblChartofAccounts ON tblSource.Account = tblChartofAc' +
        'counts.AccountID'
      
        'LEFT JOIN tblClients ON tblSource.MedTypeID = tblClients.MedType' +
        'ID'
      
        'LEFT JOIN tblAppointments ON tblClients.ClientID = tblAppointmen' +
        'ts.CusID and tblSource.MedTypeID = tblAppointments.MedTypeID and' +
        ' tblAppointments.AppDate Between :xStartDate and :xEndDate'
      'GROUP BY tblSource.MedTypeID;')
    OnCalcFields = qryMainCalcFields
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'xStartDate'
      end
      item
        DataType = ftUnknown
        Name = 'xEndDate'
      end>
