  object qryBalances: TERPQuery
    SQL.Strings = (
      'SELECT TransID, Date, Type, GlobalRef, ClassID, SaleID,'
      'PurchaseOrderID, PaymentID, AccountName, AccountID,'
      
        'AccountType, Sum(DebitsEx) as CostEx, CreditsEx, Sum(DebitsInc) ' +
        'as CostInc,'
      'CreditsInc, Active'
      'FROM tbltransactions'
      'WHERE Date Between :dtFrom and :dtTo'
      'GROUP BY AccountID;')
    Left = 279
    Top = 5
    ParamData = <
      item
