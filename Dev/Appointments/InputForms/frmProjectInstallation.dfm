  object qryWeekRoster: TERPQuery
    Connection = MyConnection
    SQL.Strings = (
      'Select distinct '
      'Convert(A.AppointID,SIGNED)  as ID,'
      'A.GlobalRef ,'
      'Convert(A.TrainerID,SIGNED)  as TrainerID,'
      'E.employeename, '
      'C.Company as ClientName,'
      'A.AppDate as Date,'
      'A.Starttime as StartTime,'
      'A.EndTime   as Endtime,'
      'Convert(A.ClassID,SIGNED)  as ClassID,'
      '#C.ClientCode,'
      
        'if(ifnull(C.ClientCode,"")="" , SUBSTR(C.company ,1,3), ClientCo' +
        'de) as ClientCode,'
      'convert(ifnull(A.Notes , A.ClientName),char) as Notes,'
      'A.HrstoInvoice hours,'
      #39'Appointment'#39' as Type,'
      
        'if(ifnull(A.LinkAppointID,0)=0 , if(ifnull(LA.AppointID,0)<>0 , ' +
        'A.AppointID , 0) , A.LinkAppointID) as groupID'
      'FROM tblappointments A'
      'Left join tblappointments LA on A.AppointID = LA.LinkAppointID'
      'INNER JOIN tblEmployees E on A.TrainerID = E.employeeId'
      'inner join tblclients C on C.clientId = A.CusId'
      
        'WHERE A.Active ="T" and A.Cancellation ="F" and  E.onPMRoster = ' +
        '"T" and  A.Active="T"'
      'and (A.ClassID =1 or '#39'T'#39' ="T")'
      'and  A.AppDate between '#39'2015-08-01'#39' and  '#39'2015-09-12'#39
      'having notes ='#39'Test Customer-1'#39)
    BeforeOpen = qryWeekRosterBeforeOpen
    AfterOpen = qryWeekRosterAfterOpen
