  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'tblBudgetPeriods.BudgetID as BudgetID, '
      'tblBudgetPeriods.BudgetPeriodID as BudgetPeriodID, '
      'tblBudgets.Name as Name, '
      'tblBudgets.Description as Description, '
      
        'if (tblBudgets.showAmount1 = '#39'T'#39' , tblBudgets.Amount1Label , '#39'NA' +
        #39') as Amount1, '
      
        'if (tblBudgets.showAmount2 = '#39'T'#39' , tblBudgets.Amount2Label , '#39'NA' +
        #39') as Amount2, '
      'tblBudgetPeriods.BudgetNo as BudgetNo, '
      'tblBudgetPeriods.StartDate as StartDate, '
      'tblBudgetPeriods.EndDate as EndDate, '
      'tblBudgetPeriods.Active as PeriodActive,  '
      'tblBudgets.Active as BudgetActive'
      'from tblBudgetPeriods, '
      'tblBudgets'
      'Where tblBudgetPeriods.BudgetID =  tblBudgets.BudgetID'
      
        'and tblBudgetPeriods.StartDate >= :FromDate and tblBudgetPeriods' +
        '.StartDate <=:ToDate')
    ParamData = <
      item
        DataType = ftString
        Name = 'FromDate'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'ToDate'
      end>
