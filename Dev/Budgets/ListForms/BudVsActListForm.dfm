  object qryBudLines: TERPQuery
    Connection = MyConnection1
    SQL.Strings = (
      'SELECT coa.AccountID,'
      #9' SUM(bl.Conservative) as "Conservative",'
      #9' SUM(bl.Aggressive) as "Aggressive",'
      #9' b.Name as Budget, bl.BudgetID'
      'FROM tblchartofaccounts as coa'
      'LEFT JOIN tblBudgetLines as bl ON'
      
        #9' (coa.AccountID = bl.AccountID) AND (bl.Date BETWEEN :txtFrom A' +
        'ND :txtTo)'
      'LEFT JOIN tblBudgets as b ON bl.BudgetID = b.BudgetID'
      'WHERE (bl.BudgetID = :xBudgetID or isnull(bl.BudgetID )) AND'
      
        '   (bl.ClassID like :xClassID OR (isnull(bl.ClassID) AND :xInclu' +
        'deNulls = '#39'Y'#39'))'
      'GROUP BY AccountID;')
    Left = 536
    Top = 9
    ParamData = <
      item
        DataType = ftString
        Name = 'txtFrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'txtTo'
      end
      item
        DataType = ftUnknown
        Name = 'xBudgetID'
      end
      item
        DataType = ftUnknown
        Name = 'xClassID'
      end
      item
        DataType = ftUnknown
        Name = 'xIncludeNulls'
      end>
  end
