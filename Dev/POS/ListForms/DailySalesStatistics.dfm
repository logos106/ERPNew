      'select '
      'T.EmployeeId'#9','
      'e.EmployeeName as EmployeeName,'
      
        'concat(DATE_FORMAT(T.TimeFrom'#9','#39'%H %p'#39'), '#39' - '#39' , DATE_FORMAT(T.T' +
        'imeTo'#9','#39'%H %p'#39')) Timeint,'
      'T.NoOfsales'#9','
      'T.NoOfCustomers'#9','
      'T.NoOfProducts'#9','
      'T.NoOfReturns'#9','
