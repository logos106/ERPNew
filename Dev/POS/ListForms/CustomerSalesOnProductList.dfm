  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'S.SaleID as SaleID,'
      'L.SaleLineID as SaleLineID,'
      'L.ProductID as ProductID,'
      'S.ClientID as ClientID,'
      'S.SaleDate as SaleDate,'
      'S.Class as Class,'
      'S.EmployeeName as Employee,'
      '#L.UnitofMeasureQtySold as Qty,'
      
        'L.UnitofMeasureShipped as Qty, L.UnitofMeasureBackorder as BOQty' +
        ', '
      'L.ProductName as ProductName,'
      'L.ProductPrintName as ProductPrintName,'
      'L.LinePriceInc as '#39'Price (Inc)'#39','
      
        'if(IsRefund = "T",'#39'Refund'#39' , if(IsCashSale = "T", '#39'CashSale'#39' , i' +
        'f(IsInvoice = "T" , '#39'Invoice'#39' , '#39#39'))) as TransType'
      'FROM tblsales AS S'
      'INNER JOIN tblsaleslines AS L USING (SaleID)'
      'WHERE (IsRefund = "T" OR IsCashSale = "T" OR IsInvoice = "T")'
      'AND ClientID = :xClientID AND ProductID = :xProductID'
      'AND SaleDate BETWEEN :xFrom AND :xTo'
      'ORDER BY S.SaleDate DESC')
    Left = 97
    Top = 129
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'xClientID'
      end
      item
        DataType = ftUnknown
        Name = 'xProductID'
      end
      item
        DataType = ftUnknown
        Name = 'xFrom'
      end
      item
        DataType = ftUnknown
        Name = 'xTo'
      end>
