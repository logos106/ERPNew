  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select Distinct'
      'S.SaleId as SaleId, '
      'S.Customername as CustomerName, '
      'S.totalAmountinc As TotalAmountinc, '
      'S.totalAmount As TotalAmount, '
      'S.SaleDate as SaleDate, '
      'S.ShipDate as shipdate, '
      'S.Comments,'
      '"F" as ISInPOS'
      'From tblSales S'
      'inner join tblsaleslines SL on S.saleID = SL.SaleId'
      'Where  SaleDate between :DateFrom and :Dateto '
      'AND   S.ISPOS ="T"'
      'union all '
      'Select Distinct'
      'S.SaleId as SaleId, '
      'S.Customername as CustomerName, '
      'S.totalAmountinc As TotalAmountinc, '
      'S.totalAmount As TotalAmount, '
      'S.SaleDate as SaleDate, '
      'S.ShipDate as shipdate, '
      'S.Comments,'
      '"T" as ISInPOS'
      'From tblSales_POS S'
      'inner join tblsaleslines_POS SL on S.saleID = SL.SaleId'
      'Order by SaleID Desc')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'Dateto'
      end>
