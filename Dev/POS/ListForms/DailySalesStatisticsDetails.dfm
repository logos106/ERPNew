  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      '2 Details, '
      's.saleId as SaleId, '
      's.SaledateTime  as Saledate_Time,'
      
        'concat(DATE_FORMAT( S.SaleDatetime, '#39'%h %p'#39'), '#39' - '#39' , DATE_FORMA' +
        'T( DAte_Add(S.SaleDatetime, interval 1 hour), '#39'%h %p'#39')) timestlo' +
        't, '
      'S.CustomerName as CustomerName,'
      's.employeeId as employeeId,'
      'E.EmployeeName as EmployeeName,'
      'S.sAleDatetime as sAleDatetime,'
      'SL.ProductName as ProductName,'
      
        'Concat(SL.UnitofMeasureSaleLines , '#39' ('#39' , SL.UnitofMeasureMultip' +
        'lier , '#39')'#39' ) as UOM,'
      'SL.UnitofMeasureShipped  as UnitofMeasureShipped ,'
      'SL.Shipped as shipped,'
      'SL.TotalLineAmount as TotalAmount,'
      'SL.TotalLineAmountInc as TotalAmountInc'
      'from tblsales S '
      'inner join tblsaleslines SL on SL.saleId = S.saleId'
      'inner join tblemployees E on S.employeeId= E.employeeId'
      ''
      'where S.saleDateTime between :dateFrom and :DateTo'
      'union all '
      'Select '
      '1 Details, '
      's.saleId as SaleId, '
      's.SaledateTime  as Saledate_Time,'
      
        'concat(DATE_FORMAT( S.SaleDatetime, '#39'%h %p'#39'), '#39' - '#39' , DATE_FORMA' +
        'T( DAte_Add(S.SaleDatetime, interval 1 hour), '#39'%h %p'#39')) timestlo' +
        't, '
      'S.CustomerName as CustomerName,'
      's.employeeId as employeeId,'
      'E.EmployeeName as EmployeeName,'
      'S.sAleDatetime as sAleDatetime,'
      'NULL as ProductName,'
      'Null as UOM,'
      'NULL as UnitofMeasureShipped ,'
      'NULL as shipped,'
      'S.TotalAmount as TotalAmount,'
      'S.TotalAmountInc as TotalAmountInc'
      'from tblsales S '
      'inner join tblemployees E on S.employeeId= E.employeeId'
      'where S.saleDateTime between :dateFrom and :DateTo'
      'union all '
      'Select '
      '2 Details, '
      's.saleId as SaleId, '
      's.SaledateTime  as Saledate_Time,'
      
        'concat(DATE_FORMAT( S.SaleDatetime, '#39'%h %p'#39'), '#39' - '#39' , DATE_FORMA' +
        'T( DAte_Add(S.SaleDatetime, interval 1 hour), '#39'%h %p'#39')) timestlo' +
        't, '
      'S.CustomerName as CustomerName,'
      's.employeeId as employeeId,'
      'E.EmployeeName as EmployeeName,'
      'S.sAleDatetime as sAleDatetime,'
      'SL.ProductName as ProductName,'
      
        'Concat(SL.UnitofMeasureSaleLines , '#39' ('#39' , SL.UnitofMeasureMultip' +
        'lier , '#39')'#39' ) as UOM,'
      'SL.UnitofMeasureShipped  as UnitofMeasureShipped ,'
      'SL.Shipped as shipped,'
      'SL.TotalLineAmount as TotalAmount,'
      'SL.TotalLineAmountInc as TotalAmountInc'
      'from tblsales_pos S '
      'inner join tblsaleslines_pos SL on SL.saleId = S.saleId'
      'inner join tblemployees E on S.employeeId= E.employeeId'
      'where S.saleDateTime between :dateFrom and :DateTo'
      'union all '
      'Select '
      '1 Details, '
      's.saleId as SaleId, '
      's.SaledateTime  as Saledate_Time,'
      
        'concat(DATE_FORMAT( S.SaleDatetime, '#39'%h %p'#39'), '#39' - '#39' , DATE_FORMA' +
        'T( DAte_Add(S.SaleDatetime, interval 1 hour), '#39'%h %p'#39')) timestlo' +
        't, '
      'S.CustomerName as CustomerName,'
      's.employeeId as employeeId,'
      'E.EmployeeName as EmployeeName,'
      'S.sAleDatetime as sAleDatetime,'
      'NULL as ProductName,'
      'Null as UOM,'
      'NULL as UnitofMeasureShipped ,'
      'NULL as shipped,'
      'S.TotalAmount as TotalAmount,'
      'S.TotalAmountInc as TotalAmountInc'
      'from tblsales_pos S '
      'inner join tblemployees E on S.employeeId= E.employeeId'
      'where S.saleDateTime between :dateFrom and :DateTo'
      'order by  Saledate_Time,details')
