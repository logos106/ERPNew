  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select  '
      '1 as Cateogory, '
      's.saleId as SaleId, '
      'T.TillName  as TillName,'
      'S.CustomerName, '
      's.saleDate , '
      'Sum(SP.Amount) as Amount    , '
      
        '(select Sum(Amount) from tblsalespayments OSP where  OSP.SaleID ' +
        '= S.saleID  and SP.paymethod <>"Cash"  ) as OtherPayments , '
      'null as CashDropped'
      'from tblsales S   '
      
        'inner join tblsalespayments SP on SP.SaleID = S.saleID  and SP.p' +
        'aymethod ="Cash"  '
      'inner join tblpostills T on T.TillID = S.tillID'
      
        'Where S.POS ="T" and S.IsLayby = "F"   and S.isPOS = "T" and S.t' +
        'illid =1'
      'group by s.saleId'
      'union all '
      'select  '
      '2 as Cateogory, '
      's.saleId as SaleId, '
      'T.TillName  as TillName,'
      'S.CustomerName, '
      's.saleDate , '
      'Sum(SP.Amount) as Amount    , '
      
        '(select Sum(Amount) from tblsalespayments_pos OSP where  OSP.Sal' +
        'eID = S.saleID  and SP.paymethod <>"Cash"  ) as OtherPayments , '
      'null as CashDropped'
      'from tblsales_pos S   '
      
        'inner join tblsalespayments_pos SP on SP.SaleID = S.saleID  and ' +
        'SP.paymethod ="Cash"  '
      'inner join tblpostills T on T.TillID = S.tillID'
      
        'Where S.POS ="T" and S.IsLayby = "F"   and S.isPOS = "T" and S.t' +
        'illid =1'
      'group by s.saleId'
      'union all '
      'Select '
      '3 as category, '
      'Null as SaleId, '
      'T.tillname as tillname , '
      'Null as CustomerName, '
      'CD.CashDropAt as SaleDate,'
      'null as Amount, '
      'Null as OtherPayments  , '
      'CD.amount as CashDropped'
      'from tblcashDrop CD '
      'inner join tblpostills T on CD.tillId = T.tillId'
      'where EOPDOne ="F"'
      'Order by Tillname , saleDate desc , SaleId desc')
    Active = True
