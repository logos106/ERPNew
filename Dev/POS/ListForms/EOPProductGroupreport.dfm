  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select S.saleId          as saleId,'
      's.customername           as customername,'
      
        'Left(SUBSTRING_INDEX(P.ProductGroup,'#39'^'#39',1),255)          as Colu' +
        'mn1,'
      
        'Left(substring(substring_index(P.ProductGroup,'#39'^'#39',2),char_length' +
        '(substring_index(P.ProductGroup,'#39'^'#39',1))+2),255)          as Colu' +
        'mn2,'
      
        'Left(substring(substring_index(P.ProductGroup,'#39'^'#39',3),char_length' +
        '(substring_index(P.ProductGroup,'#39'^'#39',2))+2),255)          as Colu' +
        'mn3,'
      'Sl.productname           as productname,'
      
        'concat(SL.UnitofMeasureSaleLines , "(" , SL.UnitofMeasureMultipl' +
        'ier  , ")" ) as UOM,'
      'SL.UnitofMeasureShipped  as UnitofMeasureShipped,'
      'SL.LinePriceInc          as LinePriceInc,'
      'SL.LinePrice             as LinePrice,'
      'SL.Discounts             as Discounts,'
      'SL.DiscountPercent       as DiscountPercent,'
      'SL.Markup                as Markup,'
      'SL.MarkupPercent         as MarkupPercent,'
      
        'Round(((SL.TotalLineAmount - (ifnull(SL.LineCost,0) * SL.shipped' +
        ')) / SL.TotalLineAmount) * 100, 5) as margin ,'
      
        'Round(((SL.TotalLineAmount - (ifnull(SL.LineCost,0) * SL.shipped' +
        ')) / SL.TotalLineAmount) * 100, 5) as marginPercent ,'
      'P.LatestCost             as LatestCost,'
      'P.AvgCost                as AvgCost,'
      'P.PRICE1                 as PRICE1,'
      'P.PRICEINC1              as PRICEINC1,'
      'SL.TotalLineAmount as TotalLineAmount,'
      'SL.TotalLineAmountInc as TotalLineAmountInc, '
      'SL.LineTax as LineTax,'
      '"T" as EOPDone'
      
        'from tblsales_POS S inner join tblsaleslines_pos SL on s.saleId ' +
        '= SL.saleId'
      'inner join tblparts P on P.partsId = SL.productId'
      'Where S.tillid in (0)')
    Active = True
