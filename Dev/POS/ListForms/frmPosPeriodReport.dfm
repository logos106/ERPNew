  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select'
      'tblPosTills.TillName,'
      'S.SaleDate,'
      'Convert(Year(S.SaleDate),Unsigned) as SaleYear,'
      'Convert(Quarter(S.SaleDate),Unsigned) as SaleQuarter,'
      'MonthName(S.SaleDate) as SaleMonth,'
      'Convert(WeekOfYear(S.SaleDate),Unsigned) as SaleWeekOfYear,'
      'DATE_FORMAT(S.SaleDate, "%W") as SaleDayName,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Cash"),SP.Amount,0.00)) as Cas' +
        'h,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("E.F.T."),SP.Amount,0.00)) as E' +
        'FTPOS,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Debit Card"),SP.Amount,0.00)) ' +
        'as DebitCard,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Cheque"),SP.Amount,0.00)) as C' +
        'heque,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Diners Club"),SP.Amount,0.00))' +
        ' as DinersClub,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Master Card"),SP.Amount,0.00))' +
        ' as MasterCard,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("VISA"),SP.Amount,0.00)) as VIS' +
        'A,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Amex"),SP.Amount,0.00)) as Ame' +
        'x,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("American Express"),SP.Amount,0' +
        '.00)) as AmericanExpress,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("BankCard"),SP.Amount,0.00)) as' +
        ' BankCard,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Credit Card"),SP.Amount,0.00))' +
        ' as CreditCard,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Direct Debt"),SP.Amount,0.00))' +
        ' as DirectDebt,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Direct Deposit"),SP.Amount,0.0' +
        '0)) as DirectDeposit,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Money Order"),SP.Amount,0.00))' +
        ' as MoneyOrder,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Layby"),SP.Amount,0.00)) as La' +
        'yby,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Voucher"),SP.Amount,0.00)) as ' +
        'Voucher,'
      
        'Sum(If(UCase(SP.Paymethod)=UCase("Voucher Reversal"),SP.Amount,0' +
        '.00)) as VoucherReversal,'
      
        'Sum(If(not ISNULL(SP.PaymentId), 0.00, S.TotalAmountInc)) as Inv' +
        'oice,'
      
        'Sum(If(not ISNULL(SP.PaymentId), SP.Amount, S.TotalAmountInc)) a' +
        's Totals'
      'From tblSales S'
      'Left Join tblsalespayments SP Using(SaleID)'
      'left join tblPosTills on tblPosTills.TillId = S.TillId '
      'WHERE (S.IsInvoice="T" OR S.IsPOS="T")'
      'AND IfNull(S.TillID,0) <> 0'
      '-- AND S.SaleDate Between :dtFrom And :dtTo'
      'Group By S.TillId'
      ',Year(S.SaleDate),DayOfYear(S.SaleDate)'
      'Order By tblPosTills.TillName, S.SaleDate')
