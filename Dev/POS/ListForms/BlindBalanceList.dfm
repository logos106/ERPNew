  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      '1 as Level,'
      'PeriodID as PeriodID,'
      'PeriodTotal as PeriodTotal,'
      'PeriodID as PeriodNumber,'
      'T.TillID as TillID,'
      'PT.TillName as TillName,'
      'EmployeeName as EmployeeName,'
      'Variation as Variation,'
      'NULL as ProductName,'
      'NULL  as QtySold,'
      'sum(TotalLineAmount) as TotalLineAmount,'
      'Sum(TotalLineAmountInc) as TotalAmountinc,'
      'StartDate as StartDate,'
      'EndDate as EndDate,'
      'Hundreds as Hundreds, '
      'Fifties as Fifties,'
      'Twenties as Twenties,'
      'Tens'#9'as'#9'Tens'#9','
      'Fives'#9'as'#9'Fives'#9','
      'Twos'#9'as'#9'Twos'#9','
      'Ones'#9'as'#9'Ones'#9','
      'FiftyCents'#9'as'#9'FiftyCents'#9','
      'TwentyCents'#9'as'#9'TwentyCents'#9','
      'TenCents'#9'as'#9'TenCents'#9','
      'FiveCents'#9'as'#9'FiveCents'#9','
      'EFTPOS'#9'as'#9'EFTPOS'#9','
      'Visa'#9'as'#9'Visa'#9','
      'MasterCard'#9'as'#9'MasterCard'#9','
      'DinersClub'#9'as'#9'DinersClub'#9','
      'Bankcard'#9'as'#9'Bankcard'#9','
      'Amex'#9'as'#9'Amex'#9','
      'CreditCard'#9'as'#9'CreditCard'#9','
      'Cheques'#9'as'#9'Cheques'#9','
      'InvoicePayments'#9'as'#9'InvoicePayments'#9','
      'PeriodTotal '#9'as'#9'PeriodTotal '#9','
      'NULL  as SaleDateTime,'
      'NULL  as InvoiceDocNumber,'
      'NULL  as SaleID,'
      'IsInDateTimeRange,'
      'NULL  as Details'
      'FROM tmp_tmp_BlindSummary_Admin_3C1163BA T'
      'LEFT JOIN tblPosTills PT on PT.TillId = T.TillId'
      'GROUP BY PeriodID'
      'UNION'
