  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'PartsId,'
      'tblfixedassets.GlobalRef as GlobalRef,'
      'tblfixedassets.AssetID as AssetID,'
      'tblfixedassets.AssetCode as AssetCode,'
      'tblfixedassets.AssetName as AssetName,'
      'tblfixedassets.Description as Description,'
      'tblfixedassets.PurchDate as PurchDate,'
      'tblfixedassets.PurchCost as PurchCost,'
      'tblfixedassets.AssetType as AssetType,'
      'tblfixedassets.LocationDescription as LocationDescription,'
      'tblclass.ClassName as Class, '
      'tblfixedassets.BrandName as BrandName,'
      'tblfixedassets.Manufacture as Manufacture,'
      'tblfixedassets.Model as Model,'
      'tblfixedassets.Serial as Serial,'
      'SUBSTRING(tblfixedassets.Notes,1,255) as Notes, '
      'case when tblfixedassets.PARTSID is null then "" '
      
        'else (select PartsDescription from tblParts where PartsId = tblf' +
        'ixedassets.partsid) '
      'end  SalesDescription,'
      'cast(tblfixedassets.Qty as signed) as Qty,'
      'tblfixedassets.AssetCondition as AssetCondition,'
      'tblfixedassets.Colour as Colour,'
      'tblfixedassets.Size as Size,'
      'tblfixedassets.Shape as Shape,'
      'tblfixedassets.Status as Status,'
      'tblfixedassets.EstimatedValue as EstimatedValue,'
      'tblfixedassets.ReplacementCost as ReplacementCost,'
      'tblfixedassets.WarrantyType as WarrantyType,'
      'tblfixedassets.WarrantyExpiresDate as WarrantyExpiresDate,'
      'tblfixedassets.InsuredBy as InsuredBy,'
      'tblfixedassets.InsurancePolicy as InsurancePolicy,'
      'tblfixedassets.InsuredUntil as InsuredUntil,'
      'tblfixedassets.BusinessUsePercent as "BusinessUse (%)",'
      'tblfixedassets.CUSTFLD1 as CUSTFLD1,'
      'tblfixedassets.CUSTFLD2 as CUSTFLD2,'
      'tblfixedassets.CUSTFLD3 as CUSTFLD3,'
      'tblfixedassets.CUSTFLD4 as CUSTFLD4,'
