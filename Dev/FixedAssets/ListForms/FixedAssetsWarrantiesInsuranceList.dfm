  inherited qryMain: TERPQuery
    SQL.Strings = (
      '#'
      '# we need AS on every field if we want search engine to work'
      '#'
      'SELECT '
      'tblfixedassets.GlobalRef as GlobalRef, '
      'AssetID as AssetID, '
      'AssetCode as AssetCode, '
      'AssetName as AssetName, '
      'PurchDate as PurchDate, '
      'PurchCost as PurchCost, '
      'AssetType as AssetType,  '
      'LocationDescription as LocationDescription,'
      'ClassName as Class, '
      'BrandName as BrandName, '
      'Manufacture as Manufacture, '
      'Model as Model, '
      'Serial as Serial, '
      'SUBSTRING(Notes,1,255) as Notes, '
      'Qty as Qty, '
      'AssetCondition as AssetCondition, '
      'Colour as Colour, '
      'Size as Size, '
      'Shape as Shape, '
      'Status as Status, '
      'EstimatedValue as EstimatedValue, '
      'ReplacementCost as ReplacementCost, '
      'WarrantyType as WarrantyType, '
      'WarrantyExpiresDate as WarrantyExpiresDate, '
      'InsuredBy as InsuredBy, '
      'InsurancePolicy as InsurancePolicy, '
      'InsuredUntil as InsuredUntil, '
      'BusinessUsePercent as "BusinessUse (%)",'
      'CUSTFLD1 as CUSTFLD1, '
      'CUSTFLD2 as CUSTFLD2, '
      'CUSTFLD3 as CUSTFLD3, '
      'CUSTFLD4 as CUSTFLD4, '
      'CUSTFLD5 as CUSTFLD5, '
      'CUSTFLD6 as CUSTFLD6, '
      'CUSTFLD7 as CUSTFLD7, '
      'CUSTFLD8 as CUSTFLD8, '
      'CUSTFLD9 as CUSTFLD9, '
      'CUSTFLD10 as CUSTFLD10, '
      'CUSTFLD11 as CUSTFLD11, '
      'CUSTFLD12 as CUSTFLD12, '
      'CUSTFLD13 as CUSTFLD13, '
      'CUSTFLD14 as CUSTFLD14, '
      'CUSTFLD15 as CUSTFLD15  '
      'FROM tblfixedassets'
      'LEFT Join tblclass using(ClassID)')
