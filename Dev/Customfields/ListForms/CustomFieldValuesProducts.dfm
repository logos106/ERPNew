  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select'
      'CFV.ID as ID,'
      'CFV.masterID as MasterID,'
      'M.Partname as MasterName ,'
      'CFL.Description as Description ,'
      'ProductPrintName as ProductPrintName,'
      'Left(SUBSTRING_INDEX(M.ProductGroup,'#39'^'#39',1),255) AS FirstColumn,'
      
        'Left(substring(substring_index(M.ProductGroup,'#39'^'#39',2),char_length' +
        '(substring_index(M.ProductGroup,'#39'^'#39',1))+2),255) AS SecondColumn,'
      
        'Left(substring(substring_index(M.ProductGroup,'#39'^'#39',3),char_length' +
        '(substring_index(M.ProductGroup,'#39'^'#39',2))+2),255) AS ThirdColumn,'
      'PARTSDESCRIPTION as SalesDescription,'
      'PURCHASEDESC as PURCHASEDESC,'
      'PRICEINC1 AS PriceInc,'
      'Discontinued as Discontinued,'
      'PartType as PartType,'
      'M.BARCODE as BARCODE,'
      'M.active as Active,'
      'M.ProductionNotes as ProductionNotes,'
      'M.GeneralNotes as GeneralNotes,'
      'M.IsSystemProduct as IsSystemProduct,'
      'M.Batch,'
      'M.Multiplebins,'
      'M.SNtracking,'
      'M.Area as Area,'
      ''
      'CFV.Value as Value'
      'from  tblcustomfieldvaluesproducts CFV'
      'inner join tblparts  M on CFV.masterId = M.PARTSID'
      
        'inner join tblCustomfieldList CFL on CFL.CFID =  CFV.CustomField' +
        'listId and CFL.Listtype = '#39'ltProducts'#39)
    inherited qryMainMasterName: TWideStringField
      DisplayLabel = 'Product~Name'
    end
