  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'E.GlobalRef as GlobalRef, '
      'E.EquipmentID as EquipmentID, '
      'E.ProductId as ProductId, '
      'E.EquipmentName as EquipmentName, '
      'E.EquipmentDescription as EquipmentDescription, '
      'E.Active as Active, '
      'if(ifNull(E.ProductID,0)=0 , '#39'F'#39' , '#39'T'#39') as ISProduct  , '
      'E.Manufacture as Manufacture, '
      'E.Model as Model, '
      'E.Serialnumber as Serialnumber, '
      'E.Onhire as Onhire,'
      'min(A.AttachmentID) as AttachmentID '
      'from tblEquipment e '
      
        'left join tblAttachments A on A.TableId = e.EquipmentId and A.ta' +
        'blename ="tblEquipment"'
      'group by E.EquipmentID')
