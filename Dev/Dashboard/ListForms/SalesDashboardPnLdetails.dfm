  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      #39'2013-01-27 23:59:59'#39' as Date,  '
      '1 as finalorder,  '
      '1 details, '
      '"Total Income" as type, '
      '"Income" '#9#9'as Description ,'#9' '
      '0 as saleID'#9',  '
      'sum(CreditsEx - DebitsEx) as "Ex" ,'#9'  '
      'sum(CreditsInc - DebitsInc) as "Inc" '#9' '#9'  '
      'FROM tbltransactions  T '#9#9#9#9#9' '
      
        'Where char_length(AccountName)>0 AND (AccountType in( "EXINC" ,"' +
        'INC") )   '#9#9#9' '
      'and Date between '#39'2012-11-23 00:00:00'#39' and '#39'2013-01-27 23:59:59'#39
      'UNION ALL   '
      'Select  '
      #39'2013-01-27 23:59:59'#39' as date,  '
      '2 as finalorder,  '
      '1 details, '
      '"Total Cogs" as type, '
      '"COGS" '#9#9#9'as Description ,'#9' '
      '0 as saleID'#9',  '
      'sum(CreditsEx - DebitsEx) as "Ex" ,'#9'  '
      'sum(CreditsInc - DebitsInc) as "Inc" '#9' '#9'  '
      'FROM tbltransactions  T '#9#9#9#9#9' '
      
        'Where char_length(AccountName)>0 AND (AccountType in( "COGS") ) ' +
        #9#9#9#9#9#9' '
      
        'and Date between '#39'2012-11-23 00:00:00'#39' and '#39'2013-01-27 23:59:59'#39 +
        ' '
      'UNION ALL   '
      'Select  '
      #39'2013-01-27 23:59:59'#39' as date,  '
      '3 as finalorder,  '
      '1 details, '
      '"Total Gross Profit" as type, '
      '"Gross Profit" as Description ,'#9' '
      '0 as saleID'#9',  '
      'sum(CreditsEx - DebitsEx) as "Ex" ,'#9'  '
      'sum(CreditsInc - DebitsInc) as "Inc" '#9' '#9'  '
      'FROM tbltransactions  T '#9#9#9#9#9' '
      
        'Where char_length(AccountName)>0 AND (AccountType in( "COGS",  "' +
        'EXINC" ,"INC") )  '
      
        'and Date between '#39'2012-11-23 00:00:00'#39' and '#39'2013-01-27 23:59:59'#39 +
        ' '
      'union all   '
      'Select  '
      'Date '#9#9' as Date,  '
      '1 as finalorder,  '
      '2 details, '
      'Type as type, '
      '"Income" '#9#9'as Description ,'#9' '
      'saleId'#9#9',  '
      'sum(CreditsEx - DebitsEx) as "Ex" ,'#9'  '
      'sum(CreditsInc - DebitsInc) as "Inc" '#9' '#9'  '
      'FROM tbltransactions  T '#9#9#9#9#9' '
      
        'Where char_length(AccountName)>0 AND (AccountType in( "EXINC" ,"' +
        'INC") )  '#9#9#9' '
      
        'and Date between '#39'2012-11-23 00:00:00'#39' and '#39'2013-01-27 23:59:59'#39 +
        ' '
      '  and type <> "Closing Date Summary"  '
      'group by saleID '
      'UNION ALL   '
      'Select  '
      'Date '#9#9' as Date,  '
      '2 as finalorder,  '
      '2 details, '
      'Type as type, '
      '"COGS" '#9#9#9'as Description ,'#9' '
      'saleId'#9#9',  '
      'sum(CreditsEx - DebitsEx) as "Ex" ,'#9'  '
      'sum(CreditsInc - DebitsInc) as "Inc" '#9' '#9'  '
      'FROM tbltransactions  T '#9#9#9#9#9' '
      
        'Where char_length(AccountName)>0 AND (AccountType in( "COGS") ) ' +
        ' '#9#9#9#9#9#9' '
      
        'and Date between '#39'2012-11-23 00:00:00'#39' and '#39'2013-01-27 23:59:59'#39 +
        '   '
      'and type <> "Closing Date Summary"  '
      'group by saleID '
      'union all   '
      'Select Date '#9#9' as Date,  '
      '1 as finalorder,  '
      '2 details, '
      'Type as type, '
      '"Income" '#9#9'as Description ,'#9' '
      'saleId'#9#9',  '
      'sum(CreditsEx - DebitsEx) as "Ex" ,'#9'  '
      'sum(CreditsInc - DebitsInc) as "Inc" '#9' '#9'  '
      'FROM tbltransactionSummarydetails  T '#9' '
      
        'Where char_length(AccountName)>0 AND (AccountType in( "EXINC" ,"' +
