  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      '1 as Category,'
      'if(S.isinvoice="T","Invoice","Sales Order") as type,'
      'S.saleID AS ID, '
      'SL.salelineId AS LineID,'
      'SL.seqno AS seqno,'
      'SL.productId AS productId,'
      'SL.productname AS productname,'
      'SL.qtysold AS qtysold,'
      'SL.shipped AS shipped,'
      'SL.backorder AS backorder,'
      'SL.UnitofMeasureQtySold AS UnitofMeasureQtySold,'
      'SL.UnitofMeasureShipped AS UnitofMeasureShipped,'
      'SL.UnitofMeasureBackorder AS UnitofMeasureBackorder,'
      'S.isinvoice,'
      'S.issalesorder,'
      '"F" as ISPO,'
      'Sl.Invoiced'
      'FROM tblSales S'
      'INNER JOIN tblsaleslines SL Using(SaleID)'
      'WHERE (S.IsInvoice='#39'T'#39' OR S.IsSalesOrder='#39'T'#39')'
      'AND    S.Deleted='#39'F'#39' '
      'AND SL.BackOrder <> 0 '
      'AND    ifnull(S.BOID , '#39#39') = '#39#39
      'AND IFNULL(SL.shipped,0)<>0'
      'union '
      'SELECT'
      '2 as Category,'
      'if(S.isinvoice="T","Invoice","Sales Order") as type,'
      'S.saleID AS ID, '
      'SL.salelineId AS LineID,'
      'SL.seqno AS seqno,'
      'SL.productId AS productId,'
      'SL.productname AS productname,'
      'SL.qtysold AS qtysold,'
      'SL.shipped AS shipped,'
      'SL.backorder AS backorder,'
      'SL.UnitofMeasureQtySold AS UnitofMeasureQtySold,'
      'SL.UnitofMeasureShipped AS UnitofMeasureShipped,'
      'SL.UnitofMeasureBackorder AS UnitofMeasureBackorder,'
      'S.isinvoice,'
      'S.issalesorder,'
      '"F" as ISPO,'
      'Sl.Invoiced'
      'FROM tblSales S'
      'INNER JOIN tblsaleslines SL Using(SaleID)'
      'WHERE (S.IsInvoice='#39'T'#39' OR S.IsSalesOrder='#39'T'#39')'
      'AND    S.Deleted='#39'F'#39' '
      'AND SL.BackOrder <> 0 '
      'AND    ifnull(S.BOID , '#39#39') = '#39#39
      'AND IFNULL(SL.shipped,0) = 0'
      'AND SL.invoiced ="T"'
      'union '
      'SELECT'
      '3 as Category,'
      '"PO" as type,'
      'S.PurchaseorderID AS ID, '
      'SL.salelineId AS LineID,'
      'SL.seqno AS seqno,'
      'SL.productId AS productId,'
      'SL.productname AS productname,'
      'SL.qtysold AS qtysold,'
      'SL.shipped AS shipped,'
      'SL.backorder AS backorder,'
      'SL.UnitofMeasureQtySold AS UnitofMeasureQtySold,'
      'SL.UnitofMeasureShipped AS UnitofMeasureShipped,'
      'SL.UnitofMeasureBackorder AS UnitofMeasureBackorder,'
      '"F" isinvoice,'
      '"F" issalesorder,'
      'ISPO, '
      'SL.invoiced'
      'FROM tblpurchaseOrders S'
      'INNER JOIN tblpurchaselines SL Using(PurchaseorderID)'
      'WHERE ISPO="T"'
      'AND    S.Deleted='#39'F'#39' '
      'AND SL.BackOrder <> 0 '
      'AND    ifnull(S.BOID , '#39#39') = '#39#39
      'AND IFNULL(SL.shipped,0)<>0'
      ''
      'union '
      'SELECT'
      '4 as Category,'
      '"PO" as type,'
      'S.PurchaseorderID AS ID, '
      'SL.salelineId AS LineID,'
      'SL.seqno AS seqno,'
      'SL.productId AS productId,'
      'SL.productname AS productname,'
      'SL.qtysold AS qtysold,'
      'SL.shipped AS shipped,'
      'SL.backorder AS backorder,'
      'SL.UnitofMeasureQtySold AS UnitofMeasureQtySold,'
      'SL.UnitofMeasureShipped AS UnitofMeasureShipped,'
      'SL.UnitofMeasureBackorder AS UnitofMeasureBackorder,'
      '"F" isinvoice,'
      '"F" issalesorder,'
      'ISPO, '
      'SL.invoiced'
      'FROM tblpurchaseOrders S'
      'INNER JOIN tblpurchaselines SL Using(PurchaseorderID)'
      'WHERE ISPO="T"'
      'AND    S.Deleted='#39'F'#39' '
      'AND SL.BackOrder <> 0 '
      'AND    ifnull(S.BOID , '#39#39') = '#39#39
      'AND IFNULL(SL.shipped,0) = 0'
      'AND SL.invoiced ="T"'
      ''
      'Order by type, ID, lineID')
