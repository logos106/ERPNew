  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      '2 as catg, '
      'T.TaskType as TaskType, '
      'T.Versionno as Versionno,'
      'T.Createdon as Createdon,'
      'T.enteredby as enteredby,'
      'T.subject as subject,'
      'T.details as details,'
      'T.estimatedHrs as estimatedHrs,'
      'T.percentagedone as percentagedone,'
      'T.mstimestamp as mstimestamp,'
      'T.Active as Active,'
      'T.ID as ID,'
      'T.Taskno as Taskno,'
      'T.ClientCode as ClientCode,'
      'T.Seqno as Seqno,'
      'T.CustomerName as CustomerName'
      'from tbltasks T '
      'where Createdon between :Datefrom and :Dateto'
      ' union all '
      'Select distinct '
      '1 as catg, '
      '" " as TaskType, '
      'Versionno as Versionno,'
      'null as Createdon,'
      'null as enteredby,'
      '"" as subject,'
      '"" as details,'
      '0 as estimatedHrs,'
      '0 as percentagedone,'
      'NULL as mstimestamp,'
      'Active as Active,'
      '0 as ID,'
      '0 as Taskno,'
      'NULL as ClientCode,'
      'NULL as Seqno,'
      'null as CustomerName'
      'from tbltasks'
      'where Createdon between :Datefrom and :Dateto'
      
        'Order by   VersionToNo(versionno)   desc,  tasktype, seqno, catg' +
        ', CreatedOn')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Datefrom'
      end
      item
        DataType = ftUnknown
        Name = 'Dateto'
      end
      item
        DataType = ftUnknown
        Name = 'Datefrom'
      end
      item
        DataType = ftUnknown
        Name = 'Dateto'
      end>
