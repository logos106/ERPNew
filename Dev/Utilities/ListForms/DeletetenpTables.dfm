  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select distinct table_name  ,  '
      
        'if(Locate('#39'erpfix_'#39' , table_name) =1, '#39'ERP Backup Table'#39' , '#39'ERP ' +
        'Temp Table'#39') as Description   , '
      'CREATE_TIME , '
      'table_rows'
      'from information_schema.`TABLES`'
      
        'where ((table_name like "erpfix%" ) or ((table_name  like '#39'tmp_%' +
        #39' and  LOCATE('#39'_'#39' , REPLACE(table_name,'#39'tmp_'#39','#39#39') )>0 ) '
      '#and CREATE_TIME < DATE_ADD(curdate(),INTERVAL -1 Day)'
      
        'and table_name not in (Select table_name from information_schema' +
        '.`TABLES` where Table_schema = '#39'erpnewdb'#39' )))'
      'and TABLE_SCHEMA =:DbName'
      'order by CREATE_TIME desc')
    CachedUpdates = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DbName'
      end>
