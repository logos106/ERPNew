  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'E.employeeId as employeeId, '
      'E.EmployeeName as EmployeeName,'
      'P.PasswordDate as PasswordDate , '
      
        '(SELECT  FieldValue FROM `tbldbpreferences` WHERE `Name` IN ('#39'Lo' +
        'ginExpireDays'#39')) as LoginExpireDays,'
      'P.Logon_Name as username,'
      'P.Logon_Password as userpassword,'
      
        'convert(if(PP.PasswordNeverExpires="T", Null , DATE_Add(P.Passwo' +
        'rdDate , INTERVAL (SELECT  FieldValue FROM `tbldbpreferences` WH' +
        'ERE `Name` IN ('#39'LoginExpireDays'#39')) Day)), datetime) LoginExpireo' +
        'n,'
      
        'if(PP.PasswordNeverExpires ="T", null , 0-DATEDIFF(curdate() , P' +
        '.PasswordDate )) as LoginExpiredDays, '
      'PP.PasswordNeverExpires as PasswordNeverExpires,'
      'PP.CanChangePassword as CanChangePassword'
      'from tblemployees E'
      'inner join tblpassword P on E.EmployeeID = P.EmployeeId'
      
        'inner join tblpersonalpreferences PP on E.EmployeeID = PP.Employ' +
        'eeID'
      
        'order by if((LoginExpireon is null) or (LoginExpireon =0),1 , 2)' +
        '  desc , LoginExpireon , employeename')
    OnCalcFields = qryMainCalcFields
