  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select * '
      'from tblLog '
      'where LogTime between :dtFrom and :dtTo'
      'and ((:Module = "All") or (:Module = Module)) '
      
        'and ((:LogType = "All") or (:LogType = "Error" and LogType = "lt' +
        'Error") or (:LogType = "Warning" and LogType = "ltWarning")) '
      'order by LogID DESC')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'dtFrom'
      end
      item
        DataType = ftUnknown
        Name = 'dtTo'
      end
      item
        DataType = ftUnknown
        Name = 'Module'
      end
      item
        DataType = ftUnknown
        Name = 'Module'
      end
      item
        DataType = ftUnknown
        Name = 'LogType'
      end
      item
