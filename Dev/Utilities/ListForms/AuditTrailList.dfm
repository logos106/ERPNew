  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'tblaudittrail.GlobalRef as GlobalRef, '
      'AuditID as AuditID,'
      'TransType as TransType, '
      'TransGlobalref as "Trans Global Ref", '
      'DataDescription as DataDescription,'
      'AuditDate as AuditDate, '
      'EmployeeName as EmployeeName,'
      '"Double Click for details" as ChangesDetails '
      'FROM tblaudittrail'
      
        'INNER JOIN tblemployees on tblemployees.EmployeeID = tblaudittra' +
        'il.EmployeeID'
      'WHERE AuditDate Between :txtfrom AND :txtto'
      'Order By AuditID Desc;')
    ParamData = <
