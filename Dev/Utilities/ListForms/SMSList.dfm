  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select  '
      'tblReference.RefId as RefId,'
      'tblReference.ContactID as ContactId,'
      'tblContacts.Company as Contact,'
      'tblReference.CusId as CustomerId,'
      'cust.Company as CustomerName,'
      'tblReference.SupID as SupplierId,'
      'supp.Company as SupplierName,'
      'tblReference.EmployeeId as EmployeeId,'
      'tblEmployees.EmployeeName as EmployeeName,'
      'tblReference.OtherContactId as OtherContactId,'
      'other.Company as OtherContactName,'
      'tblReference.MarketingContactId as MarketingContactId,'
      'tblMarketingContacts.Company as MarketingContactName,'
      'tblReference.Referencetxt as Subject,'
      'tblReference.Ref_Date as DateTime,'
      'tblReference.Active as Active,'
      'tblReference.Ref_type as RefType,'
      'tblReference.ReferenceData as MessageData,'
      'tblReference.MessageFrom as MessageFrom,'
      'tblReference.MessageTo as MessageTo,'
      'tblReference.MessageId as MessageId,'
      'tblReference.Status as Status,'
      'tblReference.Cost as Cost,'
      'tblReference.CurrencyCode as CurrencyCode'
      'from tblReference '
      
        'left join tblEmployees on tblReference.EmployeeId = tblEmployees' +
        '.EmployeeID'
      
        'left join tblContacts on tblReference.ContactID = tblContacts.Co' +
        'ntactId '
      
        'left join tblClients as cust on tblReference.CusId = cust.Client' +
        'ID'
      
        'left join tblClients as supp on tblReference.SupId = supp.Client' +
        'ID'
      
        'left join tblClients as other on tblReference.OtherContactId = o' +
        'ther.ClientID and other.Customer = "F" and other.Supplier = "F"'
      
        'left join tblMarketingContacts on tblReference.MarketingContactI' +
        'd = tblMarketingContacts.ID '
      'where tblReference.Ref_Type in ("SMS","Voice")'
      'and tblReference.Ref_Date between :DateFrom and :DateTo'
      'order by tblReference.Ref_Date ASC')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end>
