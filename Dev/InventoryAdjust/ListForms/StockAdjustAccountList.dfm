  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select distinct '
      '1 as Details , '
      'COA.AccountType as AccountType, '
      'COA.AccountName as AccountName , '
      'null  as `GlobalRef`,'
      'null  as `StockAdjustEntryID`,'
      'null  as `CreationDate`,'
      'null  as Adjustmentdate,'
      'null  as `Notes`,'
      'null  as `Employee`,'
      'null  as `Processed`,'
      'null as Adjusttype'
      'from tblstockadjustentry SAE '
      
        'inner join tblchartofaccounts COA on COA.AccountID = SAE.Account' +
        'ID'
      
        'Where SAE.Deleted ="F" and Adjustmentdate between :DateFrom and ' +
        ':DateTO'
      'union all '
      'Select distinct '
      '2 as Details , '
      'COA.AccountType as AccountType, '
      'COA.AccountName as AccountName , '
      'SAE.`GlobalRef`,'
      'SAE.`StockAdjustEntryID`,'
      'SAE.`CreationDate`,'
      'date(SAE.`Adjustmentdate`) as Adjustmentdate,'
      'SAE.`Notes`,'
      'SAE.`Employee`,'
      'SAE.`Processed`,'
      'SAE.Adjusttype asAdjusttype'
      'from tblstockadjustentry SAE '
      
        'inner join tblchartofaccounts COA on COA.AccountID = SAE.Account' +
        'ID'
      
        'Where SAE.Deleted ="F" and Adjustmentdate between :DateFrom and ' +
        ':DateTO'
      'Order by  accounttype, Details, StockAdjustEntryID')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTO'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTO'
      end>
