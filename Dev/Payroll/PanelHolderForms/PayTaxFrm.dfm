  object qryTaxList: TERPQuery
    SQL.Strings = (
      'select '
      'tblpaytaxconfig.TaxName,'
      'if(TaxType = "Employee",Sum(Amount),0.0) as EmpAmount,'
      'if(TaxType = "Company",Sum(Amount),0.0) as CoyAmount'
      'from tblpaytax'
      
        'inner join tblpaytaxconfig on tblpaytaxconfig.ID = tblpaytax.Pay' +
        'TaxConfigId'
      'where tblPayTax.PayId = :PayId'
      'group by tblpaytaxconfig.ID'
      'order by tblpaytaxconfig.PrimaryTax, tblpaytaxconfig.TaxName')
    Left = 312
    Top = 16
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'PayId'
      end>
