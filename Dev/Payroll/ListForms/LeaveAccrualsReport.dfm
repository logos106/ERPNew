  inherited qryMain: TERPQuery
    SQL.Strings = (
      '# This is dummy SQL - It is rewritten in RefreshQuery()'
      'SELECT'
      '0 as zDetails,'
      '"DetailRec" as Catagory,'
      'L.EmployeeID, '
      'E.EmployeeName, '
      '1.1 as Rate,'
      'L.Type,'
      'L.OpenningBalanceHrs, '
      'LA.Date as DateAccrued,'
      'LA.AccruedHours, '
      'E.LeaveLoading, '
      '20.1 as AmountAccrued,'
      'L.ClassID, '
      'L.ClassName'
      'FROM tblleave AS L'
      
        'INNER JOIN tblleaveaccruals AS LA on LA.EmployeeID = L.EmployeeI' +
        'D AND LA.LeaveType = L.Type'
      'INNER JOIN tblemployees AS E on L.EmployeeID = E.EmployeeID'
      '#GROUP BY L.Type, L.ClassName, LA.EmployeeID'
      '#ORDER BY L.EmployeeID'
      ''
      'UNION ALL'
      ''
      'SELECT'
      '1 as zDetails,'
      'Space(255) as Catagory,'
      'L.EmployeeID, '
      'E.EmployeeName, '
      '1.1 as Rate,'
      'L.Type,'
      'L.OpenningBalanceHrs,'
      'LA.Date as DateAccrued,'
      'SUM(LA.AccruedHours), '
      'E.LeaveLoading, '
      '0.1 as AmountAccrued,'
      'L.ClassID, '
      'L.ClassName'
      'FROM tblleave AS L'
      
        'INNER JOIN tblleaveaccruals AS LA on LA.EmployeeID = L.EmployeeI' +
        'D AND LA.LeaveType = L.Type'
      'INNER JOIN tblemployees AS E on L.EmployeeID = E.EmployeeID'
      'GROUP BY L.Type, L.ClassName, LA.EmployeeID'
      '#ORDER BY L.EmployeeID'
      '')
