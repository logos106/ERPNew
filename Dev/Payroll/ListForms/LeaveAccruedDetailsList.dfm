  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'Date as AccruedDate,'
      'EmployeeName as Employee,'
      'PayID as PayNo,'
      'AccrualType as AccruedType,'
      'LeaveType as LeaveType,'
      'Round(AccruedHours,5) as Hours,'
      
        'AccruedHours*(If(IsNull(EPR.LineTotal/EPR.Qty),HourlyRate,(EPR.L' +
        'ineTotal/EPR.Qty)) +(If(IsNull(EPR.LineTotal/EPR.Qty),HourlyRate' +
        ',(EPR.LineTotal/EPR.Qty)) *(If(LeaveType='#39'Annual Leave'#39',E.LeaveL' +
        'oading,0.00)/100))) as Value'
      'FROM tblleaveaccruals'
      'INNER JOIN tblpays as P using (PayID)'
      
        'INNER JOIN tblemployees as E on E.EmployeeID = tblleaveaccruals.' +
        'EmployeeID'
      
        'INNER JOIN tblemployeepayrates EPR on EPR.EmployeeID = tblleavea' +
        'ccruals.EmployeeID'
      
        'INNER JOIN tblpayrates PR ON (EPR.PayRateID=PR.RateID) AND (Mult' +
        'iplier=1)'
      'WHERE P.Deleted = "F"'
      'AND tblleaveaccruals.Date Between :txtfrom AND :txtto'
      'GROUP BY tblleaveaccruals.AccrualID'
      ''
      'UNION ALL'
      ''
      'Select OpenningBalanceDate as AccruedDate,'
      'EmployeeName as Employee, '
      '0 as PayNo,'
      #39'Opening'#39' as AccruedType, '
      'Type as LeaveType,'
      'Round(OpenningBalanceHrs,5) as Hours,  '
      
        'OpenningBalanceHrs*(If(IsNull(EPR.LineTotal/EPR.Qty),HourlyRate,' +
        '(EPR.LineTotal/EPR.Qty)) +(If(IsNull(EPR.LineTotal/EPR.Qty),Hour' +
        'lyRate,(EPR.LineTotal/EPR.Qty)) *(If(Type='#39'Annual Leave'#39',E.Leave' +
        'Loading,0.00)/100))) as Value'
      'from tblleave'
      'INNER JOIN tblemployees as E using (EmployeeID)'
      'INNER JOIN tblemployeepayrates EPR USING(EmployeeID)'
      
        'INNER JOIN tblpayrates as PR ON (EPR.PayRateID=PR.RateID) AND (M' +
        'ultiplier=1)'
