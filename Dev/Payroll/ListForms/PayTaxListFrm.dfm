  object qrySummary: TERPQuery
    Connection = MyConnection1
    SQL.Strings = (
      'SELECT '
      
        #9#9'CONCAT(CONVERT(p.EmployeeID, CHAR), " - " , p.EmpName) AS Empl' +
        'oyee,'
      
        #9#9'DATE_FORMAT(DatePaid, '#39'%Y-%m-%d'#39') AS DatePaid, 0 AS CheckNum, ' +
        'p.Gross,'
      #9#9'p.SocWages,'
      
        #9#9'SUM(IF(c.GroupType='#39'SocialSecurity'#39' AND c.TaxType='#39'Employee'#39', ' +
        't.Amount , 0)) AS SocTax, '
      #9#9'p.MedWages,'
      
        #9#9'SUM(IF(c.GroupType='#39'Medicare'#39' AND c.TaxType='#39'Employee'#39', t.Amou' +
        'nt , 0)) AS MedTax,'
      #9#9'0.0 AS MedOverWage,'
      #9#9'0.0 AS MedOverTax'
      #9'FROM tblpays p LEFT JOIN tblpaytax t ON p.PayID=t.PayID'
      #9#9'LEFT JOIN tblpaytaxconfig c ON t.PayTaxConfigId=c.ID'
      #9'WHERE p.Paid='#39'T'#39' AND p.DatePaid BETWEEN :dtFrom AND :dtTo'
      #9'GROUP BY p.EmployeeID, p.DatePaid')
    Left = 488
    Top = 280
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'dtFrom'
      end
      item
        DataType = ftUnknown
        Name = 'dtTo'
      end>
