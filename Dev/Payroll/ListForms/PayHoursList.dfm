  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'tblpays.EmpName as EmployeeName,'
      'tblpays.PayID as PayId,'
      'tblpays.PayDate as PayDate,'
      'tblpays.DatePaid as DatePaid,'
      'tblpays.Class as Department,'
      'tblpayrates.Description as Description,'
      'tblpayspayrates.Qty as Hours'
      'from tblpays, tblpayspayrates, tblpayrates'
      'where tblpays.PayID = tblpayspayrates.PayID'
      'and tblpayspayrates.PayRateID = tblpayrates.RateID'
      'and tblpays.Deleted = "F"'
      'and tblpays.Paid = "T"'
      'and tblpays.PayDate between :PayDateFrom and :PayDateTo'
      'and tblpays.DatePaid between :DatePaidFrom and :DatePaidTo'
      'and ((:Department = "") or (tblpays.Class = :Department))'
      'order by PayId')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'PayDateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'PayDateTo'
      end
      item
        DataType = ftUnknown
        Name = 'DatePaidFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DatePaidTo'
      end
      item
        DataType = ftUnknown
        Name = 'Department'
      end
      item
        DataType = ftUnknown
        Name = 'Department'
      end>
