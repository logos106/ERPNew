      'FROM tblpaystransactions AS PT'
      'INNER JOIN tblpays AS P on P.PayID = PT.PayID'
      'LEFT JOIN tblclass AS C on PT.ClassID = C.ClassID'
      'ORDER BY PT.EmployeeID, PT.Date')
