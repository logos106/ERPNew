  inherited qryMain: TERPQuery
    SQL.Strings = (
      '# This is a dummy SQL - See RefreshQuery()'
      'SELECT 1 as DEtails, PT.GlobalRef,P.PayID as PayNo,'
      'PT.Type, PT.Date, PT.ClassID, C.ClassName,'
      
        'IF(PT.AccountType = '#39'OCLIAB'#39' OR PT.AccountType = '#39'BANK'#39', PT.Amou' +
        'nt, 0.00) AS Credit,'
      'IF(PT.AccountType = '#39'EXP'#39', PT.Amount * -1, 0.00) AS Debit,'
      
