  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      '2 as Details, '
      'Category         AS Category , '
      'payID            AS payID, '
      'EmployeeName     AS EmployeeName, '
      'description      AS description, '
      'Qty              AS Qty, '
      'Amount           AS Amount, '
      'totalAmount      AS TotalAmount, '
      'datePaid         AS datePaid, '
      'PayDate          AS PayDate, '
      'Paid             AS paid'
      'from tmp_RatenLeave_Dene_FB9B95BA T '
      'union all'
      'Select '
      '1                AS Details, '
      'NULL             AS Category, '
      'NULL             AS payID, '
      'EmployeeName     AS EmployeeName, '
      'description      AS description, '
      'Sum(Qty)         AS Qty, '
      'NULL             AS Amount, '
      'sum(totalAmount) AS TotalAmount, '
      'Null             AS  datePaid, '
      'NULL             AS PayDate, '
      'NULL             AS Paid '
      'from tmp_RatenLeave_Dene_FB9B95BA T  '
      'where category <> 0 '
      'group by employeeName, description'
      'order by details, EmployeeName, payId , Category, Description')
