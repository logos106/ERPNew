  inherited SctMain: TERPScript
    SQL.Strings = (
      'Drop table if exists :Tablename;'
      'Create table :Tablename '
      
        'select 1 Category, P.PayId, e.EmployeeName , PR.Description , PP' +
        'R.Qty, PPR.Amount, PPR.linetotal totalAmount, P.DatePaid, P.Payd' +
        'ate  , P.Paid'
      'from tblPays P'
      'inner join tblpayspayrates PPR  on P.PayID = PPR.PayId'
      'inner join tblPayrates PR on PPR.PayRateID = PR.rateId'
      'inner join tblemployees E on P.EmployeeId = E.EmployeeID'
      
        'Where (:filterOn = 0 and P.DatePaid between :Datefrom and :Datet' +
        'o) or (:filterOn = 1 and P.Paydate  between :Datefrom and :Datet' +
        'o) '
      'and P.deleted = '#39'F'#39
      'union all '
      
        'Select 2, P.PayId, E.employeeName, A.Description , PA.Qty , PA.A' +
        'mount , PA.Qty * PA.Amount , P.DatePaid, P.Paydate  , P.Paid'
      'from tblpaysallowances  PA '
      'inner join tblallowances A on A.AllowanceID = PA.AllowanceID'
      'inner join tblPays P on PA.PayID = P.PayId'
      'inner join tblemployees E on P.EmployeeId = E.EmployeeID'
      
        'Where (:filterOn = 0 and P.DatePaid between :Datefrom and :Datet' +
        'o) or (:filterOn = 1 and P.Paydate  between :Datefrom and :Datet' +
        'o) '
      'and P.deleted = '#39'F'#39
      'union all '
      
        'Select 3, P.PayId, E.employeeName, D.Description , PD.Qty , PD.A' +
        'mount , PD.Qty * PD.Amount , P.DatePaid, P.Paydate  , P.Paid'
      'from tblpaysdeductions    PD '
      'inner join tbldeductions D on D.DeductionID = PD.DeductionID'
      'inner join tblPays P on PD.PayID = P.PayId'
      'inner join tblemployees E on P.EmployeeId = E.EmployeeID'
      
        'Where (:filterOn = 0 and P.DatePaid between :Datefrom and :Datet' +
        'o) or (:filterOn = 1 and P.Paydate  between :Datefrom and :Datet' +
        'o) '
      'and P.deleted = '#39'F'#39
      'union all '
      
        'Select 4, P.PayId, E.employeeName, CT.CommissionDesc,   1 , PC.G' +
        'ross , PC.gross , P.DatePaid, P.Paydate  , P.Paid'
      'from tblpayscommission    PC '
      
        'inner join tblcommissiontypes CT on PC.CommissionID = CT.Commiss' +
        'ionID'
      'inner join tblPays P on PC.PayID = P.PayId'
      'inner join tblemployees E on P.EmployeeId = E.EmployeeID'
      
        'Where (:filterOn = 0 and P.DatePaid between :Datefrom and :Datet' +
        'o) or (:filterOn = 1 and P.Paydate  between :Datefrom and :Datet' +
        'o) '
      'and P.deleted = '#39'F'#39
      'union all '
      
        'Select 5, P.PayId, E.employeeName, ST.SundryDesc ,   1 , PSN.Amo' +
        'unt , PSN.Amount, P.DatePaid, P.Paydate  , P.Paid'
      'from tblpayssundries    PSN '
      'inner join tblPays P on PSN.PayID = P.PayId'
      'inner join tblsundrytypes ST on PSN.SundryID = ST.SundryID'
      'inner join tblemployees E on P.EmployeeId = E.EmployeeID'
      
        'Where (:filterOn = 0 and P.DatePaid between :Datefrom and :Datet' +
        'o) or (:filterOn = 1 and P.Paydate  between :Datefrom and :Datet' +
        'o) '
      'and P.deleted = '#39'F'#39
      'union all '
      
        'Select 6, P.PayId, E.employeeName, ST.Description ,   1 , PSP.Am' +
        'ountPaid , PSP.AmountPaid, P.DatePaid, P.Paydate  , P.Paid'
      'from tblpayssuperannuation    PSP '
      'inner join tblsuperannuation SA on SA.SuperID = PSP.SuperID'
      'inner join tblsupertype ST on  ST.SuperTypeID = SA.SuperTypeID'
      'inner join tblPays P on PSP.PayID = P.PayId'
      'inner join tblemployees E on P.EmployeeId = E.EmployeeID'
      
        'Where (:filterOn = 0 and P.DatePaid between :Datefrom and :Datet' +
        'o) or (:filterOn = 1 and P.Paydate  between :Datefrom and :Datet' +
        'o) '
      'and P.deleted = '#39'F'#39
      'union all '
      
        'select Distinct 0 Category, P.PayId, e.EmployeeName , "" , NULL,' +
        ' NULL, NULL, NULL,NULL , NULL'
      'from tblPays P'
      'inner join tblemployees E on P.EmployeeId = E.EmployeeID'
      
        'Where (:filterOn = 0 and P.DatePaid between :Datefrom and :Datet' +
        'o) or (:filterOn = 1 and P.Paydate  between :Datefrom and :Datet' +
        'o) '
      'and P.deleted = '#39'F'#39
      'order by EmployeeName, payId , Category;')
  end
end
