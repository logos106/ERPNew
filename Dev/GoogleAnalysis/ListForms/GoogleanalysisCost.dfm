  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      '1 as details, '
      'GC.UId AS CostID,'
      'GA.AccountName  AS AccountName, '
      'GC.Campaign  AS Campaign, '
      'GC.Adword  AS Adword, '
      'GC.DateTime  AS DateTime, '
      'GC.Cost  AS Cost, '
      'GC.CostPerTransaction  AS CostPerTransaction, '
      'GC.costPerconversion   AS costPerconversion, '
      'GA.erpAccountName as AccountForPosting, '
      'GA.erpAccountID as AccountIDForPosting,'
      'Sum(GAcostCheque.amountPosted)  AS amountPosted, '
      'Sum(POL.totalLineAmountinc)  AS Billamount, '
      'max(GAcostCheque.PurchaseOrderId )  AS PurchaseOrderId, '
      'max(GAcostCheque.PurchaseLineID)  AS PurchaseLineID '
      'from tbltgadataforcost GC'
      'inner join tbltgacampaigns C on GC.CampaignID = C.CampaignID'
      'inner join tbltgaaccount GA on GA.AccountId = C.AccountID'
      
        'Left join tbltga_erp_Postings GAcostCheque on GC.UId = GAcostChe' +
        'que.gacostId'
      
        'Left join tblpurchaselines POL on POL.PurchaseLineID =  GAcostCh' +
        'eque.PurchaseLineID'
      'Where GC.DateTime between :Datefrom and :dateto'
      'group by GC.UId '
      'union all '
      'Select '
      '2 as details, '
      'NULL AS CostID,'
      'GA.AccountName  AS AccountName, '
      'GC.Campaign  AS Campaign, '
      'NULL AS Adword, '
      'GC.DateTime  AS DateTime, '
      'Sum(GC.Cost)  AS Cost, '
      'AVG(GC.CostPerTransaction)  AS CostPerTransaction, '
      'AVG(GC.costPerconversion)   AS costPerconversion, '
      'GA.erpAccountName as AccountForPosting, '
      'GA.erpAccountID as AccountIDForPosting,'
      'Sum(GAcostCheque.amountPosted)  AS amountPosted, '
      'Sum(POL.totalLineAmountinc)  AS Billamount, '
      'max(GAcostCheque.PurchaseOrderId )  AS PurchaseOrderId, '
      'max(GAcostCheque.PurchaseLineID)  AS PurchaseLineID '
      'from tbltgadataforcost GC'
      'inner join tbltgacampaigns C on GC.CampaignID = C.CampaignID'
      'inner join tbltgaaccount GA on GA.AccountId = C.AccountID'
      
        'Left join tbltga_erp_Postings GAcostCheque on GC.UId = GAcostChe' +
        'que.gacostId'
      
        'Left join tblpurchaselines POL on POL.PurchaseLineID =  GAcostCh' +
        'eque.PurchaseLineID'
      'Where GC.DateTime between :Datefrom and :dateto'
      'group by  GA.AccountName  , GC.Campaign  , GC.DateTime'
      'union all '
      'Select '
      '3 as details, '
      'NULL AS CostID,'
      'GA.AccountName  AS AccountName, '
      'NULL  AS Campaign, '
      'NULL  AS Adword, '
      'GC.DateTime  AS DateTime, '
      'Sum(GC.Cost)  AS Cost, '
      'AVG(GC.CostPerTransaction)  AS CostPerTransaction, '
      'AVG(GC.costPerconversion)   AS costPerconversion, '
      'GA.erpAccountName as AccountForPosting, '
      'GA.erpAccountID as AccountIDForPosting,'
      'Sum(GAcostCheque.amountPosted)  AS amountPosted, '
      'Sum(POL.totalLineAmountinc)  AS Billamount, '
      'max(GAcostCheque.PurchaseOrderId )  AS PurchaseOrderId, '
      'max(GAcostCheque.PurchaseLineID)  AS PurchaseLineID '
      'from tbltgadataforcost GC'
      'inner join tbltgacampaigns C on GC.CampaignID = C.CampaignID'
      'inner join tbltgaaccount GA on GA.AccountId = C.AccountID'
      
        'Left join tbltga_erp_Postings GAcostCheque on GC.UId = GAcostChe' +
        'que.gacostId'
      
        'Left join tblpurchaselines POL on POL.PurchaseLineID =  GAcostCh' +
        'eque.PurchaseLineID'
      'Where GC.DateTime between :Datefrom and :dateto'
      'group by GA.AccountName ,GC.DateTime'
      ''
      ''
      'Order by DateTime desc, Campaign , Adword'
      ''
      ''
      ''
      '/*Select '
      'GAcost.UId AS CostID,'
      'GAA.AccountName  AS AccountName, '
      'GAP.Title  AS Title, '
      'GAcost.Campaign  AS Campaign, '
      'GAcost.Adword  AS Adword, '
      'GAcost.DateTime  AS DateTime, '
      'GAcost.Cost  AS Cost, '
      'GAcost.CostPerTransaction  AS CostPerTransaction, '
      'GAcost.costPerconversion   AS costPerconversion, '
      'S.MediaType  AS defaultsource, '
      'S.Account as AccountIDForPosting,'
      'COA.AccountName  AS AccountForPosting, '
      'Sum(GAcostCheque.amountPosted)  AS amountPosted, '
      'Sum(POL.totalLineAmountinc)  AS Billamount, '
      'max(GAcostCheque.PurchaseOrderId )  AS PurchaseOrderId, '
      'max(GAcostCheque.PurchaseLineID)  AS PurchaseLineID '
      'from tbltgaaccount  GAA'
      'inner join tbltgaprofile GAP on GAA.uid = GAP.Ref_TGaAccount '
      'left join (tbltga_erp_source GAPs '
      'inner join tblsource S on GAPs.SourceID = S.MedTypeID'
      
        'inner join tblchartofaccounts COA on COA.AccountID = S.account) ' +
        'on GAP.UId = GAPs.ga_ProfileId and GAPs.isDefaultSource ="T"'
      
        'inner join  tbltgadataforcost GAcost on GAcost.ProfileId = GAP.P' +
        'rofileId'
      
        'Left join tbltga_erp_Postings GAcostCheque on GAcost.UId = GAcos' +
        'tCheque.gacostId'
      
        'Left join tblpurchaselines POL on POL.PurchaseLineID =  GAcostCh' +
        'eque.PurchaseLineID'
      'Where GAcost.DateTime between :Datefrom and :dateto'
      'group by GAcost.UId '
      
        'Order by GAcost.DateTime desc, GAA.AccountName , GAP.Title , GAc' +
        'ost.Campaign , GAcost.Adword'
      '*/')
    OnCalcFields = qryMainCalcFields
    ParamData = <
      item
        DataType = ftUnknown
