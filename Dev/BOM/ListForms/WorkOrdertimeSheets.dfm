  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select distinct '
      '1 as details,'
      's.SaleId, '
      's.InvoiceDocNumber as SaleDocNumber, '
      's.CustomerName as SaleCustomerName, '
      's.ShipDate as SaleShipDate, '
      'Sl.saleLineId as saleLineId,'
      'sl.ProductName as LineProductName,    '
      'sl.Product_Description as LineProductDescription, '
      'sl.UnitOfMeasureShipped as LineQty, '
      
        'concat(SL.UnitofMeasureSaleLines , '#39'('#39' , SL.UnitofMeasureMultipl' +
        'ier, '#39')'#39') as UOM, '
      'PT.proctreeId as proctreeId, '
      'PT.SequenceDown  as SequenceDown , '
      'PT.caption as caption ,'
      'Convert(TreeNodeCaption(PT.Level, PT.caption ), char(255)) as Captionint,'
      'R.HourlyLabourRate as ResourceHourlyLabourRate,'
      'PP.BundleQty as BundleQty,'
      'PT.CouponsPerBundle as CouponsPerBundle, '
      'PT.RAtePercoupon as RAtePercoupon , '
      'PT.QtyPerCoupon as QtyPerCoupon, '
      'sl.ShipDate as LineShipDate,  '
      'R.Description as REsource,'
      
        'convert(ProcessPartStatusToUserStr(PP.Status) , char(30)) as Sta' +
        'tus,  '
      'PS.Description as Processstep,'
      'NUll as TSID,'
      'NUll as TimesheetDate, '
      'sum(TS.hours) as Hours, '
      
        'convert(FormatSeconds(0, sum(TS.hours*60*60) ), char(30)) as Tim' +
        'eTaken,'
      'NUll as EmployeeName, '
      'NUll as HourlyRate, '
      'NUll as Total,'
      'NUll as ClassName'
      'FROM  tblSales s '
      
        'inner join  tblSalesLines sl on sl.SaleId = s.SaleId and (SL.BOM' +
        'GroupedLine ="F" or SL.BOMProductionLine ="T" ) '
      
        'inner join  tblProcTree pt on pt.MasterId = sl.SaleLineId    and' +
        ' pt.MasterType = "mtSalesOrder" '
      'inner join tblprocesspart PP on pt.ProcTreeId = pp.ProcTreeID '
      'inner join tblProcessStep PS on PS.ID = PP.ProcessStepId '
      
        'inner join tblbarcodexref BXR on BXR.intValue1 = Pt.proctreeId a' +
        'nd ((ifnull(BXR.intValue2,0)=0 ) or (BXR.intValue2 = PP.ID)) '
      
        'inner join tbltimesheets TS on TS.WorkOrderCouponID = concat('#39'BX' +
        'R-'#39' , BXR.ID) '
      
        'inner join tbltimesheetEntry TE on TS.TimesheetEntryID = TE.Time' +
        'sheetEntryID'
      'inner join tblemployees E on e.employeeId = TS.EmployeeID '
      
        'inner join tblprocresourceprocess PRP on PRP.ProcessStepId = PP.' +
        'ProcessStepID'
      
        'inner join tblprocresource R on R.ProcResourceId = PRP.ProcResou' +
        'rceId '
      'where S.saledate between :DateFrom and :DateTo'
      'and (TS.WorkOrderCouponID =:CouponId or :CouponId ='#39#39')'
      'Group by PT.proctreeID'
      'union all '
      'Select '
      '2 as details,'
      's.SaleId, '
      's.InvoiceDocNumber as SaleDocNumber, '
      's.CustomerName as SaleCustomerName, '
      's.ShipDate as SaleShipDate, '
      'Sl.saleLineId as saleLineId,'
      'sl.ProductName as LineProductName,    '
      'sl.Product_Description as LineProductDescription, '
      'sl.UnitOfMeasureShipped as LineQty, '
      
        'concat(SL.UnitofMeasureSaleLines , '#39'('#39' , SL.UnitofMeasureMultipl' +
        'ier, '#39')'#39') as UOM, '
      'PT.proctreeId as proctreeId, '
      'PT.SequenceDown  as SequenceDown , '
      'PT.caption as caption ,'
      'Convert(TreeNodeCaption(PT.Level, PT.caption ), char(255)) as Captionint,'
      'R.HourlyLabourRate as ResourceHourlyLabourRate,'
      'PP.BundleQty as BundleQty,'
      'PT.CouponsPerBundle as CouponsPerBundle, '
      'PT.RAtePercoupon as RAtePercoupon , '
      'PT.QtyPerCoupon as QtyPerCoupon, '
      'sl.ShipDate as LineShipDate,  '
      
        'convert(ProcessPartStatusToUserStr(PP.Status) , char(30)) as Sta' +
        'tus,  '
      'R.Description as REsource,'
      'PS.Description as Processstep,'
      'TS.ID as TSID,'
      'TS.TimesheetDate as TimesheetDate, '
      'TS.Hours as Hours, '
      
        'convert( FormatSeconds(0,(TS.hours*60*60) ), char(30)) as TimeTa' +
        'ken,'
      'TS.EmployeeName as EmployeeName, '
      'TS.HourlyRate as HourlyRate, '
      'TS.Total as Total,'
      'TS.ClassName as ClassName'
      'FROM  tblSales s '
      
        'inner join  tblSalesLines sl on sl.SaleId = s.SaleId and (SL.BOM' +
        'GroupedLine ="F" or SL.BOMProductionLine ="T" ) '
      
        'inner join  tblProcTree pt on pt.MasterId = sl.SaleLineId    and' +
        ' pt.MasterType = "mtSalesOrder" '
      'inner join tblprocesspart PP on pt.ProcTreeId = pp.ProcTreeID '
      'inner join tblProcessStep PS on PS.ID = PP.ProcessStepId '
      
        'inner join tblbarcodexref BXR on BXR.intValue1 = Pt.proctreeId a' +
        'nd ((ifnull(BXR.intValue2,0)=0 ) or (BXR.intValue2 = PP.ID)) '
      
        'inner join tbltimesheets TS on TS.WorkOrderCouponID = concat('#39'BX' +
        'R-'#39' , BXR.ID) '
      
        'inner join tbltimesheetEntry TE on TS.TimesheetEntryID = TE.Time' +
        'sheetEntryID'
      'inner join tblemployees E on e.employeeId = TS.EmployeeID '
      
        'inner join tblprocresourceprocess PRP on PRP.ProcessStepId = PP.' +
        'ProcessStepID'
      
        'inner join tblprocresource R on R.ProcResourceId = PRP.ProcResou' +
        'rceId '
      'where S.saledate between :DateFrom and :DateTo'
      'and (TS.WorkOrderCouponID =:CouponId or :CouponId ='#39#39')'
      'Order by saleID desc , SAleLineId, SequenceDown,  details')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'CouponId'
      end
      item
        DataType = ftUnknown
        Name = 'CouponId'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'CouponId'
      end
      item
        DataType = ftUnknown
        Name = 'CouponId'
      end>
