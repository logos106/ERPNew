  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select'
      'convert(PCT.ProcessTimeId, unsigned) as ProcessTimeId,'
      'PP.ID as PPid,'
      'PT.ProctreeId as ProctreeId,'
      'S.saleId as saleId,'
      'SL.saleLineId as saleLineId,'
      'S.CustomerName as CustomerName,'
      'S.SaleDate as SaleDate,'
      'SL.ShipDate as ShipDate,'
      'PT.Caption as Caption,'
      
        'replace(Convert(TreeNodeCaption(PT.Level, PT.caption ), char(255' +
        ')),"," , "\n") CaptionIntend,'
      'E.EmployeeName as EmployeeName,'
      'PR.ResourceName as ResourceName,'
      'PRD.Description as ResourceDetailName,'
      '(PCT.TimeStart) as TimeStart ,'
      'date(PCT.timeStart)as SchdDate,'
      'PT.totalqty as totalqty,'
      'PT.quantity as quantity,'
      'Pt.ManufactureUOMQty as ManufactureUOMQty,'
      'PT.FromStockUOMQty as FromStockUOMQty,'
      'PT.OnOrderUOMQty as OnOrderUOMQty,'
      
        'convert((DATE_ADD(PCT.TimeStart , INTERVAL PCT.Duration Second))' +
        ', DateTime) as TimeEnd,'
      
        'convert(FormatSecondsTotime(PCT.Duration ), char(255)) as Durati' +
        'on'
      'from tblprocesstime PCT'
      'inner join tblprocesspart PP on PCT.ProcessPartId = PP.ID'
      'inner join tblprocTree PT on PT.ProctreeId = PP.ProcTreeId'
      'inner join tblsalesLines SL on PT.masterId = SL.saleLineId'
      'inner join tblsales S on S.saleId = SL.saleID'
      
        'inner join tblprocresourcedetails PRD on PRD.ID = PCT.Resourcede' +
        'tailsID'
      
        'inner join tblprocresource PR on PRD.ProcResourceId = PR.ProcRes' +
        'ourceId'
      'inner join tblemployees E on E.EmployeeID = PCT.EmployeeId'
      'Order by '
      'EmployeeName,'
      
        'SchdDate, SaleId, SaleLineId , Sequencedown  , PP.ProcessStepSeq' +
        ';')
