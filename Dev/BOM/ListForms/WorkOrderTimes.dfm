  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      '1 as Details,'
      'S.saleId as saleId , '
      'S.sAleDate as saleDate ,'
      'SL.productname  as productname, '
      
        'concat( SL.UnitofMeasureSaleLines , '#39' ('#39' , SL.UnitofMeasureMulti' +
        'plier , '#39')'#39') as UOM, '
      'SL.UnitofMeasureShipped as Shipped, '
      
        'convert(replace(Convert(TreeNodeCaption(PT.Level, PT.caption ), ' +
        'char(255)),'#39','#39' , '#39'\n'#39') , CHAR(255)) Tree,'
      'PS.Description as ProcessStep,'
      
        'FormatSecondsTotime(PP.Duration*PT.ManufactureUOMQty) as Duratio' +
        'n,'
      
        'concat (Round(PT.ManufactureUOMQty/PT.BundleQty,2) , '#39' X '#39', PT.B' +
        'undleQty) as Bundles,'
      'PP.CouponsPerBundle as CouponsPerBundle,'
      
        'PP.CouponsPerBundle *PT.ManufactureUOMQty/PT.BundleQty as Totalc' +
        'oupons,'
      'CPONS.intValue3 as QtyPercoupon, '
      'CPONS.intValue4 as CouponNo, '
      'CPONS.intValue5 as Coupons, '
      'convert(CPONS.BarCode , char(255)) as BarCode,'
      'Null as TimesheeetEntryDate, '
      'Null  as TimesheetDate ,'
      'FormatSecondsTotime(Sum(T.Hours)*60*60) as timespent, '
      'Sum(T.Hours)*60*60 as timespentinsec, '
      'Null as EmployeeName,'
      'PR.HourlyLabourRate as HourlyLabourRate ,'
      'PR.HourlyOverheadRate as HourlyOverheadRate'
      'from tblsales S '
      'inner join tblsaleslines SL  on S.saleId = SL.saleId'
      
        'inner join tblproctree PT on Sl.SaleLineId = PT.MasterId and PT.' +
        'masterType <> '#39'mtProduct'#39
      
        'inner join tblBarcodexRef CPONS on PT.ProcTreeId =CPONS.intValue' +
        '1'
      
        'inner join tbltimesheets T on  (CPONS.barcode = T.WorkOrderCoupo' +
        'nID  or concat('#39'BXR-'#39' , CPONS.ID) = T.WorkOrderCouponID ) and CP' +
        'ONS.barcodetype ='#39'btWorkOrder'#39
      
        'inner join tbltimesheetentry TE on TE.TimesheetEntryID = T.Times' +
        'heetEntryID'
      
        'inner join tblprocesspart  PP on PP.proctreeId = PT.proctreeid a' +
        'nd (ifnull(CPONS.intValue2,0)=0 or  PP.ID =CPONS.intValue2)'
      'inner join tblprocessstep PS on PP.ProcessStepID = PS.ID'
      
        'inner join tblprocresourceprocess PRPS on PS.ID = PRPS.ProcessSt' +
        'epId'
      
        'inner join tblprocresource PR on PRPS.ProcResourceId = PR.ProcRe' +
        'sourceId'
      'Where T.TimesheetDate between :DAteFrom and :DateTo'
      'Group by saleId, sl.SaleLineId, CPONS.ID'
      'union all '
      'Select '
      '2 as Details,'
      'S.saleId as saleId , '
      'S.sAleDate as saleDate ,'
      'SL.productname  as productname, '
      
        'concat( SL.UnitofMeasureSaleLines , '#39' ('#39' , SL.UnitofMeasureMulti' +
        'plier , '#39')'#39') as UOM, '
      'SL.UnitofMeasureShipped as Shipped, '
      
        'convert(replace(Convert(TreeNodeCaption(PT.Level, PT.caption ), ' +
        'char(255)),'#39','#39' , '#39'\n'#39') , CHAR(255)) Tree,'
      'PS.Description as ProcessStep,'
      
        'FormatSecondsTotime(PP.Duration*PT.ManufactureUOMQty) as Duratio' +
        'n,'
      
        'concat (Round(PT.ManufactureUOMQty/PT.BundleQty,2) , '#39' X '#39', PT.B' +
        'undleQty) as Bundles,'
      'PP.CouponsPerBundle as CouponsPerBundle,'
      
        'PP.CouponsPerBundle *PT.ManufactureUOMQty/PT.BundleQty as Totalc' +
        'oupons,'
      'CPONS.intValue3 as QtyPercoupon, '
      'CPONS.intValue4 as CouponNo, '
      'CPONS.intValue5 as Coupons, '
      'convert(CPONS.BarCode , char(255)) as BarCode,'
      'TE.EntryDate as TimesheeetEntryDate, '
      'T.TimesheetDate  as TimesheetDate ,'
      'FormatSecondsTotime(Sum(T.Hours)*60*60) as timespent, '
      'Sum(T.Hours)*60*60 as timespentinsec, '
      'T.EmployeeName as EmployeeName ,'
      'PR.HourlyLabourRate as HourlyLabourRate ,'
      'PR.HourlyOverheadRate as HourlyOverheadRate'
      'from tblsales S '
      'inner join tblsaleslines SL  on S.saleId = SL.saleId'
      
        'inner join tblproctree PT on Sl.SaleLineId = PT.MasterId and PT.' +
        'masterType <> '#39'mtProduct'#39
      
        'inner join tblBarcodexRef CPONS on PT.ProcTreeId =CPONS.intValue' +
        '1'
      
        'inner join tbltimesheets T on  (CPONS.barcode = T.WorkOrderCoupo' +
        'nID  or concat('#39'BXR-'#39' , CPONS.ID) = T.WorkOrderCouponID ) and CP' +
        'ONS.barcodetype ='#39'btWorkOrder'#39
      
        'inner join tbltimesheetentry TE on TE.TimesheetEntryID = T.Times' +
        'heetEntryID'
      
