      'PP.CouponsPerBundle as CouponsPerBundle,'
      
        'PP.CouponsPerBundle *PT.ManufactureUOMQty/PT.BundleQty as Totalc' +
        'oupons,'
      'CPONS.intValue3 as QtyPercoupon, '
      'CPONS.intValue4 as CouponNo, '
      'CPONS.intValue5 as Coupons, '
      'convert(CPONS.BarCode , char(255)) as BarCode,'
      'TE.EntryDate as TimesheeetEntryDate, '
      'T.TimesheetDate  as TimesheetDate ,'
      'FormatSecondsTotime(T.Hours*60*60) as timespent, '
      'T.Hours*60*60 as timespentinsec, '
      'T.EmployeeName as EmployeeName,'
      ' PR.HourlyLabourRate as HourlyLabourRate ,'
