  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select  '
      '2 as catg,'
      'T.ID,'
      'T.TaskNo as TaskNo,'
      'T.Subject,'
      'if(ifnull(Versionno,'#39#39')='#39#39' , '#39'NA'#39' , Versionno) as Versionno,'
      'T.Details  , '
      'T.erphelpdocDesc as erphelpdocDesc,'
      'if(ifnull(UT.ID,0) =0,"F" , "T") as HasSeen,'
      'T.msTimeStamp as createdon,'
      'T.ClientCode as ClientCode,'
      'T.EstimatedHrs as EstimatedHrs,'
      'T.Seqno as Seqno,'
      'T.PercentageDone'
      'From erpdocumentaion.tbltasks T  '
      
        'left join tbluserviewedtasks UT on T.ID = UT.TasksID and USerId ' +
        '= :UserID'
      
        'where t.active ="T" and   (t.Tasktype = :Tasktype or T.Tasktype ' +
        '=:Tasktype2) and  T.msTimeStamp between :DAtefrom and :DateTo'
      'and ifnull(Versionno,'#39#39')<>'#39#39
      'union all '
      'Select distinct '
      '1 as catg, '
      'null as id,'
      'Null as TaskNo,'
      '" " as Subject, '
      'if(ifnull(Versionno,'#39#39')='#39#39' , '#39'NA'#39' , Versionno) as Versionno,'
      'null as Details,'
      'null as erphelpdocDesc,'
      'null as HasSeen,'
      'null as createdon,'
      'NULL as ClientCode,'
      'NULL as EstimatedHrs,'
      'NULL Seqno,'
      'NULL as PercentageDone'
      'From erpdocumentaion.tbltasks T  '
      
        'where  (t.Tasktype = :Tasktype or T.Tasktype =:Tasktype2) and  T' +
        '.msTimeStamp between :DAtefrom and :DateTo'
      'and ifnull(Versionno,'#39#39')<>'#39#39
      'Order by VersionToNo(Versionno)  desc, catg , TaskNo,seqno')
    object qryMainerphelpdocDesc: TWideStringField [6]
      DisplayWidth = 255
      FieldName = 'erphelpdocDesc'
      Size = 255
    end
