  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select  '
      'tblEmployees.GlobalRef, '
      'EmployeeID, '
      
        'if(MiddleName<>'#39#39',Concat(FirstName,'#39' '#39',MiddleName,'#39' '#39',LastName),' +
        'Concat(FirstName,'#39' '#39',LastName)) AS '#39'Employee Name'#39','
      
        'if(Street2<>'#39#39',Concat(Street,'#39' '#39',if(Street3<>'#39#39',Concat(Street2,'#39 +
        ' '#39',Street3),Street2)),Street) AS Address,'
      'Suburb, '
      'State, '
      'Postcode, '
      'Country,'
      'Phone AS Phone, '
      'Active,  '
      'Mobile, '
      'FaxNumber AS '#39'Fax Number'#39
      'FROM tblEmployees '
      'WHERE tblEmployees.Rep = '#39'T'#39
      'ORDER By FirstName')
