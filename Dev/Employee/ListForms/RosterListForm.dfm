  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      '0 as detail,'
      'T.GlobalRef as GlobalRef ,'
      'T.Date as RosterDate ,'
      
        '   Sum(if(ClockedOff="F" , 0 , ((TIME_TO_SEC(ENDTIME ) - TIME_TO' +
        '_SEC(STARTTIME ))/60/60))) as Hours,'
      
        '   FormatSecondsTotime(Sum(if(ClockedOff="F" , 0 , ((TIME_TO_SEC' +
        '(ENDTIME ) - TIME_TO_SEC(STARTTIME )))))) as fHours,'
      'RosterID as RosterID,'
      'EmployeeID as EmployeeID,'
      'EmployeeName as EmployeeName,'
      'ClassName  as ClassName,'
      
        'DATE_ADD(DATE_ADD(StartTime,INTERVAL 1 SECOND),INTERVAL -1 SECON' +
        'D) as StartTime,'
      
        'DATE_ADD(DATE_ADD(EndTime,INTERVAL 1 SECOND),INTERVAL -1 SECOND)' +
        ' as EndTime,'
      'ClockedOff as ClockedOff,'
      'StartedLate as StartedLate,'
      'FinishedEarly as FinishedEarly,'
      'NULL as RosterBreaktype,'
      'NULL  as BreakStartTime ,'
      'NULL as BreakEndtime'
      'FROM  tmp_tblRoster_Admin_3C1163BA  T'
      'GROUP BY EmployeeID , T.Date'
      ' Limit 0 UNION ALL '
      'SELECT'
      '1 as detail,'
      'T.GlobalRef as GlobalRef,'
      'T.Date as RosterDate ,'
      
        '   if(ClockedOff="F" , 0 , ((TIME_TO_SEC(ENDTIME ) - TIME_TO_SEC' +
        '(STARTTIME ))/60/60)) as Hours,'
      
        '   FormatSecondsTotime(if(ClockedOff="F" , 0 , ((TIME_TO_SEC(END' +
        'TIME ) - TIME_TO_SEC(STARTTIME ))))) as fHours,'
      'RosterID as RosterID,'
      'EmployeeID as EmployeeID,'
      'EmployeeName as EmployeeName,'
      'ClassName as ClassName,'
      
        'DATE_ADD(DATE_ADD(StartTime,INTERVAL 1 SECOND),INTERVAL -1 SECON' +
        'D) as StartTime,'
      
        'DATE_ADD(DATE_ADD(EndTime,INTERVAL 1 SECOND),INTERVAL -1 SECOND)' +
        ' as EndTime,'
      'ClockedOff as ClockedOff,'
      'StartedLate as StartedLate,'
      'FinishedEarly as FinishedEarly,'
      'NULL as RosterBreaktype,'
      'NULL  as BreakStartTime ,'
      'NULL as BreakEndtime'
      'FROM  tmp_tblRoster_Admin_3C1163BA  T'
      ' Limit 0 UNION ALL '
      'SELECT'
      '2 as detail,'
      'T.GlobalRef as GlobalRef,'
      'T.Date as RosterDate ,'
      
        '   if(ClockedOff="F" , 0 , ((TIME_TO_SEC(T.ENDTIME ) - TIME_TO_S' +
        'EC(T.STARTTIME ))/60/60)) as Hours,'
      
        '   FormatSecondsTotime(if(ClockedOff="F" , 0 , ((TIME_TO_SEC(T.E' +
        'NDTIME ) - TIME_TO_SEC(T.STARTTIME ))))) as fHours,'
      'T.RosterID as RosterID,'
      'T.EmployeeID as EmployeeID,'
      'T.EmployeeName as EmployeeName ,'
      'T.ClassName as ClassName,'
      
        'DATE_ADD(DATE_ADD(T.StartTime,INTERVAL 1 SECOND),INTERVAL -1 SEC' +
        'OND) as StartTime,'
      
        'DATE_ADD(DATE_ADD(T.EndTime,INTERVAL 1 SECOND),INTERVAL -1 SECON' +
        'D) as EndTime,'
      'T.ClockedOff as ClockedOff,'
      'T.StartedLate as StartedLate,'
      'T.FinishedEarly as FinishedEarly,'
      'RB.RosterBreaktype as RosterBreaktype,'
      'RB.Starttime as BreakStartTime ,'
      'RB.Endtime as BreakEndtime'
      'FROM  tmp_tblRoster_Admin_3C1163BA  T'
      'inner join tblrosterbreak RB  on RB.RosterID = T.RosterID'
      
        'ORDER BY  EmployeeName,Rosterdate desc , Detail,  StartTime  DES' +
        'C '
      ' Limit 0;')
    OnCalcFields = qryMainCalcFields
