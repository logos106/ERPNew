      'SELECT SPACE(30) as EmployeeName,'
      
        'If(tblSales.IsCashSale='#39'T'#39' And tblSales.IsPOS='#39'F'#39' ,'#39'Cash Sale'#39',I' +
        'f(tblSales.IsRefund='#39'T'#39','#39'Refund'#39', If(tblSales.IsCashSale='#39'T'#39' And' +
        ' tblSales.IsPOS='#39'T'#39','#39'POS'#39', If(tblSales.IsInvoice='#39'T'#39','#39'Invoice'#39','#39 +
        'Unknown'#39')))) as Type,'
      'TRIM(tblSales.EmployeeName) as Name,'
      'NULL as SaleDate,'
      'NULL as DueDate,'
      'Space(50) as '#39'Invoice Number'#39','
      'Sum(TotalLineAmount) as '#39'Total Amount (Ex)'#39','
      'Sum(TotalLineAmountInc) as '#39'Total Amount (Inc)'#39','
      '0 as SaleID,'
      'tblSales.EmployeeID as EmployeeID,'
      'tblSales.CustomerName as CustomerName,'
      '1 as Details,'
      #39'F'#39' as Paid,'
      'Sum(Shipped*LineCostInc) as LineCostInc,'
      'Sum(Shipped*LineCost) as LineCost,'
      'CONCAT(E.FirstName, " ", E.LastName) as '#39'CommissionRep'#39','
      #39'F'#39' as CommissionPaid,'
      'Sum(tblSalesCommission.CommissionAmount) as '#39'CommissionDue'#39
      'FROM tblSales'
      'INNER  JOIN tblSalesLines USING(SaleID)'
      
        'LEFT JOIN tblSalesCommission ON tblSales.SaleId = tblSalesCommis' +
        'sion.SaleId'
      
        'LEFT JOIN tblEmployees E ON tblSalesCommission.EmployeeID = E.Em' +
        'ployeeID'
      
        'WHERE (tblSales.IsPOS ='#39'T'#39' or tblSales.IsRefund ='#39'T'#39' or tblSales' +
        '.IsCashSale='#39'T'#39' or tblSales.IsInvoice='#39'T'#39')'
      
        'AND tblSalesLines.Invoiced='#39'T'#39' AND tblSales.IsLayby <>'#39'T'#39' AND tb' +
        'lSales.Deleted ='#39'F'#39
      'AND tblSales.IsSalesOrder='#39'F'#39' And tblSales.IsQuote='#39'F'#39
      'AND tblSales.SaleDate Between :txtfrom  AND :txtto'
      
        'AND ((:CommPaid = "Paid" AND CommissionPaid = "T") OR (:CommPaid' +
