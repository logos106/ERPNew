  inherited qryMain: TERPQuery
    SQL.Strings = (
      '# NOTE: this SQL is overwritten in code !!!!!'
      'SELECT'
      'ToDoID, '
      'C.GlobalRef, '
      'ToDoByDate, '
      'Description, '
      'Result, '
      'E.EmployeeID,'
      'E.EmployeeName,'
      'C.EmployeeName as "CreatedBy", '
      'CreatedDate, '
      'Done, '
      'T.Completed, '
      'T.Active, '
      'T.Type, '
      'T.OtherID,'
      'T.todoPriority,'
      'DateDiff(curdate(), T.CreatedDate) Days,'
      
        'if(ifnull(T.ToDoByDate,0)=0 or T.ToDoByDate > curdate() , 0 , Da' +
        'teDiff(curdate(), T.ToDoByDate)) OverdueDays,'
      
        'if(ifnull(T.ToDoByDate,0)=0 or T.ToDoByDate < curdate() , 0 , Da' +
        'teDiff(T.ToDoByDate,curdate())) DaysToDue,'
      
        'DateDiff(ifnull(T.ToDoByDate,curdate()), T.CreatedDate) DaysAllo' +
        'cated,'
      'T.LinkTo,'
      'T.LinkToID'
      'FROM tblToDo AS T'
      
        'LEFT JOIN tblEmployees AS C ON C.EmployeeID = T.CreatedByEmploye' +
        'eID'
      'LEFT JOIN tblEmployees AS E ON E.EmployeeID = T.EmployeeID'
      'where  (T.ToDoByDate between "2012-01-17" and "2013-01-21"'
      'and T.EmployeeID = 1'
      'and Completed = "F"'
      ')'
      ' or ('
      'E.ReportsTo = 1'
      'and Now() > T.ToDoByDate and Completed = "F"'
      ')'
      'order by ToDoByDate DESC')
    Left = 388
    Top = 272
