  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      
        'If(s.IsCashSale="T" And s.IsPOS="F" ,"Cash Sale",If(s.IsRefund="' +
        'T","Refund",If(s.IsCashSale="T" And s.IsPOS="T","POS",If(S.IsInv' +
        'oice="T","Invoice","Unknown")))) as TransType,'
      '1 as Details, '
      '0 as SalesCommissionId, '
      'SC.SaleID as SaleID, '
      'S.ClientPrintName as CustomerName,'
      'SC.employeeId as EmployeeId, '
      '0 as ProductID, '
      'S.ClassId as ClassId, '
      'S.class as Class, '
      '`SC`.`IsCommissiononpaidInvoice` as IsCommissiononpaidInvoice,'
      '`SC`.`Commissioninvoiceexprice` as Commissioninvoiceexprice,'
      '`SC`.`CommissionOn` as CommissionOn,'
      '`SC`.`Overheadbaserate` as Overheadbaserate,'
      'SC.employeeName as EmployeeName, '
      'space(100) as name, '
      'space(100) as Category, '
      '0 as CommissionRate,'
      '"F" as IsCommissionPercent,'
      
        'Sum(Round((ifnull(SC.CommissionAmount,0)),2))  as Commissionamt ' +
        ', '
      
        '/*Round(Sum(ifnull(`PCL`.CommissionAmount,0)),2) */ (Select Roun' +
        'd(Sum(ifnull(`PCL`.CommissionAmount,0)),2)  from tblpayscommissi' +
        'onlines PCL where  PCL.SalescommissionId = SC.SalesCommissionId ' +
        ')  as AmountPaid,'
      
        '(SELECT Sum(if(ifnull(SL.QtySold,0) <> 0 and ( ifnull(SL.LineCos' +
        'tInc,0)<> 0 or ifnull(SL.LinePriceInc,0)<>0)  , if(ifnull(SL.Lin' +
        'ePriceInc,0) = 0 , 0-(SL.shipped*ifnull(SL.LineCostInc,0)) ,  if' +
        '(ifnull(SL.LineCostInc,0) = 0 ,(SL.shipped*ifnull(SL.LinePriceIn' +
        'c,0)),  Round((SL.LinePriceInc - LineCostInc)*SL.shipped, 2)    ' +
        ' )   )  ,0))  FROM tblsaleslines AS SL where SL.SaleID =  S.Sale' +
        'ID) as GrossProfit,'
      
        '(SELECT (if(ifnull(SL.QtySold,0) <> 0 and ( ifnull(SL.LineCostIn' +
        'c,0)<> 0 or ifnull(SL.LinePriceInc,0)<>0)  , if(ifnull(SL.LinePr' +
        'iceInc,0) = 0 , 0-100 ,  if(ifnull(SL.LineCostInc,0) = 0 ,100,  ' +
        'Round((sum(SL.LinePriceInc - LineCostInc)/sum(SL.LinePriceInc))*' +
        '100, 2)     )   )  ,0))  FROM tblsaleslines AS SL where SL.SaleI' +
        'D =  S.SaleID) as GrossProfitPercent,'
      '0.0 as TotalCost, 0.0 as totalPrice,'
      'S.SaleDate as SaleDate'
      
        'FROM  `tblsalescommission` AS `SC` inner join tblSales S on S.Sa' +
        'leId = SC.SaleId  '
      
        '/*Left join tblpayscommissionlines PCL on PCL.SalescommissionId ' +
        '= SC.SalesCommissionId */'
      'where SC.Deleted = "F"  /*EmployeeIDfilter*/ /*ClassIDFilter*/'
      'group by SC.SaleID'
      'Order by SaleId, EmployeeId, Category')
    Filtered = True
