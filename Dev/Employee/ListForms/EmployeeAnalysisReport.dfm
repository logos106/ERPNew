  inherited qryMain: TERPQuery
    SQL.Strings = (
      '# DUMMY SQL - See RefreshQuery()'
      'SELECT'
      
        'E.GlobalRef, C.Company, C.Jobname, T.EmployeeID, T.EmployeeName,' +
        ' E.Initials,'
      'IF(T.JobID > 0, T.Hours, 0.00) AS ChargeableHours,'
      'IF(T.JobID > 0, T.Hours * PES.Rate, 0.00) As ChargedValue,'
      'IF(T.JobID = 0, T.Hours, 0.00) AS NonChargeableHours,'
      '0.00 AS RecoveryRate, T.ClassID, T.ClassName,'
      'C.HoursTakenForJob AS ChargedTime'
      'From tblclients AS C'
      'inner join  tbltimesheets AS T on T.JobId = C.clientID'
      
        'LEFT JOIN tblpaysemployeeservices AS PES on (PES.ID = T.ServiceI' +
        'D)'
      'inner  JOIN tblemployees AS E on E.EmployeeID = T.EmployeeID')
    Options.StrictUpdate = False
    Left = 799
    Top = 297
