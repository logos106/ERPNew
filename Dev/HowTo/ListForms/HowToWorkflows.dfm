  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'WF.Id as ID,'
      'WF.Description as Description,'
      'WF.Active as Active , '
      'WF.IsERpWorkflow as IsERpWorkflow , '
      'WF.LanguageID as  LanguageID,'
      'WF.TabGroup as TabGroup, '
      'WF.Category as  Category,'
      'WF.SkinsGroup as SkinsGroup,'
      'WF.CompanyName as CompanyName,'
      'E.EmployeeName as CreatedBy, '
      'L.Language as Language,'
      'WF.VideoName as VideoName'
      'from tblworkflow WF'
      'Left join tblemployees E on WF.createdby = E.EmployeeID'
      'Left join erplanguages.languages L on L.ID  = WF.LanguageID'
      
        'Where (WF.LanguageID = :LanguageID or :LanguageID =0 or WF.Langu' +
        'ageID  =0)'
      'order by SkinsGroup, Description')
    Top = 376
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'LanguageID'
      end
      item
        DataType = ftUnknown
        Name = 'LanguageID'
      end>
