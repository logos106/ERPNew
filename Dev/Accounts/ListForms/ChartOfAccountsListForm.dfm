  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT distinct '
      'ifnull(tblbudgetPeriodLines.AccountId,0) as BudgetAccountno, '
      'tblChartofAccounts.AccountNumber as AccountNo,'
      'AccDesc AS Type,'
      
        'SUBSTRING(If((char_length(tblChartofAccounts.AccountGroup)>0),Co' +
        'ncat(REPLACE(tblChartofAccounts.AccountGroup,"^"," - ")," - ",tb' +
        'lChartofAccounts.AccountName),tblChartofAccounts.AccountName),1,' +
        '255) as '#39'FullAccountName'#39','
      'tblChartofAccounts.Description as '#39'Description'#39','
      'tblChartofAccounts.Balance as '#39'Balance'#39','
      'tblChartofAccounts.Taxcode as TaxCode,'
      'tblTaxcodes.Description as TaxCodeDescription,'
      'tblChartofAccounts.BSB  AS BSB,'
      'tblChartofAccounts.BankAccountNumber as BankAccNo,'
      'tblChartofAccounts.BankAccountName as BankAccountName,'
      'tblChartofAccounts.BankNumber as '#39'Bank Number'#39','
      'tblChartofAccounts.AccountName as '#39'AccountName'#39','
      
        'SUBSTRING(If((char_length(tblChartofAccounts.AccountGroup)>0),Re' +
        'place(tblChartofAccounts.AccountGroup,"^"," - "),""),1,255) as '#39 +
        'AccountGroup'#39','
      
        'Concat(If( char_length(Level4)>0,Space(15), If( char_length(Leve' +
        'l3)>0,Space(10),If( char_length(Level2)>0,Space(5),Space(0)))),A' +
        'ccountName) as '#39'AccountTree'#39','
      'tblChartofAccounts.Active as Active,'
      'tblTaxcodes.TaxCodeID as TaxCodeID,'
      'Level1 as Level1,'
      'Level2 as Level2,'
      'Level3 as Level3,'
      'Level4 as Level4,'
      'tblChartofAccounts.AccountID as AccountID,'
      'AllowExpenseClaim as AllowExpenseClaim,'
      'SortOrder as SortOrder'
      'FROM tblChartofAccounts'
      
        'LEFT JOIN tblTaxcodes  ON tblTaxcodes.Name=tblChartofAccounts.Ta' +
        'xcode AND tblTaxcodes.RegionID= :xRegionID '
      
        'LEFT JOIN tblacctypedesc ON tblacctypedesc.AccType=tblChartofAcc' +
        'ounts.AccountType'
      
        'Left join tblbudgetPeriodLines on tblbudgetPeriodLines.AccountId' +
        ' =  tblChartofAccounts.AccountID and tblbudgetPeriodLines.Active' +
        ' <> '#39'F'#39' '
      
        'Where ((IsHeader <> "T") and (:ExcludeHeaderAccounts = "T")) or ' +
        '(IsNull(:ExcludeHeaderAccounts) or (:ExcludeHeaderAccounts = "F"' +
        '))'
      'ORDER BY  SortOrder,FullAccountName')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'xRegionID'
      end
      item
        DataType = ftUnknown
        Name = 'ExcludeHeaderAccounts'
      end
      item
        DataType = ftUnknown
        Name = 'ExcludeHeaderAccounts'
      end
      item
        DataType = ftUnknown
        Name = 'ExcludeHeaderAccounts'
      end>
