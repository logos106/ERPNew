  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'tblTerms.GlobalRef as GlobalRef,'
      'tblTerms.TermsID  ,'
      'tblTerms.Terms as Terms, '
      'tblTerms.Active as Active, '
      'tblTerms.Description as Description, '
      'tblTerms.Days as Days, '
      'if(tblTerms.Days="T" , tblTerms.TermsAmount,0) as DaysAmount, '
      'tblTerms.EOM as EOM, '
      'if(tblTerms.EOM="T" , tblTerms.TermsAmount,0) as EOMDays, '
      'tblTerms.EOMPlus as EOMPlus, '
      
        'if(tblTerms.EOMPlus="T" , tblTerms.TermsAmount,0) as EOMPlusDays' +
        ', '
      'tblTerms.PP as PP, '
      
        'if(tblTerms.PP="T" , tblTerms.ProgressPaymentInstallments,0) as ' +
        'Installments,'
      
        'if(tblTerms.PP ="F" , "" , concat(ifnull(ProgressPaymentDuration' +
        ','#39#39') , '#39' '#39' , if(ifnull(ProgressPaymenttype,'#39#39')='#39'D'#39' , '#39'Days'#39' , if' +
        '(ifnull(ProgressPaymenttype,'#39#39')='#39'W'#39' , '#39'Weeks'#39' , if(ifnull(Progre' +
        'ssPaymenttype,'#39#39')='#39'M'#39' , '#39'Months'#39','#39#39'))) )) as PPDEsc,'
      
        'if(tblTerms.PP="T" , tblTerms.TermsAmount,0) as ProgressPaymentD' +
        'aysAfterfirstPay'
      'FROM tblTerms')
    object qryMainTerms: TWideStringField
      DisplayWidth = 20
      FieldName = 'Terms'
      Origin = 'tblterms.Terms'
      Size = 50
    end
