  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '#39'Dec(2011)'#39' as period,'
      
        '(Select sum(TotalAmountEx) from  tmp_profitandlossreportperiod_F' +
        'B9B95BA where Date Between '#39'2011-12-01 00:00:01'#39' AND  '#39'2011-12-0' +
        '1 00:00:01'#39' and AccountType ="EXP") as Exp ,'
      
        '(Select sum(TotalAmountEx) from  tmp_profitandlossreportperiod_F' +
        'B9B95BA where Date Between '#39'2011-12-01 00:00:01'#39' AND  '#39'2011-12-0' +
        '1 00:00:01'#39' and AccountType ="Inc") as Inc,'
      
        '(Select sum(TotalAmountEx) from  tmp_profitandlossreportperiod_F' +
        'B9B95BA where Date Between '#39'2011-12-01 00:00:01'#39' AND  '#39'2011-12-0' +
        '1 00:00:01'#39' and AccountType ="COGS") as Cogs,'
      
        '(Select sum(TotalAmountEx) from  tmp_profitandlossreportperiod_F' +
        'B9B95BA where Date Between '#39'2011-12-01 00:00:01'#39' AND  '#39'2011-12-0' +
        '1 00:00:01'#39' and AccountType ="EXP") as PrevyrExp ,'
      
        '(Select sum(TotalAmountEx) from  tmp_profitandlossreportperiod_F' +
        'B9B95BA where Date Between '#39'2011-12-01 00:00:01'#39' AND  '#39'2011-12-0' +
        '1 00:00:01'#39' and AccountType ="Inc") as PrevyrInc,'
      
        '(Select sum(TotalAmountEx) from  tmp_profitandlossreportperiod_F' +
        'B9B95BA where Date Between '#39'2011-12-01 00:00:01'#39' AND  '#39'2011-12-0' +
        '1 00:00:01'#39' and AccountType ="COGS") as PrevyrCogs')
