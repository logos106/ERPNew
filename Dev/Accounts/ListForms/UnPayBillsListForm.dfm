  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'space(50) as SupplierName, '
      'Type, '
      'OrderDate,'
      'InvoiceDate, '
      'DueDate, '
      'PONumber, '
      'Name, '
      'Round(OriginalAmount,2) as OriginalAmount,  '
      'Round(AmountDue,2) as AmountDue, '
      'PurchaseOrderID, '
      '2 as Details, '
      'ClientID,  '
      'InvoiceNumber'
      'FROM tmp_tblaccountspayable'
      'Where AmountDue<>0.00'
      
        'AND ((:IncludeType = "All") OR (:IncludeType = "BillCredit" AND ' +
        '((Type = "Bill") or (Type = "Credit"))))'
      
        'AND ((:txtfrom1 = 0 and :txtto1 = 0) or (InvoiceDate Between  :t' +
        'xtfrom1  AND  :txtto1))'
      'Group By ClientID,PurchaseOrderID'
      ''
      'UNION ALL'
      ''
      'SELECT '
