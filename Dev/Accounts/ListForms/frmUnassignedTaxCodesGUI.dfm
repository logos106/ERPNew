  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT tblSales.SaleDate as '#39'Date'#39','
      'tblSales.SaleID as SaleID,'
      '0 as PurchaseOrderID,'
      
        'If(tblSales.IsCashSale='#39'T'#39' And tblSales.IsPOS='#39'F'#39' ,'#39'Cash Sale'#39', ' +
        'If(tblSales.IsRefund='#39'T'#39','#39'Refund'#39',If(tblSales.IsCashSale='#39'T'#39' And' +
        ' tblSales.IsPOS='#39'T'#39','#39'POS'#39',If(tblSales.IsInvoice='#39'T'#39','#39'Invoice'#39','#39'U' +
        'nknown'#39')))) as Type,'
      
        'CONCAT_WS(" ", If(InStr(tblClients.Company,'#39'^'#39') -1 >0,Left(tblCl' +
        'ients.Company,InStr(tblClients.Company,'#39'^'#39')-1),tblClients.Compan' +
        'y),IF(char_length(tblClients.JobName)>0, Concat("(",tblClients.J' +
        'obName,")"),'#39#39')) as '#39'Customer Name'#39','
      'tblSales.ClientID,'
      '"" as Company,'
      'JobName as JobName,'
      'JobNumber as JobNumber,'
      'tblSales.GlobalRef,'
      '0 as "P/O Num",'
      '0 as ETADate,'
      'tblSales.InvoiceDocNumber as '#39'Invoice Number'#39','
      'tblSales.Deleted as Deleted,'
      'tblSales.PONumber as '#39'PO Number'#39','
      '0 as  InvoiceDate,'
      '0 as CustPONumber,'
      'tblEmployees.EmployeeName AS '#39'Employee Name'#39','
      'tblSales.Class as '#39'Class Name'#39','
      'tblSales.Terms as Terms,'
      'tblSales.HoldSale as Held,'
      'tblSales.ShipDate as '#39'Ship Date'#39','
      'tblSales.Paid as '#39'Paid'#39','
      '0 as ClassID,'
      '0 as GJDID,'
      'tblSalesLines.LineTaxCode as TaxCode'
      ''
      'FROM tblSales'
      'INNER JOIN tblSalesLines Using(SaleID)'
      'INNER JOIN tblClients ON tblSales.ClientID = tblClients.ClientID'
      'LEFT JOIN tblclienttype Using(ClientTypeID)'
      
        'LEFT JOIN tblEmployees ON (tblSales.EmployeeID = tblEmployees.Em' +
        'ployeeID)'
      'WHERE '
      
        'tblSales.IsLayby = '#39'F'#39' AND tblSales.IsSalesOrder = '#39'F'#39' And tblSa' +
        'les.IsQuote = '#39'F'#39' AND'
      
        '(tblSales.IsPOS = '#39'T'#39' or tblSales.IsRefund = '#39'T'#39' or tblSales.IsC' +
        'ashSale = '#39'T'#39' or tblSales.IsInvoice = '#39'T'#39')'
      'AND tblSalesLines.Invoiced = '#39'T'#39
      'AND tblSalesLines.LineTaxCode = ""'
      'AND tblSales.SaleDate between :txtFrom and :txtTo'
      'AND tblSales.Class LIKE :xClassName'
      'Group By tblSales.SaleID'
      ''
      'UNION ALL'
      ''
      'SELECT'
      '#PURCHASE ORDERS'
      'tblPurchaseOrders.OrderDate as Date,'
      '0 as SaleID,'
      'tblPurchaseOrders.PurchaseOrderID as PurchaseOrderID,'
      
        'If(tblPurchaseOrders.IsPO='#39'T'#39','#39'Purchase Order'#39', If(tblPurchaseOr' +
        'ders.IsPOCredit='#39'T'#39','#39'PO Credit'#39','#39#39')) as Type,'
      '""   as '#39'Customer Name'#39','
      'tblClients.ClientID as ClientID,'
      'tblClients.Company as Company,'
      '"" as JobName,'
      '0 as JobNumber,'
      'tblPurchaseOrders.GlobalRef as GlobalRef,'
      
        'if (original.PurchaseOrderId = tblPurchaseOrders.PurchaseOrderId' +
        ' , if (ifnull(tblPurchaseOrders.PurchaseOrderNumber ,'#39'-'#39') = '#39'-'#39' ' +
        ', tblPurchaseOrders.PurchaseOrderId , tblPurchaseOrders.Purchase' +
        'OrderNumber), concat_ws('#39#39',tblPurchaseOrders.PurchaseOrderNumber' +
        ', '#39'('#39' , original.PurchaseOrderNumber , '#39'_'#39' , tblPurchaseOrders.S' +
        'eqNo, '#39')'#39'))  as "P/O Num",'
      'tblPurchaseOrders.ETADate as ETADate,'
      'tblPurchaseOrders.InvoiceNumber as InvoiceNumber,'
      'tblPurchaseOrders.Deleted as Deleted,'
      '0 as '#39'PO Number'#39','
      'tblPurchaseOrders.InvoiceDate as InvoiceDate,'
      'tblPurchaseOrders.CustPONumber,'
      '"" as '#39'Employee Name'#39','
      'tblPurchaseLines.Class as '#39'Class Name'#39','
      '"" as Terms,'
      '"" as Held,'
      '0 as '#39'Ship Date'#39','
      '"" as '#39'Paid'#39','
      '0 as ClassID,'
      '0 as GJDID,'
      'tblPurchaseLines.LineTaxCode as TaxCode'
      ''
      'FROM tblPurchaseOrders'
      
        'LEFT JOIN tblPurchaseLines ON  (tblPurchaseOrders.PurchaseOrderI' +
        'D = tblPurchaseLines.PurchaseOrderID)'
      
        'LEFT JOIN tblClients ON  (tblPurchaseOrders.SupplierName = tblCl' +
        'ients.Company)'
      
        'LEFT JOIN tblParts ON  (tblPurchaseLines.ProductName = tblParts.' +
        'PARTName)'
      
        'LEFT Join tblPurchaseOrders as original  on original.GlobalRef =' +
        '  tblPurchaseOrders.OriginalNo'
      'Where tblPurchaseLines.Invoiced = '#39'T'#39' and'
      '(tblPurchaseOrders.IsPO='#39'T'#39' OR tblPurchaseOrders.IsPOCredit='#39'T'#39')'
      'AND tblPurchaseLines.LineTaxCode = ""'
      'AND tblPurchaseOrders.OrderDate between :txtFrom and :txtTo'
      'AND tblPurchaseLines.Class LIKE :xClassName'
      'GROUP BY tblPurchaseOrders.PurchaseOrderID'
      ''
      'UNION ALL'
      ''
      'SELECT '
      'GJ.TransactionDate as Date,'
      '0 as SaleID,'
      '0 as PurchaseOrderID,'
      '"Journal Entry" as Type,'
      '""   as '#39'Customer Name'#39','
      '0 as ClientID,'
      '"" as Company,'
      '"" as JobName,'
      '0 as JobNumber,'
      'GJ.GLOBALREF,'
      '0  as "P/O Num",'
      '0 as ETADate,'
      '0 as InvoiceNumber,'
      '"" as Deleted,'
      '0 as '#39'PO Number'#39','
      '0 as InvoiceDate,'
      '0 as CustPONumber,'
      '"" as '#39'Employee Name'#39','
      'GLD.ClassName as '#39'Class Name'#39','
      '"" as Terms,'
      '"" as Held,'
      '0 as '#39'Ship Date'#39','
      '"" as '#39'Paid'#39','
      '0 as ClassID,'
      'GJDID,'
      'GLD.TaxCode'
      'FROM tblgeneraljournaldetails GLD'
      'INNER JOIN tblgeneraljournal GJ on GLD.GJID = GJ.GJID'
      'Where  GLD.TaxCode = ""'
      'AND GJ.TransactionDate between :txtFrom and :txtTo'
      'AND GLD.ClassName LIKE :xClassName'
      ''
      'UNION ALL'
      ''
      'SELECT'
      'tblSales.SaleDate as '#39'Date'#39','
      'tblSales.SaleID as SaleID,'
      '0 as PurchaseOrderID,'
      'if (tblSales.IsQuote="T",'#39'Quote'#39','#39'Unknown'#39') as Type,'
      
        'CONCAT_WS(" ", If(InStr(tblClients.Company,'#39'^'#39') -1 >0,Left(tblCl' +
        'ients.Company,InStr(tblClients.Company,'#39'^'#39')-1),tblClients.Compan' +
        'y),IF(char_length(tblClients.JobName)>0, Concat("(",tblClients.J' +
        'obName,")"),'#39#39')) as '#39'Customer Name'#39','
      'tblSales.ClientID,'
      '"" as Company,'
      'JobName as JobName,'
      'JobNumber as JobNumber,'
      'tblSales.GlobalRef,'
      '0 as "P/O Num",'
      '0 as ETADate,'
      'tblSales.InvoiceDocNumber as '#39'Invoice Number'#39','
      'tblSales.Deleted as Deleted,'
      'tblSales.PONumber as '#39'PO Number'#39','
      '0 as  InvoiceDate,'
      '0 as CustPONumber,'
      'tblEmployees.EmployeeName AS '#39'Employee Name'#39','
      'tblSales.Class as '#39'Class Name'#39','
      'tblSales.Terms as Terms,'
      'tblSales.HoldSale as Held,'
      'tblSales.ShipDate as '#39'Ship Date'#39','
      'tblSales.Paid as '#39'Paid'#39','
      '0 as ClassID,'
      '0 as GJDID,'
      'tblSalesLines.LineTaxCode'
      ''
      'FROM tblSales'
      'INNER JOIN tblSalesLines Using(SaleID)'
      'INNER JOIN tblClients ON tblSales.ClientID = tblClients.ClientID'
      'LEFT JOIN tblclienttype Using(ClientTypeID)'
      
        'LEFT JOIN tblEmployees ON (tblSales.EmployeeID = tblEmployees.Em' +
        'ployeeID)'
      'WHERE '
      'tblSales.IsLayby = '#39'F'#39' AND tblSales.IsSalesOrder = '#39'F'#39'  AND'
      '( tblSales.IsQuote = '#39'T'#39')'
      'AND tblSalesLines.LineTaxCode = ""'
      'AND tblSales.SaleDate between :txtFrom and :txtTo'
      'AND tblSales.Class LIKE :xClassName'
      'Group By tblSales.SaleID')
