  inherited qryMain: TERPQuery
    SQL.Strings = (
      '# Dont Changes Without Considering fInverseCreditvsDebits'
      'SELECT '
      'Date,'
      'ClassName as Class, '
      'Type, '
      'tbltransactions.GlobalRef as GlobalRef,'
      'AccountName,'
      'ClientName,'
      'DebitsEx-CreditsEx as '#39'Amount (Ex)'#39','
      '(DebitsInc-CreditsInc)-(DebitsEx-CreditsEx) as '#39'Tax Amount'#39','
      'DebitsInc-CreditsInc as '#39'Amount (Inc)'#39','
      'tbltransactions.ClassID, '
      'SaleID, '
      'PurchaseOrderID, '
      'PaymentID, '
      'PrepaymentID, '
      'FixedAssetID,'
      'TransID,'
      'ProductName, '
      'ProductDesc, '
      'TaxCode,'
      'Notes, '
      'EmployeeID, '
