  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT distinct 1 as details, '
      'T.GlobalRef, '
      'T.Name as Name,'
      'T.Description as Description,'
      'T.TaxCodeID as TaxCodeID,'
      '(T.Rate * 100) as Rate,'
      'T.Active as Active , '
      'T.taxcodeLocationCategory as Category,'
      
        'if(ifnull(LOc.Id,0)=0 , '#39#39' , if((Select count(ID) from tbltaxcod' +
        'elocations where  TaxCodeID = T.taxcodeId)=1 , LOC.LocationValue' +
        ' , '#39'...'#39')) as LocationValue'
      'FROM tblTaxcodes T'
      
        'left join tbltaxcodelocations LOC  on T.taxcodeId = LOC.TaxCodeI' +
        'D'
      'Where RegionID= :xRegionID'
      'union all '
      'SELECT 2 as details, '
      'T.GlobalRef, '
      'T.Name as Name,'
      'T.Description as Description,'
      'T.TaxCodeID as TaxCodeID,'
      '(T.Rate * 100) as Rate,'
      'T.Active as Active, '
      'T.taxcodeLocationCategory as Category,'
      'LOC.LocationValue as Locationvalue'
      'FROM tblTaxcodes T'
      
        'left join tbltaxcodelocations LOC  on T.taxcodeId = LOC.TaxCodeI' +
        'D'
      'Where RegionID= :xRegionID'
      'ORDER BY Name, details;')
    OnCalcFields = qryMainCalcFields
    Left = 205
    Top = 336
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'xRegionID'
      end
      item
        DataType = ftUnknown
