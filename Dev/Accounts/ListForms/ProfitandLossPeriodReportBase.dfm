  object qryBudLines: TERPQuery
    Connection = MyConnection1
    SQL.Strings = (
      'SELECT coa.AccountID,'
      #9' SUM(bl.Conservative) as "Conservative",'
      #9' SUM(bl.Aggressive) as "Aggressive",'
      #9' b.Name as Budget, bl.BudgetID'
      'FROM tblchartofaccounts as coa'
      'Inner JOIN tblBudgetLines as bl ON'
      
        #9' (coa.AccountID = bl.AccountID) AND (bl.Date BETWEEN :txtFrom A' +
        'ND :txtTo)'
      'inner JOIN tblBudgets as b ON bl.BudgetID = b.BudgetID'
      'WHERE (bl.BudgetID = :xBudgetID or isnull(bl.BudgetID )) AND'
      
        '   (bl.ClassID like :xClassID OR (isnull(bl.ClassID) AND :xInclu' +
        'deNulls = '#39'Y'#39'))'
      'GROUP BY AccountID;')
    Left = 698
    Top = 359
    ParamData = <
      item
        DataType = ftString
        Name = 'txtFrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'txtTo'
      end
      item
        DataType = ftUnknown
        Name = 'xBudgetID'
      end
      item
        DataType = ftUnknown
        Name = 'xClassID'
      end
      item
        DataType = ftUnknown
        Name = 'xIncludeNulls'
      end>
  end
  object qryBudgets: TERPQuery
    Connection = MyConnection1
    SQL.Strings = (
      
        'SELECT BudgetID, Name, Description , Amount1Label, Amount2Label,' +
        ' ShowAmount1, Showamount2  , startdate , enddate'
      'FROM tblBudgets'
      'WHERE Active = '#39'T'#39'  '
      'and ((startdate between :DateFrom and :Dateto)'
      'or  (endDate between :DateFrom and :Dateto)'
      'or (startDate  <= :DateFrom and endDate >= :DateTo))'
      'ORDER BY Name ASC')
    BeforeOpen = qryBudgetsBeforeOpen
    Left = 393
    Top = 359
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'DateFrom'
      end
      item
        DataType = ftDateTime
        Name = 'DateTo'
      end
      item
        DataType = ftDateTime
        Name = 'DateFrom'
      end
      item
        DataType = ftDateTime
        Name = 'Dateto'
      end
      item
        DataType = ftDateTime
        Name = 'DateFrom'
      end
      item
        DataType = ftDateTime
        Name = 'DateTo'
      end>
  end
