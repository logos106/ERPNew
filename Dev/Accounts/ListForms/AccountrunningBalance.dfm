  inherited qryMain: TERPQuery
    SQL.Strings = (
      
        'select 2 as SeqNo, Acc.AccountID, Acc.AccountName, Acc.AccountTy' +
        'pe, Acc.AccountNumber, Date, Type,'
      'Null as OpeningBalanceEx,'
      'Null as OpeningBalanceInc,'
      'Null as ClosingBalanceEx,'
      'Null as ClosingBalanceInc,'
      'DebitsEx as TotalDebitEx,'
      'DebitsInc as TotalDebitInc,'
      'CreditsEx as TotalCreditEx,'
      'CreditsInc as TotalCreditInc,'
      
        'If(Acc.AccountType="CCARD" OR Acc.AccountType = "EQUITY" OR Acc.' +
        'AccountType = "INC" OR Acc.AccountType = "OCLIAB", CreditsEx-Deb' +
        'itsEx, DebitsEx-CreditsEx) as Balance'
      'from tbltransactions as Trans'
      
        'inner join tblchartofaccounts as Acc on Acc.AccountID = Trans.Ac' +
        'countID'
      
        'where Trans.Date between '#39'2017-01-29 00:00:00'#39' and '#39'2018-02-03 2' +
        '3:59:59'#39
      'union all'
      
        'select 2 as SeqNo, Acc.AccountID, Acc.AccountName, Acc.AccountTy' +
        'pe, Acc.AccountNumber, Date, Type,'
      'Null as OpeningBalanceEx,'
      'Null as OpeningBalanceInc,'
      'Null as ClosingBalanceEx,'
      'Null as ClosingBalanceInc,'
      'DebitsEx as TotalDebitEx,'
      'DebitsInc as TotalDebitInc,'
      'CreditsEx as TotalCreditEx,'
      'CreditsInc as TotalCreditInc,'
      
        'If(Acc.AccountType="CCARD" OR Acc.AccountType = "EQUITY" OR Acc.' +
        'AccountType = "INC" OR Acc.AccountType = "OCLIAB", CreditsEx-Deb' +
        'itsEx, DebitsEx-CreditsEx) as Balance'
      'from tbltransactionsummarydetails as Trans'
      
        'inner join tblchartofaccounts as Acc on Acc.AccountID = Trans.Ac' +
        'countID'
      
        'where Trans.Date between '#39'2017-01-29 00:00:00'#39' and '#39'2018-02-03 2' +
        '3:59:59'#39
      'union all'
      
        'select SeqNo, AccountID, AccountName, AccountType, AccountNumber' +
        ', Date, Type,'
      'Sum(OpeningBalanceEx) as OpeningBalanceEx,'
      'Sum(OpeningBalanceInc) as OpeningBalanceInc,'
      'Sum(ClosingBalanceEx) as ClosingBalanceEx,'
      'Sum(ClosingBalanceInc) as ClosingBalanceInc,'
      'Sum(TotalDebitEx) as TotalDebitEx,'
      'Sum(TotalDebitInc) as TotalDebitInc,'
      'Sum(TotalCreditEx) as TotalCreditEx,'
      'Sum(TotalCreditInc) as TotalCreditInc,'
      'Sum(Balance) as Balance'
      'from ( '
      
        'select 1 as SeqNo, Acc.AccountID, Acc.AccountName, Acc.AccountTy' +
        'pe, Acc.AccountNumber, Date, Type,'
      
        'If(Date < '#39'2017-01-29 00:00:00'#39', If(Acc.AccountType="CCARD" OR A' +
        'cc.AccountType = "EQUITY" OR Acc.AccountType = "INC" OR Acc.Acco' +
        'untType = "OCLIAB", CreditsEx-DebitsEx, DebitsEx-CreditsEx), 0.0' +
        ') as OpeningBalanceEx,'
      
        'If(Date < '#39'2017-01-29 00:00:00'#39', If(Acc.AccountType="CCARD" OR A' +
        'cc.AccountType = "EQUITY" OR Acc.AccountType = "INC" OR Acc.Acco' +
        'untType = "OCLIAB", CreditsInc-DebitsInc, DebitsInc-CreditsInc),' +
        ' 0.0) as OpeningBalanceInc,'
      
        'If(Date <= '#39'2018-02-03 23:59:59'#39', If(Acc.AccountType="CCARD" OR ' +
        'Acc.AccountType = "EQUITY" OR Acc.AccountType = "INC" OR Acc.Acc' +
        'ountType = "OCLIAB", CreditsEx-DebitsEx, DebitsEx-CreditsEx), 0.' +
        '0) as ClosingBalanceEx,'
      
        'If(Date <= '#39'2018-02-03 23:59:59'#39', If(Acc.AccountType="CCARD" OR ' +
        'Acc.AccountType = "EQUITY" OR Acc.AccountType = "INC" OR Acc.Acc' +
        'ountType = "OCLIAB", CreditsInc-DebitsInc, DebitsInc-CreditsInc)' +
        ', 0.0) as ClosingBalanceInc,'
      
        'If(Date >= '#39'2017-01-29 00:00:00'#39' and Date < '#39'2018-02-03 23:59:59' +
        #39', DebitsEx, 0.0) as TotalDebitEx,'
      
        'If(Date >= '#39'2017-01-29 00:00:00'#39' and Date < '#39'2018-02-03 23:59:59' +
        #39', DebitsInc, 0.0) as TotalDebitInc,'
      
        'If(Date >= '#39'2017-01-29 00:00:00'#39' and Date < '#39'2018-02-03 23:59:59' +
        #39', CreditsEx, 0.0) as TotalCreditEx,'
      
        'If(Date >= '#39'2017-01-29 00:00:00'#39' and Date < '#39'2018-02-03 23:59:59' +
        #39', CreditsInc, 0.0) as TotalCreditInc,'
      
        'If(Date <= '#39'2018-02-03 23:59:59'#39', If(Acc.AccountType="CCARD" OR ' +
        'Acc.AccountType = "EQUITY" OR Acc.AccountType = "INC" OR Acc.Acc' +
        'ountType = "OCLIAB", CreditsEx-DebitsEx, DebitsEx-CreditsEx), 0.' +
        '0) as Balance'
      'from tbltransactions as Trans'
      
        'inner join tblchartofaccounts as Acc on Acc.AccountID = Trans.Ac' +
        'countID'
      'union all'
      
        'select 1 as SeqNo, Acc.AccountID, Acc.AccountName, Acc.AccountTy' +
        'pe, Acc.AccountNumber, Date, Type,'
      
        'If(Date < '#39'2017-01-29 00:00:00'#39', If(Acc.AccountType="CCARD" OR A' +
        'cc.AccountType = "EQUITY" OR Acc.AccountType = "INC" OR Acc.Acco' +
        'untType = "OCLIAB", CreditsEx-DebitsEx, DebitsEx-CreditsEx), 0.0' +
        ') as OpeningBalanceEx,'
      
        'If(Date < '#39'2017-01-29 00:00:00'#39', If(Acc.AccountType="CCARD" OR A' +
        'cc.AccountType = "EQUITY" OR Acc.AccountType = "INC" OR Acc.Acco' +
        'untType = "OCLIAB", CreditsInc-DebitsInc, DebitsInc-CreditsInc),' +
        ' 0.0) as OpeningBalanceInc,'
      
        'If(Date <= '#39'2018-02-03 23:59:59'#39', If(Acc.AccountType="CCARD" OR ' +
        'Acc.AccountType = "EQUITY" OR Acc.AccountType = "INC" OR Acc.Acc' +
        'ountType = "OCLIAB", CreditsEx-DebitsEx, DebitsEx-CreditsEx), 0.' +
        '0) as ClosingBalanceEx,'
      
        'If(Date <= '#39'2018-02-03 23:59:59'#39', If(Acc.AccountType="CCARD" OR ' +
        'Acc.AccountType = "EQUITY" OR Acc.AccountType = "INC" OR Acc.Acc' +
        'ountType = "OCLIAB", CreditsInc-DebitsInc, DebitsInc-CreditsInc)' +
        ', 0.0) as ClosingBalanceInc,'
      
        'If(Date >= '#39'2017-01-29 00:00:00'#39' and Date < '#39'2018-02-03 23:59:59' +
        #39', DebitsEx, 0.0) as TotalDebitEx,'
      
        'If(Date >= '#39'2017-01-29 00:00:00'#39' and Date < '#39'2018-02-03 23:59:59' +
        #39', DebitsInc, 0.0) as TotalDebitInc,'
      
        'If(Date >= '#39'2017-01-29 00:00:00'#39' and Date < '#39'2018-02-03 23:59:59' +
        #39', CreditsEx, 0.0) as TotalCreditEx,'
      
        'If(Date >= '#39'2017-01-29 00:00:00'#39' and Date < '#39'2018-02-03 23:59:59' +
        #39', CreditsInc, 0.0) as TotalCreditInc,'
      
        'If(Date <= '#39'2018-02-03 23:59:59'#39', If(Acc.AccountType="CCARD" OR ' +
        'Acc.AccountType = "EQUITY" OR Acc.AccountType = "INC" OR Acc.Acc' +
        'ountType = "OCLIAB", CreditsEx-DebitsEx, DebitsEx-CreditsEx), 0.' +
        '0) as Balance'
      'from tbltransactionsummarydetails as Trans'
      
        'inner join tblchartofaccounts as Acc on Acc.AccountID = Trans.Ac' +
        'countID'
      ') as T Group By T.AccountID'
      'Order By AccountName, SeqNo, Date')
