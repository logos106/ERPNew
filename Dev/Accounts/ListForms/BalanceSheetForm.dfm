  inherited qryMain: TERPQuery
    SQL.Strings = (
      
        'SELECT ID,SortID, AccountType as TypeID, AccountName as ACCNAME,' +
        'AccountNumber, Column1 as Column1, TRUNCATE(Column2,0) as Column' +
        '2, TRUNCATE(Column3,0) as Column3, TRUNCATE(Column4,0) as Column' +
        '4, TRUNCATE(Column5,0) as Column5'
      'FROM tblbalancesheet'
      
        'WHERE AccountType <> '#39'TOTAL LIABILITIES & EQUITY'#39' AND AccountTyp' +
        'e <> '#39'Total Capital / Equity'#39' AND AccountType <> '#39'TOTAL ASSETS'#39
      'Group By Column1'
      'Union ALL'
      
        'SELECT ID,SortID, AccountType as TypeID, AccountName as ACCNAME,' +
        'AccountNumber, Column1, TRUNCATE(Column2,0), TRUNCATE(Column3,0)' +
        ', TRUNCATE(Column4,0), TRUNCATE(Sum(Column5),0) as Column5'
      'FROM tblbalancesheet'
      'WHERE AccountType = '#39'TOTAL ASSETS'#39
      'Group By AccountType'
      'Union ALL'
      
        'SELECT ID,SortID, AccountType as TypeID, AccountName as ACCNAME,' +
        'AccountNumber, Column1, TRUNCATE(Column2,0), TRUNCATE(Column3,0)' +
        ',TRUNCATE(Sum(Column4),0),TRUNCATE(Column5,0)'
      'FROM tblbalancesheet'
      'WhERE AccountType = '#39'Total Capital / Equity'#39
      'Group BY Column1'
      'Union ALL'
      
        'SELECT ID,SortID, AccountType as TypeID, AccountName as ACCNAME,' +
        'AccountNumber, Column1, TRUNCATE(Column2,0), TRUNCATE(Column3,0)' +
        ', TRUNCATE(Column4,0), TRUNCATE(Sum(Column5),0)'
      'FROM tblbalancesheet'
      'WhERE AccountType = '#39'TOTAL LIABILITIES & EQUITY'#39
      'Group BY SortID'
      'Order by ID;')
