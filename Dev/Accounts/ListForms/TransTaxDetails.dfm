  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      '1 as Details, '
      'L.Seqno, '
      'L.Productname as Description , '
      'L.purchaselineID TransID, '
      'L.LineTaxCode, '
      'L.LineTaxRate , '
      '0.0 as CapAmount,'
      '0.0 as AmountPerUnit,'
      'null as UOM,'
      '0.0 as ThresholdAmount, '
      '"Amount Ex" PercentageOn , '
      'L.TotalLineamount, '
      'L.LineTax, '
      'L.TotalLineAmountinc '
      'from  tblpurchaselines L '
      'where L.PurchaseOrderID = 23'
      'union '
      'Select '
      '2 as Details ,'
      'L.Seqno, '
      '"" as Description , '
      'TTC.PurchaseLineId, '
      'TTCL.`SubTaxCode`, '
      'TTCL.`Percentage`, '
      'TTCL.CapAmount as CapAmount,'
      'TTCL.AmountPerUnit,'
      'TTCL.UOM,'
      'TTCL.ThresholdAmount, '
      'TTCL.`PercentageOn`, '
      '0,'
      'TTCL.`TaxAmount`,'
      '0'
      'from `tblpurchasetaxcodelines` TTCL'
      
        'inner join `tblpurchasetaxcodes` TTC on TTCL.PurchaseTaxCodeID =' +
        ' TTC.ID'
      
        'inner join tblpurchaselines L  on L.PurchaseLineID = TTC.Purchas' +
        'eLineId'
      'where TTC.PurchaseOrderID = 23'
      'order by Seqno , details , `PercentageOn`')
    Filter = 'Details = 1'
