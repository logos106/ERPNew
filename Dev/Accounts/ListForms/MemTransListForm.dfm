        'ike '#39'Sales Order%'#39'  or tblmemtrans.Type like '#39'Quote%'#39')'
      
        'Left join tblgeneraljournal gj on tblmemtrans.Type = "Journal En' +
        'try" and tblmemtrans.TransId = gj.GJID'
