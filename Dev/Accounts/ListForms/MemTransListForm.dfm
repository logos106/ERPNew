  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT tblmemtrans.MemTransID , tblmemtrans.GlobalRef,'
      'DateDue, '
      'Concat(tblmemtrans.Type, '#39'#'#39', TransID) AS Type, '
      'tblmemtrans.Type as TransactionType,'
      'Company as company,'
      'AutoPost as AutoPost, '
      'CreatedDate as CreatedDate, '
      'formClass as formClass, '
      'TransID as TransID , '
      
        'if(ifnull(PO.PurchaseOrderID,0) = 0 , S.totalAmountInc , PO.Tota' +
        'lAmountInc) as TotalAmountInc,'
      'gj.Memo as Memo'
      
        'FROM tblmemtrans LEFT JOIN tblclients ON tblmemtrans.ClientID = ' +
        'tblclients.ClientID '
      
        'Left join tblpurchaseorders PO on PO.PurchaseOrderId = tblmemtra' +
