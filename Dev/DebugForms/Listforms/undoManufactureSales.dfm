  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select  distinct '
      's.SaleId as SaleId, '
      's.CustomerName as customername, '
      's.SAledate  as Saledate, '
      'S.isinvoice as isinvoice, '
      'S.issalesorder as issalesorder, '
      'S.converted as converted,'
      'S.TotalAmountinc as TotalAmountinc'
      'from erpfix_sales s '
      'inner join erpfix_saleslines SL on Sl.saleId = s.saleID'
      
        'inner join erpfix_Proctree PT on SL.SaleLineID = PT.masterId and' +
        ' PT.mastertype <> '#39'mtProduct'#39
      'group by s.SaleId'
      'order by SaleId desc')
    object qryMainSaleId: TIntegerField
      DisplayLabel = 'Sale #'
      DisplayWidth = 10
      FieldName = 'SaleId'
      Origin = 's.SaleId'
    end
    object qryMaincustomername: TWideStringField
      DisplayLabel = 'Customer'
      DisplayWidth = 30
      FieldName = 'customername'
      Origin = 's.customername'
      Size = 255
    end
    object qryMainSaledate: TDateField
      DisplayLabel = 'Sale Date'
      DisplayWidth = 10
      FieldName = 'Saledate'
      Origin = 's.Saledate'
    end
    object qryMainisinvoice: TWideStringField
      DisplayLabel = 'Invoice?'
      DisplayWidth = 1
      FieldName = 'isinvoice'
      Origin = 's.isinvoice'
      FixedChar = True
      Size = 1
    end
    object qryMainissalesorder: TWideStringField
      DisplayLabel = 'Sales Order?'
      DisplayWidth = 1
      FieldName = 'issalesorder'
      Origin = 's.issalesorder'
      FixedChar = True
      Size = 1
    end
    object qryMainconverted: TWideStringField
      DisplayLabel = 'Converted?'
      DisplayWidth = 1
      FieldName = 'converted'
      Origin = 's.converted'
      FixedChar = True
      Size = 1
    end
    object qryMainTotalAmountinc: TFloatField
      DisplayLabel = 'Total Amount'
      DisplayWidth = 10
      FieldName = 'TotalAmountinc'
      Origin = 's.TotalAmountinc'
    end
  end
