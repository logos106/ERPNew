  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'if(isSalesOrder="T",'#39'Sales Order'#39' , '#39'Invoice'#39') as Type, '
      'tblsaleslines.SaleId as ID, '
      'sum(qtysold) qtysold, '
      'sum(shipped) shipped, '
      'sum(backorder) backorder, '
      'isSalesOrder, '
      'isinvoice, '
      '"F" as ISPO'
      'from tblsales inner join tblsaleslines using (SaleID)'
      
        'where ifnull(BOID,'#39#39')<> '#39#39' and BOID not in (Select globalref fro' +
        'm tblsales where deleted ="F" and cancelled="F")'
      'Group by tblsaleslines.SaleId'
      'union all'
      'Select '
      '"PO" as type,'
      'tblpurchaseLines.PurchaseOrderID as Id, '
      'sum(qtysold) qtysold, '
      'sum(shipped) shipped, '
      'sum(backorder) backorder, '
      '"F" as isSalesOrder, '
      '"F" as isinvoice, '
      '"T" as isPO'
      
        'from tblpurchaseOrders inner join tblpurchaseLines using (Purcha' +
        'seOrderID)'
      
        'where ifnull(BOID,'#39#39')<> '#39#39' and BOID not in (Select globalref fro' +
        'm tblpurchaseOrders where deleted ="F" and cancelled="F")'
      'Group by tblpurchaseLines.PurchaseOrderID'
      'order by ID')
    object qryMainType: TWideStringField
      DisplayWidth = 11
      FieldName = 'Type'
      Size = 11
    end
