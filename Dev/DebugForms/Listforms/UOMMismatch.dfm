  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'Trans.productname as Productname,  '
      #39'PQA'#39' as TransType, '
      '1 As Seqno, '
      'Trans.uomID '#9#9#9#9#9'  as TransUOMID , '
      'uom.unitId  as UOMID, '
      'UOM.UnitName as UnitName, '
      'uom.Multiplier as Multiplier , '
      'IFNULl(uom.PartID,0) as ProductID '
      
        'from tblpqa '#9#9#9#9#9#9#9#9'Trans inner join tblunitsofmeasure uom on Tr' +
        'ans.ProductID = IFNULl(uom.PartID,0) and Trans.UOM '#9#9#9#9#9#9#9#9'= uom' +
        '.unitname  where Trans.uomID '#9#9#9#9#9'<> uom.unitId union all '
      'Select '
      'Trans.productname as Productname,   '
      #39'Sales'#39' as TransType, '
      '2 As Seqno , '
      'Trans.UnitOfMeasureID '#9' as TransUOMID , '
      'uom.unitId  as UOMID, '
      'UOM.UnitName as UnitName, '
      'uom.Multiplier as Multiplier , '
      'IFNULl(uom.PartID,0) as ProductID '
      
        'from tblsaleslines '#9#9#9#9#9#9'Trans inner join tblunitsofmeasure uom ' +
        'on Trans.ProductID = IFNULl(uom.PartID,0) and Trans.UnitofMeasur' +
        'eSaleLines '#9'= uom.unitname where  Trans.UnitOfMeasureID '#9'<> uom.' +
        'unitId union all '
      'Select '
      'Trans.productname as Productname,   '
      #39'PO'#39' as TransType, '
      '3 As Seqno,'
      'Trans.UnitOfMeasureID '#9' as TransUOMID , '
      'uom.unitId  as UOMID, '
      'UOM.UnitName as UnitName, '
      'uom.Multiplier as Multiplier , '
      'IFNULl(uom.PartID,0) as ProductID '
      
        'from tblpurchaselines '#9#9#9#9'Trans inner join tblunitsofmeasure uom' +
        ' on Trans.ProductID = IFNULl(uom.PartID,0) and Trans.UnitofMeasu' +
        'rePOLines '#9#9'= uom.unitname Where  Trans.UnitOfMeasureID '#9'<> uom.' +
        'unitId union all '
      ''
      'Select '
      'Trans.productname as Productname,   '
      #39'Tree'#39' as TransType, '
      '4 As Seqno,'
      'Trans.UnitOfMeasureId  as TransUOMID , '
      'uom.unitId  as UOMID, '
      'UOM.UnitName as UnitName, '
      'uom.Multiplier as Multiplier , '
      'IFNULl(uom.PartID,0) as ProductID '
      'from tblproctreepart '#9'Trans '
      
        'inner join tblunitsofmeasure uom on IFNULL(Trans.ProductID,0) = ' +
        'IFNULl(uom.PartID,0) and Trans.UnitOfMeasure'#9' '#9#9#9#9'= uom.unitname' +
        ' Where Trans.UnitOfMeasureId '#9'<>'#9'uom.unitId '
      ''
      'union all '
      'Select '
      'Trans.caption as Productname,   '
      #39'Tree'#39' as TransType, '
      '4 As Seqno,'
      'Trans.TreePartUOMID  as TransUOMID , '
      'uom.unitId  as UOMID, '
      'UOM.UnitName as UnitName, '
      'uom.Multiplier as Multiplier , '
      'UOM.PartID as ProductID '
      'from tblproctree '#9'Trans '
      
        'inner join tblunitsofmeasure uom on Trans.PartsId = uom.PartID a' +
        'nd Trans.TreePartUOM'#9' '#9#9#9#9'= uom.unitname Where Trans.TreePartUOM' +
        'ID '#9'<>'#9'uom.unitId  AND IFNULL(Trans.PartsId,0) <>0'
      'union all '
      'Select '
      'Trans.productname as Productname,   '
      #39'Stock Movement'#39' as TransType, '
      '5 As Seqno,'
      'Trans.UOMID '#9#9#9#9#9' as TransUOMID , '
      'uom.unitId  as UOMID, '
      'UOM.UnitName as UnitName, '
      'uom.Multiplier as Multiplier , '
      'UOM.PartID as ProductID '
      
        'from tblstockmovementlines '#9#9'Trans inner join tblunitsofmeasure ' +
        'uom on Trans.ProductID = uom.PartID and Trans.UOM '#9#9#9#9#9#9#9#9'= uom.' +
        'unitname Where Trans.UOMID '#9#9#9#9#9'<> uom.unitId union all '
      'Select '
      'Trans.productname as Productname,   '
      #39'Stock Adjustment'#39' as TransType, '
      '6 As Seqno,'
      'Trans.UOMID '#9#9#9#9#9' as TransUOMID , '
      'uom.unitId  as UOMID, '
      'UOM.UnitName as UnitName, '
      'uom.Multiplier as Multiplier , '
      'UOM.PartID as ProductID '
      
        'from tblstockadjustentrylines '#9#9'Trans inner join tblunitsofmeasu' +
        're uom on Trans.ProductID = uom.PartID and Trans.UOM '#9#9#9#9#9#9#9#9'= u' +
        'om.unitname Where Trans.UOMID '#9#9#9#9#9'<> uom.unitId union all '
      'Select '
      'Trans.productname as Productname,   '
      #39'Stock Transfer'#39' as TransType, '
      '7 As Seqno,'
      'Trans.UOMID '#9#9#9#9#9' as TransUOMID , '
      'uom.unitId  as UOMID, '
      'UOM.UnitName as UnitName, '
      'uom.Multiplier as Multiplier , '
      'UOM.PartID as ProductID '
      
        'from tblstocktransferentrylines '#9'Trans inner join tblunitsofmeas' +
        'ure uom on Trans.ProductID = uom.PartID and Trans.UOM '#9#9#9#9#9#9#9#9'= ' +
        'uom.unitname Where Trans.UOMID '#9#9#9#9#9'<> uom.unitId union all '
      'Select '
      'Trans.PartName as Productname,   '
      #39'RepairParts'#39' as TransType, '
      '8 As Seqno,'
      'Trans.UnitofMeasureID '#9' as TransUOMID , '
      'uom.unitId  as UOMID, '
      'UOM.UnitName as UnitName, '
      'uom.Multiplier as Multiplier , '
      
        'UOM.PartID as ProductID from tblrepairparts '#9#9#9#9#9#9'Trans inner jo' +
        'in tblunitsofmeasure uom on Trans.PartsID '#9'= uom.PartID and Tran' +
        's.UnitofMeasure '#9#9#9#9#9'= uom.unitname Where Trans.UnitofMeasureID ' +
        #9'<> uom.unitId'
      'ORDER BY UOMID')
