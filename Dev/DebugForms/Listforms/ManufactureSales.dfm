  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select  distinct '
      's.SaleId as SaleId, '
      's.CustomerName as customername, '
      's.SAledate  as Saledate, '
      'S.isinvoice as isinvoice, '
      'S.issalesorder as issalesorder, '
      'S.converted as converted,'
      'S.TotalAmountinc as TotalAmountinc,'
      'S.TotalAmountinc as oldTotalAmountinc,'
      'SLsum.Totalcostinc, '
      'SLsum.Totalcost , '
      'SLsum.oldTotalcostinc, '
      'SLsum.oldTotalcost'
      'from tblsales s '
      
        'inner join (Select saleID, Sum(LineCostInc * QtySold) Totalcosti' +
        'nc,  Sum(LineCost * QtySold) Totalcost ,  Sum(LineCostInc * QtyS' +
        'old) oldTotalcostinc,  Sum(LineCost * QtySold) oldTotalcost from' +
        ' tblsaleslines group by saleID) SLsum on S.SaleId = SLsum.saleID'
      'inner join tblsaleslines SL on Sl.saleId = s.saleID'
      
        'inner join tblProctree PT on SL.SaleLineID = PT.masterId and PT.' +
        'mastertype <> '#39'mtProduct'#39
      'group by s.SaleId'
      'order by SaleId desc')
    OnCalcFields = qryMainCalcFields
    Active = True
