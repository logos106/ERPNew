  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      ' pt.masterId, '
      'treepart.partsID as ProductID,'
      's.SaleID,'
      
        ' if (pt.MasterType ='#39'mtProduct'#39' , '#39' Product'#39' , if (pt.MasterType' +
        ' ='#39'mtsalesOrder'#39' , '#39'Sales Order'#39' , if (pt.MasterType ='#39'mtQuote'#39' ' +
        ', '#39'Quote'#39' ,if (pt.MasterType ='#39'mtInvoice'#39' , '#39'Invoice'#39' ,pt.Master' +
        'Type)))) as Type,  '
      'treepart.partname as partname, '
      'pt.caption,'
      's.CustomerName as SaleCustomerName, '
      's.SaleDate, '
      'sl.ProductName as SaleLineProductName, '
      'ps.Description as ProcessDesc'
      ' FROM  tblProcTree pt '
      ' left join ( tblSales s '
      
        ' JOIN tblSalesLines sl ON s.SaleId = sl.SaleId ) on pt.MasterId ' +
        '= sl.SaleLineId and pt.MasterType <> "mtProduct" '
      
        ' left join tblparts treepart on  treepart.partsId = Pt.masterId ' +
        'and pt.MasterType = "mtProduct" '
      ' JOIN tblProcessPart pp ON pp.ProcTreeId = pt.ProcTreeId '
      ' JOIN tblProcessStep ps ON ps.Id = pp.ProcessStepId '
      
        ' WHERE (ifnull(pp.Duration ,0)+ifnull(pp.SetupDuration ,0) +ifnu' +
        'll( pp.BreakdownDuration,0)) =0 and (ifnull(pt.ManufactureUOMQty' +
        '  ,0)<>0 or pt.MasterType = "mtProduct" )'
      
        ' and pt.MasterType  in ('#39'mtProduct'#39' , '#39'mtsalesOrder'#39', '#39'mtQuote'#39')' +
        ' '
      
        'and (ifnull(treepart.partsId,0)<> 0 or (ifnull(S.saleId,0)<>0 an' +
        'd PP.timestart <= "1899-12-30 00:00:00" and  PP.timeend <= "1899' +
        '-12-30 00:00:00") )'
      ' ORDER BY type, partname, saleId')
    Active = True
