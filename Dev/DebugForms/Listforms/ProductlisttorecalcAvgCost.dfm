      'P.costinc1 as costinc1,'
      'P.Cost2 as Cost2, '
      'P.costinc2 as costinc2 , '
      'P.Cost3 as Cost3, '
      'P.costinc3 as costinc3,'
      'P.AvgCost as AvgCost,'
      'P.AvgCost as oldAvgcost'
