  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select S.sAleID, SL.salelineId, SL.productname, '
      
        'SL.UnitofMeasureSaleLines SLUOM , pqa.uom PQAUOM, ptpqa.uom  PTP' +
        'QAUOM, '
      
        'SL.UnitofMeasureMultiplier SLUOMMUlt, PQA.UOMMultiplier pqaUOMMU' +
        'lt, PTPqa.UOMMultiplier ptppqaUOMMUlt, '
      
        'SL.UnitOfMeasureID SLUOMID, PQA.uomid pqaUOMID, PTPqa.uomid ptpq' +
        'aUOMID, '
      
        'SL.UnitofMeasureShipped SLuomqty, pqa.uomqty pqauomqty, ptpqa.uo' +
        'mqty ptpqauomqty,'
      'SL.Shipped SLQty, PTPQA.qty ptpqaQty , pqa.qty pqaQty,'
      'PQA.PQAID , PTPQa.PQAID PTPQaID'
      
        'from tblsales S inner join tblsaleslines SL on S.SAleId = SL.sal' +
        'eID'
      
        'inner join tblPQa pqa on SL.SaleLineID = PQA.translineId and SL.' +
        'saleId = PQa.transid and PQa.transtype = '#39'TSalesOrderLine'#39
      
        'inner join tblproctree PT on PT.masterId = SL.SaleLineId and PT.' +
        'mastertype = '#39'mtSalesOrder'#39' and ifnull(PT.parentID,0) =0'
      'inner join tblproctreepart PTP on PT.ProcTreeId = PTP.ProcTreeId'
      
        'inner join tblpqa PTPqa on PTPQA.transId = S.saleId and PTPQA.tr' +
        'anslineID = PTP.ProcTreePartId and PTPQA.transtype = '#39'TProcTreeP' +
        'art'#39
      'where '
      
        'SL.UnitofMeasureMultiplier <> PTPqa.UOMMultiplier or SL.UnitofMe' +
        'asureMultiplier <> PQA.UOMMultiplier or SL.Shipped <> pqa.Qty or' +
        ' SL.Shipped <> PTPqa.qty')
