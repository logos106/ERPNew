  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select'
      'H.HireID as HireID,'
      'HL.HireLineID as HireLineID,'
      'H.HireDate as HireDate,'
      'H.CustomerName  as CustomerName ,'
      'H.ShiptoAddress  as ShiptoAddress ,'
      'H.BillToAddress as BillToAddress,'
      'A.AssetName as ItemName,'
      'HL.HireFrom as HireFrom,'
      'HL.HireFromTime as HireFromTime,'
      'HL.HireTo as HireTo,'
      'HL.HiretoTime as HiretoTime,'
      'HL.ActualHireFrom as ActualHireFrom,'
      'HL.ActualHireFromTime as ActualHireFromTime ,'
      'HL.ActualHireTo as ActualHireTo,'
      'HL.ActualHiretoTime as ActualHiretoTime,'
      'IfNull(ActualHireFrom,HireFrom) as HireOut,'
      'IfNull(ActualHireTo,HireTo) as HireIn'
      'From tblhire H'
      'Inner join tblhirelines HL on H.HireID = HL.HireID'
      'inner join tblassethire AH on AH.AssetId = HL.AssetID'
      'inner join  tblfixedassets A on A.AssetID = AH.AssetId'
      'Where (:AssetId = 0 OR AH.AssetId = :AssetId)'
      'order by A.AssetName, HireOut')
    OnCalcFields = qryMainCalcFields
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'AssetId'
      end
      item
        DataType = ftUnknown
        Name = 'AssetId'
      end>
