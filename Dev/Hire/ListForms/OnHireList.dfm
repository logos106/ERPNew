  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'H.hireId as hireId  , '
      'H.CustomerName  as CustomerName ,'
      'H.shipToAddress as shipToAddress,'
      'H.BillcustomerName as BillcustomerName,'
      'H.BillToAddress as BillToAddress, '
      'H.Classname as Classname,'
      'H.notes as notes,'
      'H.Terms as Terms, '
      'H.employeeName  as employeeName ,'
      'HL.EquipmentName  as EquipmentName,'
      'HL.hirefrom as hirefrom,'
      'HL.HireTo as HireTo, '
      'HL.LineAmountInc as LineAmountInc,'
      'HL.AssetProductName as ProductName, '
      'HL.HireQty as HireQty, '
      'concat(HL.UOM , '#39' ('#39' , HL.UOMMult, '#39')'#39') as UOM, '
      'HL.converted as converted'
      'from tblhirelines HL'
      'inner join tblHire H on H.hireID = HL.hireId'
      'where CreatedDate  between :DateFrom and :DateTo')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end>
