  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'tblhire.HireId as HireId,'
      'CustomerName as CustomerName, '
      'ShiptoAddress as ShiptoAddress, '
      'CreatedDate as CreatedDate, '
      'classname as classname, '
      'SalesTotal as SalesTotal, '
      'Hiretotal as Hiretotal, '
      'Notes  as Notes , '
      'Terms as Terms,'
      'tblhire.employeeName as EmployeeName, '
      'HireStatus(tblhire.Status)  as Status,'
      'tblhire.Status as  Hirestatus,'
      'tblhire.converted as Converted,'
      'Max(ifnull(tblhirelines.HireTo,0)) as MaxHireTo,'
      'Max(ifnull(tblhirelines.ActualHireTo,0)) as MaxActualHireTo'
      'from tblhire'
      
        'left join tblhirelines on tblhire.HireID = tblhirelines.HireID a' +
        'nd tblhirelines.Deleted = "F" and tblhirelines.Converted = "F"'
      'where CreatedDate between :DateFrom and :DateTo'
      'and tblHire.Deleted = "F"'
      'group by tblhire.HireID'
      'Order by HireId desc')
    OnCalcFields = qryMainCalcFields
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end>
