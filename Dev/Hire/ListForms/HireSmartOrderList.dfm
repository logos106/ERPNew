  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'SmartOrderID as ID, '
      'SmartOrderDesc as Description,'
      'CreationDate as CreationDate, '
      'Done as converted, '
      '"Smart Order" as Type'
      'from '
      'tblSmartOrder S Where S.HireGlobalref = :HireGlobalref'
      'union all '
      'Select distinct '
      'PO.PurchaseOrderID as ID, '
      'PO.SupplierName as Description,'
      'PO.OrderDate as Date, '
      '"F" as Converted, '
      '"Purchase Order" as Type'
      'from '
      
        'tblSmartOrder S  inner join tblpurchaselines PL on PL.SmartOrder' +
        'ID = S.SmartOrderID inner join tblpurchaseorders PO on PO.Purcha' +
        'seOrderID = PL.PurchaseOrderID'
      'Where S.HireGlobalref = :HireGlobalref')
    Active = True
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'HireGlobalref'
      end
      item
        DataType = ftUnknown
        Name = 'HireGlobalref'
      end>
