  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      '0 as Details,'
      'tblFixedAssets.AssetId,'
      'tblFixedAssets.AssetName as ItemName,'
      'null as AssetCode,'
      'null as HireID,'
      'null as DurationQty,'
      'null as DurationName,'
      'null as HireOut,'
      'null as HireIn,'
      'null as CustomerName,'
      
        '(select Count(FA.AssetID) from tblFixedAssets FA where FA.AssetN' +
        'ame = tblFixedAssets.AssetName) as TotalQty,'
      '(select Count(FA.AssetID) '
      '  from tblFixedAssets FA'
      '  inner Join tblAssetHire AH on AH.AssetId = FA.AssetId'
      '  inner join tblhirelineasset HLA on HLA.AssetID = AH.AssetID'
      
        '  inner join tblhirelines HL on HL.HireLineId = HLA.HireLineID a' +
        'nd HL.Deleted = "F"'
      
        '  and :AsOn >= ifnull(HL.ActualHireFrom,HL.HireFrom) and :AsOn <' +
        ' ifnull(HL.ActualHireTo,HL.HireTo)'
      
        '  inner join tblhire H on H.HireID = HL.HireID and HL.Deleted = ' +
        '"F" '
      '  where FA.AssetName = tblFixedAssets.AssetName'
      ') as OnHireQty,'
      
        '(select Count(FA.AssetID) from tblFixedAssets FA where FA.AssetN' +
        'ame = tblFixedAssets.AssetName) -'
      '(select Count(FA.AssetID) '
      '  from tblFixedAssets FA'
      '  inner Join tblAssetHire AH on AH.AssetId = FA.AssetId'
      '  inner join tblhirelineasset HLA on HLA.AssetID = AH.AssetID'
      
        '  inner join tblhirelines HL on HL.HireLineId = HLA.HireLineID a' +
        'nd HL.Deleted = "F"'
      
        '  and :AsOn >= ifnull(HL.ActualHireFrom,HL.HireFrom) and :AsOn <' +
        ' ifnull(HL.ActualHireTo,HL.HireTo)'
      
        '  inner join tblhire H on H.HireID = HL.HireID and HL.Deleted = ' +
        '"F" '
      '  where FA.AssetName = tblFixedAssets.AssetName'
      ') as AvailableQty'
      'from tblAssetHire'
      
        'inner join tblfixedassets on tblAssetHire.AssetId = tblfixedasse' +
        'ts.AssetID '
      'where tblAssetHire.Active = "T"'
      'group By tblFixedAssets.AssetName'
      'union all'
      '/* select hires that fall into date range */'
      'select '
      '1 as Details,'
      'tblFixedAssets.AssetId,'
      'tblFixedAssets.AssetName as ItemName,'
      'tblFixedAssets.AssetCode as AssetCode,'
      'tblhirelines.HireID,'
      'tblhirelines.DurationQty as DurationQty,'
      'tblhirelines.DurationName as DurationName,'
      
        'ifnull(tblhirelines.ActualHireFrom,tblhirelines.HireFrom) as Hir' +
        'eOut,'
      'ifnull(tblhirelines.ActualHireTo,tblhirelines.HireTo) as HireIn,'
      'tblHire.CustomerName as CustomerName,'
      '1 as TotalQty,'
      'if (isnull(tblhirelineasset.AssetId),0,1) as OnHireQty,'
      'if (isnull(tblhirelineasset.AssetId),1,0) as AvailableQty'
      'from tblAssetHire'
      
        'inner join tblfixedassets on tblAssetHire.AssetId = tblfixedasse' +
        'ts.AssetID '
      
        'inner join tblhirelineasset on tblhirelineasset.AssetID = tblFix' +
        'edAssets.AssetId'
      
        'inner join tblHireLines on tblhireLines.HireLineID = tblhireline' +
        'asset.HireLineID and tblhireLines.Deleted = "F" and tblhireLines' +
        '.Converted = "F"  '
      
        'and :AsOn >= ifnull(tblhirelines.ActualHireFrom,tblhirelines.Hir' +
        'eFrom) and :AsOn < ifnull(tblhirelines.ActualHireTo,tblhirelines' +
        '.HireTo)'
      
        'inner join tblhire on tblhire.HireID = tblHireLines.HireId and t' +
        'blhire.Deleted = "F" and tblhire.Converted = "F"'
      'where tblAssetHire.Active = "T"'
      'union all'
      'select '
      '/* select all items withount any hire */'
      '1 as Details,'
      'tblFixedAssets.AssetId,'
      'tblFixedAssets.AssetName as ItemName,'
      'tblFixedAssets.AssetCode as AssetCode,'
      'tblhire.HireID as HireID,'
      'null as DurationQty,'
      'null as DurationName,'
      'null as HireOut,'
      'null as HireIn,'
      'null CustomerName,'
      '1 as TotalQty,'
      '0 as OnHireQty,'
      '1 as AvailableQty'
      'from tblAssetHire'
      
        'inner join tblfixedassets on tblAssetHire.AssetId = tblfixedasse' +
        'ts.AssetID '
      
        'left join tblhirelineasset on tblhirelineasset.AssetID = tblFixe' +
        'dAssets.AssetId'
      
        'left join tblHireLines on tblhireLines.HireLineID = tblhirelinea' +
        'sset.HireLineID and tblhireLines.Deleted = "F" and tblhireLines.' +
        'Converted = "F" '
      
        'left join tblhire on tblhire.HireID = tblHireLines.HireId and tb' +
        'lhire.Deleted = "F" and tblhire.Converted = "F"'
      'where tblAssetHire.Active = "T" and IsNull(tblhire.HireID) '
      'Order by ItemName, AssetCode, Details')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'AsOn'
      end
      item
        DataType = ftUnknown
        Name = 'AsOn'
      end
      item
        DataType = ftUnknown
        Name = 'AsOn'
      end
      item
        DataType = ftUnknown
        Name = 'AsOn'
      end
      item
        DataType = ftUnknown
        Name = 'AsOn'
      end
      item
        DataType = ftUnknown
        Name = 'AsOn'
      end>
