  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'H.CreatedDate as CreatedDate,'
      'H.HireDate as HireDate,'
      'H.CustomerName as CustomerName,'
      'H.BillCustomerName as BillCustomerName,'
      'H.CustomerPONumber as CustomerPONumber,'
      'H.HireId,'
      'HL.AssetName as AssetName,'
      'HL.AssetCode as AssetCode,'
      'HL.UOM as UOM,'
      'HL.HireUOMQty as HireUOMQty,'
      'HL.HireFrom as HireFrom,'
      'HL.HireTo as HireTo,'
      'HL.ActualHireFrom as ActualHireFrom,'
      'HL.ActualHireTo as ActualHireTo,'
      'HireStatus(H.Status) as Status,'
      'HireStatus(HL.Status) as HireStatus,'
      'HL.DurationName as DurationName,'
      'HL.DurationRate as DurationRate,'
      'HL.DurationQty as DurationQty,'
      'HL.PriceEx as PriceEx,'
      'HL.PriceInc As PriceInc,'
      'HL.LineAmount as LineAmountEx,'
      'HL.LineAmountInc as LineAmountInc,'
      'HL.LineTaxCode as LineTaxCode'
      'from'
      'tblHire H'
      
        'inner join tblHireLines HL on HL.HireId = H.HireId and HL.Delete' +
        'd = "F"'
      'where H.Converted = "F" and HL.Converted = "F"'
      'and H.Deleted = "F"'
      'and H.HireDate between :DateFrom and :DateTo'
      'and (:Status = "" or :Status = HL.Status)'
      'order by H.CustomerName, H.HireId')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'STATUS'
      end
      item
        DataType = ftUnknown
        Name = 'STATUS'
      end>
