  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT distinct'
      'convert(E.equipmentID ,signed) as equipmentID ,'
      'P.PARTSID as PARTSID,'
      'P.PARTNAME as ProductName,'
      'Left(SUBSTRING_INDEX(P.ProductGroup,'#39'^'#39',1),255) AS FirstColumn,'
      
        'Left(substring(substring_index(P.ProductGroup,'#39'^'#39',2),char_length' +
        '(substring_index(P.ProductGroup,'#39'^'#39',1))+2),255) AS SecondColumn,'
      
        'Left(substring(substring_index(P.ProductGroup,'#39'^'#39',3),char_length' +
        '(substring_index(P.ProductGroup,'#39'^'#39',2))+2),255) AS ThirdColumn,'
      'PARTSDESCRIPTION as SalesDescription,'
      'PURCHASEDESC as PURCHASEDESC,'
      'Round(PRICE1 + (PRICE1 * tc.Rate), 2) AS PriceInc,'
      'Discontinued as Discontinued,'
      'PartType as PartType,'
      'CUSTFLD1 as CUSTFLD1,'
      'CUSTFLD2 as CUSTFLD2,'
      'CUSTFLD3 as CUSTFLD3,'
      'CUSTFLD4 as CUSTFLD4,'
      'CUSTFLD5 as CUSTFLD5,'
      'CUSTFLD6 as CUSTFLD6,'
      'CUSTFLD7 as CUSTFLD7,'
      'CUSTFLD8 as CUSTFLD8,'
      'CUSTFLD9 as CUSTFLD9,'
      'CUSTFLD10 as CUSTFLD10,'
      'CUSTFLD11 as CUSTFLD11,'
      'CUSTFLD12 as CUSTFLD12,'
      'CUSTFLD13 as CUSTFLD13,'
      'CUSTFLD14 as CUSTFLD14,'
      'CUSTFLD15 as CUSTFLD15,'
      'CUSTDATE1 as CUSTDATE1,'
      'CUSTDATE2 as CUSTDATE2,'
      'CUSTDATE3 as CUSTDATE3,'
      'P.BARCODE as BARCODE,'
      'P.active as Active,'
      'P.ProductionNotes as ProductionNotes,'
      'P.GeneralNotes as GeneralNotes,'
      'P.IsSystemProduct as IsSystemProduct,'
      'P.Batch,'
      'P.Multiplebins,'
      'P.SNtracking,'
      'P.Area as Area,'
      'p.PREFEREDSUPP as PreferedSupp,'
      'TPQA.Qty as Qty,'
      'TPQA.UOMQty as UOMQty'
      'FROM tblequipment E'
      'inner join tblParts P on E.ProductID = P.partsId'
      'inner join tblproductclasses Pc on P.partsID = PC.ProductID'
      
        'LEFT JOIN tbltaxcodes AS tc ON (tc.Name = p.TAXCODE) AND (tc.Reg' +
        'ionID = '#39'1'#39')'
      
        ' Left join tmp_Hire_Admin_3C1163BA TPQA on TPQA.PartsId = P.Part' +
        'sID'
      
        'WHERE E.OnHire ="T" and  E.active ="T" and char_length(p.PARTNAM' +
        'E) > 0 AND not IsNull(p.PARTNAME)'
      'AND NOT ((PartType = '#39#39') OR (PartType = '#39#39'))'
      'AND ('#39'T'#39' ="T" or PC.ClassID = '#39'0'#39' )')
    OnCalcFields = qryMainCalcFields
