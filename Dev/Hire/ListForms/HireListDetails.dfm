  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'Distinct '
      '1 as Details,'
      'H.HireId as HireId,'
      'H.CustomerName as CustomerName, '
      'H.ShiptoAddress as ShiptoAddress, '
      'H.CreatedDate as CreatedDate, '
      'H.classname as classname, '
      'H.Notes  as Notes  , '
      'H.Terms as Terms ,'
      'H.employeeName as employeeName, '
      'HireStatus(H.Status)  as Status,'
      'NULL as AssetName ,'
      'NULL as AssetCode,'
      'NULL as ProductName,'
      'NULL as HireFrom,'
      'NULL as HireTo, '
      'ifnull(H.Salestotal,0) + ifnull(H.Hiretotal,0) as Amountinc,'
      'NULL as converted, '
      'NULL  as HireStatus,'
      'NULL as UOM,'
      'NULL as UOMQty'
      'from tblhire H'
      
        'inner join tblHireLines HL on H.hireId = HL.hireID and HL.Delete' +
        'd = "F"'
      'where H.CreatedDate  between :DateFrom and :DateTo'
      'and H.Deleted = "F"'
      'union all'
      'select '
      '2 as Details,'
      'H.HireId as HireId,'
      'H.CustomerName as CustomerName, '
      'H.ShiptoAddress as ShiptoAddress, '
      'H.CreatedDate as CreatedDate, '
      'H.classname as classname, '
      'H.Notes  as Notes  , '
      'H.Terms as Terms ,'
      'HL.EmployeeName as EmployeeName,'
      'HireStatus(H.Status)  as Status,'
      'FA.AssetName as AssetName,'
      'FA.AssetCode as AssetCode,'
      'NULL as ProductName,'
      'ifnull(HL.ActualHireFrom,HL.HireFrom) as HireFrom,'
      'ifnull(HL.ActualHireTo,HL.HireTo) as HireTo, '
      'HL.Lineamountinc as Amountinc,'
      'HL.converted as converted, '
      'HireStatus(HL.Status)  as HireStatus, '
      'NULL as UOM,'
      'NULL as UOMQty'
      'from tblhire H'
      
        'inner join tblHireLines HL on H.hireId = HL.hireID and HL.Delete' +
        'd = "F"'
      'inner join tblAssetHire AH on HL.AssetId = AH.AssetID'
      'inner join tblFixedAssets FA on HL.AssetId = FA.AssetID'
      'where H.CreatedDate  between :DateFrom and :DateTo'
      'and H.Deleted = "F"'
      'union all '
      'select '
      '3 as Details,'
      'H.HireId as HireId,'
      'H.CustomerName as CustomerName, '
      'H.ShiptoAddress as ShiptoAddress, '
      'H.CreatedDate as CreatedDate, '
      'H.classname as classname, '
      'H.Notes  as Notes  , '
      'H.Terms as Terms ,'
      'H.employeeName as employeeName, '
      'HireStatus(H.Status)  as Status,'
      'NULL as AssetName ,'
      'NULL as AssetCode,'
      'HS.ProductName as ProductNAme,'
      'NULL as HireFrom,'
      'NULL as HireTo, '
      'HS.LineTotalInc as Amountinc,'
      'HS.converted as converted, '
      'NULL as HireStatus,'
      
        'Concat(HS.UnitofMeasure,'#39' ('#39',HS.UnitofMeasureMultiplier , '#39')'#39') a' +
        's UOM,'
      'HS.UOMQty as UOMQty'
      'from tblhire H'
      'inner join tblhirepartssale HS on H.hireId = HS.hireID'
      'where H.CreatedDate  between :DateFrom and :DateTo'
      'and H.Deleted = "F"'
      ''
      'Order by HireId desc, details')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end>
