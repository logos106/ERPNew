  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'distinct '
      'O.OrderID as OrderID,'
      'O.Createdtime as Createdtime,'
      'O.OrderStatus as OrderStatus,'
      'O.Shippedtime as Shippedtime,'
      'O.Totalcurcode as Totalcurcode, '
      'O.TotalAmt as TotalAmt,'
      'O.paidAmt as paidAmt, '
      'O.EbayAccountname as EbayAccountname,'
      'convert(ifnull(O.salesorderID,0), SIGNED)  as salesorderID , '
      'ifnull(SA.Name_ , :CashCustomer) as CustomerName,'
      
        'convert((Select min(inv.saleId) from tblsales inv inner join tbl' +
        'sales so on inv.salesorderglobalref = so.GlobalRef where so.Sale' +
        'ID = O.salesorderID ), SIGNED)   as InvoiceID'
      'from tblebay_orders O'
      'left join tblebay_orders_shippingaddress SA on O.ID = SA.OrderID'
      'Where  Createdtime between :DateFrom and :Dateto')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'CashCustomer'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'Dateto'
      end>
