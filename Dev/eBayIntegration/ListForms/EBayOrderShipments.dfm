      'SPML.Active AS ShiplistActive,'
      'SPML.Status AS STATUS,'
      'TSSL.UOMQty AS ShipQty,'
      
        'if (SPML.mstimestamp >= :DateFrom OR TSSL.mstimestamp >= :DateFr' +
        'om , "F" , "T") Synched'
      'from tblSales S '
      
        'inner join tblebay_orders EBS on S.saleId = EBS.SalesOrderId  an' +
        'd S.IsSalesOrder= "T" AND (S.IsCashSale="F" OR S.IsLayByTOS= "T"' +
        ') '
      'INNER JOIN tblshipmentlist SPML ON S.saleId =  SPML.SaleID '
      
        'INNER JOIN tblShipment'#9'SHP'#9' ON SPML.ShipmentListId=SHP.shipmentL' +
        'istID'
      
        'INNER JOIN tblshipmentsaleline TSSL ON SHP.ShipmentId = TSSL.Shi' +
        'pmentId AND TSSL.UOMQty<>0'
      'Where s.deleted ="F" '
      'ORDER BY TIMESTAMPtofilter')
