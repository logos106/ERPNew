      'S.terms              AS terms, '
      'S.Comments           AS Comments, '
      'S.Class              AS ClassName, '
      'S.dueDate            AS dueDate, '
      'ReplaceNewLine(S.ShipTo,"") as ShipTo , '
      'If(IsNull(S.ClientSignature),"F","T") AS HasSignature ,'
      
        'if (SHP.mstimestamp<TSSL.mstimestamp , SHP.mstimestamp , TSSL.ms' +
        'timestamp ) AS TIMESTAMPtofilter,'
      'SHP.ShipmentId as ShipmentId,'
      'SHP.ShipAddressId as ShipAddressId,'
