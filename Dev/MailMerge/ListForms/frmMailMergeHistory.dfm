  object qryRecipientLookup: TERPQuery
    Connection = MyConnection1
    SQL.Strings = (
      
        'Select  MailMergeID, RecipientId,MMGroup,EmployeeName as Recipie' +
        'ntName'
      
        'from tblmailmerge inner join tblemployees on employeeID= Recipie' +
        'ntID'
      'and MMGroup = "Employees"'
      ' union'
      
        'Select  MailMergeID, RecipientId,MMGroup,Company  as RecipientNa' +
        'me'
      'from tblmailmerge inner join tblclients on ClientID= RecipientID'
      
        'and ((MMGroup = "Suppliers") or (MMGroup = "Customers") or (MMGr' +
        'oup = "OtherContacts") or (MMGroup = "Loyalty"))'
      'union'
      
        'Select  MailMergeID, RecipientId,MMGroup,Company  as RecipientNa' +
        'me'
      
        'from tblmailmerge inner join tblmarketingleads on LeadID= Recipi' +
        'entID'
      'and MMGroup = "Marketing"')
    Left = 384
    Top = 200
  end
