  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select '
      'Cast(PQAD.PQADetailID  as Signed) as PQADetailID,'
      
        'Cast(if(PQAD.pqatype ='#39'Batch'#39' ,  PQAD.PQADetailID ,if(PQAp1.pqat' +
        'ype ='#39'Batch'#39' ,  PQAp1.PQADetailID ,if(PQAp2.pqatype ='#39'Batch'#39' ,  ' +
        'PQAp2.PQADetailID ,null)))  as Signed)  as PQABAtchId, '
      
        'Cast(if(PQAD.pqatype ='#39'Bin'#39' ,  PQAD.PQADetailID ,if(PQAp1.pqatyp' +
        'e ='#39'Bin'#39' ,  PQAp1.PQADetailID , null) )  as Signed) as PQABinID,' +
        ' '
      
        'Cast(if(PQAD.pqatype ='#39'SN'#39' ,  PQAD.PQADetailID ,NULL)   as Signe' +
        'd) as PQASNID, '
      ''
      
        'cast(if(P.SNTracking ="T" , "SN" , if(P.MultipleBins="T" , "Bin"' +
        ' ,if(P.batch="T" , "Batch" , Null) )) as char(10))  ProductType,' +
        ' '
      'cast(PQAD.pqatype as char(10))  as pqatype,'
      
        'if(PQAD.pqatype ='#39'Batch'#39' ,  PQAD.Value ,if(PQAp1.pqatype ='#39'Batch' +
        #39' ,  PQAp1.Value ,if(PQAp2.pqatype ='#39'Batch'#39' ,  PQAp2.Value ,null' +
        ')))  as BAtchno, '
      
        'if(PQAD.pqatype ='#39'Batch'#39' ,  PQAD.Expirydate ,if(PQAp1.pqatype ='#39 +
        'Batch'#39' ,  PQAp1.Expirydate ,if(PQAp2.pqatype ='#39'Batch'#39' ,  PQAp2.E' +
        'xpirydate ,null)))  as Expirydate, '
      
        'if(PQAD.pqatype ='#39'Batch'#39' ,  PQAD.UOMQty ,if(PQAp1.pqatype ='#39'Batc' +
        'h'#39' ,  PQAp1.UOMQty ,if(PQAp2.pqatype ='#39'Batch'#39' ,  PQAp2.UOMQty ,n' +
        'ull)))  as UOMBatchQty, '
      'if(PQAD.pqatype ='#39'Bin'#39' ,  PQAD.UOMQty , null) UOMBinQTY,'
      
        'if(PQAD.pqatype ='#39'Batch'#39' ,  PQAD.Qty ,if(PQAp1.pqatype ='#39'Batch'#39' ' +
        ',  PQAp1.Qty ,if(PQAp2.pqatype ='#39'Batch'#39' ,  PQAp2.Qty ,null)))  a' +
        's BatchQty, '
      'if(PQAD.pqatype ='#39'Bin'#39' ,  PQAD.Qty , null) BinQTY,'
      'PB.BinLocation,PB.BinNumber,'
      'PB.BinID, '
      'if(PQAD.pqatype ='#39'SN'#39' ,  PQAD.Value ,NULL)  as Serialno '
      'from tblpurchaselines POL'
      'inner join tblparts P on P.partsId = POL.ProductID'
      
        'inner join tblpqa PQA on POL.PurchaseLineID = PQa.translineID an' +
        'd POL.PurchaseOrderID = PQA.TransID and PQA.TransType = '#39'TPurcha' +
        'seOrderLine'#39
      'inner join tblpqadetails PQAD on PQA.PQAID = PQAD.PQAID'
      
        'Left join tblpqadetails PQAP1 on PQAD.parentREf = PQAP1.globalre' +
        'f'
      
        'Left join tblpqadetails PQAP2 on PQAP1.parentREf = PQAP2.globalr' +
        'ef'
      
        'Left join tblproductbin PB on (PQAD.PQAType ='#39'Bin'#39' and  PQAD.bin' +
        'id = PB.BinID ) or (PQAP1.PQAType ='#39'Bin'#39' and  PQAP1.binid = PB.B' +
        'inID ) '
      
        'inner join tblproctree PT on PT.ProctreeId = POL.proctreeId and ' +
        'POL.unitofmeasureID = PT.TreePartUOMID'
      'where PT.proctreeId   = :ProctreeID'
      'and PQA.active ="T"'
      
        'order by CONCAT(IFNULL(pqap2.GLOBALREF,'#39#39') , IFNULL(pqap1.GLOBAL' +
        'REF ,'#39#39'), IFNULL(pqad.GLOBALREF,'#39#39'))')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ProctreeID'
      end>
