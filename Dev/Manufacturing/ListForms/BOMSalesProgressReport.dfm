  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      '0 AS Seqno,'
      'S.saleID as saleID , '
      'SL.ProductID as ProductID,'
      'SL.productname as productname, '
      'PT.SequenceDown as SequenceDown, '
      'TreenodeCaption(PT.level, PT.caption) as Caption,'
      'PT.totalQty as totalQty,'
      'NULL AS PQAID,'
      'null as transtype, '
      'NULL AS CTranstype,'
      'null as alloctype,'
      'PT.totalQty as Qty, '
      'null as pqatype, '
      'null as value,'
      'PT.proctreeId as proctreeId, '
      'null AS Inqty,'
      'null AS Outqty'
      'FROM tblsales S'
      'INNER JOIN tblsaleslines SL ON S.saleId = SL.saleId'
      
        'INNER JOIN tblproctree PT ON PT.MasterId = SL.SaleLineID AND PT.' +
        'MasterType <> "mtProduct" '
      
        'Where SL.salelineId = :SaleLineID and (:ProctreeID =0 or PT.Proc' +
        'treeID = :ProctreeID)'
      'UNION '
      'SELECT '
      '1 AS Seqno,'
      'S.saleID as saleID , '
      'SL.ProductID as ProductID,'
      'SL.productname as productname, '
      'PT.SequenceDown as SequenceDown, '
      'TreenodeCaption(PT.level, PT.caption) as Caption,'
      'PT.totalQty as totalQty,'
      'PQA.pqaid as pqaid,'
      
        'PQA.transtype,PQAAllocType(PQA.AllocType,PQA.Transtype,PQA.Qty,0' +
        ','#39#39','#39#39') as CTranstype,'
      'PQA.alloctype as alloctype,'
      'PQA.Qty as Qty, '
      'pqad.pqatype as pqatype, '
      'pqad.value as value,'
      'PT.proctreeId as proctreeId, '
      'if(PQA.alloctype='#39'IN'#39' , pqad.Qty , 0) AS Inqty,'
      'if(PQA.alloctype='#39'OUT'#39' , pqad.Qty , 0) AS Outqty'
      'FROM tblsales S'
      'INNER JOIN tblsaleslines SL ON S.saleId = SL.saleId'
      
        'INNER JOIN tblproctree PT ON PT.MasterId = SL.SaleLineID AND PT.' +
        'MasterType <> "mtProduct"'
      'INNER JOIN tblproctreepart PTP ON PT.ProcTreeId = PTP.ProcTreeId'
      
        'INNER JOIN tblpqa pqa ON pqa.TransLineID = PTP.ProcTreePartId AN' +
        'D pqa.TransType = '#39'TProcTreePart'#39
      'LEft JOIN tblpqadetails pqad ON pqa.pqaid = pqad.pqaid'
      
        'Where SL.salelineId = :SaleLineID and (:ProctreeID =0 or PT.Proc' +
        'treeID = :ProctreeID)'
      'UNION '
      'SELECT '
      '2 AS Seqno,'
      'S.saleID as saleID , '
      'SL.ProductID as ProductID,'
      'SL.productname as productname, '
      'PT.SequenceDown as SequenceDown, '
      'TreenodeCaption(PT.level, PT.caption) as Caption,'
      'PT.totalQty as totalQty,'
      'PQA.pqaid as pqaid,'
      
        'PQA.transtype,PQAAllocType(PQA.AllocType,PQA.Transtype,PQA.Qty,0' +
        ','#39#39','#39#39') as CTranstype,'
      'PQA.alloctype as alloctype,'
      'PQA.Qty as Qty, '
      'pqad.pqatype as pqatype, '
      'pqad.value as value,'
      'PT.proctreeId as proctreeId, '
      'if(PQA.alloctype='#39'IN'#39' , pqad.Qty , 0) AS Inqty,'
      'if(PQA.alloctype='#39'OUT'#39' , pqad.Qty , 0) AS Outqty'
      'FROM tblsales S'
      'INNER JOIN tblsaleslines SL ON S.saleId = SL.saleId'
      
        'INNER JOIN tblproctree PT ON PT.MasterId = SL.SaleLineID AND PT.' +
        'MasterType <> "mtProduct" AND IFNULL(PT.ParentId,0)=0'
      
        'INNER JOIN  tblpqa pqa ON pqa.translineId = sl.salelineId AND pq' +
        'a.transtype ='#39'TSalesOrderLine'#39
      'LEft JOIN tblpqadetails pqad ON pqa.pqaid = pqad.pqaid'
      
        'Where SL.salelineId = :SaleLineID and (:ProctreeID =0 or PT.Proc' +
        'treeID = :ProctreeID)'
      'UNION '
      'SELECT '
      '3 AS Seqno,'
