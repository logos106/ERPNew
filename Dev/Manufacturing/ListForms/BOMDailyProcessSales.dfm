  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      ' S.saleID            AS saleID         , '
      ' if(Status =  '#39'psNotScheduled'#39', "F","T") as Scheduled,'
      ' sl.ProductID        AS ProductID      , '
      ' sl.SaleLineId       AS SaleLineId     , '
      ' sl.ProductName      AS ProductName    , '
      ' S.saledate          AS saledate       , '
      ' Sum(sl.Shipped)     AS Shipped        , '
      ' prpr.seqno          AS seqno          , '
      ' PS.Description      AS Processstep    , '
      ' PRPR.ProcresourcedetailID as ProcresourcedetailID    , '
      ' PTP.Proctreepartid  AS Proctreepartid , '
      ' PT.proctreeid       AS proctreeid       '
      ' FROM tblsales s '
      
        ' inner join tblsaleslines           SL    ON s.saleId           ' +
        '= SL.saleId'
      
        ' INNER JOIN tblproctree             PT    ON PT.MasterId        ' +
        '= SL.SaleLineID AND PT.MasterType <> "mtProduct" AND PT.proctree' +
        'id = PT.TreeRootId '
      
        ' INNER JOIN tblprocesspart          PP    ON PT.ProcTreeId      ' +
        '= PP.ProcTreeId '
      
        ' INNER JOIN tblproctreepart         PTP   ON PTP.ProcTreeId     ' +
        '= PT.ProcTreeId '
      
        ' INNER JOIN tblprocessstep          PS    ON PS.ID              ' +
        '= PP.ProcessStepID '
      
        ' INNER JOIN tblprocresourceprocess  PRP   ON PP.ProcessStepID   ' +
        '= PRP.ProcessStepId '
      
        ' INNER JOIN tblprocresource         PR    ON PRP.ProcResourceId ' +
        '= PR.ProcResourceId  and PR.UsedWithProductsInSeq ="T"'
      
        ' INNER JOIN tblprocresourcedetails  PRD   ON PR.ProcResourceId  ' +
        '= PRD.ProcResourceId '
      
        ' INNER JOIN tblprocresourceproducts PRPR  ON PRPR.Procresourcede' +
        'tailID = PRD.ID AND SL.ProductID = PRPR.ProductId '
      ' WHERE S.issalesorder ="T" and S.COnverted ="F" '
      'Group by sl.salelineId, S.SaleDate, SL.ProductID'
      'ORDER BY SaleDate DESC, Productname')
    OnCalcFields = qryMainCalcFields
