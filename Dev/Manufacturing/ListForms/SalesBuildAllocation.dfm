  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'TreeNodeCaption(PT.Level, PT.Caption) as TreeNodeCaption,'
      'PT.proctreeid as proctreeid, '
      'pqabatch.value AS batchno,'
      'pqabatch.expirydate as expirydate,'
      'PBin.binlocation as binlocation,'
      'PBin.binnumber as binnumber,'
      'pqasn.value AS serialnumebr,'
      'PT.SequenceDown as SequenceDown,'
      'PQA.alloctype as alloctype,'
      'pqa.pqaid as pqaid,'
      
        'sum(if(ifnull(PQASN.PQADEtailID,0)<>0 , PQASN.UOMQty ,    if(ifn' +
        'ull(PQABins.PQADEtailID,0)<>0 , PQABins.UOMQty ,     if(ifnull(P' +
        'QABatch.UOMQty,0)<>0 , PQABatch.uomQty , PQA.uomQty)))) UOMQty'
      'FROM tblsales S'
      'INNER JOIN tblsaleslines SL ON S.saleId = SL.saleID'
      
        'INNER JOIN tblproctree PT ON SL.saleLineID = PT.masterId AND PT.' +
        'masterType <> '#39'mtProduct'#39
      
        'INNER JOIN tblproctreepart ptp ON pt.proctreeid = ptp.proctreeid' +
        ' '
      
        'INNER JOIN tblpqa pqa ON pqa.TransLineID = ptp.ProcTreePartId AN' +
        'D pqa.TransType IN ('#39'TProcTreePart'#39' )'
      'INNER JOIN tblparts P ON PQA.productID = P.partsId'
      
        'left join `tblPqaDetails` PQABatch  on PQABatch.PQAId '#9'= pqa.pqa' +
        'id and PQABatch.PQAType = "Batch"  and PQABatch.Active= "T"'
      
        'left join `tblPqaDetails` PQABins  on PQABins.PQAId '#9'= pqa.pqaid' +
        ' '
      'and PQABins.PQAType = "Bin" and PQABins.Active = "T" and  '
      
        '( (PQABatch.globalref = IFNULL(PQABins.ParentRef,"") and P.Batch' +
        '="T" and P.multiplebins = "T")'
      
        '         or (ifnull(PQABins.Parentref ,"") = "" AND P.Batch="F" ' +
        'and P.multiplebins = "T"))'
      
        'Left join `tblProductBin` PBin     on PBin.binId '#9'= PQABins.BinI' +
        'D'
      ''
      
        'left join `tblPqaDetails` PQASN    on (PQASN.PQAId '#9'= pqa.pqaid ' +
        'and PQASN.PQAType = "SN"   and PQASN.Active = "T" and PQASN.Qty ' +
        '<> 0'
      
        'and ((PQABatch.globalref = IFNULL(PQASN.ParentRef,"") and P.Batc' +
        'h="T" and P.multiplebins = "F" and P.SNTracking = "T")'
      
        '                          or (PQAbins.Globalref = IFNULL(PQASn.P' +
        'arentRef,"") and P.Multiplebins = "T" and P.SNTracking = "T") '
      
        '                          or ((ifnull(PQASN.Parentref ,"") = "")' +
        ' and P.Batch="F" and P.Multiplebins = "F" and P.SNTracking = "T"' +
        ')))'
      'WHERE SL.saleLineId = :saleLineId'
      
        'GROUP BY PT.proctreeId , PQAbatch.value , pqabins.value , pqasn.' +
        'value'
      'UNION'
      'SELECT '
      'TreeNodeCaption(PT.Level, PT.Caption) as TreeNodeCaption,'
      'PT.proctreeid as proctreeid, '
      'pqabatch.value AS batchno,'
      'pqabatch.expirydate as expirydate,'
      'PBin.binlocation as binlocation,'
      'PBin.binnumber as binnumber,'
      'pqasn.value AS serialnumebr,'
      'PT.SequenceDown as SequenceDown,'
      'PQA.alloctype as alloctype,'
      'pqa.pqaid as pqaid,'
      
        'sum(if(ifnull(PQASN.PQADEtailID,0)<>0 , PQASN.UOMQty ,    if(ifn' +
        'ull(PQABins.PQADEtailID,0)<>0 , PQABins.UOMQty ,     if(ifnull(P' +
        'QABatch.UOMQty,0)<>0 , PQABatch.uomQty , PQA.uomQty)))) UOMQty'
      'FROM tblsales S'
      'INNER JOIN tblsaleslines SL ON S.saleId = SL.saleID'
      
        'INNER JOIN tblproctree PT ON SL.saleLineID = PT.masterId AND PT.' +
        'masterType <> '#39'mtProduct'#39
      
        'INNER JOIN tblproctreepart ptp ON pt.proctreeid = ptp.proctreeid' +
        ' '
      
        'INNER JOIN tblpqa pqa ON pqa.TransLineID = ptp.ProcTreePartId AN' +
        'D pqa.TransType IN ( '#39'TProcTreePartIN'#39')'
      'INNER JOIN tblparts P ON PQA.productID = P.partsId'
      
