  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select'#9#9#9
      's.SaleID as SaleID'#9','
      'SL.saleLineId as saleLineId'#9','
      'SL.ProductName  as ProductName '#9','
      'S.CustomerName  as CustomerName '#9','
      'S.SaleDate as SaleDate'#9','
      'S.sAleDatetime as sAleDatetime'#9','
      'S.converted  as converted '#9','
      
        'if(S.IsInternalOrder='#39'T'#39' ,if(S.Converted='#39'T'#39' , "Finished Interna' +
        'l Order" , "InternalOrder") ,"Sales Order" ) as Category'#9','
      'PTI.Caption  as Caption'#9','
      
        'concat(PTI.TreePartUOM , '#39'('#39' , PTI.TreePartUOMMultiplier , '#39')'#39') ' +
        'as UOM'#9','
      'PTI.FromStockUOMQty '#9'as'#9'FromStockUOMQty '#9','
      'PTI.ManufactureUOMQty '#9'as'#9'ManufactureUOMQty '#9','
      'PTI.OnOrderUOMQty as OnOrderUOMQty'#9','
      'PTI.ManufacturedQty as BuiltQty ,'
      'PTI.TotalQty - PTI.ManufacturedQty as ToBeBuildQty '#9','
      'PTI.Quantity as Quantity,'
      'PTI.TotalQty as  TotalQty,'
      'PTI.ProductUnitPrice as ProductUnitPrice , '
      'PTI.ProductUnitCost as ProductUnitCost,'
      'PTI.TotalQty *PTI.ProductUnitPrice  as TotalPrice,'
      'PTI.TotalQty *PTI.ProductUnitCost  as Totalcost,'
      'PTI.Quantity *PTI.ProductUnitPrice  as Price,'
      'PTI.Quantity *PTI.ProductUnitCost  as cost,'
      'fe.FormulaName as FormulaName'#9','
      'DescribeFormula(fe.Formula) AS descFormula ,'
      'PTI.FormulaQtyValue1 as FormulaQtyValue1'#9','
      'PTI.FormulaQtyValue2 as FormulaQtyValue2'#9','
      'PTI.FormulaQtyValue3 as FormulaQtyValue3'#9','
      'PTI.FormulaQtyValue4 as FormulaQtyValue4'#9','
      'PTI.FormulaQtyValue5 as FormulaQtyValue5'#9','
      'convert(PTI.info , char(255)) as info,'
      'convert(PT.caption, char(255)) as Detailedcaption'#9
      'from tblsales S'
      'inner join tblsaleslines SL on S.SAleId = SL.SaleID'
      
        'inner join tblproctree PT on PT.MasterId = SL.SaleLineId and PT.' +
        'MasterType <> '#39'mtProduct'#39' and parentId =0'
      'inner join tblproctree PTI on PT.ProcTreeId = PTI.ParentId'
      'lEFT JOIN tblfeformula fe ON PTI.Formulaid = fe.FormulaID'
      
        'where (S.Converted ='#39'F'#39' or S.IsInternalOrder = '#39'T'#39') and S.IsSale' +
        'sorder = '#39'T'#39
      'and S.SaleDate between :dateFrom and :dateTo'
      'order by SaleId desc')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'dateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'dateTo'
      end>
  end
