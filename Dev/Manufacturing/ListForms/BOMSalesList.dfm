  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select'
      
        's.SaleID, SL.saleLineId,  SL.ProductName , S.CustomerName , S.Sa' +
        'leDate,'
      'S.sAleDatetime,'
      'S.converted ,'
      
        'if(S.IsQuote='#39'T'#39', '#39'Quote'#39' , if(S.IsInvoice='#39'T'#39' , '#39'Invoice'#39' , if(' +
        'S.IsSalesOrder='#39'T'#39', if(S.IsInternalOrder='#39'T'#39' ,if(S.Converted='#39'T'#39 +
        ' , "Finished Internal Order" , "InternalOrder") ,"Sales Order" )' +
        ',""))) Category, '
      
        'concat(PT.TreePartUOM , '#39'('#39' , PT.TreePartUOMMultiplier , '#39')'#39') as' +
        ' UOM, '
      'PT.FromStockUOMQty , PT.ManufactureUOMQty , PT.OnOrderUOMQty, '
      'SL.BOMProductionLine , '
      'SL.BOMGroupedLine,'
      'PT.ManufacturedQty as BuiltQty,'
      'PT.TotalQty - PT.ManufacturedQty as ToBeBuildQty, '
      
        '(Select group_Concat(distinct PTP.status separator '#39', '#39') from tb' +
        'lprocesspart PTP  inner join tblproctree PTs on PTP.proctreeId= ' +
        'PTs.proctreeId where PTS.masterId = Sl.sALeLineID and PTs.Master' +
        'Type = PT.MasterType) status'
      'from tblsales S'
      'inner join tblsaleslines SL on S.SAleId = SL.SaleID'
      
        'inner join tblproctree PT on PT.MasterId = SL.SaleLineId and PT.' +
        'MasterType <> '#39'mtProduct'#39' and parentId =0'
      
        'where (S.Converted ='#39'F'#39' or S.IsInternalOrder = '#39'T'#39') and S.IsSale' +
        'sorder = '#39'T'#39
      'and S.SaleDate between :dateFrom and :dateTo'
      'order by SaleId desc')
    OnCalcFields = qryMainCalcFields
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'dateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'dateTo'
      end>
