  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select'
      
        's.SaleID, s.InvoiceDocNumber, s.SaleDate, s.CustomerName, s.Clie' +
        'ntPrintName as CustomerPrintName, s.ClientID as CustomerID,'
      'sl.ProductName, sl.ProductPrintName, sl.ProductID, sl.MemoLine,'
      
        'snd.SerialNumber, snd.BatchNumber, snd.BatchProductName, snd.Bat' +
        'chProductID, snd.BatchQty'
      'from tblsales s'
      'inner join tblsaleslines sl on sl.SaleID = s.SaleID'
      
        'inner join tblpqa pqa on pqa.TransLineID = sl.SaleLineID and pqa' +
        '.TransID = sl.SaleID and pqa.TransType = "TInvoiceLine"'
      
        'inner join tblpqadetails pqad on pqad.PQAType = "SN" and pqad.PQ' +
        'AID = pqa.PQAID'
      'inner join'
      '(select s.SaleID  , sl.SaleLineID, sl.ProductName, sl.ProductID,'
      'pqad.Value as BatchNumber, sum(pqad.Qty) as BatchQty,'
      
        'ptp.ProductName as BatchProductName, ptp.ProductId as BatchProdu' +
        'ctID, pt.ParentId, ppg.InProcProgressId, pqadin.Value as SerialN' +
        'umber'
      'from tblsales s'
      'inner join tblsaleslines sl on s.saleId = sl.saleId'
      
        'inner join tblproctree pt on sl.salelineId = pt.masterId and pt.' +
        'mastertype <> "mtProduct"'
      'inner join tblproctreepart ptp on pt.proctreeid = ptp.proctreeid'
      
        'inner join tblprocprogress ppg on ptp.ProcTreePartId = ppg.ProcT' +
        'reePartID and ppg.Alloctype = "OUT"'
      
        'inner join tblpqa pqa on pqa.TransLineID = ppg.ProcProgressID  a' +
        'nd pqa.TransType = "TProcProgressOUT"'
      
        'inner join tblpqadetails pqad on pqad.PQAID = pqa.PQAID and pqad' +
        '.PQAType = "Batch"'
      
        'inner join tblprocprogress ppgin on ppg.InProcProgressId = ppgin' +
        '.ProcProgressID'
      
        'inner join tblpqa pqain on pqain.TransLineID = ppgin.ProcProgres' +
        'sID and pqain.TransType = "TProcProgressIN"'
      
        'inner join tblpqadetails pqadin on pqadin.PQAID = pqain.PQAID an' +
        'd pqadin.PQAType ="SN"'
      'where s.IsSalesOrder = "T"'
      'and s.SaleDate between '#39'2020-03-06'#39' and '#39'2021-03-11'#39
      
        'group by ppg.ProcProgressID, pqad.Value) as snd on snd.SerialNum' +
        'ber = pqad.Value')
