  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      '1 as Details,'
      'Id as ID,'
      'Description as Description , '
      'Active as Active,'
      'null as EmployeeId, '
      'Null as EmployeeName,'
      'null EmpProcessStepsID'
      ' from tblprocessstep'
      ' union all '
      'select '
      '2 as Details,'
      'PS.ID as Id,'
      'PS.Description as Description,'
      'PS.active as Active,'
      'EPS.employeeId as EmployeeId,'
      'EPS.EmployeeName as EmployeeName, '
      'EPS.EmpProcessStepsID as EmpProcessStepsID'
      
        ' from tblprocessstep PS Left join tblemployeeprocesssteps EPS on' +
        ' PS.ID = EPS.ProcessStepID'
      'Order by  Description, Details,EmployeeName')
