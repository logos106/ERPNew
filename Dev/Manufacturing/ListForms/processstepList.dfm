  object QryEmpProcessStep: TERPQuery
    Connection = MyConnection1
    SQL.Strings = (
      'select '
      'PP.*'
      'from tblprocesspart PP '
      'inner join tblprocesstime PT on PP.ID = PT.ProcessPartId'
      'where PP.ProcessStepID = :PSID and PT.EmployeeId= :EMPID'
      'limit 1')
    Left = 80
    Top = 417
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'PSID'
      end
      item
        DataType = ftUnknown
        Name = 'EMPID'
      end>
  end
  object QryUsedPS: TERPQuery
    Connection = MyConnection1
    SQL.Strings = (
      'select '
      'sl.saleId , '
      'PT.masterId, '
      
        'convert(concat(if(PT.mastertype ="mtProduct" , "Product # " , "S' +
        'ale #"),   if(PT.mastertype ="mtProduct" , "" , SL.saleId),   ":' +
        '" , RPT.caption, "." , PT.caption ), char(255)) as Caption  , '
      
        'E.employeeName , PS.Description,  PST.TimeStart , FormatSecondsT' +
        'otime(PST.Duration) Duration , PS.Active '
      
        'from  tblProctree PT Left join  tblProctree RPT on PT.TreeRootId' +
        ' = RPT.proctreeid  left join tblsaleslines sl on sl.salelineId =' +
        ' PT.masterId  inner join tblprocesspart PP  on PT.proctreeId = P' +
        'P.proctreeId  inner join tblprocesstime PST on PP.ID = PST.Proce' +
        'ssPartId  inner join tblEmployees E on E.EmployeeId = PST.Employ' +
        'eeId  inner join tblprocessstep PS on PS.ID = PP.ProcessStepID '
      ' Order by description, employeeName'
      'Limit 1')
    FilterOptions = [foCaseInsensitive, foNoPartialCompare]
    Left = 171
    Top = 456
    object QryUsedPSDescription: TWideStringField
