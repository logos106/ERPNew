  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select  '
      
        'convert( @rownum:=if(@SaleLineId <> Sl.SaleLineId,  @rownum:= 1 ' +
        ',  @rownum+1) , Signed INTEGER) as rownum , '
      ' @SaleLineId := sl.SaleLineId,'
      'S.saleId as Saleid, '
      
        'If(S.IsCashSale="T" And S.IsPOS="F", "Cash Sale",If(S.IsRefund="' +
        'T"     , "Refund",If(S.ISSalesOrder="T" , "Sales Order",If(S.IsQ' +
        'uote="T"      , "Quote",If(S.IsCashSale="T" And S.IsPOS="T", if(' +
        ' S.IsLaybyPayment="T" ,if(  S.TotalAmountInc <0 , "Layby Reverse' +
        ' Transaction ","Layby Payment" ), "POS"),If(S.Islayby="T"    And' +
        ' S.IsPOS="T", "Layby",If(S.IsInvoice="T"    , "Invoice", '#39'Unknow' +
        'n'#39' )))))))  as Saletye,'
      'S.converted as converted, '
      'S.CustomerName as CustomerName, '
      'SL.BOMComments as BOMComments, '
      'SL.Product_Description as Product_Description, '
      'Sl.saleLineId as saleLineId, '
      'S.saleDate as SaleDate, '
      'SL.ProductName as SaleProduct, '
      'S.InvoiceDocNumber as InvoiceDocNumber, '
      'SL.UnitofMeasureQtySold as UnitofMeasureQtySold, '
      'SL.UnitofMeasureShipped as UnitofMeasureShipped, '
      'SL.UnitofMeasureBackorder as UnitofMeasureBackorder, '
      
        'concat (SL.UnitofMeasureSaleLines , "(" , SL.UnitofMeasureMultip' +
        'lier , ")") as SalesUOM, '
      
        'convert(TreeNodeCaption(PT.Level, PT.caption ), char(255)) as Ca' +
        'ption, '
      'FE.FormulaName as FormulaName, '
      'FE.Formula as Formula, '
      'DescribeFormula(FE.Formula) as UFormula, '
      'PT.TreePartUOMQuantity as Quantity, '
      'PT.TreePartUOMTotalQty as TotalQty, '
      'PT.FormulaQtyValue1 as FormulaQtyValue1, '
      'PT.FormulaQtyValue2 as FormulaQtyValue2, '
      'PT.FormulaQtyValue3 as FormulaQtyValue3, '
      'PT.FormulaQtyValue4 as FormulaQtyValue4, '
      'PT.FormulaQtyValue5 as FormulaQtyValue5, '
      'PT.FormulaQtyValue as FormulaQtyValue, '
      'PT.BuildFormulaQtyValue1 as BuildFormulaQtyValue1, '
      'PT.BuildFormulaQtyValue2 as BuildFormulaQtyValue2, '
      'PT.BuildFormulaQtyValue3 as BuildFormulaQtyValue3, '
      'PT.BuildFormulaQtyValue4 as BuildFormulaQtyValue4, '
      'PT.BuildFormulaQtyValue5 as BuildFormulaQtyValue5, '
      'PT.BuildFormulaQtyValue as BuildFormulaQtyValue, '
      'PT.OrderFormulaQtyValue1 as OrderFormulaQtyValue1, '
      'PT.OrderFormulaQtyValue2 as OrderFormulaQtyValue2, '
      'PT.OrderFormulaQtyValue3 as OrderFormulaQtyValue3, '
      'PT.OrderFormulaQtyValue4 as OrderFormulaQtyValue4, '
      'PT.OrderFormulaQtyValue5 as OrderFormulaQtyValue5, '
      'PT.OrderFormulaQtyValue as OrderFormulaQtyValue, '
      'BOMTypetoStr(PT.PartSource) as PartSource, '
      'PT.selected as selected, '
      'InputTypetoStr(Pt.InputType) as InputType, '
      'PT.Price as PriceinTree, '
      'PT.Cost as CostinTree, '
      
        'concat(PT.TreePartUOM,"(", PT.TreePartUOMMultiplier,")") as Tree' +
        'UOM, '
      'convert(PT.info , char(255)) as info, '
      
        'InputTypetoStr(ifnull(ParentPt.InputType,"itNone")) as parentInp' +
        'utType '
      'FROM '
      ' (SELECT @rownum:=0) rno , '
      ' (SELECT @SaleLineId:=0) slid, '
      'tblsales S '
      'inner join tblsaleslines SL on S.saleId = Sl.saleId '
      
        'inner join tblProcTree PT   on PT.MasterId = Sl.saleLineid and P' +
        'T.Mastertype <> "mtProduct" '
      
        'inner join tblProcTree ParentPt on ParentPt.ProcTreeId = Pt.Pare' +
        'ntID '
      'Left join tblfeformula FE on FE.FormulaID = PT.formulaid '
      
        'where S.IsSalesOrder ="T" and ifnull(pt.parentID,0) <> 0 and S.s' +
        'aleDate Between '#39'11/05/2017'#39' and '#39'01/09/2018'#39
      
        'Order by S.saleId desc , SL.Productname ,SL.saleLineId,  PT.sequ' +
        'encedown')
