  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      '1 as Seqno, '
      'SL.SaleID, '
      'SL.SaleLineId,'
      'SL.productName , '
      
        'Convert(TreeNodeCaption(PT.Level, PT.caption ) ,char(255)) capti' +
        'on  ,  '
      'PT.BOMGroupedLine PTBOMGroupedLine ,'
      'PT.BOMProductionLine  PTBOMProductionLine, '
      'PTP.Qty PTPQty, '
      'PQA.Qty PQAQty, '
      'PQA.active  PQAactive, '
      'PTP.BOMGroupedLine PTPBOMGroupedLine, '
      'PTP.BOMProductionLine PTPBOMProductionLine,'
      'PQa.PQAID,'
      'PT.ProcTreeId,'
      'PTP.ProcTreePartId,'
      'PT.Sequencedown'
      'from tblsaleslines SL'
      
        'inner join tblproctree PT on SL.SaleLineID = PT.MasterId and PT.' +
        'MasterType <> "mtProduct"'
      'left join tblproctreepart PTP on PT.ProcTreeId = PTP.ProcTreeId'
      
        'left join tblpqa pqa on pqa.TransID = SL.SaleID and PTP.ProcTree' +
        'PartId = PQA.TransLineID           and PQA.TRanstype in ('#39'TProcT' +
        'reePart'#39' , '#39'TProcTreePartIN'#39')'
      'where (:SaleID=0 or SL.SaleID =:SaleID)'
      'and (:SaleLineId=0 or SL.SaleLineId =:SAleLineID)'
      'union all '
      'Select '
      '2 as Seqno,'
      'SL.SaleID, '
      'SL.SaleLineId,'
      'SL.productName , '
      'SL.Productname,  '
      'SL.BOMGroupedLine PTBOMGroupedLine ,'
      'SL.BOMProductionLine  PTBOMProductionLine, '
      'NULL, '
      'PQA.Qty PQAQty, '
      'PQA.active  PQAactive, '
      'NULL, '
      'NULL,'
      'PQa.PQAID,'
      '0,'
      '0,'
      '0'
      'from tblsaleslines SL'
      
        'inner join tblpqa pqa on pqa.TransID = SL.SaleID and SL.sAleLine' +
        'ID = PQA.TransLineID           and PQA.TRanstype in ('#39'Tsalesorde' +
        'rLine'#39' , '#39'TInvoiceline'#39')'
      'where (:SaleID=0 or SL.SaleID =:SaleID)'
      'and (:SaleLineId=0 or SL.SaleLineId =:SAleLineID)'
      'order by SaleLineId,seqno desc , Sequencedown')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'SaleID'
      end
      item
        DataType = ftUnknown
        Name = 'SaleID'
      end
      item
        DataType = ftUnknown
        Name = 'SaleLineId'
      end
      item
        DataType = ftUnknown
        Name = 'SAleLineID'
      end
      item
        DataType = ftUnknown
        Name = 'SaleID'
      end
      item
        DataType = ftUnknown
        Name = 'SaleID'
      end
      item
        DataType = ftUnknown
        Name = 'SaleLineId'
      end
      item
        DataType = ftUnknown
        Name = 'SAleLineID'
      end>
