  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'SOL.smartOrderId, '
      'SOL.SmartOrderLinesID,'
      'S.SaleDate, '
      'SL.shipdate,'
      'PT.ProcTreeId,'
      'S.SaleId,'
      'SL.SaleLineID, '
      'SL.ProductName as TreeProduct,'
      
        'Convert(TreeNodeCaption(PT.Level, PT.caption ) ,char(255)) Capti' +
        'on, '
      'SOL.SupplierName, '
      
        'concat(PT.TreePartUOM , if(PT.TreePartUOMMultiplier=1, '#39#39' , conc' +
        'at('#39'('#39' , PT.TreePartUOMMultiplier , '#39')'#39'))) as UOM, '
      'PT.TreePartUOMTotalQty, '
      'PT.FromStockUOMQty,'
      'PT.ManufactureUOMQty , '
      'PT.OnOrderUOMQty , '
      
        'concat(SOL.UnitofMeasure , if(SOL.UnitofMeasureMultiplier=1, '#39#39' ' +
        ', concat('#39'('#39' , SOL.UnitofMeasureMultiplier , '#39')'#39'))) as SOUOM, '
      'SOL.UnitofMeasureQty as Ordered,'
      
        'concat(POL.UnitofMeasurePOLines, if(POL.UnitofMeasureMultiplier=' +
        '1, '#39#39' , concat('#39'('#39' , POL.UnitofMeasureMultiplier, '#39')'#39'))) as POUO' +
        'M, '
      'POL.UnitofMeasureShipped  UnitofMeasureShipped,'
      
        'if(ifnull(POL.UnitofMeasureShipped,0) = 0 , POL.UnitofMeasureBac' +
        'korder , 0) UnitofMeasureBackorder , '
      'PT.TotalQty ,'
      'PT.FromStockQty, '
      'PT.ManufactureQty , '
      'PT.OnOrderQty , '
      'SOL.Qty , '
      'POL.shipped  shipped,'
      'if(ifnull(POL.shipped,0) = 0 , POL.Backorder, 0) Backorder,'
      'POL.purchaseOrderId'
      'From tblSales S '
      'inner join tblSaleslines SL on s.SAleId = Sl.SaleID'
      
        'inner join tblproctree PT on SL.SaleLineId = PT.MasterId and PT.' +
        'mastertype = '#39'mtSalesOrder'#39
      
        'Left join tblSmartOrderLines SOL on SOL.ProcTreeId = PT.ProcTree' +
        'Id'
      
        'left  join tblPurchaseLines POL  on ifnull(POL.smartorderlineID,' +
        '0) =SOL.SmartOrderLinesID'
      
        'where (SL.BOmGroupedLine='#39'F'#39' or SL.BOMProductionLine ='#39'T'#39') and i' +
        'fnull(PT.TreePartUOMQuantity,0)<> 0 and '
      
        '((SL.shipdate between :FromDate and :ToDate and :SOLineID=0 and ' +
        ':ProctreeId=0 and :SaleLineID=0) or '
      '(:SaleLineID <> 0 and Sl.SaleLineId = :SaleLineID) or '
      '(:ProctreeID <> 0 and PT.ProcTreeId= :ProctreeID) or '
      '(:SOLineID <> 0SOL.SmartOrderLinesID = :SOLineID))')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'FromDate'
      end
      item
        DataType = ftUnknown
        Name = 'ToDate'
      end
      item
        DataType = ftUnknown
        Name = 'SOLineID'
      end
      item
        DataType = ftUnknown
        Name = 'ProctreeId'
      end
      item
        DataType = ftUnknown
        Name = 'SaleLineID'
      end
      item
        DataType = ftUnknown
        Name = 'SaleLineID'
      end
      item
        DataType = ftUnknown
        Name = 'SaleLineID'
      end
      item
        DataType = ftUnknown
        Name = 'ProctreeID'
      end
      item
        DataType = ftUnknown
        Name = 'ProctreeID'
      end
      item
        DataType = ftUnknown
        Name = 'SOLineID'
      end
      item
        DataType = ftUnknown
        Name = 'SOLineID'
      end>
