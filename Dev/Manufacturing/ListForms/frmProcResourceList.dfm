  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      '1 as Details, '
      'convert( 1 , Signed INTEGER) as rownum ,'
      'PR.ProcResourceId as ProcResourceId, '
      'PR.ResourceName as ResourceName, '
      'PR.Description as Description, '
      'PR.ResourceAvailable as ResourceAvailable, '
      'PR.UsedWithProductsInSeq as UsedWithProductsInSeq, '
      'PR.BreakdownHoursDay as BreakdownHoursDay, '
      'PR.HourlyLabourRate as HourlyLabourRate, '
      'PR.HourlyOverheadRate as HourlyOverheadRate, '
      'PR.Active as Active, '
      'PR.ResourceCount as ResourceCount,'
      'Null as DetailName, '
      'Null as DetResourceAvailable,'
      'Null as DetActive,'
      'PR.SetupHoursDay as SetupHoursDay,'
      'PRD.Sunday as Sunday, '
      'PRD.SundayCapacityHoursDay as SundayCapacityHoursDay, '
      'PRD.Monday as Monday,'
      'PRD.MondayCapacityHoursDay as MondayCapacityHoursDay,'
      'PRD.TuesDay as TuesDay,'
      'PRD.TuesdayCapacityHoursDay as TuesdayCapacityHoursDay,'
      'PRD.WednesDay as WednesDay,'
      'PRD.WednesdayCapacityHoursDay as WednesdayCapacityHoursDay,'
      'PRD.Thursday as Thursday,'
      'PRD.ThursdayCapacityHoursDay as ThursdayCapacityHoursDay,'
      'PRD.Friday as Friday,'
      'PRD.FridayCapacityHoursDay as FridayCapacityHoursDay,'
      'PRD.SAturday as SAturday,'
      'PRD.SaturdayCapacityHoursDay as SaturdayCapacityHoursDay,'
      'Null as ResDetailId'
      'FROM tblProcResource PR'
      
        'Left join tblProcResourceDays PRD on PR.ProcResourceId = PRD.Pro' +
        'cResourceId'
      'union all '
      'SELECT'
      '2 as Details, '
      
        'convert( @rownum:=if(@ProcResourceId <> PR.ProcResourceId,  @row' +
        'num:= 1 ,  @rownum+1) , Signed INTEGER) as rownum ,'
      '@ProcResourceId := PR.ProcResourceId,'
      'PR.ResourceName as ResourceName, '
      'PR.Description as Description, '
      'PR.ResourceAvailable as ResourceAvailable, '
      'PR.UsedWithProductsInSeq as UsedWithProductsInSeq, '
      'PR.BreakdownHoursDay as BreakdownHoursDay, '
      'PR.HourlyLabourRate as HourlyLabourRate, '
      'PR.HourlyOverheadRate as HourlyOverheadRate, '
      'PR.Active as Active, '
      'PR.ResourceCount as ResourceCount,'
      'Det.Description as DetailName, '
      'Det.ResourceAvailable as DetResourceAvailable,'
      'Det.active as DetActive,'
      'PR.SetupHoursDay as SetupHoursDay,'
      'PRD.Sunday as Sunday, '
      'PRD.SundayCapacityHoursDay as SundayCapacityHoursDay, '
      'PRD.Monday as Monday,'
      'PRD.MondayCapacityHoursDay as MondayCapacityHoursDay,'
      'PRD.TuesDay as TuesDay,'
      'PRD.TuesdayCapacityHoursDay as TuesdayCapacityHoursDay,'
      'PRD.WednesDay as WednesDay,'
      'PRD.WednesdayCapacityHoursDay as WednesdayCapacityHoursDay,'
      'PRD.Thursday as Thursday,'
      'PRD.ThursdayCapacityHoursDay as ThursdayCapacityHoursDay,'
      'PRD.Friday as Friday,'
      'PRD.FridayCapacityHoursDay as FridayCapacityHoursDay,'
      'PRD.SAturday as SAturday,'
      'PRD.SaturdayCapacityHoursDay as SaturdayCapacityHoursDay,'
      'Det.Id as ResDetailId'
      'FROM '
      ' (SELECT @rownum:=0) r ,'
      ' (SELECT @ProcResourceId:=0) s,'
      'tblProcResource PR'
      
        'Left join tblProcResourceDays PRD on PR.ProcResourceId = PRD.Pro' +
        'cResourceId'
      
        'inner join tblprocresourcedetails det  on PR.ProcResourceId = de' +
        't.ProcResourceId'
      'Order by ResourceName , DetailName')
    OnCalcFields = qryMainCalcFields
