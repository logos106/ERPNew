  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select'#9'Distinct '
      'if(PT.TreeRootId  = PT.ProcTreeId ,"T" , "F") as  isRoot,'
      'if(ifnull(PTC.ProcTreeId,0)=0 , "F" , "T") as hasChildren,'
      's.SaleID as SaleID'#9','
      'SL.saleLineId as saleLineId'#9','
      'SL.ProductName  as ProductName '#9','
      'S.CustomerName  as CustomerName '#9','
      'S.SaleDate as SaleDate'#9','
      'S.sAleDatetime as sAleDatetime'#9','
      'S.converted  as converted '#9','
      
        'if(S.IsInternalOrder='#39'T'#39' ,if(S.Converted='#39'T'#39' , "Finished Interna' +
        'l Order" , "InternalOrder") ,"Sales Order" ) '#9'as'#9'Category'#9','
      'PT.caption as Caption'#9','
      
        'concat(PT.TreePartUOM , '#39'('#39' , PT.TreePartUOMMultiplier , '#39')'#39') '#9'a' +
        's'#9'UOM'#9','
      'PT.FromStockUOMQty '#9'as'#9'FromStockUOMQty '#9','
      'PT.ManufactureUOMQty '#9'as'#9'ManufactureUOMQty '#9','
      'PT.OnOrderUOMQty as OnOrderUOMQty'#9','
      'PT.ManufacturedQty as BuiltQty'#9','
      'PT.TotalQty - PT.ManufacturedQty '#9'as'#9'ToBeBuildQty '#9','
      'PT.Quantity as Quantity,'
      'PT.TotalQty as  TotalQty,'
      'PT.ProductUnitPrice as ProductUnitPrice , '
      'PT.ProductUnitCost as ProductUnitCost,'
      
        'if(PT.TreeRootId  = PT.ProcTreeId , 1,PT.TotalQty) *PT.ProductUn' +
        'itPrice  as TotalPrice,'
      
        'if(PT.TreeRootId  = PT.ProcTreeId , 1,PT.TotalQty)*PT.ProductUni' +
        'tCost  as Totalcost,'
      
        'if(PT.TreeRootId  = PT.ProcTreeId , 1,PT.TotalQty)*PT.ProductUni' +
        'tPrice  as Price,'
      
        'if(PT.TreeRootId  = PT.ProcTreeId , 1,PT.TotalQty)*PT.ProductUni' +
        'tCost  as cost,'
      'fe.FormulaName as FormulaName'#9','
      'DescribeFormula(fe.Formula) '#9'as'#9'descFormula'#9','
      'PT.FormulaQtyValue1 as FormulaQtyValue1'#9','
      'PT.FormulaQtyValue2 as FormulaQtyValue2'#9','
      'PT.FormulaQtyValue3 as FormulaQtyValue3'#9','
      'PT.FormulaQtyValue4 as FormulaQtyValue4'#9','
      'PT.FormulaQtyValue5 as FormulaQtyValue5'#9','
      'convert(PT.info , char(255)) as info,'
        'Convert(TreeNodeCaption(PT.Level, PT.caption ), char(255)) '#9'as'#9'Detailedcaption'#9
      'from tblsales S'#9#9#9
      'inner join tblsaleslines SL on S.SAleId = SL.SaleID'#9#9#9
      
        'inner join tblproctree PT on PT.MasterId = SL.SaleLineId and PT.' +
        'MasterType <> '#39'mtProduct'#39' '#9#9#9
      'Left join tblproctree PTC on PT.ProcTreeId = PTC.ParentId'
      'lEFT JOIN tblfeformula fe ON PT.Formulaid = fe.FormulaID'#9#9#9
      
        'where (S.Converted ='#39'F'#39' or S.IsInternalOrder = '#39'T'#39') and S.IsSale' +
        'sorder = '#39'T'#39#9#9#9
      'and S.SaleDate between :dateFrom and :dateTo'#9#9#9
      'order by SaleId desc, SaleLineId, ProductName  , PT.sequencedown')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'dateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'dateTo'
      end>
