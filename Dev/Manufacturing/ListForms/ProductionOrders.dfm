  inherited qryMain: TERPQuery
    SQL.Strings = (
      'select distinct '
      'S.classId, '
      '`S`.`SaleID`,'
      '`S`.`CustomerName`,'
      'SL.totalLineamountInc amountinc,'
      '`SL`.`ProductName`,'
      'S.SAleDate,'
      'SL.ProductId,'
      'Sl.SaleLineId,'
      '`SL`.`Product_Description`,'
      '`SL`.`Product_Description_Memo`,'
      '`SL`.`UnitofMeasureQtySold`,'
      '`SL`.`UnitofMeasureShipped`,'
      '`SL`.`UnitofMeasureBackorder`,'
      '`SL`.`UnitofMeasureSaleLines`,'
      '`SL`.`UnitofMeasureMultiplier`,'
      '`S`.`ClientID`, '
      '`SL`.`UnitOfMeasureID`, '
      'if(ifnull(PT.ProcTreeId,0)=0 , "F" , "T") as HasTree , '
      'SLUOM.Weight*`SL`.`UnitofMeasureShipped` SaleProductWeight,'
      'SLUOM.Length*`SL`.`UnitofMeasureShipped` SaleProductLength, '
      'SLUOM.Volume*`SL`.`UnitofMeasureShipped` SaleProductVolume,'
      
        'if(ifnull(PT.parentId,0)=0 , (Select Sum(ifnull(PPQ.QtyBuilt,0))' +
        ' from tblprocesspartqty PPQ inner join tblprocesspart PP on PPQ.' +
        'ProcessPartID = PP.ID where PP.ProcTreeId = PT.ProctreeId) , 0) ' +
        'as Qtybuilt'
      'from  tblsales S'
      
        'inner join tblsaleslines SL  on S.SaleID = SL.SaleID and (SL.BOM' +
        'GroupedLine ='#39'F'#39' or SL.BOMProductionLine ='#39'T'#39' )'
      
        'inner join tblunitsofmeasure SLUOM on SLUOM.UnitID = SL.UnitOfMe' +
        'asureID'
      'inner join tblparts P on P.partsId = Sl.ProductID'
      
        '/*inner join tblproctree PPT on P.PartsId = PPt.masterID and PPt' +
        '.mastertype ="mtProduct" and ifnull(PPt.parentID,0)=0*/'
      
        'Left join tblproctree PT  on PT.MasterId = SL.SaleLineID and PT.' +
        'MasterType <> "mtProduct" and ifnull(PT.parentID,0)=0 and ifnull' +
        '(PT.ManufactureQty,0)<> 0'
      
        'inner join tblPQA PQA  on SL.ProductId = PQa.ProductID and S.cla' +
        'ssId = PQA.DepartmentId and SL.UnitOfMeasureID = PQa.UOMID'
      
        'where ifnull(S.Converted,"F") = "F" and `SL`.`UnitofMeasureShipp' +
        'ed` <> 0'
      
        'and ((:IncludeInternalOrders = "F" and ifnull(S.IsInternalOrder,' +
        '"F") = "F") or (:IncludeInternalOrders = "T"))'
      'and S.IsSalesOrder="T"'
      'and S.Saledate between :Datefrom and :dateTo'
      'group by S.SaleId, SL.productname, SL.SaleLineID'
      'Order by saleId desc')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'IncludeInternalOrders'
      end
      item
        DataType = ftUnknown
        Name = 'IncludeInternalOrders'
      end
      item
        DataType = ftDateTime
        Name = 'Datefrom'
        Value = 36526d
      end
      item
        DataType = ftDateTime
        Name = 'dateTo'
        Value = 41275d
      end>
