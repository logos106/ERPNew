  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select'#9#9#9
      's.SaleID as SaleID'#9','
      'SL.saleLineId as saleLineId'#9','
      'SL.ProductName as ProductName '#9','
      'S.CustomerName as CustomerName '#9','
      'S.SaleDate as SaleDate'#9','
      'S.sAleDatetime as sAleDatetime'#9','
      'S.converted as converted '#9','
      
        'if(S.IsInternalOrder='#39'T'#39' ,if(S.Converted='#39'T'#39' , "Finished Interna' +
        'l Order" , "InternalOrder") ,"Sales Order" )'#9'as'#9'Category'#9','
      'PT.Caption as Caption'#9','
      
        'concat(PT.TreePartUOM , '#39'('#39' , PT.TreePartUOMMultiplier , '#39')'#39') as' +
        ' UOM'#9','
      'PT.FromStockUOMQty as FromStockUOMQty '#9','
      'PT.ManufactureUOMQty as ManufactureUOMQty '#9','
      'PT.OnOrderUOMQty as OnOrderUOMQty'#9','
      'PT.ManufacturedQty'#9'as'#9'BuiltQty'#9','
      'PT.TotalQty - PT.ManufacturedQty'#9'as'#9'ToBeBuildQty '#9','
      'PT.Quantity as Quantity,'
      'PT.TotalQty as  TotalQty,'
      'PT.ProductUnitPrice as ProductUnitPrice , '
      'PT.ProductUnitCost as ProductUnitCost,'
      'PT.ProductUnitPrice  as TotalPrice,'
      'PT.ProductUnitCost  as Totalcost,'
      'PT.ProductUnitPrice  as Price,'
      'PT.ProductUnitCost  as cost,'
      'fe.FormulaName as FormulaName'#9','
      'DescribeFormula(fe.Formula)'#9'as'#9'descFormula'#9','
      'PT.FormulaQtyValue1 as FormulaQtyValue1'#9','
      'PT.FormulaQtyValue2 as FormulaQtyValue2'#9','
      'PT.FormulaQtyValue3 as FormulaQtyValue3'#9','
      'PT.FormulaQtyValue4 as FormulaQtyValue4'#9','
      'PT.FormulaQtyValue5 as FormulaQtyValue5'#9','
      'convert(PT.info , char(255)) as info,'
      'PTC.Caption as  cCaption ,'#9#9#9
      
        'concat(PTC.TreePartUOM , '#39'('#39' , PTC.TreePartUOMMultiplier , '#39')'#39') ' +
        'as  cUOM,'#9#9#9
      'PTC.FromStockUOMQty as  cFromStockUOMQty ,'#9#9#9
      'PTC.ManufactureUOMQty as  cManufactureUOMQty ,'#9#9#9
      'PTC.OnOrderUOMQty as  cOnOrderUOMQty,'#9#9#9
      'PTC.ManufacturedQty as  cBuiltQty,'#9#9#9
      'PTC.TotalQty - PTC.ManufacturedQty as  cToBeBuildQty,'#9#9#9
      'PTC.Quantity as CQuantity,'
      'PTC.TotalQty as  CTotalQty,'
      'PTC.ProductUnitPrice as CProductUnitPrice , '
      'PTC.ProductUnitCost as CProductUnitCost,'
      'PTC.TotalQty *PTC.ProductUnitPrice  as CTotalPrice,'
      'PTC.TotalQty *PTC.ProductUnitCost  as CTotalcost,'
      'PTC.Quantity *PTC.ProductUnitPrice  as CPrice,'
      'PTC.Quantity *PTC.ProductUnitCost  as Ccost,'
      ''
      'fe.FormulaName as  cFormulaName,'#9#9#9
      'DescribeFormula(fe.Formula) as  cdescFormula,'#9#9#9
      'PTC.FormulaQtyValue1 as  cFormulaQtyValue1,'#9#9#9
      'PTC.FormulaQtyValue2 as  cFormulaQtyValue2,'#9#9#9
      'PTC.FormulaQtyValue3 as  cFormulaQtyValue3,'#9#9#9
      'PTC.FormulaQtyValue4 as  cFormulaQtyValue4,'#9#9#9
      'PTC.FormulaQtyValue5 as  cFormulaQtyValue5,'#9#9#9
      'convert(PTC.info , char(255)) as info,'
      'PTI.Caption as iCaption ,'#9#9#9
      
        'concat(PTI.TreePartUOM , '#39'('#39' , PTI.TreePartUOMMultiplier , '#39')'#39') ' +
        'as iUOM,'#9#9#9
      'PTI.FromStockUOMQty as  iFromStockUOMQty ,'#9#9#9
      'PTI.ManufactureUOMQty as  iManufactureUOMQty ,'#9#9#9
      'PTI.OnOrderUOMQty as  iOnOrderUOMQty,'#9#9#9
      'PTI.ManufacturedQty as iBuiltQty,'#9#9#9
      'PTI.TotalQty - PTI.ManufacturedQty as iToBeBuildQty,'#9#9#9
      'PTI.Quantity as IQuantity,'
      'PTI.TotalQty as  ITotalQty,'
      'PTI.ProductUnitPrice as IProductUnitPrice , '
      'PTI.ProductUnitCost as IProductUnitCost,'
      'PTI.TotalQty *PTI.ProductUnitPrice  as ITotalPrice,'
      'PTI.TotalQty *PTI.ProductUnitCost  as ITotalcost,'
      'PTI.Quantity *PTI.ProductUnitPrice  as IPrice,'
      'PTI.Quantity *PTI.ProductUnitCost  as Icost,'
      'fe.FormulaName as  iFormulaName,'#9#9#9
      'DescribeFormula(fe.Formula) AS idescFormula,'#9#9#9
      'PTI.FormulaQtyValue1 as  iFormulaQtyValue1,'#9#9#9
      'PTI.FormulaQtyValue2 as  iFormulaQtyValue2,'#9#9#9
      'PTI.FormulaQtyValue3 as  iFormulaQtyValue3,'#9#9#9
      'PTI.FormulaQtyValue4 as  iFormulaQtyValue4,'#9#9#9
      'PTI.FormulaQtyValue5 as  iFormulaQtyValue5,'#9#9#9
      'convert(PTI.info , char(255)) as info,'
      
        'convert(concat_WS('#39'->'#39',PT.caption , PTI.caption, PT.caption), ch' +
        'ar(255)) as Detailedcaption'#9
      'from '
      'tblsales S'#9#9#9
      'inner join tblsaleslines SL on S.SAleId = SL.SaleID'#9#9#9
      
        'inner join tblproctree PT on PT.MasterId = SL.SaleLineId and PT.' +
        'MasterType <> '#39'mtProduct'#39' and parentId =0'#9#9#9
      'Left join tblproctree PTI on PT.ProcTreeId = PTI.ParentId'#9#9#9
      'Left join tblproctree PTC on PTI.ProcTreeId = PTC.ParentId'#9#9#9
      'lEFT JOIN tblfeformula fe ON PTC.Formulaid = fe.FormulaID'#9#9#9
      
        'where (S.Converted ='#39'F'#39' or S.IsInternalOrder = '#39'T'#39') and S.IsSale' +
        'sorder = '#39'T'#39#9#9#9
      'and S.SaleDate between :dateFrom and :dateTo'#9#9#9
      
        'order by SaleId desc , saleLineId , PT.sequencedown , PTI.sequen' +
        'cedown , PTC.sequencedown')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'dateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'dateTo'
      end>
