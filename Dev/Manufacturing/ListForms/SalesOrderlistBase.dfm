  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'SOL.smartOrderId, '
      'SOL.SmartOrderLinesID,'
      'S.SaleDate, '
      'SL.shipdate,'
      'PT.ProcTreeId,'
      'S.SaleId,'
      'SL.SaleLineID, '
      'SL.ProductName as TreeProduct,'
      
        'Convert(TreeNodeCaption(PT.Level, PT.caption ) ,char(255)) Capti' +
        'on, '
      'SOL.SupplierName, '
      
        'concat(PT.TreePartUOM , if(PT.TreePartUOMMultiplier=1, '#39#39' , conc' +
        'at('#39'('#39' , PT.TreePartUOMMultiplier , '#39')'#39'))) as UOM, '
      'PT.TreePartUOMTotalQty, '
      'PT.FromStockUOMQty,'
      'PT.ManufactureUOMQty , '
      'PT.OnOrderUOMQty , '
      
        'concat(SOL.UnitofMeasure , if(SOL.UnitofMeasureMultiplier=1, '#39#39' ' +
        ', concat('#39'('#39' , SOL.UnitofMeasureMultiplier , '#39')'#39'))) as SOUOM, '
      'SOL.UnitofMeasureQty as Ordered,'
      
        'concat(POL.UnitofMeasurePOLines, if(POL.UnitofMeasureMultiplier=' +
        '1, '#39#39' , concat('#39'('#39' , POL.UnitofMeasureMultiplier, '#39')'#39'))) as POUO' +
        'M, '
      'Sum(POL.UnitofMeasureShipped)  UnitofMeasureShipped,'
      
        'Sum(if(ifnull(POL.UnitofMeasureShipped,0) = 0 , POL.UnitofMeasur' +
        'eBackorder , 0)) UnitofMeasureBackorder , '
      'PT.TotalQty ,'
      'PT.FromStockQty, '
      'PT.ManufactureQty , '
      'PT.OnOrderQty , '
      'SOL.Qty , '
      'Sum(POL.shipped)  shipped,'
      'Sum(if(ifnull(POL.shipped,0) = 0 , POL.Backorder, 0)) Backorder'
      'From tblSales S '
      'inner join tblSaleslines SL on s.SAleId = Sl.SaleID'
      
        'inner join tblproctree PT on SL.SaleLineId = PT.MasterId and PT.' +
        'mastertype = '#39'mtSalesOrder'#39
      
        'inner join tblSmartOrderLines SOL on SOL.ProcTreeId = PT.ProcTre' +
        'eId'
      
        'inner join tblSmartOrder SO on SO.SmartOrderID = SOL.SmartOrderI' +
        'D'
      
        'left join tblPurchaseLines POL on POL.smartorderlineID =SOL.Smar' +
        'tOrderLinesID'
      
        'Left join tblPurchaseOrders PO on PO.PurchaseOrderID = POL.purch' +
        'aseorderID '
      
        'where (SL.shipdate between :FromDate and :ToDate and :SaleLineId' +
        '=0 and  :ProctreeId=0) or (:SaleLineID <> 0 and Sl.SaleLineId = ' +
        ':SaleLineID) or (:ProctreeId <> 0 and SOL.proctreeID = :Proctree' +
        'Id)'
      
        'group by PT.ProcTreeId , SO.SmartOrderID , POL.UnitofMeasurePOLi' +
        'nes')
    ParamData = <
