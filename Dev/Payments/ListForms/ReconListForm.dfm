  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'tblReconciliation.GlobalRef as GlobalRef, '
      'tblReconciliation.ReconciliationID as ReconciliationID, '
      'tblReconciliation.ReconciliationDate as ReconciliationDate,'
      'tblReconciliation.StatementNo as StatementNo, '
      'tblReconciliation.ClassID as ClassID, '
      'tblClass.ClassName as ClassName,'
      'tblReconciliation.AccountID as AccountID, '
      'tblChartofAccounts.AccountName as AccountName,'
      'tblReconciliation.EmployeeID as EmployeeID, '
      'Concat(FirstName,'#39' '#39',LastName) AS EmployeeName,'
      'tblReconciliation.Notes as Notes, '
      'tblReconciliation.OpenBalance as OpenBalance,'
      'tblReconciliation.CloseBalance as CloseBalance, '
      'tblReconciliation.Deleted as Deleted,'
      'tblReconciliation.Finished as Finished'
      'FROM ((tblReconciliation'
      'LEFT JOIN tblEmployees'
      'ON tblReconciliation.EmployeeID=tblEmployees.EmployeeID)'
      'LEFT JOIN tblClass'
      'ON tblReconciliation.ClassID=tblClass.ClassID)'
      'LEFT JOIN tblChartofAccounts'
      'ON tblReconciliation.AccountID=tblChartofAccounts.AccountID'
      'WHERE ReconciliationDate Between :txtfrom AND :txtto;')
    ParamData = <
      item
        DataType = ftString
        Name = 'txtfrom'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'txtto'
      end>
