  inherited qryMain: TERPQuery
    SQL.Strings = (
      
        'select  "Invoice" as Category, tblclass.ClassName PaymentClass, ' +
        'tblsales.saleId as TransactionID, tblsales.Class SalesClass, tbl' +
        'sales.CustomerName , tblsales.SaleDate , tblsales.TotalAmountInc' +
        ' , '
      
        'tbldeposits.PaymentDate , tbldepositline.Payment , tbldeposits.P' +
        'aymentID'
      'from tblsales  '
      
        'inner join tbldepositline  on tblsales.saleId = tbldepositline.S' +
        'aleID'
      
        'inner join tbldeposits on tbldeposits.PaymentID = tbldepositline' +
        '.PaymentID'
      'inner join tblclass on tbldeposits.ClassID = tblclass.ClassID'
      'Where tbldeposits.ClassID <> tblsales.ClassID'
      'and tbldeposits.deleted ='#39'F'#39
      'union all '
      
        'select  "Customer Prepayment" as Category , tblclass.ClassName P' +
        'aymentClass, '
      
        'tblprepayments.PrePaymentID, Prepayclass.Classname , tblprepayme' +
        'nts.CompanyName, tblprepayments.PrePaymentDate , tblprepayments.' +
        'PayAmount, '
      
        'tbldeposits.PaymentDate , tbldepositline.Payment , tbldeposits.P' +
        'aymentID'
      'from tblprepayments'
      
        'inner join tbldepositline  on tblprepayments.PrePaymentID = tbld' +
        'epositline.PrePaymentID'
      
        'inner join tbldeposits on tbldeposits.PaymentID = tbldepositline' +
        '.PaymentID'
      'inner join tblclass on tbldeposits.ClassID = tblclass.ClassID'
      
        'inner join tblclass Prepayclass on tblprepayments.ClassID = Prep' +
        'ayclass.ClassID'
      'Where tbldeposits.ClassID <> tblprepayments.ClassID'
      'and tbldeposits.deleted ='#39'F'#39
      'order by paymentdate desc')
    object qryMainCategory: TWideStringField
      DisplayWidth = 10
      FieldName = 'Category'
      Origin = '.Category'
      Size = 7
    end
