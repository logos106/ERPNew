  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      '1 as Transtypeno,'
      '"Cheque" As Transtype'#9', '
      'PO.PurchaseOrderID as TransID, '
      'C.ClientID '#9'as ClientID, '
      'C.Company as Clientname,'
      'PO.PurchaseOrderNumber '#9'AS TransNo, '
      'CONVERT(PO.OrderDate, DATE) As TransDate, '
      'PO.Comments'#9'as Comments,'
      'C.Contact1 as Contact1, '
      'PO.PrintFlag as IsPrinted, '
      'C.Phone as Phone, '
      'PO.InvoiceNumber as ReferenceNo,'
      'PO.TotalAmount as totalAmountEx, '
      'PO.TotalTax as totalTax,'
      'PO.TotalAmountInc as totalAmountInc,'
      'PO.ForeignTotalAmount as ForeignTotalAmount '
      'FROM tblPurchaseOrders '#9'PO '
      'inner JOIN tblClients C ON PO.clientID '#9'= C.clientID '
      
        'LEFT JOIN tblprintdocs Pd ON Pd.docname = '#39'TCheque'#39' AND Pd.DocId' +
        ' = PO.purchaseorderID'
      
        'WHERE Deleted ="F" And OrderDate Between :dtfrom AND :dtto and (' +
        'IFNULL(Pd.PrintdocId,0)=0 or :IncludePrinted ="T") And PO.IsCheq' +
        'ue = '#39'T'#39' '
      'union all '
      'select '
      '2 as Transtypeno,'
      '"Payment" As Transtype'#9', '
      'W.PaymentID as TransID, '
      'W.SupplierID as ClientID, '
      'W.CompanyName as Clientname,'
      'W.PaymentNo  '#9'AS TransNo, '
      'CONVERT(W.PaymentDate, DATE) As TransDate, '
      'W.Notes as Comments,'
      'C.Contact1 as Contact1, '
      'W.ChequePrinted as IsPrinted, '
      'C.Phone as Phone, '
      'W.ReferenceNo '#9'as ReferenceNo,'
      'W.Amount '#9'as totalAmountEx, '
      '0  as totalTax,'
      'W.amount  as totalAmountInc,'
      'W.ForeignCurrencyAmount as ForeignTotalAmount '
      'from tblwithdrawal '#9'W '
      'inner join tblclients C on W.supplierId '#9'= c.clientID '
      
        'inner join tblpaymentmethods  PM on PM.PaymethodID  =W.PayMethod' +
        'ID and PM.name = "Cheque"'
      
        'where Deleted ="F" And W.PaymentDate Between :dtfrom AND :dtto a' +
        'nd (W.ChequePrinted ="F" or :IncludePrinted ="T")'
      'union all '
      'Select '
      '3 as Transtypeno,'
      '"Prepayment" As Transtype'#9', '
      'PP.PrePaymentID  as TransID, '
      'PP.ClientID'#9'as ClientID, '
      'PP.CompanyName'#9'as Clientname,'
      'PP.PrePaymentID AS TransNo, '
      'CONVERT(PP.PrePaymentDate, DATE) As TransDate, '
      'PP.Notes as Comments,'
      'C.Contact1 as Contact1, '
      'PP.ChequePrinted '#9'as IsPrinted, '
      'C.Phone as Phone, '
      'PP.ReferenceNo'#9'as ReferenceNo,'
      'PP.PayAmount as totalAmountEx, '
      '0  as totalTax,'
      'PP.PayAmount as totalAmountInc,'
      'PP.ForeignPayAmount as ForeignTotalAmount '
      'from tblprepayments PP '
      'inner join tblclients C on PP.ClientID '#9'= C.ClientID '
      
        'inner join tblpaymentmethods  PM on PM.PaymethodID  =PP.PayMetho' +
        'dID and PM.name = "Cheque"'
      
        'where Deleted ="F" And PP.PrePaymentDate Between :dtfrom AND :dt' +
        'to and (PP.ChequePrinted="F" or :IncludePrinted ="T")'
      'And PP.Supplier ="T"'
      'Order by TransDate desc, clientname, transtype')
