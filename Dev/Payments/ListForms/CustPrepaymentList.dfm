  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'tblprepayments.PrePaymentID,'
      'tblprepayments.GlobalRef,'
      'tblprepayments.BankAccountName,'
      'tblprepayments.PrePaymentID  as ReceiptNo,'
      'tblprepayments.CompanyName as CompanyName,'
      'tblprepayments.PrePaymentDate as '#39'PaymentDate'#39', '
      'tblprepayments.PayAmount as Amount,'
      'tblprepayments.Balance as balance , '
      'tblprepayments.Payment as Applied,'
      'tblprepayments.ReferenceNo as ReferenceNo,'
      'tblprepayments.Deleted as Deleted,'
      'tblpaymentmethods.Name as '#39'PaymentMethod'#39','
      'tblclass.ClassName as '#39'Department'#39','
      'tblprepayments.Notes as Notes,'
      'tblprepayments.EmployeeID as EmployeeID,'
      'tblprepayments.ClientID as ClientID,'
      'tblclients.ClientNo as AccountNo'
      'FROM tblprepayments'
      'INNER JOIN tblclients USING(ClientID)'
      'INNER JOIN tblclass ON tblclass.ClassID= tblprepayments.ClassID'
      
        'LEFT JOIN tblpaymentmethods ON tblpaymentmethods.PayMethodID=tbl' +
        'prepayments.PayMethodID'
      'WHERE tblprepayments.Customer='#39'T'#39' AND '
      'tblprepayments.PrePaymentDate Between :txtfrom AND :txtto'
      
