  inherited qryMain: TERPQuery
    SQL.Strings = (
      'Select '
      'BD.depositId as  depositId , '
      'BD.DEpositdate as DEpositdate, '
      'E.EmployeeName as EmployeeName, '
      'BDL.PaymentID as  PaymentID , '
      'BDL.PaymentMethod as PaymentMethod, '
      'BDL.PaymentDate as PaymentDate , '
      'BDL.CompanyName as CompanyName, '
      
        'If(BDL.TrnsType="Deposit Entry", "Deposit Entry", "Deposit") as ' +
        'Type,'
      'BDL.ReferenceNo as ReferenceNo,'
      'BDL.Amount as Amount,'
      'BDL.FromDeposited as FromDeposited, '
      'BDL.Reconciled as Reconciled,'
      'BD.deleted as Deleted'
      'From tbldeposits D '
      'INNER JOIN tbldepositline DL USING(PaymentID) '
      'INNER JOIN tblbankdepositlines BDL ON '
      'BDL.PaymentID = D.PaymentID AND '
      'BDL.PaymentDate = D.PaymentDate AND '
      'BDL.AccountID = D.AccountID AND '
      'BDL.Amount = D.Amount  AND '
      'BDL.CompanyName = D.CompanyName AND '
      'BDL.CusID = D.CusID AND '
      'BDL.TrnsType = "Customer Payment" '
      'inner join tblBankdeposit BD on BDL.DepositID = BD.depositId'
      'inner join tblemployees E on E.EmployeeID = BD.employeeId'
      'Where  D.Deleted<>"T" '
      'AND BDL.FromDeposited="T" '
      
        'AND ((:PaymentID =0 and :TrnsType ='#39#39' and DepositDate Between :d' +
        'ateFrom AND :dateto) OR( (BDL.PaymentID  = :PaymentID OR :Paymen' +
        'tID =0 ) and  (BDL.TrnsType = :TrnsType OR :TrnsType ='#39#39'  )))'
      'order by BD.depositId  desc')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'PaymentID'
      end
      item
        DataType = ftUnknown
        Name = 'TrnsType'
      end
      item
        DataType = ftUnknown
        Name = 'dateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'dateto'
      end
      item
        DataType = ftUnknown
        Name = 'PaymentID'
      end
      item
        DataType = ftUnknown
        Name = 'PaymentID'
      end
      item
        DataType = ftUnknown
        Name = 'TrnsType'
      end
      item
        DataType = ftUnknown
        Name = 'TrnsType'
      end>
