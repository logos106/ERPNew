  object QryPayments: TERPQuery
    Connection = MyConnection1
    SQL.Strings = (
      'SELECT'
      '`DL`.TransIDFieldName as TRansID,'
      '`D`.`Reconciled`,'
      '`D`.`PaymentDate`,'
      '`D`.`ReconciliationID`,'
      '`DL`.`Payment`,'
      '`DL`.`AmountOutstanding`,'
      '`DL`.`AmountDue`'
      
        'from  PaymentLineTablename AS `DL`  Inner Join PaymentTablename ' +
        'AS `D` ON `DL`.`PaymentID` = `D`.`PaymentID`'
      
        'WHERE ifnull(DL.TransIDFieldName,0) <> 0 and `D`.`Deleted` <>  "' +
        'T"')
    Left = 648
    Top = 184
    object QryPaymentsTRansID: TIntegerField
      FieldName = 'TRansID'
    end
    object QryPaymentsReconciled: TWideStringField
      FieldName = 'Reconciled'
      FixedChar = True
      Size = 1
    end
    object QryPaymentsPaymentDate: TDateField
      FieldName = 'PaymentDate'
    end
    object QryPaymentsReconciliationID: TIntegerField
      FieldName = 'ReconciliationID'
    end
    object QryPaymentsPayment: TFloatField
      FieldName = 'Payment'
    end
    object QryPaymentsAmountOutstanding: TFloatField
