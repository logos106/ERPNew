  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT'
      'SaleID as SaleID,'
      'InvoiceDocNumber AS InvoiceNumber,'
      'SaleDate as SaleDate,'
      'Account AS AccountName,'
      'tblsales.ClientID as ClientID,'
      'tblsales.CustomerName as CustomerName,'
      'TotalAmountInc as TotalAmountInc,'
      'tblsales.EmployeeName as EmployeeName,'
      'tblclients.Phone as Phone,'
      'ARNotes as ARNotes'
      'FROM tblsales, tblclients'
      'where tblsales.ClientID = tblclients.ClientID'
      'and ARNotes IS NOT NULL;')
