  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT tblPurchaseOrders.GlobalRef,'
      'tblPurchaseOrders.PurchaseOrderID AS '#39'PurchaseOrderID'#39','
      'tblClients.ClientID AS '#39'ClientID'#39','
      'tblClients.Company AS '#39'Company'#39','
      'tblPurchaseOrders.PurchaseOrderNumber AS '#39'P/O Num'#39','
      'tblPurchaseOrders.OrderDate AS '#39'OrderDate'#39','
      'tblPurchaseOrders.Comments AS '#39'Comments'#39','
      'tblPurchaseLines.AccountName,'
      'tblPurchaseLines.LineTaxCode as '#39'Tax Code'#39','
      'tblPurchaseLines.LineTax as '#39'Tax'#39','
      'tblPurchaseLines.Product_Description as '#39'Description'#39','
      'tblPurchaseLines.TotalLineAmount as '#39'Amount (Ex)'#39','
      'tblPurchaseLines.TotalLineAmountInc as '#39'Amount (Inc)'#39','
      'tblClients.Contact1 AS '#39'Contact1'#39','
      'tblPurchaseOrders.ETADate AS '#39'ETADate'#39','
      'tblClients.Phone AS '#39'Phone'#39','
      'tblPurchaseLines.Invoiced AS '#39'Invoiced'#39','
      'tblPurchaseOrders.InvoiceNumber AS '#39'ChequeNumber'#39','
      'tblPurchaseOrders.Deleted AS '#39'Deleted'#39','
      'tblPurchaseOrders.SalesComments AS '#39'SalesComments'#39','
      'tblPurchaseOrders.RefNo as '#39'ReferenceNo'#39','
      'tblPurchaseLines.CustomerJob,'
      'tblPurchaseLines.CustomerJobID,'
      'tblPurchaseLines.Class as '#39'Department'#39
      'FROM tblPurchaseLines'
      
        'INNER JOIN tblPurchaseOrders ON  (tblPurchaseOrders.PurchaseOrde' +
        'rID = tblPurchaseLines.PurchaseOrderID)'
      
        'LEFT JOIN tblClients ON  (tblPurchaseOrders.SupplierName = tblCl' +
        'ients.Company)'
      
        'LEFT JOIN tblParts ON  (tblPurchaseLines.ProductName = tblParts.' +
        'PARTName)'
      
        'WHERE tblPurchaseOrders.OrderDate And tblPurchaseOrders.IsCheque' +
