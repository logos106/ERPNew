  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'Concat_ws("",PaymentNo) as ReceiptNo,'
      'tbldeposits.GlobalRef as GlobalRef, '
      'convert(tbldeposits.PaymentDate,datetime) as PaymentDate,'
      'tblClients.Company as CustomerName,'
      'tblClients.printjobname as printjobname, '
      'tblClients.jobname as jobname, '
      'tbldeposits.Balance as OpeningBalance,'
      'Amount as PaymentAmount,'
      'Name as PaymentMethod,'
      'ReferenceNo as ReferenceNo,'
      'AccountName as BankAccount,'
      'ClassName as Department,'
      'tbldeposits.Notes as Notes,'
      'tbldeposits.Deleted as Deleted,'
      'tbldeposits.ClassID as ClassID,'
      'ClientID as ClientID,'
      'PaymentID as PaymentID,'
      'EmployeeID as EmployeeID,'
      'tbldeposits.AccountID as AccountID'
      'FROM tbldeposits'
      'INNER JOIN tblClients ON tblClients.ClientID=CusID'
      
        'LEFT JOIN tblpaymentmethods ON tblpaymentmethods.PayMethodID= tb' +
        'ldeposits.PayMethodID'
      
        'INNER JOIN tblchartofaccounts ON tblchartofaccounts.AccountID=tb' +
        'ldeposits.AccountID'
      'INNER JOIN tblclass ON tblclass.ClassID= tbldeposits.ClassID'
      'WHERE CustomerPayment='#39'T'#39' AND tbldeposits.Customer='#39'T'#39' '
      'AND PaymentDate  Between :Datefrom AND :Dateto'
      'ORDER BY PaymentDate DESC, PaymentID desc')
