  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'tblSales.SaleID                        as SaleID, '
      'If(IsRefund="T",'#39'Refund'#39','#39'Invoice'#39') as Type, '
      'CustomerName                           as CustomerName,'
      'SaleDate                               as SaleDate, '
      'tblSales.GlobalRef                     as GlobalRef, '
      'InvoiceDocNumber                       as InvoiceDocNumber,'
      'TotalAmountInc                         as TotalAmountInc,'
      'tblSales.Balance                       as Balance, '
      'tblSales.Balance                       as AmountOutstanding, '
      'tblSales.ForeignTotalAmount            as ForeignTotalAmount, '
      
        'tblSales.ForeignBalanceAmount          as ForeignBalanceAmount ,' +
        ' '
      
        'tblSales.ForeignBalanceAmount          as ForeignAmountOutstandi' +
        'ng, '
      '0                                      as Payment, '
      '0                                      as ForeignPayment, '
      'tblSales.ForeignExchangeCode           as ForeignExchangeCode, '
      'tblSales.ForeignExchangeRate           as ForeignExchangeRate, '
      'Paid                                   as Paid, '
      '0                                      as Discount, '
      '0                                      as Withholding , '
      '0                                      as Refunding ,'
      '0                                      as RefundPercent , '
      '"F"                                    as Apply,'
      'convert(tblSales.Comments , char(255)) as RefNo,'
      'tblSales.EnteredBy                     as EnteredBy'
      'FROM tblSales '
      
        'INNER JOIN tblSalesLines ON  tblSales.SaleID = tblSalesLines.Sal' +
        'eID '
      
        'Where (IsInvoice="T" OR IsRefund="T") AND  Paid="F" AND Invoiced' +
        '="T"'
      'AND tblSales.Deleted="F" AND Account<>'#39'Undeposited Funds'#39
      'AND tblSales.ClientID in (0) '
      'AND tblSales.Balance <> 0'
      'GROUP By tblSales.SaleID '
      'UNION ALL '
      'SELECT '
      'tblprepayments.PrePaymentID        as saleID, '
      #39'Prepayment'#39'          as Type,'
      'CompanyName                        as CustomerName,'
      'tblprepayments.PrePaymentDate      as SaleDate, '
      'tblprepayments.GlobalRef           as GlobalRef,'
      'tblprepayments.PrePaymentID        as InvoiceDocNumber,'
      '-tblprepayments.PayAmount          as TotalAmountInc, '
      '-tblprepayments.Balance            as Balance,'
      '-tblprepayments.Balance            as AmountOutstanding,'
      '-tblprepayments.ForeignPayAmount   as ForeignTotalAmount, '
      '-tblprepayments.ForeignBalance     as ForeignBalanceAmount, '
      '-tblprepayments.ForeignBalance     as ForeignAmountOutstanding, '
      '0                                  as Payment, '
      '0                                  as ForeignPayment,'
      'tblprepayments.ForeignExchangeCode as ForeignExchangeCode, '
      'tblprepayments.ForeignExchangeRate as ForeignExchangeRate, '
      'tblprepayments.Paid                as Paid,'
      '0                                  as Discount, '
      '0                                  as Withholding ,'
      '0                                  as Refunding , '
      '0                                  as RefundPercent, '
      '"F"                                as Apply,'
      'Convert(tblprepayments.ReferenceNo , char(255))  as RefNo,'
      'E.EmployeeName                     as EnteredBy'
      'FROM tblprepayments '
      
        'INNER JOIN tblemployees E on tblprepayments.EmployeeID = E.Emplo' +
        'yeeID'
      'Where Customer="T" AND tblprepayments.Paid="F"'
      'AND tblprepayments.Deleted="F" '
      'AND tblprepayments.ClientID in (0) '
      'AND tblprepayments.Balance <> 0'
      'GROUP By tblprepayments.PrePaymentID '
      'ORDER BY SaleID;')
