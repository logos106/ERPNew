  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      'transid, '
      'SaleID, '
      'GLOBALREF as GLOBALREF ,'
      '`TYPE` as `TYPE` ,'
      'sum(DEBITSEX) as DEBITSEX ,'
      'sum(CREDITSEX) as CREDITSEX ,'
      'sum(DEBITSINC) as DEBITSINC ,'
      'sum(CREDITSINC) as CREDITSINC ,'
      'Sum(CREDITSEX- DEBITSEX )  as EXDiff,'
      'Sum(CREDITSINC - DEBITSINC ) as IncDiff '
      'from tmp_erp_86B872DB_tmp_transactionjournal_4385V3K4'
      'group by  Globalref, TYPE'
      'HAVING abs(EXDiff -IncDiff)>0.5'
      'Order by Date DESC')
