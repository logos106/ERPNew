  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT '
      #9'"Exp" AS Detail,'
      '        e.EmployeeName AS EmployeeName, e.EmployeeID,'
      #9'SUM(P.Tax) AS TotalTaxWithheld,'
      #9'SUM(P.Allowances) AS Allowances,'
      
        #9'SUM(P.Commission) + IF(SUN.TaxExempt = '#39'F'#39', SUM(SUN.Amount), 0)' +
        ' AS Other,  '
      
        #9'SUM(P.Wages) - SUM(P.CDEPGross) + SUM(P.Commission) AS GrossPay' +
        'ments,'
      #9'SUM(IF(PD.UnionFees = '#39'F'#39', PD.Amount, 0)) AS Deductions,'
      #9'SUM(IF(PD.UnionFees = '#39'T'#39', PD.Amount, 0)) AS UnionFees,'
      #9'SUM(p.Tax) AS TotalEmployeeTax,'
      #9'SUM(p.CompanyTax) AS TotalCompanyTax'
      'FROM tblpays p'
      #9'INNER JOIN tblemployees e ON e.EmployeeID = p.EmployeeID'
      #9'LEFT JOIN tblpaysdeductions AS PD ON P.PayID = PD.PayID'
      #9'LEFT JOIN tbldeductions AS D ON D.DeductionID = PD.DeductionID'
      #9'LEFT JOIN tblpayssundries AS SUN ON P.PayID = SUN.PayID'
      'WHERE p.Paid = "T"'
      #9'AND p.Deleted = "F"'
      #9'AND (p.PayDate BETWEEN :dtFrom AND :dtTo)'
      'GROUP BY e.EmployeeID'
      'ORDER BY e.EmployeeID')
    Left = 179
    Top = 344
    ParamData = <
      item
        DataType = ftDate
        Name = 'dtFrom'
        ParamType = ptInput
      end
      item
        DataType = ftDate
        Name = 'dtTo'
        ParamType = ptInput
      end>
