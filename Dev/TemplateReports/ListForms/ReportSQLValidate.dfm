  inherited qryMain: TERPQuery
    SQL.Strings = (
      
        'Select '#39'{Detail}'#39' as Dataname, '#39'SELECT  SL.Product_Description a' +
        's PARTSDESCRIPTION,(SL.UnitofMeasureShipped) as qShipped,(SL.Uni' +
        'tofMeasureBackorder) as qBackorder , (UnitofMeasureQtySold) as q' +
        'Ordered, (SL.totalLineAmountInc) as totalLineAmountInc,P.Product' +
        'Code,SL.Product_Description_Memo as PARTSDESCRIPTION_Memo,SL.Att' +
        'rib1Sale,SL.Attrib2Sale,PBin.Binlocation, PBin.binnumber,PQABatc' +
        'h.Value as batchno, PQABatch.TruckLoadNo , PQABatch.ExpiryDate, ' +
        'sum(if(ifnull(PQASN.PQADEtailID,0)<>0 , PQASN.UOMQty ,if (ifnull' +
        '(PQABins.PQADEtailID,0)<>0 , PQABins.UOMQty , PQABatch.UOMQty)))' +
        ' as BinQty,GROUP_CONCAT(PQASN.Value) as SoldSerials,if(P.Batch  ' +
        '     ='#39#39'T'#39#39','#39#39'Batch number'#39#39' ,'#39#39#39#39') as BatchnumberCaption,if(P.B' +
        'atch       ='#39#39'T'#39#39','#39#39'Expiry Date'#39#39'  ,'#39#39#39#39') as ExpiryDateCaption,i' +
        'f(P.MultipleBins='#39#39'T'#39#39','#39#39'Bin Location'#39#39' ,'#39#39#39#39') as BinLocationCap' +
        'tion,if(P.MultipleBins='#39#39'T'#39#39','#39#39'Bin Number'#39#39'   ,'#39#39#39#39') as Binnumbe' +
        'rCaption,if(P.SNtracking  ='#39#39'T'#39#39','#39#39'Serial Number'#39#39','#39#39#39#39') as Seri' +
        'alnumberCaption,if(P.SNtracking  ='#39#39'T'#39#39' or P.MultipleBins='#39#39'T'#39#39' ' +
        'or P.Batch='#39#39'T'#39#39' , '#39#39'Qty Allocated'#39#39' , '#39#39#39#39')  as QtyAllocated, i' +
        'f(P.SNtracking  ='#39#39'T'#39#39' or P.MultipleBins='#39#39'T'#39#39' or P.Batch='#39#39'T'#39#39' ' +
        ', '#39#39'Qty Picked'#39#39' , '#39#39#39#39')  as QtyPicked,  CONCAT('#39#39'S-'#39#39',S.SaleId)' +
        ' as SaleBarcode, S.*,`SL`.`WarrantyPeriod`,`SL`.`WarrantyEndsOn`' +
        ', SL.sortID,SL.ProductID, SL.ProductName, SL.UnitofMeasureSaleLi' +
        'nes,  if (HoldSale = '#39#39'T'#39#39', '#39#39'HOLD'#39#39', '#39#39#39#39') AS Hold,    P.BARCOD' +
        'E as ProductBarCode,   CONCAT(SL.ProductName,Product_Description' +
        ') AS GroupLine,    CL.DeliveryNotes , CL.CUSTFLD1,CL.CUSTFLD2,CL' +
        '.CUSTFLD3,CL.CUSTFLD4,CL.CUSTFLD5,    CL.CUSTFLD6,CL.CUSTFLD7,CL' +
        '.CUSTFLD8,CL.CUSTFLD9, CL.CUSTFLD10,    CL.CUSTFLD11,CL.CUSTFLD1' +
        '2,CL.CUSTFLD13,CL.CUSTFLD14,CL.CUSTFLD15,    CL.Phone as Custome' +
        'rPhone,CL.FaxNumber as CustomerFaxNumber,    CL.Mobile as Custom' +
        'erMobile,     Left(substring(substring_index(P.ProductGroup,'#39#39'^'#39 +
        #39',3),char_length(substring_index(P.ProductGroup,'#39#39'^'#39#39',2))+2),255' +
        ')  as Dept,   Left(substring(substring_index(P.ProductGroup,'#39#39'^'#39 +
        #39',2),char_length(substring_index(P.ProductGroup,'#39#39'^'#39#39',1))+2),255' +
        ') as type,   Left(SUBSTRING_INDEX(P.ProductGroup,'#39#39'^'#39#39',1),255) a' +
        's Manufacture, Sl.SaleLineID ,(SL.UnitofMeasureShipped) as qShip' +
        'ped, (SL.UnitofMeasureBackorder) as qBackorder , (UnitofMeasureQ' +
        'tySold) as qOrdered, (SL.totalLineAmountInc) as totalLineAmountI' +
        'nc,Concat('#39#39'Invoice No: '#39#39', S.invoicedocnumber) AS Groupfield ,C' +
        'LT.TypeName,DPB.BinLocation as DefaultBinLocation,DPB.BinNumber ' +
        'as DefaultBinNumber ,P.CUSTFLD1 AS ProductCustFld1, P.CUSTFLD2 A' +
        'S ProductCustFld2, P.CUSTFLD3 AS ProductCustFld3, P.CUSTFLD4 AS ' +
        'ProductCustFld4, P.CUSTFLD5 AS ProductCustFld5, P.CUSTFLD6 AS Pr' +
        'oductCustFld6, P.CUSTFLD7 AS ProductCustFld7, P.CUSTFLD8 AS Prod' +
        'uctCustFld8, P.CUSTFLD9 AS ProductCustFld9, P.CUSTFLD10 AS Produ' +
        'ctCustFld10, P.CUSTFLD11 AS ProductCustFld11, P.CUSTFLD12 AS Pro' +
        'ductCustFld12, P.CUSTFLD13 AS ProductCustFld13, P.CUSTFLD14 AS P' +
        'roductCustFld14, P.CUSTFLD15 AS ProductCustFld15, P.CUSTDATE1 AS' +
        ' ProductCustDate1, P.CUSTDATE2 AS ProductCustDate2, P.CUSTDATE3 ' +
        'AS ProductCustDate3,  UOM.Height  as Height, UOM.Width  as Width' +
        ', UOM.Length  as Length, UOM.Weight  as Weight, UOM.Volume  as V' +
        'olume,  SL.UnitofMeasureShipped * UOM.Length as shippedLength,  ' +
        'SL.UnitofMeasureShipped * UOM.Weight as shippedWeight    FROM tb' +
        'lParts as P    inner join tblPQA as PQA on PQA.ProductId = P.Par' +
        'tsID       inner join tblunitsofmeasure UOM on UOM.UnitID = PQA.' +
        'UOMID  inner join tblclass as C on PQA.departmentID = C.ClassID ' +
        '    Left join tblProductClasses as PC on PC.ProductID = PQA.Prod' +
        'uctID and PC.ClassID = PQA.departmentID    left join tblPqaDetai' +
        'ls PQABatch  on PQABatch.PQAId  = PQA.PQAID and PQABatch.PQAType' +
        ' = '#39#39'Batch'#39#39'  and PQABatch.Active= '#39#39'T'#39#39'    left join tblPqaDeta' +
        'ils PQABins  on PQABins.PQAId  = PQA.PQAID   and PQABins.PQAType' +
        ' = '#39#39'Bin'#39#39' and PQABins.Active = '#39#39'T'#39#39' and    ( (PQABatch.globalr' +
        'ef = IFNULL(PQABins.ParentRef,'#39#39#39#39') and P.Batch='#39#39'T'#39#39'   and P.mu' +
        'ltiplebins = '#39#39'T'#39#39') or (ifnull(PQABins.Parentref ,'#39#39#39#39') = '#39#39#39#39' A' +
        'ND P.Batch='#39#39'F'#39#39' and P.multiplebins = '#39#39'T'#39#39'))    Left join tblPr' +
        'oductBin PBin     on PBin.binId  = PQABins.BinID    left join tb' +
        'lPqaDetails PQASN    on (PQASN.PQAId  = PQA.PQAID and PQASN.PQAT' +
        'ype = '#39#39'SN'#39#39'   and PQASN.Active = '#39#39'T'#39#39'  and ((PQABatch.globalre' +
        'f = IFNULL(PQASN.ParentRef,'#39#39#39#39') and P.Batch='#39#39'T'#39#39' and P.multipl' +
        'ebins = '#39#39'F'#39#39' and P.SNTracking = '#39#39'T'#39#39')  or (PQAbins.Globalref =' +
        ' IFNULL(PQASn.ParentRef,'#39#39#39#39') and P.Multiplebins = '#39#39'T'#39#39' and P.S' +
        'NTracking = '#39#39'T'#39#39') or  (ifnull(PQASN.Parentref ,'#39#39#39#39') = '#39#39#39#39') an' +
        'd P.Batch='#39#39'F'#39#39' and P.Multiplebins = '#39#39'F'#39#39' and P.SNTracking = '#39#39 +
        'T'#39#39' ))     inner join tblsaleslines AS SL on SL.ProductID = P.Pa' +
        'rtsID   inner JOIN tblsales AS S ON  SL.SaleID = S.SaleID   inne' +
        'r JOIN tblclients AS CL ON S.ClientID = CL.ClientID   Left join ' +
        'tblclienttype CLT on CLT.ClientTypeID = CL.ClientTypeID   Left J' +
        'oin tblproductbin as DPB on DPB.BinID = PC.DefaultBinID  WHERE S' +
        '.SaleID in( 2) and PQA.transtype in ('#39#39'TSalesOrderLine'#39#39') and PQ' +
        'A.TransId = SL.saleId and PQA.TransLineId = SL.SaleLineId      G' +
        'roup by GroupField,   S.SaleID, SL.SortID,Sl.SalelineID,   PBin.' +
        'binnumber,PBin.Binlocation, PQABatch.Value ,PQABatch.TruckLoadNo' +
        ', PQABatch.ExpiryDate, SL.SaleLineID     Union All   SELECT  SL.' +
        'Product_Description as PARTSDESCRIPTION,(SL.UnitofMeasureShipped' +
        ') as qShipped,(SL.UnitofMeasureBackorder) as qBackorder , (Unito' +
        'fMeasureQtySold) as qOrdered, (SL.totalLineAmountInc) as totalLi' +
        'neAmountInc,P.ProductCode,SL.Product_Description_Memo as PARTSDE' +
        'SCRIPTION_Memo,SL.Attrib1Sale,SL.Attrib2Sale,Null,Null,Null,Null' +
        ',null,null,null,null,null,null,null,null,null, null,  CONCAT('#39#39'S' +
        '-'#39#39',S.SaleId) as SaleBarcode, s.*,`SL`.`WarrantyPeriod`,`SL`.`Wa' +
        'rrantyEndsOn`, SL.SortId,SL.ProductID, SL.ProductName, SL.Unitof' +
        'MeasureSaleLines,  if (HoldSale = '#39#39'T'#39#39', '#39#39'HOLD'#39#39', '#39#39#39#39') AS Hold' +
        ',    P.BARCODE as ProductBarCode,   CONCAT(SL.ProductName,Produc' +
        't_Description) AS GroupLine,    CL.DeliveryNotes , CL.CUSTFLD1,C' +
        'L.CUSTFLD2,CL.CUSTFLD3,CL.CUSTFLD4,CL.CUSTFLD5,    CL.CUSTFLD6,C' +
        'L.CUSTFLD7,CL.CUSTFLD8,CL.CUSTFLD9, CL.CUSTFLD10,    CL.CUSTFLD1' +
        '1,CL.CUSTFLD12,CL.CUSTFLD13,CL.CUSTFLD14,CL.CUSTFLD15,    CL.Pho' +
        'ne as CustomerPhone,CL.FaxNumber as CustomerFaxNumber,    CL.Mob' +
        'ile as CustomerMobile,     Left(substring(substring_index(P.Prod' +
        'uctGroup,'#39#39'^'#39#39',3),char_length(substring_index(P.ProductGroup,'#39#39'^' +
        #39#39',2))+2),255)  as Dept,   Left(substring(substring_index(P.Prod' +
        'uctGroup,'#39#39'^'#39#39',2),char_length(substring_index(P.ProductGroup,'#39#39'^' +
        #39#39',1))+2),255) as type,   Left(SUBSTRING_INDEX(P.ProductGroup,'#39#39 +
        '^'#39#39',1),255) as Manufacture, Sl.SaleLineID , (SL.UnitofMeasureShi' +
        'pped) as qShipped, (SL.UnitofMeasureBackorder) as qBackorder ,  ' +
        '(UnitofMeasureQtySold) as qOrdered, (SL.totalLineAmountInc) as t' +
        'otalLineAmountInc,Concat('#39#39'Invoice No: '#39#39', S.invoicedocnumber) A' +
        'S Groupfield ,CLT.TypeName,NULL, NULL ,P.CUSTFLD1 AS ProductCust' +
        'Fld1, P.CUSTFLD2  AS ProductCustFld2, P.CUSTFLD3  AS ProductCust' +
        'Fld3, P.CUSTFLD4  AS ProductCustFld4, P.CUSTFLD5  AS ProductCust' +
        'Fld5, P.CUSTFLD6  AS ProductCustFld6, P.CUSTFLD7  AS ProductCust' +
        'Fld7, P.CUSTFLD8  AS ProductCustFld8, P.CUSTFLD9  AS ProductCust' +
        'Fld9, P.CUSTFLD10 AS ProductCustFld10, P.CUSTFLD11 AS ProductCus' +
        'tFld11, P.CUSTFLD12 AS ProductCustFld12, P.CUSTFLD13 AS ProductC' +
        'ustFld13, P.CUSTFLD14 AS ProductCustFld14, P.CUSTFLD15 AS Produc' +
        'tCustFld15, P.CUSTDATE1 AS ProductCustDate1, P.CUSTDATE2 AS Prod' +
        'uctCustDate2, P.CUSTDATE3 AS ProductCustDate3,  UOM.Height  as H' +
        'eight, UOM.Width  as Width, UOM.Length  as Length, UOM.Weight  a' +
        's Weight, UOM.Volume  as Volume,  SL.UnitofMeasureShipped * UOM.' +
        'Length as shippedLength,  SL.UnitofMeasureShipped * UOM.Weight a' +
        's shippedWeight  FROM tblParts as P    inner join tblsaleslines ' +
        'AS SL on SL.ProductID = P.PartsID    inner join tblunitsofmeasur' +
        'e UOM on UOM.UnitID = SL.UnitOfMeasureID  inner JOIN tblsales AS' +
        ' S ON  SL.SaleID = S.SaleID    inner JOIN tblclients AS CL ON S.' +
        'ClientID = CL.ClientID    Left join tblclienttype CLT on CLT.Cli' +
        'entTypeID = CL.ClientTypeID Where P.PartType <> '#39#39'INV'#39#39' AND S.Sa' +
        'leID in ( 2)     Group by  GroupField,SL.SaleLineID, S.SaleID, S' +
        'L.SortID, Sl.SaleLineId    ORDER BY SaleID, SortID'#39' as  SQLStrin' +
        'g'
      'union all '
      
        'Select '#39'{Master}'#39' as Dataname, '#39'SELECT  CompanyName, Address, Ad' +
        'dress2, City, State, Postcode,   PhoneNumber AS PhoneNumber,FaxN' +
        'umber AS FaxNumber, ABN, Email as CompanyEmail, URL as CompanyUR' +
        'L FROM tblCompanyInformation'#39' as  SQLString'
      'union all '
      
        'Select '#39'{AttribCaptions}'#39' as Dataname, '#39'SELECT  '#39#39'M'#39#39' as Product' +
        'Attrib1Name, '#39#39'Weight'#39#39' as ProductAttrib2Name, '#39#39'Price / M'#39#39' as ' +
        'ProductAttrib1SaleRate'#39' as  SQLString'
      'union all '
      
        'Select '#39'{BinBatchStock}'#39' as Dataname, '#39' Select PQa.UOM,C.Classna' +
        'me, SL.Product_Description as PARTSDESCRIPTION,SL.Product_Descri' +
        'ption_Memo as PARTSDESCRIPTION_Memo,SL.Attrib1Sale,SL.Attrib2Sal' +
        'e,PBin.Binlocation, PBin.binnumber,PQABatch.Value as batchno, PQ' +
        'ABatch.TruckLoadNo, PQABatch.ExpiryDate,   sum(if (ifnull(PQABin' +
        's.PQADEtailID,0)<>0 , PQABins.UOMQty , PQABatch.UOMQty)) as BinQ' +
        'ty,   sum(if (ifnull(PQABins.PQADEtailID,0)<>0 , PQABins.UOMQty ' +
        ', PQABatch.UOMQty)) as AllocatedQty,   CONCAT('#39#39'S-'#39#39',S.SaleId) a' +
        's SaleBarcode, S.*,`SL`.`WarrantyPeriod`,`SL`.`WarrantyEndsOn`, ' +
        'SL.sortID,SL.ProductID, SL.ProductName, SL.UnitofMeasureSaleLine' +
        's,  if (HoldSale = '#39#39'T'#39#39', '#39#39'HOLD'#39#39', '#39#39#39#39') AS Hold,    P.BARCODE ' +
        'as ProductBarCode,   CONCAT(SL.ProductName,Product_Description) ' +
        'AS GroupLine,    CL.DeliveryNotes , CL.CUSTFLD1,CL.CUSTFLD2,CL.C' +
        'USTFLD3,CL.CUSTFLD4,CL.CUSTFLD5,    CL.CUSTFLD6,CL.CUSTFLD7,CL.C' +
        'USTFLD8,CL.CUSTFLD9, CL.CUSTFLD10,    CL.CUSTFLD11,CL.CUSTFLD12,' +
        'CL.CUSTFLD13,CL.CUSTFLD14,CL.CUSTFLD15,    CL.Phone as CustomerP' +
        'hone,CL.FaxNumber as CustomerFaxNumber,    CL.Mobile as Customer' +
        'Mobile,     Left(substring(substring_index(P.ProductGroup,'#39#39'^'#39#39',' +
        '3),char_length(substring_index(P.ProductGroup,'#39#39'^'#39#39',2))+2),255) ' +
        ' as Dept,   Left(substring(substring_index(P.ProductGroup,'#39#39'^'#39#39',' +
        '2),char_length(substring_index(P.ProductGroup,'#39#39'^'#39#39',1))+2),255) ' +
        'as type,   Left(SUBSTRING_INDEX(P.ProductGroup,'#39#39'^'#39#39',1),255) as ' +
        'Manufacture, Sl.SaleLineID ,(SL.UnitofMeasureShipped) as qShippe' +
        'd, (SL.UnitofMeasureBackorder) as qBackorder , (UnitofMeasureQty' +
        'Sold) as qOrdered, (SL.totalLineAmountInc) as totalLineAmountInc' +
        ',Concat('#39#39'Invoice No: '#39#39', S.invoicedocnumber) AS Groupfield ,CLT' +
        '.TypeName,DPB.BinLocation as DefaultBinLocation,DPB.BinNumber as' +
        ' DefaultBinNumber ,P.CUSTFLD1 AS ProductCustFld1, P.CUSTFLD2 AS ' +
        'ProductCustFld2, P.CUSTFLD3 AS ProductCustFld3, P.CUSTFLD4 AS Pr' +
        'oductCustFld4, P.CUSTFLD5 AS ProductCustFld5, P.CUSTFLD6 AS Prod' +
        'uctCustFld6, P.CUSTFLD7 AS ProductCustFld7, P.CUSTFLD8 AS Produc' +
        'tCustFld8, P.CUSTFLD9 AS ProductCustFld9, P.CUSTFLD10 AS Product' +
        'CustFld10, P.CUSTFLD11 AS ProductCustFld11, P.CUSTFLD12 AS Produ' +
        'ctCustFld12, P.CUSTFLD13 AS ProductCustFld13, P.CUSTFLD14 AS Pro' +
        'ductCustFld14, P.CUSTFLD15 AS ProductCustFld15, P.CUSTDATE1 AS P' +
        'roductCustDate1, P.CUSTDATE2 AS ProductCustDate2, P.CUSTDATE3 AS' +
        ' ProductCustDate3  FROM tblParts as P  inner join tblPQA as PQA ' +
        'on PQA.ProductId = P.PartsID   and ((P.multiplebins = '#39#39'F'#39#39' and ' +
        'P.Batch='#39#39'F'#39#39' and P.SNTracking = '#39#39'F'#39#39') OR (PQA.PQAID in (Select' +
        ' PQAID from tblPQADetails)))  inner join tblclass as C on PQA.de' +
        'partmentID = C.ClassID   Left join tblProductClasses as PC on PC' +
        '.ProductID = PQA.ProductID and PC.ClassID = PQA.departmentID  le' +
        'ft join tblPqaDetails PQABatch  on PQABatch.PQAId  = PQA.PQAID a' +
        'nd PQABatch.PQAType = '#39#39'Batch'#39#39'  and PQABatch.Active= '#39#39'T'#39#39'  lef' +
        't join tblPqaDetails PQABins  on PQABins.PQAId  = PQA.PQAID   an' +
        'd PQABins.PQAType = '#39#39'Bin'#39#39' and PQABins.Active = '#39#39'T'#39#39' and    ( ' +
        '(PQABatch.globalref = IFNULL(PQABins.ParentRef,'#39#39#39#39') and P.Batch' +
        '='#39#39'T'#39#39'   and P.multiplebins = '#39#39'T'#39#39') or (ifnull(PQABins.Parentre' +
        'f ,'#39#39#39#39') = '#39#39#39#39' AND P.Batch='#39#39'F'#39#39' and P.multiplebins = '#39#39'T'#39#39'))  ' +
        'Left join tblProductBin PBin     on PBin.binId  = PQABins.BinID ' +
        '  inner join tblSaleslines SL on SL.ProductId = P.PartsId  inner' +
        ' JOIN tblsales AS S ON  SL.SaleID = S.SaleID  inner join tblClie' +
        'nts CL on CL.ClientID = S.ClientID Left join tblclienttype CLT o' +
        'n CLT.ClientTypeID = CL.ClientTypeID  Left Join tblproductbin as' +
        ' DPB on DPB.BinID = PC.DefaultBinID  Where P.PartType = '#39#39'INV'#39#39' ' +
        'AND  S.SaleID in( 2)   Group by GroupField,   S.SaleID, SL.SortI' +
        'D,Sl.SalelineID,   PBin.binnumber,PBin.Binlocation, PQABatch.Val' +
        'ue , PQABatch.TruckLoadNo, PQABatch.ExpiryDate, SL.SaleLineID'#39' a' +
        's  SQLString')
