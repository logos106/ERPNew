  inherited qryMain: TERPQuery
    SQL.Strings = (
      'SELECT  distinct '
      '1 as details,'
      's.SaleId, '
      'S.IsInternalOrder, '
      'NULL as PSAID ,'
      'S.customername , '
      'S.SaleDate, '
      'SL.productName, '
      
        'concat(PT.TreePartUOM , '#39'  ('#39' , PT.TreePartUOMMultiplier , '#39')'#39' )' +
        ' as UOM,'
      'SL.UnitofMeasureShipped, '
      'PT.proctreeId, '
      'PT.FromStockUOMQty,'
      'PT.OnOrderUOMQty,'
      'PT.ManufactureUOMQty,'
      'PTS.TotalStowedQty / PT.TreePartUOMMultiplier TotalStowedQty, '
      
        '(PTS.TotalStowedQty - PT.ManufactureQty) / PT.TreePartUOMMultipl' +
        'ier  ExtraQtyManufactures, '
      
        'PTS.ExtraQtyToCustomer / PT.TreePartUOMMultiplier ExtraQtyToCust' +
        'omer, '
      'PTS.ExtraQtytoStore  / PT.TreePartUOMMultiplier ExtraQtytoStore,'
      'null as EmployeeName , '
      'null as StowAwayDate ,'
      'null as StowQty,'
      'NULL as StowAwayUOM,'
      'NULL as LineStowQty'
      'FROM  tblSales S'
      'inner join tblSaleslines SL  on  S.SaleId = SL.SaLeId'
      
        'inner join tblproctree PT  on PT.MasterId = SL.SaleLineId and if' +
        'null(PT.parentId,0)=0 and mastertype = '#39'mtsalesOrder'#39
      
        'inner join tblprocesspart PP on PP.proctreeId = PT.proctreeID an' +
        'd PP.timestart <> '#39'1899-12-30 00:00:00'#39
      'Left join tblproctreestow PTS on PTS.ProcTreeId = PT.ProcTreeId'
      
        'Where ifnull(PT.ManufactureUOMQty,0)<> 0 and ifnull(PTS.ProcTree' +
        'Id ,0)=0'
      'and  PP.timestart between :DateFrom and :DateTo'
      'union all '
      'SELECT  distinct '
      '2 as details,'
      's.SaleId, '
      'S.IsInternalOrder, '
      'NULL as PSAID ,'
      'S.customername , '
      'S.SaleDate, '
      'SL.productName, '
      
        'concat(PT.TreePartUOM , '#39'  ('#39' , PT.TreePartUOMMultiplier , '#39')'#39' )' +
        ' as UOM,'
      'SL.UnitofMeasureShipped, '
      'PT.proctreeId, '
      'PT.FromStockUOMQty,'
      'PT.OnOrderUOMQty,'
      'PT.ManufactureUOMQty,'
      'PTS.TotalStowedQty / PT.TreePartUOMMultiplier TotalStowedQty, '
      
        '(PTS.TotalStowedQty - PT.ManufactureQty) / PT.TreePartUOMMultipl' +
        'ier  ExtraQtyManufactures, '
      
        'PTS.ExtraQtyToCustomer / PT.TreePartUOMMultiplier ExtraQtyToCust' +
        'omer, '
      'PTS.ExtraQtytoStore  / PT.TreePartUOMMultiplier ExtraQtytoStore,'
      'null as EmployeeName , '
      'null as StowAwayDate ,'
      'null as StowQty,'
      'NULL as StowAwayUOM,'
      'NULL as LineStowQty'
      'FROM  tblSales S'
      'inner join tblSaleslines SL  on  S.SaleId = SL.SaLeId'
      
        'inner join tblproctree PT  on PT.MasterId = SL.SaleLineId and if' +
        'null(PT.parentId,0)=0 and mastertype = '#39'mtsalesOrder'#39
      
        'inner  join tblproctreestow PTS on PTS.ProcTreeId = PT.ProcTreeI' +
        'd'
      'inner join tblprocstowaway PSA on PSA.ProctreeStowId = PTS.ID'
      'Where ifnull(PT.ManufactureUOMQty,0)<> 0'
      'and  PSA.StowAwayDate  between :DateFrom and :DateTo'
      'union all '
      'SELECT '
      '3 as details,'
      's.SaleId, '
      'S.IsInternalOrder, '
      'PSA.ID as PSAID ,'
      'S.customername , '
      'S.SaleDate, '
      'SL.productName, '
      
        'concat(PT.TreePartUOM , '#39'  ('#39' , PT.TreePartUOMMultiplier , '#39')'#39' )' +
        ' as UOM,'
      'SL.UnitofMeasureShipped, '
      'PT.proctreeId, '
      'PT.FromStockUOMQty,'
      'PT.OnOrderUOMQty,'
      'PT.ManufactureUOMQty,'
      'PTS.TotalStowedQty / PT.TreePartUOMMultiplier TotalStowedQty, '
      
        '(PTS.TotalStowedQty - PT.ManufactureQty) / PT.TreePartUOMMultipl' +
        'ier  ExtraQtyManufactures, '
      
        'PTS.ExtraQtyToCustomer / PT.TreePartUOMMultiplier ExtraQtyToCust' +
        'omer, '
      'PTS.ExtraQtytoStore  / PT.TreePartUOMMultiplier ExtraQtytoStore,'
      'PSA.EmployeeName , '
      'PSA.StowAwayDate ,'
      'PSA.UOMQty StowQty,'
      'NULL as StowAwayUOM,'
      'NULL as LineStowQty'
      'FROM  tblSales S'
      'inner join tblSaleslines SL  on  S.SaleId = SL.SaLeId'
      'inner join tblproctree PT  on PT.MasterId = SL.SaleLineId'
      'inner join tblproctreestow PTS on PTS.ProcTreeId = PT.ProcTreeId'
      'inner join tblprocstowaway PSA on PSA.ProctreeStowId = PTS.ID'
      'Where ifnull(PT.ManufactureUOMQty,0)<> 0'
      'and  PSA.StowAwayDate  between :DateFrom and :DateTo'
      'union all '
      'SELECT '
      '4 as details,'
      's.SaleId, '
      'S.IsInternalOrder, '
      'PSA.ID as PSAID ,'
      'S.customername , '
      'S.SaleDate, '
      'SL.productName, '
      
        'concat(PT.TreePartUOM , '#39'  ('#39' , PT.TreePartUOMMultiplier , '#39')'#39' )' +
        ' as UOM,'
      'SL.UnitofMeasureShipped, '
      'PT.proctreeId, '
      'PT.FromStockUOMQty,'
      'PT.OnOrderUOMQty,'
      'PT.ManufactureUOMQty,'
      'PTS.TotalStowedQty / PT.TreePartUOMMultiplier TotalStowedQty, '
      
        '(PTS.TotalStowedQty - PT.ManufactureQty) / PT.TreePartUOMMultipl' +
        'ier  ExtraQtyManufactures, '
      
        'PTS.ExtraQtyToCustomer / PT.TreePartUOMMultiplier ExtraQtyToCust' +
        'omer, '
      'PTS.ExtraQtytoStore  / PT.TreePartUOMMultiplier ExtraQtytoStore,'
      'PSA.EmployeeName , '
      'PSA.StowAwayDate ,'
      'PSA.UOMQty StowQty,'
      'concat(PSAL.UOM , '#39'  ('#39' , PSAL.UOMMult , '#39')'#39' ) as StowAwayUOM,'
      'PSAL.UOMQty LineStowQty'
      'FROM  tblSales S'
      'inner join tblSaleslines SL  on  S.SaleId = SL.SaLeId'
      'inner join tblproctree PT  on PT.MasterId = SL.SaleLineId'
      'inner join tblproctreestow PTS on PTS.ProcTreeId = PT.ProcTreeId'
      'inner join tblprocstowaway PSA on PSA.ProctreeStowId = PTS.ID'
      'inner join tblprocstowawaylines PSAL on PSA.ID = PSAL.StowAwayID'
      'Where ifnull(PT.ManufactureUOMQty,0)<> 0'
      'and  PSA.StowAwayDate  between :DateFrom and :DateTo'
      'order by saleId desc , PSAID , details, StowAwayDate desc')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end
      item
        DataType = ftUnknown
        Name = 'DateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'DateTo'
      end>
